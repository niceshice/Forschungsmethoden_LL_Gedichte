{
    "title": "zwei-entzweiung-5715",
    "gender": "w",
    "author": "Elfriede Czurda",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "schwarz, sphärisch schwer"
        },
        "line.2": {
            "text": "bricht gewaltsam unter"
        },
        "line.3": {
            "text": "dem eis unterschütterlich"
        },
        "line.4": {
            "text": "der fels,entzweit sich"
        },
        "line.5": {
            "text": "das fundament mit dem"
        },
        "line.6": {
            "text": "firmament, nacht verträgt"
        },
        "line.7": {
            "text": "nicht den tag das eis"
        },
        "line.8": {
            "text": "nicht das meer, wasser"
        },
        "line.9": {
            "text": "unter der wucht des wassers"
        },
        "line.10": {
            "text": "das buch unter der wucht"
        },
        "line.11": {
            "text": "der namen, höhlung eins"
        },
        "line.12": {
            "text": "dem andern, drehung"
        }
    }
}