{
    "title": "blind-date-8409",
    "gender": "w",
    "author": "Doris Runge",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "es muss ja nicht"
        },
        "line.2": {
            "text": "gleich sein"
        },
        "line.3": {
            "text": "nicht hier sein"
        },
        "line.4": {
            "text": "zwischen tür und"
        },
        "line.5": {
            "text": "engel abflug"
        },
        "line.6": {
            "text": "und ankunft"
        },
        "line.7": {
            "text": "in zugigen höfen"
        },
        "line.8": {
            "text": "es könnte"
        },
        "line.9": {
            "text": "im sommer sein"
        },
        "line.10": {
            "text": "wenn man"
        },
        "line.11": {
            "text": "den schatten liebt"
        },
        "line.12": {
            "text": "es wird keine"
        },
        "line.13": {
            "text": "liebe sein"
        },
        "line.14": {
            "text": "jedenfalls keine"
        },
        "line.15": {
            "text": "fürs leben"
        }
    }
}