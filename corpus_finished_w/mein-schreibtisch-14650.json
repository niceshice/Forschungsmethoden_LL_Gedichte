{
    "title": "mein-schreibtisch-14650",
    "gender": "w",
    "author": "Lisa Elsässer",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "hat eine affaire mit"
        },
        "line.2": {
            "text": "einer alten dichterin"
        },
        "line.3": {
            "text": "ich lasse sie und ihn"
        },
        "line.4": {
            "text": "ein paar gedichte"
        },
        "line.5": {
            "text": "schreiben über die so"
        },
        "line.6": {
            "text": "genannte liebe meine"
        },
        "line.7": {
            "text": "freiheit grenzenlos"
        },
        "line.8": {
            "text": "am see straffe ich"
        },
        "line.9": {
            "text": "die lider für meinen"
        },
        "line.10": {
            "text": "ausgebrochenen tisch"
        },
        "line.11": {
            "text": "genossen und hoffe"
        },
        "line.12": {
            "text": "bange lieb ihn doch"
        }
    }
}