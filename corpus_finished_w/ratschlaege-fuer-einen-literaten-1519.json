    "gender": "w",
    "author": "Elfriede Gerstl",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "in wien fall hin"
        },
        "line.2": {
            "text": "nach berlin kannst ziehn"
        },
        "line.3": {
            "text": "nach bogota alser toter"
        },
        "line.4": {
            "text": "faulenz in florenz"
        },
        "line.5": {
            "text": "in prag tag"
        },
        "line.6": {
            "text": "in paris an gstiess"
        },
        "line.7": {
            "text": "in rom"
        },
        "line.8": {
            "text": "such den stefansdom"
        },
        "line.9": {
            "text": "in den abruzzen"
        },
        "line.10": {
            "text": "kauf stuzzen"
        },
        "line.11": {
            "text": "lieb in leoben"
        },
        "line.12": {
            "text": "fozen in bozen"
        },
        "line.13": {
            "text": "hab an lebenswandel"
        },
        "line.14": {
            "text": "im alpenlandl"
        },
        "line.15": {
            "text": "und dicht"
        },
        "line.16": {
            "text": "Ã¼berhaupt nicht"
        }
