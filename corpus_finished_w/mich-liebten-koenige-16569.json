{
    "title": "mich-liebten-koenige-16569",
    "gender": "w",
    "author": "Daniela  Chana",
    "author_birth": "1985",
    "poem": {
        "line.1": {
            "text": "Mich liebten Könige, sie"
        },
        "line.2": {
            "text": "Küssten mich in Labyrinthen"
        },
        "line.3": {
            "text": "Männer kreisten mich ein und"
        },
        "line.4": {
            "text": "Umstellten mich mit Spiegeln"
        },
        "line.5": {
            "text": "Von jeder Seite des Panopticons"
        },
        "line.6": {
            "text": "Sah ich ein bisschen anders aus"
        },
        "line.7": {
            "text": "Mich liebten Könige, sie"
        },
        "line.8": {
            "text": "Zerrten mich in große Betten"
        },
        "line.9": {
            "text": "Jeder überließ mir Diener und Sklaven"
        },
        "line.10": {
            "text": "Als Abschiedsgeschenk"
        },
        "line.11": {
            "text": "Auf jeder Seite des Ozeans"
        },
        "line.12": {
            "text": "Sahen sie ein bisschen anders aus"
        },
        "line.13": {
            "text": "Ich ritt Löwen"
        },
        "line.14": {
            "text": "Könige, Prinzen, ein paar Piraten"
        },
        "line.15": {
            "text": "Schrieben mir Briefe, bis sie starben"
        },
        "line.16": {
            "text": "Manchmal gehe ich zum Markt"
        },
        "line.17": {
            "text": "Kaufe Orangen und Zitronen"
        },
        "line.18": {
            "text": "Betrüge die Obstfrauen lächelnd"
        },
        "line.19": {
            "text": "Um ein bisschen Geld"
        },
        "line.20": {
            "text": "Denn mich liebten Könige, mich"
        },
        "line.21": {
            "text": "Aber nicht sie"
        }
    }
}