{
    "title": "schliefen-die-oefen-iii-malczyce-maltsch-3735",
    "gender": "w",
    "author": "Uljana Wolf",
    "author_birth": "1979",
    "poem": {
        "line.1": {
            "text": "malczyce / maltsch"
        },
        "line.2": {
            "text": "als die ausrangierten wagen"
        },
        "line.3": {
            "text": "auf dem nebengleis träumten"
        },
        "line.4": {
            "text": "vom umschlagplatz am oderknie"
        },
        "line.5": {
            "text": "von güterschütt und kohlenstaub"
        },
        "line.6": {
            "text": "stahlen wir einen trägen waggon"
        },
        "line.7": {
            "text": "aus seinem gleis aus seinem bett"
        },
        "line.8": {
            "text": "ließen in den leeren lagerhallen"
        },
        "line.9": {
            "text": "funken springen die uns galten"
        }
    }
}