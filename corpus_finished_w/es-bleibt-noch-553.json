{
    "title": "es-bleibt-noch-553",
    "gender": "w",
    "author": "Rose Ausländer",
    "author_birth": "1901",
    "poem": {
        "line.1": {
            "text": "Dennoch herrlich"
        },
        "line.2": {
            "text": "Staub aus Fleisch"
        },
        "line.3": {
            "text": "Diese Lichtgeburt"
        },
        "line.4": {
            "text": "im Wimpernschoß"
        },
        "line.5": {
            "text": "Lippen"
        },
        "line.6": {
            "text": "ja"
        },
        "line.7": {
            "text": "es bleibt noch"
        },
        "line.8": {
            "text": "viel zu sagen"
        }
    }
}