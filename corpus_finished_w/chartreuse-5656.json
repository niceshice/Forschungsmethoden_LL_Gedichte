{
    "title": "chartreuse-5656",
    "gender": "w",
    "author": "Sabine Scho",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "glaub das doch bitte nicht"
        },
        "line.2": {
            "text": "was man sagt, was man sieht"
        },
        "line.3": {
            "text": "die abgewedelten deprivationen"
        },
        "line.4": {
            "text": "der aufgehellte fleck, und frag"
        },
        "line.5": {
            "text": "nicht nach fahnen, am malen"
        },
        "line.6": {
            "text": "ist doch was dran, man kann"
        },
        "line.7": {
            "text": "auch taschenschirme klonen"
        },
        "line.8": {
            "text": "und behaupten sie seien geklaut"
        },
        "line.9": {
            "text": "bis davon was sichtbar wird"
        },
        "line.10": {
            "text": "bleibt regen regen, es l√§uft"
        },
        "line.11": {
            "text": "es rollt die leere fassade"
        },
        "line.12": {
            "text": "hat es je jemanden interessiert"
        },
        "line.13": {
            "text": "was man dahinter wie destilliert"
        }
    }
}