{
    "title": "gott-will-immer-alles-panieren-14876",
    "gender": "w",
    "author": "Dagmara Kraus",
    "author_birth": "1981",
    "poem": {
        "line.1": {
            "text": "gott will immer alles panieren"
        },
        "line.2": {
            "text": "er wühlt in mehl"
        },
        "line.3": {
            "text": "und eischleim"
        },
        "line.4": {
            "text": "und panade"
        },
        "line.5": {
            "text": "er pfeift"
        },
        "line.6": {
            "text": "und singt dabei"
        },
        "line.7": {
            "text": "und was hier fehlt"
        },
        "line.8": {
            "text": "das wälzt und würzt er gerade"
        }
    }
}