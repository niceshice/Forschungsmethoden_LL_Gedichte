{
    "title": "steilkueste-11924",
    "gender": "w",
    "author": "Anja Kampmann",
    "author_birth": "1983",
    "poem": {
        "line.1": {
            "text": "schon bald ist sonntag"
        },
        "line.2": {
            "text": "in den klippen verfangen sich"
        },
        "line.3": {
            "text": "die wölfe so klingt das meer das uns trifft"
        },
        "line.4": {
            "text": "das rollen der steine von vorn ein paar stiefel"
        },
        "line.5": {
            "text": "im fels wie sich die wogen waschen an der luft"
        },
        "line.6": {
            "text": "im laufe bläht der wind das cape den raum"
        },
        "line.7": {
            "text": "für dein kleines gedächtnis gelb"
        },
        "line.8": {
            "text": "als"
        },
        "line.9": {
            "text": "sie rannten"
        },
        "line.10": {
            "text": "kinder die ihre zungen"
        },
        "line.11": {
            "text": "in den regen strecken meer salz das heulen"
        },
        "line.12": {
            "text": "des winds zu erlernen von vorn"
        },
        "line.13": {
            "text": "mit der gischt kommt die liebe rau"
        },
        "line.14": {
            "text": "in all ihren alten sprachen."
        }
    }
}