{
    "title": "susurrus-10808",
    "gender": "w",
    "author": "Katharina Schultens",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "ich habe das verklärt was du nicht bist"
        },
        "line.2": {
            "text": "rauschen: du bist ein anderes geräusch"
        },
        "line.3": {
            "text": "du bist ein unerkannter susurrus"
        },
        "line.4": {
            "text": "du blendest für mich alles andre aus"
        },
        "line.5": {
            "text": "sprichst du über den bienenschwarm hinaus"
        },
        "line.6": {
            "text": "so summt darin mein denken ("
        },
        "line.7": {
            "text": "bienenhaus"
        },
        "line.8": {
            "text": ")"
        },
        "line.9": {
            "text": "ich kann dir sagen was die bienen sehen"
        },
        "line.10": {
            "text": "ich kann notieren welche zeichen"
        },
        "line.11": {
            "text": "der schwarm verwendet hat"
        },
        "line.12": {
            "text": "dein bienengeist: er dunkelt unter licht"
        },
        "line.13": {
            "text": "verlässt du weiterhin den stock nicht"
        },
        "line.14": {
            "text": "muss ich erinnern was du weißt"
        },
        "line.15": {
            "text": "ich sammle dir ereignisse"
        },
        "line.16": {
            "text": "sobald ich sie verwandelt habe"
        },
        "line.17": {
            "text": "bringst du das zuckerwasser: tausch"
        },
        "line.18": {
            "text": "du füllst den imkeranzug susurr"
        },
        "line.19": {
            "text": "du hast den schleier nie verloren"
        },
        "line.20": {
            "text": "du bist hier eigentlich die braut"
        }
    }
}