{
    "title": "schliefen-die-oefen-iv-die-kleinen-bahnhoefe-ohne-einen-3736",
    "gender": "w",
    "author": "Uljana Wolf",
    "author_birth": "1979",
    "poem": {
        "line.1": {
            "text": "die kleinen bahnhöfe ohne einen ort"
        },
        "line.2": {
            "text": "wolfgang koeppen"
        },
        "line.3": {
            "text": "als wir aber auf offner strecke"
        },
        "line.4": {
            "text": "aufstoben zwischen den orten"
        },
        "line.5": {
            "text": "weil auch die sterne sagt man"
        },
        "line.6": {
            "text": "ihren ofen über uns anheizen"
        },
        "line.7": {
            "text": "funkten wir durch landschaft"
        },
        "line.8": {
            "text": "die wie flugasche um uns lag"
        },
        "line.9": {
            "text": "noch einmal die route durch"
        },
        "line.10": {
            "text": "zum haus der weichenwärter"
        }
    }
}