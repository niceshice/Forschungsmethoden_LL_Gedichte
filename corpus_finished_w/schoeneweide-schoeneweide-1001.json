{
    "title": "schoeneweide-schoeneweide-1001",
    "gender": "w",
    "author": "Ulrike Draesner",
    "author_birth": "1962",
    "poem": {
        "line.1": {
            "text": "heute werden die frösche einst prinzliche"
        },
        "line.2": {
            "text": "wurfgeschosse in den tiefen des lebens"
        },
        "line.3": {
            "text": "gelöst manche sagen die tiefe sei einfach"
        },
        "line.4": {
            "text": "ein keller dunkel ein wenig feucht sie"
        },
        "line.5": {
            "text": "kleben versteht sich in diesem sinn:"
        },
        "line.6": {
            "text": "abgemacht."
        },
        "line.7": {
            "text": "schöneweide schöneweide"
        },
        "line.8": {
            "text": "sie zieht den mund sich weit dem kind"
        },
        "line.9": {
            "text": "die spange von den zähnen: beiß"
        },
        "line.10": {
            "text": "in diesen haufen reis. ab sofort patschhände"
        },
        "line.11": {
            "text": "patsch ist der cent nach dem simultan"
        },
        "line.12": {
            "text": "man sich bückt kupferdraht."
        },
        "line.13": {
            "text": "schöneweide schöneweide"
        },
        "line.14": {
            "text": "kupferdraht"
        },
        "line.15": {
            "text": "verwendung wand: brautkleid in fetzen"
        },
        "line.16": {
            "text": "sbahnscheiben sicht: stöckelschuh"
        },
        "line.17": {
            "text": "auf kellerstrich fest sie zieht"
        },
        "line.18": {
            "text": "den mund sich nach von der wand"
        },
        "line.19": {
            "text": "unnachahmlich soft die frage rinnt"
        },
        "line.20": {
            "text": "stimmchen kind:"
        },
        "line.21": {
            "text": "zerhochtungsfest"
        },
        "line.22": {
            "text": ", ihr"
        },
        "line.23": {
            "text": "macht?"
        },
        "line.24": {
            "text": "schöneweide schöneweide"
        },
        "line.25": {
            "text": "fahr"
        },
        "line.26": {
            "text": "an der scheibe schlangenseite"
        },
        "line.27": {
            "text": "verwendung wand: herz hand hirn ent-"
        },
        "line.28": {
            "text": "hocht ein haufen reis"
        },
        "line.29": {
            "text": "schöneweide schöneweide"
        },
        "line.30": {
            "text": "gestartet einst, gestaunt, gestaut"
        },
        "line.31": {
            "text": "star am steig gestern noch gefleckt"
        },
        "line.32": {
            "text": "die brust (irr tüpfel weiß) fetzen kleid"
        },
        "line.33": {
            "text": "gestreut gestarrt zu herzen"
        },
        "line.34": {
            "text": "ging: schlangenei die hügel vor der scheibe"
        },
        "line.35": {
            "text": "schlangenhülse hab mich"
        },
        "line.36": {
            "text": "lieb"
        },
        "line.37": {
            "text": "im keller batterienkraft wäsche frosch"
        },
        "line.38": {
            "text": "kommodenbild am öltank schuh gestellt"
        },
        "line.39": {
            "text": "gewärmt gehöhlt kind - beeil dich -"
        },
        "line.40": {
            "text": "gemacht (jeder weiß wo das liegt)"
        },
        "line.41": {
            "text": "schöneweide schöneweide"
        },
        "line.42": {
            "text": "fahr"
        },
        "line.43": {
            "text": "folie auf dem sbahnfenster (hab mich"
        },
        "line.44": {
            "text": "verliebt): nachts auf offnem wagen decke"
        },
        "line.45": {
            "text": "bis zum kinn gestarrt als junge frau"
        },
        "line.46": {
            "text": "als regen kam blitzten unter plane"
        },
        "line.47": {
            "text": "sterne nach: himmelsreis, ihm und"
        },
        "line.48": {
            "text": "ihr zuhauf"
        },
        "line.49": {
            "text": "festgenagelt (mit der zunge an die wand)"
        },
        "line.50": {
            "text": "zusammenziehen um zu sparen (wuhle"
        },
        "line.51": {
            "text": "eiche grüne trift) im brautkleid sbahn"
        },
        "line.52": {
            "text": "fuhr verhielten jäger im hochsitz"
        },
        "line.53": {
            "text": "sich die himmel jokerlich"
        },
        "line.54": {
            "text": "schöneweide schöneweide"
        },
        "line.55": {
            "text": "alles vor ihr-ihm-uns und"
        },
        "line.56": {
            "text": "keiner da"
        },
        "line.57": {
            "text": "kascheln gegangen, eis, unterm aquafilm"
        },
        "line.58": {
            "text": "zerkratztes gesicht das kind seinen pinguin"
        },
        "line.59": {
            "text": "auf geliehenen schuhen schiebt die ängstlichkeit"
        },
        "line.60": {
            "text": "nachts zwischen uns"
        },
        "line.61": {
            "text": "schöneweide schöneweide"
        },
        "line.62": {
            "text": "streckten uns, steif im keller"
        },
        "line.63": {
            "text": "jagt echohexe schuh zu schuh"
        },
        "line.64": {
            "text": "(am hochzeitstag) hinterm kessel"
        },
        "line.65": {
            "text": "hinter fehlern ich die steuerung"
        },
        "line.66": {
            "text": "(heizsystem) nach oben riss"
        },
        "line.67": {
            "text": "zwei pfeile rot gebogen aufeinander"
        },
        "line.68": {
            "text": "zu REPEAT"
        },
        "line.69": {
            "text": "schweineöde schweineöde"
        },
        "line.70": {
            "text": "schub"
        },
        "line.71": {
            "text": "elstern krähen amselfeld die alte"
        },
        "line.72": {
            "text": "wie die frische brut, klebrige rotte"
        },
        "line.73": {
            "text": "letztjährige kraft an den sohlen"
        },
        "line.74": {
            "text": "vorm tank wummernd das hirn"
        },
        "line.75": {
            "text": "gekaschelt gekuschelt gekuscht sah"
        },
        "line.76": {
            "text": "wer die verquollenen augen"
        },
        "line.77": {
            "text": "putzte im spiegel zähne sich"
        },
        "line.78": {
            "text": "das bild sah wer das krustenlächeln"
        },
        "line.79": {
            "text": "im glas das rostrote glänzen"
        },
        "line.80": {
            "text": "der scham"
        },
        "line.81": {
            "text": "schöneweide schöneweide"
        },
        "line.82": {
            "text": "reis, eingeschweißt"
        },
        "line.83": {
            "text": "verwendung wand"
        },
        "line.84": {
            "text": "verbogene kerze, angelehnt das kind"
        },
        "line.85": {
            "text": "quappenglas durchgefärbt gebrannt"
        },
        "line.86": {
            "text": "auf dem schoß die wedelnden algen"
        },
        "line.87": {
            "text": "(hautige ware ) mit goldenem"
        },
        "line.88": {
            "text": "ball gespielt"
        },
        "line.89": {
            "text": "schöneweide schöneweide"
        },
        "line.90": {
            "text": "ganz bei sich"
        },
        "line.91": {
            "text": "schwimmen gelernt"
        },
        "line.92": {
            "text": "kupferdraht."
        },
        "line.93": {
            "text": "keller geräumt gebrochen"
        },
        "line.94": {
            "text": "verfahren die tiefe sei einfach"
        },
        "line.95": {
            "text": "sagen manche abgemacht, süchtig"
        },
        "line.96": {
            "text": "das kesselherz wummert wimmert"
        },
        "line.97": {
            "text": "klebt (glauben daran) silberne"
        },
        "line.98": {
            "text": "kanten fahren schlitter"
        },
        "line.99": {
            "text": "den himmel ab"
        },
        "line.100": {
            "text": "hinaus"
        },
        "line.101": {
            "text": "mit roller und rad"
        },
        "line.102": {
            "text": "pumpe picknick muskelkraft"
        },
        "line.103": {
            "text": "folienverklebt (ball gespielt)"
        },
        "line.104": {
            "text": "die fenster der bahn: rehe"
        },
        "line.105": {
            "text": "jäger lillyfee"
        },
        "line.106": {
            "text": "helm halb überm gesicht"
        },
        "line.107": {
            "text": "zieht das kind an der gardine"
        },
        "line.108": {
            "text": "(pferdewagen, ehe"
        },
        "line.109": {
            "text": "dem) lacht und zieht"
        },
        "line.110": {
            "text": "den mund mir weit den"
        },
        "line.111": {
            "text": "mund mir ab"
        },
        "line.112": {
            "text": "schöneweide schöneweide"
        },
        "line.113": {
            "text": "fahr"
        }
    }
}