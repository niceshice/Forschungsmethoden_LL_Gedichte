{
    "title": "hotel-7721",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "wollen wir"
        },
        "line.2": {
            "text": "lieber"
        },
        "line.3": {
            "text": "ein waldbuch führen logieren wir"
        },
        "line.4": {
            "text": "lieber im wald ich trage dich"
        },
        "line.5": {
            "text": "ein unter schlafem namen"
        },
        "line.6": {
            "text": "komme dir nach"
        },
        "line.7": {
            "text": "in dein tanngemach das"
        },
        "line.8": {
            "text": "nadelbett hält uns wach wie"
        },
        "line.9": {
            "text": "eine unverhoffte haut"
        },
        "line.10": {
            "text": "hippolyta"
        },
        "line.11": {
            "text": "löse ich dir auch"
        },
        "line.12": {
            "text": "die eselsohren aus"
        },
        "line.13": {
            "text": "den andern seiten schreibe"
        },
        "line.14": {
            "text": "nicht weiter nicht auf"
        },
        "line.15": {
            "text": "hören will ich wieder"
        },
        "line.16": {
            "text": "von den schlitzen in der nacht"
        },
        "line.17": {
            "text": "gardine dass es nichts als lang"
        },
        "line.18": {
            "text": "zeitlichtungen sind von sternen"
        },
        "line.19": {
            "text": "werde ich diesmal wissen as if"
        },
        "line.20": {
            "text": "I was your girlfriend"
        },
        "line.21": {
            "text": "/ your room"
        },
        "line.22": {
            "text": "mate"
        }
    }
}