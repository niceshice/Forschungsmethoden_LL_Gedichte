{
    "title": "21-september-5717",
    "gender": "w",
    "author": "Elfriede Czurda",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "gilt nicht die stille"
        },
        "line.2": {
            "text": "steht in der halle mitten"
        },
        "line.3": {
            "text": "im raum bedeckt von der"
        },
        "line.4": {
            "text": "decke aus schwarzem samt"
        },
        "line.5": {
            "text": "katafalk mit dem offenen"
        },
        "line.6": {
            "text": "sarg das hochamt beginnt"
        },
        "line.7": {
            "text": "stimmt die orgel sich"
        },
        "line.8": {
            "text": "ein ruft den k√§mpfer in"
        },
        "line.9": {
            "text": "waffen zu kapitulieren"
        },
        "line.10": {
            "text": "herbei frau singt der ton"
        },
        "line.11": {
            "text": "genug tapferkeit jetzt"
        },
        "line.12": {
            "text": "komm hier ist dein bett"
        },
        "line.13": {
            "text": "21. september; tante mathilde bricht auf"
        }
    }
}