{
    "title": "hartmut-sucht-teneriffa-717",
    "gender": "w",
    "author": "Sabine Scho",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "- 5.MRZ.1957"
        },
        "line.2": {
            "text": "Hartmut sucht Teneriffa"
        },
        "line.3": {
            "text": "direkt im Diercke Welt-"
        },
        "line.4": {
            "text": "atlas  neben einem Schuh-"
        },
        "line.5": {
            "text": "karton, darunter (mehrbändig)"
        },
        "line.6": {
            "text": "ein Brehm, vermutlich, denn"
        },
        "line.7": {
            "text": "das ist nicht genau zu sehn"
        },
        "line.8": {
            "text": "(die Hälfte d. dt. Haushalte"
        },
        "line.9": {
            "text": "besitzen bis auf Schulbüch-"
        },
        "line.10": {
            "text": "er kein Buch) liebäugelt kurz-"
        },
        "line.11": {
            "text": "sichtig ein Mutant im Reise-"
        },
        "line.12": {
            "text": "fieber wenn Du der Welt nicht"
        },
        "line.13": {
            "text": "nachläufst noch nicht nonstop"
        },
        "line.14": {
            "text": "geflogen, ein Märzgefallen mit"
        },
        "line.15": {
            "text": "dem Finger, vornübergebeugt"
        },
        "line.16": {
            "text": "an Karten, die vor Schüleraugen"
        },
        "line.17": {
            "text": "gewissermaßen Körper gewinnen"
        },
        "line.18": {
            "text": "maßstäblich verkleinert und gene-"
        },
        "line.19": {
            "text": "ralisiert (Generalstabskarten) 1:15"
        },
        "line.20": {
            "text": "000 000 kommt die Welt zu Dir!"
        },
        "line.21": {
            "text": "als Schönwettermonopol (Kanari-"
        },
        "line.22": {
            "text": "ban und Karmesin prosperiern)"
        },
        "line.23": {
            "text": "lausig"
        },
        "line.24": {
            "text": "Ach, man lernt, wenn man muß an"
        },
        "line.25": {
            "text": "Regennachmittagen, am Schreibtisch"
        },
        "line.26": {
            "text": "mit dem Rücken zur Wand man lernt"
        },
        "line.27": {
            "text": "wenn man einen Ausweg will, man lernt"
        },
        "line.28": {
            "text": "rücksichtslosangedrückt an eine Kisten-"
        },
        "line.29": {
            "text": "wand"
        },
        "line.30": {
            "text": "ein Bericht für die Preuß. Akademie"
        },
        "line.31": {
            "text": "Kistenverwendung  Kistenersatz"
        },
        "line.32": {
            "text": "Kiste als Hindernis  Zwei Kisten"
        },
        "line.33": {
            "text": "Erste Kiste zu ebener Erde"
        },
        "line.34": {
            "text": "Zweite Kiste darüber!"
        },
        "line.35": {
            "text": "Naive Statik"
        },
        "line.36": {
            "text": "Köhler"
        },
        "line.37": {
            "text": "glaube für ein hochgehängtes Ziel"
        },
        "line.38": {
            "text": "Gestaltgesetze gebändigter Affen: Mach Dich lang"
        },
        "line.39": {
            "text": "wenn Du mußt, steh mit aufgehobenen Armen still!"
        },
        "line.40": {
            "text": "Wissen sie schon, wie die Schimpansin zu uns kam"
        },
        "line.41": {
            "text": "("
        },
        "line.42": {
            "text": "fertiggemachtes Mädchen"
        },
        "line.43": {
            "text": ") 1914 Anthropoidenstation"
        },
        "line.44": {
            "text": "Tafel I."
        },
        "line.45": {
            "text": "Nueva 5 Tage vor dem Tode"
        },
        "line.46": {
            "text": "den Irrsinn des verwirrten dressierten Tieres"
        },
        "line.47": {
            "text": "im Blickhabeichmehrere Schimpansen"
        },
        "line.48": {
            "text": "krank und einige von ihnen sterben sehen"
        },
        "line.49": {
            "text": "servil hockt ein Domestik (genotzüchtigter"
        },
        "line.50": {
            "text": "Haltungstrieb) mit vorgebogenen Schultern"
        },
        "line.51": {
            "text": "Daumen und Zeigefinger opponiert, aufgeräumt"
        },
        "line.52": {
            "text": "vor einer Inselwelt,"
        },
        "line.53": {
            "text": "stuff as dreams are made of"
        },
        "line.54": {
            "text": "und stiert  hartnäckig, hornbebrillt"
        },
        "line.55": {
            "text": "Hartmut sucht Teneriffa, fährt fingergeübt Kontinente"
        },
        "line.56": {
            "text": "entlang, lernt Flächenfarben gehörig unterscheiden"
        },
        "line.57": {
            "text": "stürmt mit brennendem Fernweh den Teide bergan"
        },
        "line.58": {
            "text": "hats geblickt"
        },
        "line.59": {
            "text": "Gebirge in braunen Schraffen mit zusätzlichen"
        },
        "line.60": {
            "text": "Schummerungen, hydrografische Elemente blau"
        },
        "line.61": {
            "text": "Flächenentwürfe azimutal projiziert, winkeltreu"
        },
        "line.62": {
            "text": "ein Weltbild blickdicht schraffiert"
        },
        "line.63": {
            "text": "wissen sie schon von der"
        },
        "line.64": {
            "text": "Bevölkerung im Neolithikum"
        },
        "line.65": {
            "text": "Schwidetzky hat 2000 Schädel"
        },
        "line.66": {
            "text": "ausgezählt  Guanchen waren"
        },
        "line.67": {
            "text": "Stahlgewitter"
        },
        "line.68": {
            "text": "unbekannt  Schmus:"
        },
        "line.69": {
            "text": "Guanchen gehörn bei Hagenbeck"
        },
        "line.70": {
            "text": "an die Kistenwand"
        },
        "line.71": {
            "text": "immermann"
        },
        "line.72": {
            "text": "rinn!"
        },
        "line.73": {
            "text": "Laut Genfer Konvention hat man darauf-"
        },
        "line.74": {
            "text": "hin die Hölleninsel umbemannt (phreno-"
        },
        "line.75": {
            "text": "logisch begründeter Azorenhohn)"
        },
        "line.76": {
            "text": "mutatis mutandis Nur leer scheinende"
        },
        "line.77": {
            "text": "Karten prägen sich dem Gedächtnisse ein"
        },
        "line.78": {
            "text": "artig geschulter-"
        },
        "line.79": {
            "text": "ter"
        },
        "line.80": {
            "text": "globe, gentle will"
        },
        "line.81": {
            "text": "Amateur"
        },
        "line.82": {
            "text": "theater"
        },
        "line.83": {
            "text": "für"
        },
        "line.84": {
            "text": "ein raumloses Volk"
        },
        "line.85": {
            "text": "warum das?"
        },
        "line.86": {
            "text": "Der Diercke"
        },
        "line.87": {
            "text": "immaganz voan, immamann rann"
        },
        "line.88": {
            "text": "Schul-Atlas, Weltatlas, richtig"
        },
        "line.89": {
            "text": "zweckmäßig, schön"
        },
        "line.90": {
            "text": "zufrieden im Glanz"
        },
        "line.91": {
            "text": "seiner gebräunten Karten"
        },
        "line.92": {
            "text": "täuschte im Einband"
        },
        "line.93": {
            "text": "hoffnungsvoll grün"
        },
        "line.94": {
            "text": "Gesamtherstellung Westermann"
        }
    }
}