{
    "title": "eigentlich-1415",
    "gender": "w",
    "author": "Christine Nöstlinger",
    "author_birth": "1936",
    "poem": {
        "line.1": {
            "text": "Mein Papa"
        },
        "line.2": {
            "text": "hat eine neue Frau."
        },
        "line.3": {
            "text": "Angeblich"
        },
        "line.4": {
            "text": "sind ihre Augen blau."
        },
        "line.5": {
            "text": "Halb so alt"
        },
        "line.6": {
            "text": "wie Mama soll sie sein."
        },
        "line.7": {
            "text": "Und"
        },
        "line.8": {
            "text": "sehr schlank und sehr klein."
        },
        "line.9": {
            "text": "Eigentlich"
        },
        "line.10": {
            "text": "würde ich sie gern mal sehn."
        },
        "line.11": {
            "text": "Aber Mama"
        },
        "line.12": {
            "text": "tät das nicht verstehn!"
        }
    }
}