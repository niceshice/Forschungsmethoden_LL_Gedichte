{
    "title": "hingegend-364",
    "gender": "w",
    "author": "Anne Duden",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "Hundsrosen ins eigene Gebein gelegt"
        },
        "line.2": {
            "text": "Heckenseide gezogen über die Augen"
        },
        "line.3": {
            "text": "der Gezeitenfrucht Kind."
        },
        "line.4": {
            "text": "Entlang zwangseingewiesener Luftholer"
        },
        "line.5": {
            "text": "wütet das Hirn in Schüben"
        },
        "line.6": {
            "text": "und blutet zur Seite"
        },
        "line.7": {
            "text": "stromtote Ableger"
        },
        "line.8": {
            "text": "ins süße Faulen der Nebenarme"
        },
        "line.9": {
            "text": "Robinien- und Pappelgestöber."
        },
        "line.10": {
            "text": "Rauhreife Blickabsteige"
        },
        "line.11": {
            "text": "schneeiger Roggen"
        },
        "line.12": {
            "text": "Hoch-, Hitzegesirr."
        },
        "line.13": {
            "text": "Hopfenschnellen am Regen"
        },
        "line.14": {
            "text": "lächeln sich zu Boden."
        },
        "line.15": {
            "text": "Ichausweser"
        },
        "line.16": {
            "text": "gasgesamt"
        },
        "line.17": {
            "text": "um die Dome getrieben"
        },
        "line.18": {
            "text": "der Worte verwiesen"
        },
        "line.19": {
            "text": "tropfenweise verflüssigt"
        },
        "line.20": {
            "text": "Ins Rührei geschwemmt."
        },
        "line.21": {
            "text": "Auf- und Unterwühlhalde Precinct"
        },
        "line.22": {
            "text": "Ab-Ahnhof Hannover."
        },
        "line.23": {
            "text": "Hier sagten Bäume"
        },
        "line.24": {
            "text": "zwei Sprechboten"
        },
        "line.25": {
            "text": "gegen das Licht."
        },
        "line.26": {
            "text": "Unter Autorücken aus Zu- und Abschlagstoffen"
        },
        "line.27": {
            "text": "Tiefgang durchs weggetretene Meer"
        },
        "line.28": {
            "text": "beim unaufhörlichen Wachsen der Stockwerke"
        },
        "line.29": {
            "text": "weiter absinkend."
        },
        "line.30": {
            "text": "Im Grunde"
        },
        "line.31": {
            "text": "nötigt sich eine Musikzeile auf."
        },
        "line.32": {
            "text": "Und Baumkronen"
        },
        "line.33": {
            "text": "zerfleddert in anderthalb Sätzen."
        },
        "line.34": {
            "text": "Um das: gehöhter Augenblick"
        },
        "line.35": {
            "text": "Sturm und Verdurstung."
        },
        "line.36": {
            "text": "Wippgang ins Nervengebüsch"
        },
        "line.37": {
            "text": "Laub abgekämpfter Eschen."
        },
        "line.38": {
            "text": "Schluchzen Sie nicht."
        },
        "line.39": {
            "text": "Lassen Sie hängen"
        },
        "line.40": {
            "text": "das Abgesparte vom Mund."
        },
        "line.41": {
            "text": "Ich gehe jetzt"
        },
        "line.42": {
            "text": "alles Vorgefundene"
        },
        "line.43": {
            "text": "hinter öffentlich verschlossenen Lippen"
        },
        "line.44": {
            "text": "in die Wahrnehmung"
        },
        "line.45": {
            "text": "Hingegend um Nienburg"
        },
        "line.46": {
            "text": "auf den von oben schon überfahrenen Landstrich"
        },
        "line.47": {
            "text": "schnell abgeblickte Schönseite."
        },
        "line.48": {
            "text": "Denn zeitgleich und überall"
        },
        "line.49": {
            "text": "auf ihren Tod getroffen"
        },
        "line.50": {
            "text": "geschleudert"
        },
        "line.51": {
            "text": "gefetzt"
        },
        "line.52": {
            "text": "geklatscht."
        },
        "line.53": {
            "text": "Im Strecksprung Flug Watschelgang"
        },
        "line.54": {
            "text": "dabei eigens noch gegengestachelt gebuckelt"
        },
        "line.55": {
            "text": "-geballt."
        },
        "line.56": {
            "text": "Großer Eisvogel nun"
        },
        "line.57": {
            "text": "geschwänt hinter Lidern"
        },
        "line.58": {
            "text": "Gestalt unter Tag"
        },
        "line.59": {
            "text": "der Gebeinfreien"
        },
        "line.60": {
            "text": "der Gehör- und Gehäuslosen."
        },
        "line.61": {
            "text": "Einem ausgeweinten König"
        },
        "line.62": {
            "text": "abends ins Nest gelegt"
        },
        "line.63": {
            "text": "Eulen-Plumeaus"
        },
        "line.64": {
            "text": "darunter schwerhöriges Wimmern"
        },
        "line.65": {
            "text": "kleiner Organansammlungen."
        },
        "line.66": {
            "text": "Stellen Sie sich baldmöglichst ruhig."
        },
        "line.67": {
            "text": "Nun entschlafen Sie endlich."
        }
    }
}