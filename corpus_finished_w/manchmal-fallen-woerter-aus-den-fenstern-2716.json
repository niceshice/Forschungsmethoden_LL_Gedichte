{
    "title": "manchmal-fallen-woerter-aus-den-fenstern-2716",
    "gender": "w",
    "author": "Sabina Naef",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "manchmal fallen Wörter aus den Fenstern"
        },
        "line.2": {
            "text": "oder es regnet in den Büchern"
        },
        "line.3": {
            "text": "dann tagelang nichts"
        },
        "line.4": {
            "text": "und immer beim Aufwachen die Frage"
        },
        "line.5": {
            "text": "wie Schlaf riecht"
        }
    }
}