{
    "title": "grundlegende-7723",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "dennoch gaben wir eine parole aus wie"
        },
        "line.2": {
            "text": "wiesenschaumkraut waldlehrpfad hießen"
        },
        "line.3": {
            "text": "wir uns einander in solchen"
        },
        "line.4": {
            "text": "zeiten vertraute und solchen leugneten"
        },
        "line.5": {
            "text": "wir die langue ab die legende diesem"
        },
        "line.6": {
            "text": "wald-und wiesenlexikon keine karte"
        },
        "line.7": {
            "text": "ward den grundbucheintragungen"
        },
        "line.8": {
            "text": "beigegeben in der tat"
        },
        "line.9": {
            "text": "aber nahmen wir welche vor und"
        },
        "line.10": {
            "text": "zugegeben verklebten wir zwittrige"
        },
        "line.11": {
            "text": "blüten mit kuckucksspucke"
        },
        "line.12": {
            "text": "vermessene"
        },
        "line.13": {
            "text": "sind wir zu nennen und schwänzer"
        },
        "line.14": {
            "text": "lichtkeimer an und für sich kultur"
        },
        "line.15": {
            "text": "follower von spuren strukturen"
        },
        "line.16": {
            "text": "in einem acte de volont"
        },
        "line.17": {
            "text": "verlaufene sind"
        },
        "line.18": {
            "text": "wir auftrag und grund noch nicht"
        },
        "line.19": {
            "text": "mal am ende negativ zu beschreiben"
        },
        "line.20": {
            "text": "als das was"
        },
        "line.21": {
            "text": "sämtliche anderen nicht"
        },
        "line.22": {
            "text": "sind wir unsere eigene"
        },
        "line.23": {
            "text": "wegzehrung suchen uns zurück"
        },
        "line.24": {
            "text": "zu rufen ein abkommen in"
        },
        "line.25": {
            "text": "die ungeschilderten bäume"
        }
    }
}