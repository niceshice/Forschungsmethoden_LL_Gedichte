{
    "title": "telepylos-12644",
    "gender": "w",
    "author": "Daniela Danz",
    "author_birth": "1976",
    "poem": {
        "line.1": {
            "text": "kein leichtes Boot im dunklen"
        },
        "line.2": {
            "text": "Tunnelwasser kein Plätschern"
        },
        "line.3": {
            "text": "eintauchender Ruder nur eine eiserne"
        },
        "line.4": {
            "text": "Ration Homer:"
        },
        "line.5": {
            "text": "im herrlichen Hafen"
        },
        "line.6": {
            "text": "die stählernen Buckel der U-Boote"
        },
        "line.7": {
            "text": "da könnte ein Mann ohne Schlaf"
        },
        "line.8": {
            "text": "sich doppelten Lohn verdienen"
        },
        "line.9": {
            "text": "nachts in Netzen die glänzenden"
        },
        "line.10": {
            "text": "Rücken der Meeräschen tags"
        },
        "line.11": {
            "text": "die U-Boote mit Waffen beladen"
        },
        "line.12": {
            "text": "durchschwimm die Bucht verbinde"
        },
        "line.13": {
            "text": "ihre Ufer in einer Naht"
        },
        "line.14": {
            "text": "aus Atemzug und Tauchen"
        },
        "line.15": {
            "text": "solang an den Klippen das Meer"
        },
        "line.16": {
            "text": "sich müht aus Kieseln Würfel"
        },
        "line.17": {
            "text": "für ein Spielchen uns zu schleifen"
        }
    }
}