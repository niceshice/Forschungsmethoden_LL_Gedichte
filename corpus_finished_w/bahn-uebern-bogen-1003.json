{
    "title": "bahn-uebern-bogen-1003",
    "gender": "w",
    "author": "Ulrike Draesner",
    "author_birth": "1962",
    "poem": {
        "line.1": {
            "text": "blinkt"
        },
        "line.2": {
            "text": "bar & busen"
        },
        "line.3": {
            "text": "blinkt"
        },
        "line.4": {
            "text": "buntgestreifte domina"
        },
        "line.5": {
            "text": "stellt sich"
        },
        "line.6": {
            "text": "auf,"
        },
        "line.7": {
            "text": "auf: die"
        },
        "line.8": {
            "text": "straßen ohne regung, rollen"
        },
        "line.9": {
            "text": "lippen, licken, rollen"
        },
        "line.10": {
            "text": "schwarze strümpfe über"
        },
        "line.11": {
            "text": "hüften, lippen"
        },
        "line.12": {
            "text": "warten ohne regung: straßen,"
        },
        "line.13": {
            "text": "lecken krusten ganz"
        },
        "line.14": {
            "text": "im strumpf semipermeabel"
        },
        "line.15": {
            "text": "schwarze linien"
        },
        "line.16": {
            "text": "blinkt,"
        },
        "line.17": {
            "text": "was ein gesicht"
        },
        "line.18": {
            "text": "war zwei brüste,"
        },
        "line.19": {
            "text": "blinkt bar &"
        },
        "line.20": {
            "text": "code: sie"
        },
        "line.21": {
            "text": "wechselt das standbein"
        },
        "line.22": {
            "text": "stumm gleiten passanten"
        },
        "line.23": {
            "text": "die blicke ab rattert"
        },
        "line.24": {
            "text": "bahn übern bogen"
        },
        "line.25": {
            "text": "polster, plüsch nebenan"
        },
        "line.26": {
            "text": "caf hegel, happy hour"
        },
        "line.27": {
            "text": "schnippt der kellner"
        },
        "line.28": {
            "text": "reich wie runter,"
        },
        "line.29": {
            "text": "lallt verirrter GI"
        },
        "line.30": {
            "text": "von glimpses & glucks,"
        },
        "line.31": {
            "text": "sie den stiefel"
        },
        "line.32": {
            "text": "hoch die hüfte"
        },
        "line.33": {
            "text": "vor: die"
        },
        "line.34": {
            "text": "taxifahrer reden ost,"
        },
        "line.35": {
            "text": "was fakt ist ist fakt,"
        },
        "line.36": {
            "text": "take-away an der ecke"
        },
        "line.37": {
            "text": "riecht wie nasses papier"
        },
        "line.38": {
            "text": "pope mit geschlossenen augen"
        },
        "line.39": {
            "text": "steht sich, madonnenbereich,"
        },
        "line.40": {
            "text": "beine in den bauch"
        },
        "line.41": {
            "text": "rattert bahn übern bogen,"
        },
        "line.42": {
            "text": "handschuh, glatt, weiß, zeigt"
        },
        "line.43": {
            "text": "kahle büsche voller kameras"
        },
        "line.44": {
            "text": "schwanken klicken los"
        },
        "line.45": {
            "text": "richtung"
        },
        "line.46": {
            "text": "kant-straße"
        },
        "line.47": {
            "text": "rollt"
        },
        "line.48": {
            "text": "der kindische geist von"
        },
        "line.49": {
            "text": "professor savigny"
        },
        "line.50": {
            "text": "leise kichernd dass"
        },
        "line.51": {
            "text": "er et"
        },
        "line.52": {
            "text": "-was verg-"
        },
        "line.53": {
            "text": "essen hat"
        },
        "line.54": {
            "text": "hunger was"
        },
        "line.55": {
            "text": "geschichte war"
        },
        "line.56": {
            "text": "scharfe eß-bahnen"
        },
        "line.57": {
            "text": "(& wie er da lacht)"
        },
        "line.58": {
            "text": "blinkt"
        },
        "line.59": {
            "text": "vom pappteller"
        },
        "line.60": {
            "text": "in seiner hand"
        },
        "line.61": {
            "text": "unleserlich"
        },
        "line.62": {
            "text": "blinkt"
        },
        "line.63": {
            "text": "ein chinesisches"
        },
        "line.64": {
            "text": "zeichen."
        }
    }
}