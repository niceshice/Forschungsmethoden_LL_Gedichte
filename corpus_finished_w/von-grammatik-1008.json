{
    "title": "von-grammatik-1008",
    "gender": "w",
    "author": "Ulrike Draesner",
    "author_birth": "1962",
    "poem": {
        "line.1": {
            "text": "wie licht"
        },
        "line.2": {
            "text": "in spalten höhlen türmen"
        },
        "line.3": {
            "text": "fließt licht in zellen"
        },
        "line.4": {
            "text": "auf. du bist, doch wo?"
        },
        "line.5": {
            "text": "die see rauscht schon"
        },
        "line.6": {
            "text": "genug. wie schwerter drei"
        },
        "line.7": {
            "text": "wolkenhaie sich übern himmel"
        },
        "line.8": {
            "text": "schieben: formation. die"
        },
        "line.9": {
            "text": "nacht ist hell. das rudel"
        },
        "line.10": {
            "text": "ruft. licht wie es in"
        },
        "line.11": {
            "text": "spalten höhlen steine"
        },
        "line.12": {
            "text": "fließt. du bist nicht"
        },
        "line.13": {
            "text": "wo, nicht wer. du"
        },
        "line.14": {
            "text": "gehst, der wald steht still."
        },
        "line.15": {
            "text": "die erde dreht. das lamm"
        },
        "line.16": {
            "text": "springt in die see. ein"
        },
        "line.17": {
            "text": "schatten ruft. was altes"
        },
        "line.18": {
            "text": "weiß von dir. die kehle"
        },
        "line.19": {
            "text": "streckt sich schon. der"
        },
        "line.20": {
            "text": "wolf liebt seinen satz."
        },
        "line.21": {
            "text": "das rudel ruft."
        }
    }
}