{
    "title": "wunderhorn-4317",
    "gender": "w",
    "author": "Dagmar Leupold",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "Feinsliebchen bedient im coffee shop"
        },
        "line.2": {
            "text": "Hiram's hot dog bei Hackensack,"
        },
        "line.3": {
            "text": "New Jersey. Noch nie hat einer"
        },
        "line.4": {
            "text": "ihr Nachtigall buchstabiert"
        },
        "line.5": {
            "text": "oder Maiglöckchen"
        },
        "line.6": {
            "text": "klingen lassen"
        },
        "line.7": {
            "text": "auf dem Tresen aus Resopal"
        },
        "line.8": {
            "text": "Und doch und doch"
        },
        "line.9": {
            "text": "kennt sie die Weisen"
        },
        "line.10": {
            "text": "und die Knaben"
        },
        "line.11": {
            "text": "Mal Senf"
        },
        "line.12": {
            "text": "mal Ketchup"
        },
        "line.13": {
            "text": "aufs Glück"
        }
    }
}