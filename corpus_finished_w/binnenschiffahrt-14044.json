{
    "title": "binnenschiffahrt-14044",
    "gender": "w",
    "author": "Anja Bayer",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "Binnenschiffahrt"
        },
        "line.2": {
            "text": "zugeknotet"
        },
        "line.3": {
            "text": "und zu gekonnt"
        },
        "line.4": {
            "text": "kontern"
        },
        "line.5": {
            "text": "und kentern im / im"
        },
        "line.6": {
            "text": "Sprachfluss"
        },
        "line.7": {
            "text": "der so harm"
        },
        "line.8": {
            "text": "los daherkommt"
        },
        "line.9": {
            "text": "daher"
        },
        "line.10": {
            "text": "kommt mein Misstraun"
        },
        "line.11": {
            "text": "den Misston verfehlt"
        },
        "line.12": {
            "text": "die Warn"
        },
        "line.13": {
            "text": "flagge eingeholt"
        },
        "line.14": {
            "text": "den Zeigefinger"
        },
        "line.15": {
            "text": "zur√ºckgezogen"
        },
        "line.16": {
            "text": "das Dokument re"
        },
        "line.17": {
            "text": "signiert"
        }
    }
}