{
    "title": "i-dann-fing-ich-noch-einmal-mit-der-zeile-i-11645",
    "gender": "w",
    "author": "Odile Kennel",
    "author_birth": "1967",
    "poem": {
        "line.1": {
            "text": "auf einmal, da war ich"
        },
        "line.2": {
            "text": "an dieser Stelle"
        },
        "line.3": {
            "text": "aus meinem Leben"
        },
        "line.4": {
            "text": "heraus, an dieser Stelle war"
        },
        "line.5": {
            "text": "wenn man genau hinsah, nichts"
        },
        "line.6": {
            "text": "Nennenswertes, kein Nennwert, kein"
        },
        "line.7": {
            "text": "Ding, kein Name für das Ding."
        },
        "line.8": {
            "text": "Hätte ich geklopft, man hätte mich vielleicht"
        },
        "line.9": {
            "text": "hereingelassen. Ich brauche dieses blasse"
        },
        "line.10": {
            "text": "Licht im März, sage ich, ich brauche es"
        },
        "line.11": {
            "text": "am Samstagnachmittag das Klirren"
        },
        "line.12": {
            "text": "der Glascontainer zu hören und dabei zu denken"
        },
        "line.13": {
            "text": "dass ich an nichts denke, ich brauche es"
        },
        "line.14": {
            "text": "dass ein Schuh nur ein Schuh ist"
        },
        "line.15": {
            "text": "ein Kühlschrank ein Kühlschrank"
        },
        "line.16": {
            "text": "und der Wecker da draußen"
        },
        "line.17": {
            "text": "der jeden Tag um die gleiche Zeit piept"
        },
        "line.18": {
            "text": "nur ein Wecker da draußen"
        },
        "line.19": {
            "text": "der jeden Tag um die gleiche Zeit piept."
        },
        "line.20": {
            "text": "Diese neue Zeile, die ich beginne, endet"
        },
        "line.21": {
            "text": "vielleicht nie, oder hebt ab"
        },
        "line.22": {
            "text": "so wie ein Hund hinausliefe, nur"
        },
        "line.23": {
            "text": "weil die Tür offen steht"
        },
        "line.24": {
            "text": "nach Zeilen von Rolf Dieter Brinkmann"
        }
    }
}