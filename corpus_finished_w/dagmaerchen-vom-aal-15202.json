{
    "title": "dagmaerchen-vom-aal-15202",
    "gender": "w",
    "author": "Dagmara Kraus",
    "author_birth": "1981",
    "poem": {
        "line.1": {
            "text": "es kam ein aal zu einem maler:"
        },
        "line.2": {
            "text": "bitte male ein porträt von mir."
        },
        "line.3": {
            "text": "ein memento wünsch' ich, zier-"
        },
        "line.4": {
            "text": "arm, kerze, schädel, bloß nicht kahler."
        },
        "line.5": {
            "text": "haare, paar, wo haare fehlen,"
        },
        "line.6": {
            "text": "verwirr's toupet auf der tonsur;"
        },
        "line.7": {
            "text": "dass ich ja mit tolle schwirre,"
        },
        "line.8": {
            "text": "schirr' mir nicht die graumehlkur."
        },
        "line.9": {
            "text": "führ' den blick weg von den gräten,"
        },
        "line.10": {
            "text": "mal' mich füllig um die hüften;"
        },
        "line.11": {
            "text": "schiele nicht zur dürren käthe:"
        },
        "line.12": {
            "text": "klüfte tüftle-dufte klüfte."
        },
        "line.13": {
            "text": "stell' mich dar mit beiden nasen,"
        },
        "line.14": {
            "text": "cassopi schnitt' sich was ab;"
        },
        "line.15": {
            "text": "verschlanke mir die fischschwimmblase,"
        },
        "line.16": {
            "text": "bitte, bitte, nicht zu knapp."
        },
        "line.17": {
            "text": "den vomer schärfe, aalvermeer,"
        },
        "line.18": {
            "text": "ich möcht' kein bodengucker sein;"
        },
        "line.19": {
            "text": "himmelnd blicken in die wolken,"
        },
        "line.20": {
            "text": "schweifen über'n molkenrain."
        },
        "line.21": {
            "text": "ein halalaal war' ich zu gern,"
        },
        "line.22": {
            "text": "mit sauberkleid, das tu' mir an;"
        },
        "line.23": {
            "text": "aus saragossa kam, sargassofern,"
        },
        "line.24": {
            "text": "schmucke, schier ein glanzfasan."
        },
        "line.25": {
            "text": "sackmaul keins, hübsch tütenmund,"
        },
        "line.26": {
            "text": "hab' abgelaicht, ohne zu sterben;"
        },
        "line.27": {
            "text": "den aalpokal dafür, bekund',"
        },
        "line.28": {
            "text": "steht es mir zu zu erben."
        },
        "line.29": {
            "text": "skandaloaal und randalls aal,"
        },
        "line.30": {
            "text": "ein sandaalbild von einem aal;"
        },
        "line.31": {
            "text": "madonnengoldgrundputti will"
        },
        "line.32": {
            "text": "und drauf paar broodthaersschalen."
        },
        "line.33": {
            "text": "alllabeaal!\", aalaugenweide!\","
        },
        "line.34": {
            "text": "krakeelt's, makrhmakrelen!"
        },
        "line.35": {
            "text": "yalla. lilecker wie tibi ..."
        },
        "line.36": {
            "text": "nur 's gift nicht verhehlt!"
        },
        "line.37": {
            "text": "dem bundgarn der belte, bald bratmarinade,"
        },
        "line.38": {
            "text": "blecke ich, bricke und braun;"
        },
        "line.39": {
            "text": "ein fickriger seehund für drei schuppen hickelt,"
        },
        "line.40": {
            "text": "wo ich aus dem kilt kiek' und raun':"
        },
        "line.41": {
            "text": "ich bin der überaalealleskann!"
        },
        "line.42": {
            "text": "ich bin der kahle aalestrahlemann!"
        },
        "line.43": {
            "text": "und beton' mir jaaa die monoflosse;"
        },
        "line.44": {
            "text": "am aalpo die stell' nicht so aus;"
        },
        "line.45": {
            "text": "die erste schwamm um nilkolosse,"
        },
        "line.46": {
            "text": "die zweite küss' mir sanktolaus."
        },
        "line.47": {
            "text": "und der seemooraal von dem gericht:"
        },
        "line.48": {
            "text": "man mors' mal moorseeaalmus ins"
        },
        "line.49": {
            "text": "/  /   /  /     /  /"
        }
    }
}