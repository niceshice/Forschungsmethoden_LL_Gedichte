{
    "title": "westerland-oder-waste-land-13799",
    "gender": "w",
    "author": "Yoko Tawada",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "Humane Hormone"
        },
        "line.2": {
            "text": "fließen als Fußgängerzone"
        },
        "line.3": {
            "text": "zwischen Knochenhäusern"
        },
        "line.4": {
            "text": "Fischfrikadellen"
        },
        "line.5": {
            "text": "Matjesbrötchen"
        },
        "line.6": {
            "text": "Urlaubsromane mit verschwitzten Busen auf dem Titelbild"
        },
        "line.7": {
            "text": "Eine einheitliche Augenfarbe ultraviolett"
        },
        "line.8": {
            "text": "Deine Ebbe und Flut"
        },
        "line.9": {
            "text": "synchronisieren sich mit der Schaumwirtschaft"
        },
        "line.10": {
            "text": "Eine Brise streut Salz"
        },
        "line.11": {
            "text": "ins Gesicht"
        },
        "line.12": {
            "text": "Hast du gesehen"
        },
        "line.13": {
            "text": "Der Koch liegt tot"
        },
        "line.14": {
            "text": "neben ihm seine drei Arme"
        },
        "line.15": {
            "text": "abgeschnitten und hingeworfen"
        },
        "line.16": {
            "text": "eine Schürze, faltig"
        },
        "line.17": {
            "text": "wie die Nordsee, die er"
        },
        "line.18": {
            "text": "täglich auf den Teller brachte"
        },
        "line.19": {
            "text": "Nahrhaftes, Buntes, Gemischtes"
        },
        "line.20": {
            "text": "Die Servietten sind beschmutzt von Hagebutten und dem Himmel"
        },
        "line.21": {
            "text": "Als Nationalfahne nicht definierbar"
        },
        "line.22": {
            "text": "Auch die Bettlaken mit ihren Traumspuren"
        },
        "line.23": {
            "text": "lassen sich nicht waschen mitten in"
        },
        "line.24": {
            "text": "verblassten Ordnungsresten ein Wachstum"
        },
        "line.25": {
            "text": "oder industrielle Milchzähne als Säulen"
        },
        "line.26": {
            "text": "ohne Dach"
        }
    }
}