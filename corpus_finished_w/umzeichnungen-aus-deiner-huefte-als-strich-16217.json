{
    "title": "umzeichnungen-aus-deiner-huefte-als-strich-16217",
    "gender": "w",
    "author": "Caca  Savic",
    "author_birth": "1977",
    "poem": {
        "line.1": {
            "text": "Umzeichnungen aus deiner Hüfte als Strich"
        },
        "line.2": {
            "text": "lässt ein Dort entstehen, vielleicht ein Sein"
        },
        "line.3": {
            "text": "in Stellung, eine in Fleisch reaktive Farbe."
        },
        "line.4": {
            "text": "Kerben gestrichelter Zuordnung vertuschen"
        },
        "line.5": {
            "text": "eine flüchtige Komposition des anderen."
        },
        "line.6": {
            "text": "rhythmische Kontur lässt im Aufsteigen"
        },
        "line.7": {
            "text": "Heulen eindringen. Zeilen weiter: würgst"
        },
        "line.8": {
            "text": "verrenkte Kinder, ziehst über die Kluft und"
        },
        "line.9": {
            "text": "willst Unterwerfungen anhängen auf Augen-"
        },
        "line.10": {
            "text": "farben aufhellen in Sprachen angehäuft. die"
        },
        "line.11": {
            "text": "verrenkten Kinder hergeben für gestärkten"
        },
        "line.12": {
            "text": "Loden, zum Gelächter auf Buckel drapiert"
        },
        "line.13": {
            "text": "zu noblen Hüten, die einst Hirschköpfe auf"
        },
        "line.14": {
            "text": "Antilopen kränzten. Vergewaltigertrophäen"
        }
    }
}