{
    "title": "der-blick-4315",
    "gender": "w",
    "author": "Dagmar Leupold",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "Ohne Augenmaß kreuzte er"
        },
        "line.2": {
            "text": "den Raum und traf"
        },
        "line.3": {
            "text": "an dessen Ende"
        },
        "line.4": {
            "text": "auf lächelnde"
        },
        "line.5": {
            "text": "Erwartung"
        },
        "line.6": {
            "text": "kein Wind ging"
        },
        "line.7": {
            "text": "nur ein Wind"
        }
    }
}