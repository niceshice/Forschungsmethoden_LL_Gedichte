{
    "title": "schliefen-die-oefen-v-legnica-liegnitz-3737",
    "gender": "w",
    "author": "Uljana Wolf",
    "author_birth": "1979",
    "poem": {
        "line.1": {
            "text": "legnica / liegnitz"
        },
        "line.2": {
            "text": "als wir fuhren in den zügen trugen"
        },
        "line.3": {
            "text": "männer die nicht unsre väter warn"
        },
        "line.4": {
            "text": "das land in handgeflochtnen körben"
        },
        "line.5": {
            "text": "(pilze biere) schläfrig durchs abteil"
        },
        "line.6": {
            "text": "der rauch aus ihren mündern hing"
        },
        "line.7": {
            "text": "wie nacht uns lange noch im haar"
        }
    }
}