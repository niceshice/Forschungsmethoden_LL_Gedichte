{
    "title": "apropos-herr-goselmanu-13562",
    "gender": "w",
    "author": "Mara-Daria Cojocaru",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "Also, ich bin seine Katze, zur"
        },
        "line.2": {
            "text": "Blutdrucksenkung den Ägyptern"
        },
        "line.3": {
            "text": "Schon bekannt, und"
        },
        "line.4": {
            "text": "Als der Alte wusste"
        },
        "line.5": {
            "text": "Wie es um seine Seelenruhe stand"
        },
        "line.6": {
            "text": "Entzog er sich"
        },
        "line.7": {
            "text": "Zunächst den Schlaf"
        },
        "line.8": {
            "text": "Dann hat das Schlitzohr"
        },
        "line.9": {
            "text": "Mich in einer Flasche aus"
        },
        "line.10": {
            "text": "Halluzinogen verquicktem"
        },
        "line.11": {
            "text": "Zuckerglas in den Fluss"
        },
        "line.12": {
            "text": "Gesteckt. Ich leckte brav"
        },
        "line.13": {
            "text": "Substanz  der Effekt"
        },
        "line.14": {
            "text": "War: Darwin"
        },
        "line.15": {
            "text": "Biss mir meine Krallen ab"
        },
        "line.16": {
            "text": "In der Tat"
        },
        "line.17": {
            "text": "Herrn Goselmanus Blutdruck sank und"
        },
        "line.18": {
            "text": "Unterdessen trank ich, fand"
        },
        "line.19": {
            "text": "Mich, mit der Auflösung"
        },
        "line.20": {
            "text": "Der Flaschenwand"
        },
        "line.21": {
            "text": "In seiner ruhigen Hand  seither"
        },
        "line.22": {
            "text": "Schnurre ich, wenn man mich"
        },
        "line.23": {
            "text": "Aus dem Wasser fischt"
        }
    }
}