{
    "title": "kammerherz-362",
    "gender": "w",
    "author": "Anne Duden",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "Herzaufgänge"
        },
        "line.2": {
            "text": "als stünde die Welt"
        },
        "line.3": {
            "text": "nur einmal im Laub"
        },
        "line.4": {
            "text": "pro Leben."
        },
        "line.5": {
            "text": "Dunstvergötterte"
        },
        "line.6": {
            "text": "Silberschrift"
        },
        "line.7": {
            "text": "englisch"
        },
        "line.8": {
            "text": "über alle Anzeichen"
        },
        "line.9": {
            "text": "hinweggeschmiegt"
        },
        "line.10": {
            "text": "grasige Weite."
        },
        "line.11": {
            "text": "Mitten im Totschlag"
        },
        "line.12": {
            "text": "betritt Geißblatt das Haus"
        },
        "line.13": {
            "text": "säugt die Zimmer"
        },
        "line.14": {
            "text": "flurwärts"
        },
        "line.15": {
            "text": "Schädel und Nebenhöhlen"
        },
        "line.16": {
            "text": "ankert in der Schwebe"
        },
        "line.17": {
            "text": "ausrißbereit."
        },
        "line.18": {
            "text": "Mundgewölbe und Ohrmuschel"
        },
        "line.19": {
            "text": "geborsten"
        },
        "line.20": {
            "text": "zerschallt"
        },
        "line.21": {
            "text": "von Pennergebissen."
        },
        "line.22": {
            "text": "Hinter Vorhängen"
        },
        "line.23": {
            "text": "jetzt noch das Kammerherz."
        },
        "line.24": {
            "text": "Genickfänger"
        },
        "line.25": {
            "text": "fest im Griff."
        },
        "line.26": {
            "text": "Auf der Suche nach Schmauchspuren"
        },
        "line.27": {
            "text": "und immer fündig"
        },
        "line.28": {
            "text": "säumen"
        },
        "line.29": {
            "text": "Faustpfandleiher"
        },
        "line.30": {
            "text": "Lager und Betten"
        },
        "line.31": {
            "text": "überwachen die Praktiken"
        },
        "line.32": {
            "text": "schneiden"
        },
        "line.33": {
            "text": "klar umrissen"
        },
        "line.34": {
            "text": "das Wort ab"
        },
        "line.35": {
            "text": "jedem"
        },
        "line.36": {
            "text": "der nicht"
        },
        "line.37": {
            "text": "außerdem"
        },
        "line.38": {
            "text": "zusticht und köpft"
        },
        "line.39": {
            "text": "oder genauso gut"
        },
        "line.40": {
            "text": "erdrosselt und abwürgt."
        },
        "line.41": {
            "text": "Es hütet noch gerade"
        },
        "line.42": {
            "text": "das Haus"
        },
        "line.43": {
            "text": "Verstummung"
        },
        "line.44": {
            "text": "am Rand."
        }
    }
}