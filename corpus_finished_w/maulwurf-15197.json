{
    "title": "maulwurf-15197",
    "gender": "w",
    "author": "Anja Kampmann",
    "author_birth": "1983",
    "poem": {
        "line.1": {
            "text": "früher haben wir sie aus den wiesen gehoben"
        },
        "line.2": {
            "text": "mit mistgabeln, spaten"
        },
        "line.3": {
            "text": "rosige rüssel und grabehände die"
        },
        "line.4": {
            "text": "nicht schwarz waren weiche felle"
        },
        "line.5": {
            "text": "durchpflügte wiesen. dann gab es"
        },
        "line.6": {
            "text": "metallene stäbe"
        },
        "line.7": {
            "text": "die man in den boden stach"
        },
        "line.8": {
            "text": "für schallwellen"
        },
        "line.9": {
            "text": "einen deutlichen rhythmus"
        },
        "line.10": {
            "text": "der sie forttreiben sollte, aus ihren gängen,"
        },
        "line.11": {
            "text": "die tiere."
        },
        "line.12": {
            "text": "der tiefe schlaf"
        },
        "line.13": {
            "text": "das fressen und wühlen"
        },
        "line.14": {
            "text": "unter der erde heute"
        },
        "line.15": {
            "text": "trägst du zwei elektroden im kopf"
        },
        "line.16": {
            "text": "fingerlang ragen sie in die tiefschwarze erde"
        },
        "line.17": {
            "text": "des hirns"
        },
        "line.18": {
            "text": "wo sie wellen aussenden kleine schocks"
        },
        "line.19": {
            "text": "ein namloses system von gängen"
        },
        "line.20": {
            "text": "untiefen"
        },
        "line.21": {
            "text": "mit dem namen einer krankheit. kaum mehr."
        },
        "line.22": {
            "text": "die wiese war von hügeln durchzogen"
        },
        "line.23": {
            "text": "irgendwo im dunkeln wurde ein käfer geknackt"
        },
        "line.24": {
            "text": "ein regenwurm"
        },
        "line.25": {
            "text": "verspeist das war  die gute zeit"
        },
        "line.26": {
            "text": "in der man über maulwürfe sprechen konnte"
        },
        "line.27": {
            "text": "und wusste, womit man es zu tun hat."
        }
    }
}