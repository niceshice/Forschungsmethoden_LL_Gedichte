{
    "title": "fooled-i-knew-7720",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "Wise men say only fools rush in."
        },
        "line.2": {
            "text": "Elvis Presley"
        },
        "line.3": {
            "text": "ließen die leitung offen lauschten"
        },
        "line.4": {
            "text": "unserem schlafen"
        },
        "line.5": {
            "text": "I slept, say"
        },
        "line.6": {
            "text": "zwei jahre zwillings"
        },
        "line.7": {
            "text": "atem im ohrensausen ein rosa"
        },
        "line.8": {
            "text": "rauschen"
        },
        "line.9": {
            "text": "a snake masked"
        },
        "line.10": {
            "text": "es gibt dich"
        },
        "line.11": {
            "text": "verschwindend länger als mich die"
        },
        "line.12": {
            "text": "verbindung"
        },
        "line.13": {
            "text": "among black rocks"
        },
        "line.14": {
            "text": "unsrer ohrmuscheln gibt"
        },
        "line.15": {
            "text": "ein meer serendipität nur"
        },
        "line.16": {
            "text": "nähe trennte uns"
        },
        "line.17": {
            "text": "as"
        },
        "line.18": {
            "text": "a black rock"
        },
        "line.19": {
            "text": "irgendwann"
        },
        "line.20": {
            "text": "wurde"
        },
        "line.21": {
            "text": "in the white"
        },
        "line.22": {
            "text": "das telefon"
        },
        "line.23": {
            "text": "gefunden der regen"
        },
        "line.24": {
            "text": "darin"
        },
        "line.25": {
            "text": "hiatus of winter"
        },
        "line.26": {
            "text": "ein"
        },
        "line.27": {
            "text": "einrauschen"
        }
    }
}