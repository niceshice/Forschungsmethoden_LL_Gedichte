{
    "title": "kurze-sache-6060",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "ende august wird adventliches"
        },
        "line.2": {
            "text": "wieder denkbar wolle etwas"
        },
        "line.3": {
            "text": "auf deiner haut außer"
        },
        "line.4": {
            "text": "weißlichen herden der trockenheit"
        },
        "line.5": {
            "text": "sommerschnee sommerliches"
        },
        "line.6": {
            "text": "hängt schwer in deinen gesprächen"
        },
        "line.7": {
            "text": "wie späte beeren ein saurer"
        },
        "line.8": {
            "text": "vorwurf des nichtverzehrtwordenseins"
        },
        "line.9": {
            "text": "zu munde oder zu grunde doch"
        },
        "line.10": {
            "text": "der abtrieb der gegensätze hat unlängst"
        },
        "line.11": {
            "text": "begonnen ihre ängstlichen glöckchen"
        },
        "line.12": {
            "text": "verklingen bald wie abende und"
        },
        "line.13": {
            "text": "die bücher werden nicht länger geführt"
        },
        "line.14": {
            "text": "über flüchtigkeiten wie nachtregen"
        },
        "line.15": {
            "text": "schlagschatten klar und in stereo"
        },
        "line.16": {
            "text": "auch die träume ein fragment die letzte"
        },
        "line.17": {
            "text": "wäsche der kurzen sachen"
        }
    }
}