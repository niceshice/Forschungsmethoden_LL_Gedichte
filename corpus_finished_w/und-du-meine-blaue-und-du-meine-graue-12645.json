{
    "title": "und-du-meine-blaue-und-du-meine-graue-12645",
    "gender": "w",
    "author": "Daniela Danz",
    "author_birth": "1976",
    "poem": {
        "line.1": {
            "text": "winters am Fluss wo wir gehen"
        },
        "line.2": {
            "text": "fliegen zwei Ringeltauben vom"
        },
        "line.3": {
            "text": "Schiefer deiner Iris auf als eine"
        },
        "line.4": {
            "text": "Feder fällt dein Schlaf auf dich"
        },
        "line.5": {
            "text": "still und wolkenähnlich gehst"
        },
        "line.6": {
            "text": "du hinüber: eine eigene Arbeit"
        },
        "line.7": {
            "text": "wie ein Flusskiesel rollt"
        },
        "line.8": {
            "text": "dein Schlaf über den Grund"
        },
        "line.9": {
            "text": "dieser Winternacht"
        },
        "line.10": {
            "text": "da du schläfst auf der seidenen"
        },
        "line.11": {
            "text": "Haut deiner Wangen sind zu Stoff"
        },
        "line.12": {
            "text": "die Träume geworden vergessen"
        },
        "line.13": {
            "text": "stehe ich vor persischen Kacheln"
        },
        "line.14": {
            "text": "des achtzehnten Jahrhunderts"
        },
        "line.15": {
            "text": "aus den Vitrinen der würzige Duft"
        },
        "line.16": {
            "text": "eines Krauts in den Bergen des"
        },
        "line.17": {
            "text": "Hindukusch meine Unruhe perlt"
        },
        "line.18": {
            "text": "am Kupfer deiner Gewissheit ab"
        }
    }
}