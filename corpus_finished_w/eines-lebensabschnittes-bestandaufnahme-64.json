{
    "title": "eines-lebensabschnittes-bestandaufnahme-64",
    "gender": "w",
    "author": "Friederike  Mayröcker",
    "author_birth": "1924",
    "poem": {
        "line.1": {
            "text": "in meinem Tornister"
        },
        "line.2": {
            "text": "ein Thymianstämmchen"
        },
        "line.3": {
            "text": "zwei Münzen"
        },
        "line.4": {
            "text": "ein stumpfer Bleistift"
        },
        "line.5": {
            "text": "zerknitterte Notizen"
        },
        "line.6": {
            "text": "Keksbrösel"
        },
        "line.7": {
            "text": "eine grüne Wäscheklammer"
        },
        "line.8": {
            "text": "die Visitkarte einer japanischen Germanistin"
        },
        "line.9": {
            "text": "ein zerbrochener kleiner Kamm"
        },
        "line.10": {
            "text": "Dals Ameisen auf einem verschatteten Notenblatt"
        },
        "line.11": {
            "text": "für Emi Neckamm"
        }
    }
}