{
    "title": "schreibtisch-selbstbild-tagebuch-12839",
    "gender": "w",
    "author": "Margret Kreidl",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "SCHREIBTISCH"
        },
        "line.2": {
            "text": "Der Salatkopf wächst. Ich komme mit den Äpfeln ins Gespräch."
        },
        "line.3": {
            "text": "Ich beiße niemanden. Der Zweig blüht weiter, während er Früchte"
        },
        "line.4": {
            "text": "trägt. Zwischen den Zwiebeln geht etwas zu Ende. Ich schäle"
        },
        "line.5": {
            "text": "eine Gurke, dann kämme ich die Pfirsiche. Gelb muss leiden,"
        },
        "line.6": {
            "text": "sagt die Frau, die hinter dem Tisch steht. Ich will aufstehen,"
        },
        "line.7": {
            "text": "aber ich kann nicht. Sie beugt sich zu mir: Schreib Erdbeeren"
        },
        "line.8": {
            "text": "auf die Serviette."
        },
        "line.9": {
            "text": "Einfache Erklärung: Träume kann man essen."
        },
        "line.10": {
            "text": "SELBSTBILD"
        },
        "line.11": {
            "text": "Mein Vater ist gestorben."
        },
        "line.12": {
            "text": "Ich trage sein Gesicht weiter,"
        },
        "line.13": {
            "text": "weicher,"
        },
        "line.14": {
            "text": "dunkler,"
        },
        "line.15": {
            "text": "freier,"
        },
        "line.16": {
            "text": "grüner,"
        },
        "line.17": {
            "text": "salziger,"
        },
        "line.18": {
            "text": "nackter,"
        },
        "line.19": {
            "text": "strenger,"
        },
        "line.20": {
            "text": "trauriger,"
        },
        "line.21": {
            "text": "fremder,"
        },
        "line.22": {
            "text": "kälter,"
        },
        "line.23": {
            "text": "wacher,"
        },
        "line.24": {
            "text": "jünger."
        },
        "line.25": {
            "text": "Einfache Erklärung: Je älter ich werde, desto ähnlicher sehe ich meinem Vater."
        },
        "line.26": {
            "text": "TAGEBUCH"
        },
        "line.27": {
            "text": "Nichts geträumt."
        },
        "line.28": {
            "text": "Fast nichts geträumt."
        },
        "line.29": {
            "text": "Heute nichts geträumt."
        },
        "line.30": {
            "text": "So lange nichts geträumt."
        },
        "line.31": {
            "text": "Nichts geträumt."
        },
        "line.32": {
            "text": "Nichts, nichts."
        },
        "line.33": {
            "text": "Die ganze Zeit nichts geträumt."
        },
        "line.34": {
            "text": "Wieder nichts geträumt."
        },
        "line.35": {
            "text": "Bei einem Baum gewesen, so schöne Sachen geträumt."
        },
        "line.36": {
            "text": "Einfache Erklärung: Manchmal muss man in den Wald gehen"
        },
        "line.37": {
            "text": "um zu träumen."
        }
    }
}