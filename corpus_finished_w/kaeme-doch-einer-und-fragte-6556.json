{
    "title": "kaeme-doch-einer-und-fragte-6556",
    "gender": "w",
    "author": "Ilana Shmueli",
    "author_birth": "1924",
    "poem": {
        "line.1": {
            "text": "Käme doch einer und fragte..."
        },
        "line.2": {
            "text": "nach dem Was und dem Wie"
        },
        "line.3": {
            "text": "und Muss-es-so-sein?"
        },
        "line.4": {
            "text": "Käme doch einer"
        },
        "line.5": {
            "text": "bestünde auf Antwort"
        },
        "line.6": {
            "text": "gäbe acht"
        },
        "line.7": {
            "text": "auf das Was und das Wie"
        },
        "line.8": {
            "text": "wär ich doch einer der täte"
        },
        "line.9": {
            "text": "was sein muss"
        }
    }
}