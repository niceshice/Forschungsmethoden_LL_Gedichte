{
    "title": "mau-mau-16235",
    "gender": "w",
    "author": "Cornelia  Hülmbauer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "wer hungrig ist muss essen"
        },
        "line.2": {
            "text": "wer reste sucht ist in der not"
        },
        "line.3": {
            "text": "der sucht das maul der nachbarn ab"
        },
        "line.4": {
            "text": "wer süchtig ist statt tot"
        },
        "line.5": {
            "text": "die onkel gehen jagen"
        },
        "line.6": {
            "text": "die tanten klauben auf"
        },
        "line.7": {
            "text": "die tanten werfen bluter"
        },
        "line.8": {
            "text": "die onkel steigen drauf"
        },
        "line.9": {
            "text": "sprengsel im gewebe"
        },
        "line.10": {
            "text": "aus dem auswuchs wo es franst"
        },
        "line.11": {
            "text": "freiwild zu den angeleinten"
        },
        "line.12": {
            "text": "kittel und geschürzte münder"
        },
        "line.13": {
            "text": "silber ist hier gold genug"
        },
        "line.14": {
            "text": "im kasten steht ein häferl"
        },
        "line.15": {
            "text": "mehr braucht ein mädl nicht"
        },
        "line.16": {
            "text": "am kasten fehlen schrauben"
        },
        "line.17": {
            "text": "viel hält der kasten nicht"
        },
        "line.18": {
            "text": "milch mit zucker"
        },
        "line.19": {
            "text": "schuh aus essig"
        },
        "line.20": {
            "text": "einem stellts die patschen auf"
        },
        "line.21": {
            "text": "einer fiebert hin"
        },
        "line.22": {
            "text": "das mädchen schlingt sich haare um den hals"
        },
        "line.23": {
            "text": "das mädchen hält den atem"
        },
        "line.24": {
            "text": "das mädchen spinnt sich falter vor die augen"
        },
        "line.25": {
            "text": "das mädchen ist verpuppt"
        },
        "line.26": {
            "text": "himmel hölle auf & zu"
        },
        "line.27": {
            "text": "spalten falten hände weg"
        },
        "line.28": {
            "text": "kanten schneiden häute ein"
        },
        "line.29": {
            "text": "nasen bluten schnell"
        }
    }
}