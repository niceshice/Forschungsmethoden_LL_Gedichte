{
    "title": "selbstwert-6973",
    "gender": "w",
    "author": "Ann Cotten",
    "author_birth": "1982",
    "poem": {
        "line.1": {
            "text": "Das Öl will lecken."
        },
        "line.2": {
            "text": "Die Zunge will fauchen."
        },
        "line.3": {
            "text": "Der Mensch will Öl"
        },
        "line.4": {
            "text": "und Zunge gebrauchen."
        },
        "line.5": {
            "text": "Der Mensch leckt mit"
        },
        "line.6": {
            "text": "der Zunge Öl."
        },
        "line.7": {
            "text": "Das Öl erfand"
        },
        "line.8": {
            "text": "das Ehrgefühl."
        }
    }
}