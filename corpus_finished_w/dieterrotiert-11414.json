{
    "title": "dieterrotiert-11414",
    "gender": "w",
    "author": "Dagmara Kraus",
    "author_birth": "1981",
    "poem": {
        "line.1": {
            "text": "der berindete sterz  guckloch"
        },
        "line.2": {
            "text": "schmerzt der auch?"
        },
        "line.3": {
            "text": "und ob."
        },
        "line.4": {
            "text": "ob  ob  fauch"
        },
        "line.5": {
            "text": "auch lasenohren lanzinieren?"
        },
        "line.6": {
            "text": "und ob."
        },
        "line.7": {
            "text": "ob  ob die filzkönigskerzen"
        },
        "line.8": {
            "text": "pikiert sind, wenn man im notfall"
        },
        "line.9": {
            "text": "mal unschlitt gebraucht? oder biene?"
        },
        "line.10": {
            "text": "und ob."
        },
        "line.11": {
            "text": "ob  ob globose gewölle weltanfangs"
        },
        "line.12": {
            "text": "wirklich vom himmel fielen?"
        },
        "line.13": {
            "text": "ja."
        },
        "line.14": {
            "text": "wie ja?  ja?! ja, ja einfach so?"
        },
        "line.15": {
            "text": "und  warns  warns kotzengrobe?"
        },
        "line.16": {
            "text": "hautbois oder obo?"
        }
    }
}