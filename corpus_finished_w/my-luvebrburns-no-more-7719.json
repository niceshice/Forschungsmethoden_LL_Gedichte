{
    "title": "my-luvebrburns-no-more-7719",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "O my Luves"
        },
        "line.2": {
            "text": "wie ein pilzgericht"
        },
        "line.3": {
            "text": "das mundet jahrelang"
        },
        "line.4": {
            "text": "O my Luves"
        },
        "line.5": {
            "text": "wie ein allerlei"
        },
        "line.6": {
            "text": "das macht zuletzt ganz bang"
        },
        "line.7": {
            "text": "so reicherst du mich sachte an"
        },
        "line.8": {
            "text": "wie jene ingredienz"
        },
        "line.9": {
            "text": "man handelt dich nicht mehr"
        },
        "line.10": {
            "text": "my Dear"
        },
        "line.11": {
            "text": "es geht hier nicht um trends"
        },
        "line.12": {
            "text": "es geht um jenes mahl"
        },
        "line.13": {
            "text": "my Dear"
        },
        "line.14": {
            "text": "nach dem die lorchel sticht"
        },
        "line.15": {
            "text": "nach dem das gehen bleibt"
        },
        "line.16": {
            "text": "my Dear"
        },
        "line.17": {
            "text": "ein pilzrest und ich nicht"
        },
        "line.18": {
            "text": "die letzte strophe fehlt"
        },
        "line.19": {
            "text": "my Luve"
        },
        "line.20": {
            "text": "ein nachtisch nur ein wort"
        },
        "line.21": {
            "text": "sprich only w√§rms nicht auf"
        },
        "line.22": {
            "text": "my Luve"
        },
        "line.23": {
            "text": "wirf topf und deckel fort"
        }
    }
}