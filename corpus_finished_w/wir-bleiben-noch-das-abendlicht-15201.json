{
    "title": "wir-bleiben-noch-das-abendlicht-15201",
    "gender": "w",
    "author": "Anja Kampmann",
    "author_birth": "1983",
    "poem": {
        "line.1": {
            "text": "hängt in dem hohen zweig der kirsche"
        },
        "line.2": {
            "text": "die schwalben stochern noch ein wenig"
        },
        "line.3": {
            "text": "in der glut"
        },
        "line.4": {
            "text": "das hohe blau fragt nach den"
        },
        "line.5": {
            "text": "restlichen der zeugen"
        },
        "line.6": {
            "text": "der alte photograph des himmels"
        },
        "line.7": {
            "text": "schickt stille blitze über warmes land"
        },
        "line.8": {
            "text": "wo man im schatten trockener"
        },
        "line.9": {
            "text": "maisplantagen flüstert"
        },
        "line.10": {
            "text": "und wo das tor zur nacht"
        },
        "line.11": {
            "text": "ein dunkler spalt bloß ist"
        },
        "line.12": {
            "text": "ein scheunentor bewegt von alten winden"
        },
        "line.13": {
            "text": "nichts weiter"
        },
        "line.14": {
            "text": "wenn jetzt der abend sein gewölle spuckt"
        },
        "line.15": {
            "text": "bleibt ein papier"
        },
        "line.16": {
            "text": "bleicher noch als haut"
        },
        "line.17": {
            "text": "es wird so wenig sein"
        },
        "line.18": {
            "text": "die länder die du sahst"
        },
        "line.19": {
            "text": "nähen ihre schüsse"
        },
        "line.20": {
            "text": "in die traumgestalt von vögeln"
        },
        "line.21": {
            "text": "und in den ersten dunst"
        },
        "line.22": {
            "text": "nach einem langen regen"
        },
        "line.23": {
            "text": "es wird so wenig sein"
        },
        "line.24": {
            "text": "wenn wir ins dunkel fahren"
        },
        "line.25": {
            "text": "und leise miteinander reden."
        }
    }
}