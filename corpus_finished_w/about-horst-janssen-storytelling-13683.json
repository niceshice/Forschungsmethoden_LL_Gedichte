{
    "title": "about-horst-janssen-storytelling-13683",
    "gender": "w",
    "author": "Claudia Gabler",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "By Storytelling"
        },
        "line.2": {
            "text": "will ich mit Knut nichts mehr zum tun ham!"
        },
        "line.3": {
            "text": "Weisch, weischt:"
        },
        "line.4": {
            "text": ""
        },
        "line.5": {
            "text": "\"Bei gestauter Geilheit\""
        },
        "line.6": {
            "text": "sollen sich, so K., SpielzeugLandschaften aufbauen"
        },
        "line.7": {
            "text": "oder Gebäude nach Vorlage des B. (also B wie Kunst) (?)"
        },
        "line.8": {
            "text": ""
        },
        "line.9": {
            "text": "Ach ist doch wurscht: Nur in diesem Ton jedenfalls geht die Gunst!"
        },
        "line.10": {
            "text": "Das Bild muss sich zum Hirn neigen,"
        },
        "line.11": {
            "text": "gähnende Schluchten, Lichtreflexe. Natürlich,"
        },
        "line.12": {
            "text": ""
        },
        "line.13": {
            "text": "natürlich, natürlich und später:"
        },
        "line.14": {
            "text": "Orgasmuas. Muass sein, weischt: Ohne Eruption"
        },
        "line.15": {
            "text": "kann dir auch keine Locke aus der Mutter fahrn."
        },
        "line.16": {
            "text": ""
        },
        "line.17": {
            "text": "Freundschaft also mit Knut?"
        },
        "line.18": {
            "text": "Nee,"
        },
        "line.19": {
            "text": "lass uns lieber noch n bißchen weiter fighten!"
        }
    }
}