{
    "title": "new-york-und-retour-1517",
    "gender": "w",
    "author": "Elfriede Gerstl",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "ratlos gewesen"
        },
        "line.2": {
            "text": "angst gehabt"
        },
        "line.3": {
            "text": "weit gefahren"
        },
        "line.4": {
            "text": "gefahr gesucht"
        },
        "line.5": {
            "text": "mut bekommen"
        },
        "line.6": {
            "text": "beherzt heimgekehrt"
        },
        "line.7": {
            "text": "erzählen können"
        },
        "line.8": {
            "text": "hohe türme"
        },
        "line.9": {
            "text": "dunkle schluchten"
        },
        "line.10": {
            "text": "mut bekommen"
        },
        "line.11": {
            "text": "gefahr gesucht"
        },
        "line.12": {
            "text": "weit gefahren"
        },
        "line.13": {
            "text": "angst gehabt"
        },
        "line.14": {
            "text": "ratlos wie immer"
        }
    }
}