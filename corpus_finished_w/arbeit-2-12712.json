{
    "title": "arbeit-2-12712",
    "gender": "w",
    "author": "Martina Hefter",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "Das Zwicken werkelt im Genick."
        },
        "line.2": {
            "text": "Ihr braucht Sehhilfen für den bösen Blick."
        },
        "line.3": {
            "text": "Die Zuwachszahlen meiner Defizite"
        },
        "line.4": {
            "text": "schieben sich immer dazwischen,"
        },
        "line.5": {
            "text": "sobald ich meine Hände prüfe,"
        },
        "line.6": {
            "text": "ob sie wimmeln vor Fingern."
        },
        "line.7": {
            "text": "Sehr kleine Stürme"
        },
        "line.8": {
            "text": "sprüh ich mit ihnen."
        },
        "line.9": {
            "text": "Hüstel, hüstel. No spirit."
        },
        "line.10": {
            "text": "Die Zuwachszahlen meiner Defizite schwimmen,"
        },
        "line.11": {
            "text": "seh ich ohne Brille."
        },
        "line.12": {
            "text": "Die Füße, die meine arbeitenden Füße sein sollen,"
        },
        "line.13": {
            "text": "sie knicken nach innen, wie meine Knie."
        },
        "line.14": {
            "text": "Ich hüte mein kaputtes Figürchen."
        },
        "line.15": {
            "text": "Sprüh eine Million Gifte."
        },
        "line.16": {
            "text": "Wackel dabei mit der Hüfte durch Mega-Regionen"
        },
        "line.17": {
            "text": "herbstlichen Waldes."
        },
        "line.18": {
            "text": "Humpeln und Kriechen,"
        },
        "line.19": {
            "text": "wie definiert man diese Nischen?"
        }
    }
}