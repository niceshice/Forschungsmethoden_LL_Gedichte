{
    "title": "maribor-11918",
    "gender": "w",
    "author": "Anja Kampmann",
    "author_birth": "1983",
    "poem": {
        "line.1": {
            "text": "eine herzschwäche des lichts"
        },
        "line.2": {
            "text": "ein übergriff ins gestern"
        },
        "line.3": {
            "text": "ein fluss mit pflaumen an den ufern"
        },
        "line.4": {
            "text": "birnen ein markt"
        },
        "line.5": {
            "text": "und als tito kam die dörfer"
        },
        "line.6": {
            "text": "und als tito kam die gesunden"
        },
        "line.7": {
            "text": "männer und die gesunden frauen"
        },
        "line.8": {
            "text": "und die kinder die ihre hunde an stricken"
        },
        "line.9": {
            "text": "mit fortnahmen"
        },
        "line.10": {
            "text": "und danach"
        },
        "line.11": {
            "text": "und in den wäldern die geschosse"
        },
        "line.12": {
            "text": "den wäldern den wäldern den hügeln"
        },
        "line.13": {
            "text": "mit weichem grünlichem licht"
        },
        "line.14": {
            "text": "und im herbst im sommer winter"
        },
        "line.15": {
            "text": "und das frühe verlassene jahr"
        },
        "line.16": {
            "text": "wie es den anderen folgte"
        },
        "line.17": {
            "text": "folgte folgten einander fort"
        },
        "line.18": {
            "text": "die angler die metzger"
        },
        "line.19": {
            "text": "nüsse zu sammeln nüsse"
        },
        "line.20": {
            "text": "von innen pocht es hohl"
        },
        "line.21": {
            "text": "eine leere in den früchten und wer"
        },
        "line.22": {
            "text": "hob sie auf wer aß"
        },
        "line.23": {
            "text": "davon was blieb maribor"
        },
        "line.24": {
            "text": "mit wirbeln aus glas aus glas"
        },
        "line.25": {
            "text": "sind die hunde zurückgekommen zuerst"
        },
        "line.26": {
            "text": "mit ihren losen langen stricken"
        },
        "line.27": {
            "text": "die niemals gelassen wurden nur fester"
        },
        "line.28": {
            "text": "gedrückt und gepresst in den"
        },
        "line.29": {
            "text": "blinden händen den vergessenen händen"
        },
        "line.30": {
            "text": "mit den wimpern"
        },
        "line.31": {
            "text": "zum baden gehen zum tauchen"
        },
        "line.32": {
            "text": "im dorfsee im dorfteich"
        },
        "line.33": {
            "text": "in den vertiefungen der landschaft"
        },
        "line.34": {
            "text": "in den spiegelungen eines neuen tags"
        },
        "line.35": {
            "text": "die wimpern wimpern und der strick"
        },
        "line.36": {
            "text": "des hunds am tag und unter"
        },
        "line.37": {
            "text": "den erinnerungslosen wolken"
        },
        "line.38": {
            "text": "im grünlichen grünlichen"
        },
        "line.39": {
            "text": "wo einer kam wo alles im gehen"
        },
        "line.40": {
            "text": "verblasst."
        }
    }
}