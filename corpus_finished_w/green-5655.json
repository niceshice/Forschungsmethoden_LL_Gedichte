{
    "title": "green-5655",
    "gender": "w",
    "author": "Sabine Scho",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "jemand will, dass ich gras"
        },
        "line.2": {
            "text": "sage und eine decke aus-"
        },
        "line.3": {
            "text": "breite, gutes gras, die reine"
        },
        "line.4": {
            "text": "üppigkeit der wiederkäuer"
        },
        "line.5": {
            "text": "es ist nichts, gebe ich bereit-"
        },
        "line.6": {
            "text": "willig zu verstehen, nichts als"
        },
        "line.7": {
            "text": "wind in den weiden, marstaug-"
        },
        "line.8": {
            "text": "lichkeit, ein blaumann aus der"
        },
        "line.9": {
            "text": "schnellreinigung, vorzugsweise"
        },
        "line.10": {
            "text": "photosynthese, stromatolithen-"
        },
        "line.11": {
            "text": "felder, temperaturstürze in wüster"
        },
        "line.12": {
            "text": "blüte, verkrustete aussicht, kosten-"
        },
        "line.13": {
            "text": "lose lasergravur, nichts und kein"
        },
        "line.14": {
            "text": "bisschen niederschlag"
        }
    }
}