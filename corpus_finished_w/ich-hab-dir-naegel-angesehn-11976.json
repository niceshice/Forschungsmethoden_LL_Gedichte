{
    "title": "ich-hab-dir-naegel-angesehn-11976",
    "gender": "w",
    "author": "Carolin Callies",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "was schrafft dort am kiesgrab"
        },
        "line.2": {
            "text": "ein grasbefund, scheurig,"
        },
        "line.3": {
            "text": "der närbt sich, der färbt"
        },
        "line.4": {
            "text": "bankberücktes, das holz blättert ab?"
        },
        "line.5": {
            "text": "wo liegt dieses pforten am hals?"
        },
        "line.6": {
            "text": "es grub sich dir heimlich"
        },
        "line.7": {
            "text": "holzmasernd hautunter"
        },
        "line.8": {
            "text": "& war zuvor holznarbengroß"
        },
        "line.9": {
            "text": "& fährt dir ein schären darüber?"
        },
        "line.10": {
            "text": "was trocknet &"
        },
        "line.11": {
            "text": "was feucht bleibt,"
        },
        "line.12": {
            "text": "das fällt dir früh vornüber."
        }
    }
}