{
    "title": "summertime-6064",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "zur linken wuchs meine mutter"
        },
        "line.2": {
            "text": "auf herausgestorben sind ihr"
        },
        "line.3": {
            "text": "mutter und vater stumpf"
        },
        "line.4": {
            "text": "und stiel ein schrumpfhaus woraus"
        },
        "line.5": {
            "text": "sechs kerne sechs kinder"
        },
        "line.6": {
            "text": "fielen nicht weit ein kreis"
        },
        "line.7": {
            "text": "von steinwürfen ascheradien streut"
        },
        "line.8": {
            "text": "das haus aus frischen gardinen"
        },
        "line.9": {
            "text": "geranien und allem die beiden"
        },
        "line.10": {
            "text": "berliner männer verbrennen auf"
        },
        "line.11": {
            "text": "den nägeln karnickelfangschläge"
        },
        "line.12": {
            "text": "geht ein sehnen"
        },
        "line.13": {
            "text": "schenkt ein"
        },
        "line.14": {
            "text": "meine mutter mein vater wachsen"
        },
        "line.15": {
            "text": "langsam am rande an acker"
        },
        "line.16": {
            "text": "winden zeitweise weht"
        },
        "line.17": {
            "text": "ein einzelnes kind"
        },
        "line.18": {
            "text": "vorbei misst keinem einzigen eine"
        },
        "line.19": {
            "text": "seele zu blutsuppe alles"
        },
        "line.20": {
            "text": "dampft ab die totgesagten"
        },
        "line.21": {
            "text": "glänzen schmachtfetzen in"
        },
        "line.22": {
            "text": "ihren fenstern das kind geht"
        },
        "line.23": {
            "text": "gemach erst wieder beim haus der zwei"
        },
        "line.24": {
            "text": "männer kommt nicht"
        },
        "line.25": {
            "text": "ins schwitzen höchstens der hitze wegen"
        },
        "line.26": {
            "text": "hier kriegt man die mittagsfrau"
        },
        "line.27": {
            "text": "noch zu sehen manchmal"
        },
        "line.28": {
            "text": "den feuermann treuliche blaffer"
        },
        "line.29": {
            "text": "stauben die regentrude"
        },
        "line.30": {
            "text": "ist ebenfalls arbeitslos"
        },
        "line.31": {
            "text": "un de katten sün high"
        },
        "line.32": {
            "text": "die roten raupen fressen bedächtig"
        },
        "line.33": {
            "text": "den raps nachts und das güldne getreide"
        },
        "line.34": {
            "text": "steht zitternd daneben noch doch"
        },
        "line.35": {
            "text": "kommt zeit kommt mähdrescher"
        },
        "line.36": {
            "text": "der mond hängt voll überm sandweg"
        },
        "line.37": {
            "text": "und schwer wie der bauch des nachbarn"
        }
    }
}