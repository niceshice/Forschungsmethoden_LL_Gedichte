{
    "title": "tiere-zu-fragen-11648",
    "gender": "w",
    "author": "Odile Kennel",
    "author_birth": "1967",
    "poem": {
        "line.1": {
            "text": "Fragen zu Tieren"
        },
        "line.2": {
            "text": "und Tiere zu Fragen"
        },
        "line.3": {
            "text": "zu Flugscharen von"
        },
        "line.4": {
            "text": "schartigen Schaben"
        },
        "line.5": {
            "text": "Schabernack treibenden"
        },
        "line.6": {
            "text": "Staren, treiben"
        },
        "line.7": {
            "text": "windige Winzlinge"
        },
        "line.8": {
            "text": "ins Auge oder sind das"
        },
        "line.9": {
            "text": "Flusen, Pudelmuster"
        },
        "line.10": {
            "text": "auf Blusen, schon"
        },
        "line.11": {
            "text": "wummern Hufe"
        },
        "line.12": {
            "text": "an Schl채fen, z채hl"
        },
        "line.13": {
            "text": "Sch채fchen, z채hl"
        },
        "line.14": {
            "text": "wolliges Dasein"
        },
        "line.15": {
            "text": "auf Deichen, in Teichen"
        },
        "line.16": {
            "text": "Schalenweichtiere"
        },
        "line.17": {
            "text": "und Schnabeltiereier"
        },
        "line.18": {
            "text": "im Lolch, Molche"
        },
        "line.19": {
            "text": "besing, besing solche"
        },
        "line.20": {
            "text": "die schweigen, Zwei-"
        },
        "line.21": {
            "text": "seitentiere, weiche"
        },
        "line.22": {
            "text": "Korallen, und Quallen"
        },
        "line.23": {
            "text": "vergiss nicht, die"
        },
        "line.24": {
            "text": "auch Durst haben"
        },
        "line.25": {
            "text": "Mondfische, Woll-"
        },
        "line.26": {
            "text": "schweine, eigentliche"
        },
        "line.27": {
            "text": "Eulen, Chako-Pekaris"
        },
        "line.28": {
            "text": "Okapis, Gorgonen"
        },
        "line.29": {
            "text": "polternde Drohnen"
        },
        "line.30": {
            "text": "und jetzt die Fragen:"
        }
    }
}