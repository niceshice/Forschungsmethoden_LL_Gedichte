{
    "title": "die-illusion-von-linien-16218",
    "gender": "w",
    "author": "Caca  Savic",
    "author_birth": "1977",
    "poem": {
        "line.1": {
            "text": "die Illusion von Linien und die Skizze der"
        },
        "line.2": {
            "text": "Wirklichkeit als schonungslose Vorsehung."
        },
        "line.3": {
            "text": "zerteilter Beton ähnlich einem Ziel, nur"
        },
        "line.4": {
            "text": "immerwährend, dazwischen zwingend."
        },
        "line.5": {
            "text": "unbebaute Gebiete werden durch"
        },
        "line.6": {
            "text": "Begrifflichkeiten bestimmt, illusorisch genannt"
        }
    }
}