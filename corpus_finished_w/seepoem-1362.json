{
    "title": "seepoem-1362",
    "gender": "w",
    "author": "Marion Poschmann",
    "author_birth": "1969",
    "poem": {
        "line.1": {
            "text": "weit hinaus"
        },
        "line.2": {
            "text": "schwimmen"
        },
        "line.3": {
            "text": "schwingender, nur in Ausschnitten bekannter Korpus"
        },
        "line.4": {
            "text": "leichte Beute des Blicks und"
        },
        "line.5": {
            "text": "auf seinem eigenen Schatten dahingleitend"
        },
        "line.6": {
            "text": "unter der Haut"
        },
        "line.7": {
            "text": "empfanden wir die ungedeckten Stellen nach,"
        },
        "line.8": {
            "text": "weiche Rosetten, wulstig"
        },
        "line.9": {
            "text": "wie ein aufgeschwemmtes Spiegelbild auf Christbaumkugeln,"
        },
        "line.10": {
            "text": "überstäubt, gepudert, ein"
        },
        "line.11": {
            "text": "Spielball der Wellen, der Gischt; deren"
        },
        "line.12": {
            "text": "plötzlicher Wintereinbruch,"
        },
        "line.13": {
            "text": "die starken Gebrauchsspuren eines fernen Bildes"
        },
        "line.14": {
            "text": "machte sich einbalsamiert auf den Weg,"
        },
        "line.15": {
            "text": "weiße Fracht, ein Schrein für die"
        },
        "line.16": {
            "text": "Flüchtigkeit des Gefühls, er"
        },
        "line.17": {
            "text": "setzte dem Wasser ein inneres Maß entgegen"
        }
    }
}