{
    "title": "nur-1017",
    "gender": "w",
    "author": "Brigitte Oleschinski",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "war deine Nähe nicht eßbar, leer"
        },
        "line.2": {
            "text": "bäumt sich der Magen nach dir, speit"
        },
        "line.3": {
            "text": "seinen Hunger nach diesem hohen Hundeton"
        },
        "line.4": {
            "text": "über allen Deutschfrequenzen, speit,"
        },
        "line.5": {
            "text": "speit"
        },
        "line.6": {
            "text": "Erbrechen wollte ich mich vor Glück"
        },
        "line.7": {
            "text": "mit deinem schmelzenden Margarinewürfel"
        },
        "line.8": {
            "text": "im Leib, im Gleichtakt des Stechzwangs"
        },
        "line.9": {
            "text": "spukblaß"
        },
        "line.10": {
            "text": "wie links und rechts am Bett"
        },
        "line.11": {
            "text": "die echten Rindsblasenschirme"
        }
    }
}