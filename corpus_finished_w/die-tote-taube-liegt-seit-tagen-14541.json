{
    "title": "die-tote-taube-liegt-seit-tagen-14541",
    "gender": "w",
    "author": "Ruth Wolf-Rehfeldt",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "Die tote Taube liegt seit Tagen"
        },
        "line.2": {
            "text": "Und keiner wagt"
        },
        "line.3": {
            "text": "Sie fortzutragen"
        },
        "line.4": {
            "text": "Die tote Taube liegt seit Tagen"
        },
        "line.5": {
            "text": "Auf Pflastersteinen"
        },
        "line.6": {
            "text": "Im häßlichen Hof"
        }
    }
}