{
    "title": "im-november-erst-im-november-16574",
    "gender": "w",
    "author": "Daniela  Chana",
    "author_birth": "1985",
    "poem": {
        "line.1": {
            "text": "Im November, erst im November"
        },
        "line.2": {
            "text": "Kommt mein Liebster zurück"
        },
        "line.3": {
            "text": "Bis dahin:"
        },
        "line.4": {
            "text": "Werden die Wölfe mir ein Anstecktuch"
        },
        "line.5": {
            "text": "Aus ihrem Pelz reichen"
        },
        "line.6": {
            "text": "Und ein Riechsalz"
        },
        "line.7": {
            "text": "Und mir anonym Blumen schicken"
        },
        "line.8": {
            "text": "Ich werde dicke Bücher lesen"
        },
        "line.9": {
            "text": "Und mich auf den Postboten freuen"
        },
        "line.10": {
            "text": "Der mich fragt, wer mein Verehrer ist"
        },
        "line.11": {
            "text": "Ich werde Konstellationen auf einer Tafel verrücken"
        },
        "line.12": {
            "text": "Dass November etwas schneller kommt"
        },
        "line.13": {
            "text": "Und die Wölfe ihre Motoren starten"
        },
        "line.14": {
            "text": "Und wieder in ihren Höhlen parken"
        },
        "line.15": {
            "text": "Dann kommt mein Liebster zurück"
        },
        "line.16": {
            "text": "Im November, erst im November"
        },
        "line.17": {
            "text": "Werde ich wieder kochen und mich schön machen"
        },
        "line.18": {
            "text": "Vorher laufe ich hässlich mit den Wölfen"
        },
        "line.19": {
            "text": "Und wir bestellen Pizza"
        }
    }
}