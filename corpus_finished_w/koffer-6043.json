{
    "title": "koffer-6043",
    "gender": "w",
    "author": "Ilma Rakusa",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Koffer sind Koffer"
        },
        "line.2": {
            "text": "sind Abschied"
        },
        "line.3": {
            "text": "sind Leder"
        },
        "line.4": {
            "text": "sind Fass-mal-an"
        },
        "line.5": {
            "text": "sind Pack-mich-voll"
        },
        "line.6": {
            "text": "und wieder aus"
        },
        "line.7": {
            "text": "sind Bäuche"
        },
        "line.8": {
            "text": "sind Häuschen"
        },
        "line.9": {
            "text": "sind Wir-ziehen-von-"
        },
        "line.10": {
            "text": "hier-nach-dort"
        },
        "line.11": {
            "text": "und von dort"
        },
        "line.12": {
            "text": "ach ja"
        },
        "line.13": {
            "text": "nach weiter"
        }
    }
}