{
    "title": "erste-spruenge-16166",
    "gender": "w",
    "author": "Sandra Hubinger",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "Erste Sprünge"
        },
        "line.2": {
            "text": "in der Terrassenverglasung"
        },
        "line.3": {
            "text": "vom Aufprall kleiner Vögel und unsichtbar"
        },
        "line.4": {
            "text": "die Sporen in den Mauern was da spröde"
        },
        "line.5": {
            "text": "brüchig wurde ohne die Weichmacher"
        },
        "line.6": {
            "text": "ummantelte nun ein grüner Flaum"
        },
        "line.7": {
            "text": "Heraus gefallen die rostigen Nägel"
        },
        "line.8": {
            "text": "in ihren Löchern dehnte das Wasser"
        },
        "line.9": {
            "text": "allen voran das Wasser in Ritzen und"
        },
        "line.10": {
            "text": "Schnittkanten unter Türschwellen in Häuser"
        },
        "line.11": {
            "text": "Absackende Mauern unter dem Druck"
        },
        "line.12": {
            "text": "der Schwerkraft Wände sich zur Seite lehnten"
        },
        "line.13": {
            "text": "das Dach nicht mehr hielten"
        }
    }
}