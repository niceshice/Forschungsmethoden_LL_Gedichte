{
    "title": "wirrsprunggesicht-zugriss-durchruck-2881",
    "gender": "w",
    "author": "Elisabeth  Wandeler-Deck",
    "author_birth": "1939",
    "poem": {
        "line.1": {
            "text": "Wirrsprunggesicht Zugriss Durchruck"
        },
        "line.2": {
            "text": "augengepickt frohgesperrt fließrig"
        },
        "line.3": {
            "text": "süßstofflicher Wasserfall glitzriges Abschaumgefälle digitale Notwehr"
        },
        "line.4": {
            "text": "durch aus von nach"
        },
        "line.5": {
            "text": "aufwärts durchher hinweg"
        },
        "line.6": {
            "text": "(hatte schon Zugriff schon bald und entglitzert die Durchdrückpackung :"
        },
        "line.7": {
            "text": "mein Ponstan! mein Panadol! Schrie der Kecke und flog auf)"
        }
    }
}