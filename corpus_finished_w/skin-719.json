{
    "title": "skin-719",
    "gender": "w",
    "author": "Sabine Scho",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "säureschutz und druck"
        },
        "line.2": {
            "text": "empfindlichkeit, zeig"
        },
        "line.3": {
            "text": "mal das bein,"
        },
        "line.4": {
            "text": "a heart-"
        },
        "line.5": {
            "text": "shaped bruise"
        },
        "line.6": {
            "text": ", my"
        },
        "line.7": {
            "text": "business, was geht"
        },
        "line.8": {
            "text": "es dich an, bestimm-"
        },
        "line.9": {
            "text": "ungsübung an reinzieh-"
        },
        "line.10": {
            "text": "sachen, die sorge kam"
        },
        "line.11": {
            "text": "doch schon längst ab-"
        },
        "line.12": {
            "text": "handen, flächenüber-"
        },
        "line.13": {
            "text": "greifend einen oberarm"
        },
        "line.14": {
            "text": "umfassen, ein funkeln"
        },
        "line.15": {
            "text": "ein zischen bemäntelt"
        },
        "line.16": {
            "text": "das lachen, das muss"
        },
        "line.17": {
            "text": "erst einziehen, die epi-"
        },
        "line.18": {
            "text": "kritischen stellen, warte"
        },
        "line.19": {
            "text": "ich halte das kühlkissen"
        }
    }
}