{
    "title": "abrechnung-gegen-1802",
    "gender": "w",
    "author": "Isabeella Beumer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "abrechnung gegen"
        },
        "line.2": {
            "text": "gleichzeitigkeit und"
        },
        "line.3": {
            "text": "den schwarzen fleck der glut gefacht"
        },
        "line.4": {
            "text": "feuer roter augenfahnen"
        },
        "line.5": {
            "text": "einer fieberhaftentrommel"
        },
        "line.6": {
            "text": "unregelmäszige schläge der herzen"
        },
        "line.7": {
            "text": "mechanische kälte in die zeit gepresst"
        },
        "line.8": {
            "text": "nichts als gekämmte narben"
        },
        "line.9": {
            "text": "spitzendieschärfeglänzendersäbel"
        },
        "line.10": {
            "text": "pfade in die angst signiert"
        },
        "line.11": {
            "text": "bis das schwarz der vögel"
        },
        "line.12": {
            "text": "blasse gesichter härtet."
        },
        "line.13": {
            "text": "fieberhafte schläge in die zeit gepresst"
        },
        "line.14": {
            "text": "das schweigen der glut gefacht"
        },
        "line.15": {
            "text": "schläge der herzen verwittern das geräusch"
        },
        "line.16": {
            "text": "wind als schrei hinter türen"
        },
        "line.17": {
            "text": "an die wände gespritzt"
        },
        "line.18": {
            "text": "in augen toter pferde ein lied gesungen"
        },
        "line.19": {
            "text": "frauen wenden ihr gesicht"
        },
        "line.20": {
            "text": "in tiefstürzenden atem"
        },
        "line.21": {
            "text": "der furcht gebar."
        }
    }
}