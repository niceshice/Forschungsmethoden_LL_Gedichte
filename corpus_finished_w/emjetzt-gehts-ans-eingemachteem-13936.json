{
    "title": "emjetzt-gehts-ans-eingemachteem-13936",
    "gender": "w",
    "author": "Róža Domašcyna",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "auf alleen sandhaufen"
        },
        "line.2": {
            "text": "asche anstelle schneeaufschüttungen"
        },
        "line.3": {
            "text": "war das mal haussaum? randlagig gebaut"
        },
        "line.4": {
            "text": "dann gekapert ausgeraubt"
        },
        "line.5": {
            "text": "lauter machtränke statt efeuranken"
        },
        "line.6": {
            "text": "kein aufgebot an anheimnahme dafür"
        },
        "line.7": {
            "text": "halbherziges warmhalten kein augentrost"
        },
        "line.8": {
            "text": "ein auf und ab: baugrund aufstockung bombastisches"
        },
        "line.9": {
            "text": "dann worte wie schutzgebiete dann abriss abraum"
        },
        "line.10": {
            "text": "verfestigungen greifen nicht"
        },
        "line.11": {
            "text": "wandeln baut auf wandern"
        },
        "line.12": {
            "text": "wie jede düne auf den wind"
        },
        "line.13": {
            "text": "jede zeichnung am strand auf die welle"
        },
        "line.14": {
            "text": "jedes herz auf die materialermüdung"
        },
        "line.15": {
            "text": "aller aufbau auf die begradigung"
        },
        "line.16": {
            "text": "das fasst alles"
        }
    }
}