{
    "title": "bauplaene-im-kopf-16165",
    "gender": "w",
    "author": "Sandra Hubinger",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "Baupläne im Kopf"
        },
        "line.2": {
            "text": "das Material ausgelegt"
        },
        "line.3": {
            "text": "auf einer trockenen Plane unter Beachtung"
        },
        "line.4": {
            "text": "von Relationsregeln lag in Reihen geordnet"
        },
        "line.5": {
            "text": "nach Gunst nach Verführungskunst jüngst Erbeutetes"
        },
        "line.6": {
            "text": "Gewachsenes wie Beere Blüte Feder"
        },
        "line.7": {
            "text": "Gemachtes wie Strohhalm Haarband Haftel"
        },
        "line.8": {
            "text": "bedeckt von häkchenförmigem Moos verknotet"
        },
        "line.9": {
            "text": "verflochten mit Wurzeln fadendünn griffen"
        },
        "line.10": {
            "text": "Unsere Finger dazwischen schoben"
        },
        "line.11": {
            "text": "die Objekte hoben sie öffneten"
        },
        "line.12": {
            "text": "Zwischenräume und Gänge das Sprechen"
        },
        "line.13": {
            "text": "der Dinge ein visuelles Wispern"
        }
    }
}