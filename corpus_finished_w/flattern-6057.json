{
    "title": "flattern-6057",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "das sind"
        },
        "line.2": {
            "text": "die stürme die blinden"
        },
        "line.3": {
            "text": "enden des winters das"
        },
        "line.4": {
            "text": "äquinoktium schiebt sich"
        },
        "line.5": {
            "text": "tief in meinen"
        },
        "line.6": {
            "text": "gamat"
        },
        "line.7": {
            "text": "mein gemüt"
        },
        "line.8": {
            "text": "fiebert von heizungswinden"
        },
        "line.9": {
            "text": "bindet die enden das fürchten"
        },
        "line.10": {
            "text": "der ritterbürtigen in meine"
        },
        "line.11": {
            "text": "wände concrete ein zerren"
        },
        "line.12": {
            "text": "und zerren in diesen"
        },
        "line.13": {
            "text": "tagen heiß ich annette"
        },
        "line.14": {
            "text": "ist es"
        },
        "line.15": {
            "text": "nicht gut wenn das wachen"
        },
        "line.16": {
            "text": "anwächst unter der sonne"
        },
        "line.17": {
            "text": "kein ding verborgen"
        },
        "line.18": {
            "text": "bleiben kann meerhäschen"
        },
        "line.19": {
            "text": "sitzt mir im dutt"
        },
        "line.20": {
            "text": "gleich"
        },
        "line.21": {
            "text": "einem artigen kinde"
        },
        "line.22": {
            "text": "ich rufe durch meine wände"
        },
        "line.23": {
            "text": "dringt es wie: turm"
        },
        "line.24": {
            "text": "alle wissen"
        },
        "line.25": {
            "text": "mich in sicherheit winken"
        },
        "line.26": {
            "text": "meiner betonfrisur"
        }
    }
}