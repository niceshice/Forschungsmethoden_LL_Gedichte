{
    "title": "wieder-14654",
    "gender": "w",
    "author": "Lisa Elsässer",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "schichte ich alte hölzer"
        },
        "line.2": {
            "text": "sie scheuen feuer und so"
        },
        "line.3": {
            "text": "brennt wieder nur der tag"
        },
        "line.4": {
            "text": "erloschen weht seine"
        },
        "line.5": {
            "text": "asche in die dämmerung"
        },
        "line.6": {
            "text": "kalt die nacht ein tier"
        },
        "line.7": {
            "text": "ohne fell und hier höre ich"
        },
        "line.8": {
            "text": "die sprache zwischen holz"
        },
        "line.9": {
            "text": "und scheu brennen"
        },
        "line.10": {
            "text": "alle schichten lichterloh"
        }
    }
}