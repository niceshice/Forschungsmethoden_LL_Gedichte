{
    "title": "ist-wirklich-alles-durch-beatmung-begruendet-13684",
    "gender": "w",
    "author": "Claudia Gabler",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "Ist wirklich alles durch Beatmung begründet? Und was alles"
        },
        "line.2": {
            "text": "zwischen der Erde und dir bleibt eigentlich unausgesprochen?"
        },
        "line.3": {
            "text": ""
        },
        "line.4": {
            "text": "Schau, es ist doch nicht nur lustig"
        },
        "line.5": {
            "text": "mit der Vollendung."
        },
        "line.6": {
            "text": ""
        },
        "line.7": {
            "text": "Nun ist schon wieder eine Sekunde vergangen,"
        },
        "line.8": {
            "text": "in der du kein Gedicht geschrieben hast."
        },
        "line.9": {
            "text": ""
        },
        "line.10": {
            "text": "Schau, wie Personen reden."
        },
        "line.11": {
            "text": "Wie die Dinge reden."
        },
        "line.12": {
            "text": ""
        },
        "line.13": {
            "text": "Schon glitzert die Künstlichkeit"
        },
        "line.14": {
            "text": "und raubt dir den einen oder anderen Stern."
        },
        "line.15": {
            "text": ""
        },
        "line.16": {
            "text": "Nach dem du greifen wolltest,"
        },
        "line.17": {
            "text": "wenn die Balkone gestrichen sind."
        }
    }
}