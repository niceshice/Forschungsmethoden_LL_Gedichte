{
    "title": "die-farne-lappen-den-fluss-431",
    "gender": "w",
    "author": "Ilma Rakusa",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Die Farne lappen in den Fluß"
        },
        "line.2": {
            "text": "der trüb durch Böhmen fließt"
        },
        "line.3": {
            "text": "das Mückengift verstört"
        },
        "line.4": {
            "text": "die Hunde mit den langen Zungen"
        },
        "line.5": {
            "text": "hecheln durch das Gras"
        },
        "line.6": {
            "text": "ein Wanderer liegt"
        },
        "line.7": {
            "text": "und aus den Schloten"
        },
        "line.8": {
            "text": "der Papierfabrik"
        },
        "line.9": {
            "text": "steigt schnelles Gas"
        }
    }
}