{
    "title": "planetarium-15196",
    "gender": "w",
    "author": "Anja Kampmann",
    "author_birth": "1983",
    "poem": {
        "line.1": {
            "text": "hören wir uns atmen in der nacht"
        },
        "line.2": {
            "text": "am morgen ein specht"
        },
        "line.3": {
            "text": "wertet die lochkarten aus"
        },
        "line.4": {
            "text": "der gewöhnlichen träume"
        },
        "line.5": {
            "text": "dort sind wir nicht gewesen"
        },
        "line.6": {
            "text": "schwärze wie ein fliehendes reh"
        },
        "line.7": {
            "text": "die uns umschloss fern der häuser"
        },
        "line.8": {
            "text": "wenig lässt sich sagen"
        },
        "line.9": {
            "text": "über die verbindungslinien der sterne"
        },
        "line.10": {
            "text": "die bäume stehen nass und voller tropfen"
        },
        "line.11": {
            "text": "im planetarium früherer zeit"
        },
        "line.12": {
            "text": "war es eine lochkarte"
        },
        "line.13": {
            "text": "die jemand mit nadeln durchstach"
        },
        "line.14": {
            "text": "wie um die schmerzenden stellen"
        },
        "line.15": {
            "text": "zu finden."
        }
    }
}