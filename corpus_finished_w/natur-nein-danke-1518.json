{
    "title": "natur-nein-danke-1518",
    "gender": "w",
    "author": "Elfriede Gerstl",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "von zeit zu zeit seh ich sie gern"
        },
        "line.2": {
            "text": "die vergifteten bäume"
        },
        "line.3": {
            "text": "die befallenen wiesen"
        },
        "line.4": {
            "text": "diese verlauste landschaft"
        },
        "line.5": {
            "text": "aus dem zugfenster meines abteils"
        },
        "line.6": {
            "text": "wo ich mich gerüstet fühle"
        },
        "line.7": {
            "text": "mit tinkturen und"
        },
        "line.8": {
            "text": "tabletten und"
        },
        "line.9": {
            "text": "anderer munition"
        },
        "line.10": {
            "text": "gegen die bissigen bakterien"
        },
        "line.11": {
            "text": "die killerviren"
        },
        "line.12": {
            "text": "das riesige feindliche heer"
        },
        "line.13": {
            "text": "an mir und in mir"
        },
        "line.14": {
            "text": "soll ich vielleicht hinaustreten"
        },
        "line.15": {
            "text": "ins verseuchte grün"
        },
        "line.16": {
            "text": "wo neue feinde warten"
        },
        "line.17": {
            "text": "nein danke sage ich zu meinen freunden"
        },
        "line.18": {
            "text": "den berg- und talsteigern"
        },
        "line.19": {
            "text": "ich habe hier drinnen"
        },
        "line.20": {
            "text": "schon genug natur"
        },
        "line.21": {
            "text": "ein beiwort entbieten"
        },
        "line.22": {
            "text": "einen satz setzen"
        },
        "line.23": {
            "text": "eine tat träumen"
        },
        "line.24": {
            "text": "deinen mund mit meinem"
        },
        "line.25": {
            "text": "verwechseln"
        },
        "line.26": {
            "text": "wo bitte ist der bahnhof"
        },
        "line.27": {
            "text": "ich bin fremd in dieser stadt"
        },
        "line.28": {
            "text": "wo ist denn deine hand"
        },
        "line.29": {
            "text": "wo ist denn dein mund"
        },
        "line.30": {
            "text": "hier bin ich nicht mehr"
        },
        "line.31": {
            "text": "zu hause"
        },
        "line.32": {
            "text": "die krähen fliegen"
        },
        "line.33": {
            "text": "wie in einem gedicht"
        },
        "line.34": {
            "text": "diese winterlandschaft"
        },
        "line.35": {
            "text": "ist ein bild hinter glas"
        },
        "line.36": {
            "text": "diese bahnfahrt ist ein schlechter"
        },
        "line.37": {
            "text": "trauriger film"
        },
        "line.38": {
            "text": "den ich bis zum ende"
        },
        "line.39": {
            "text": "anschauen muss"
        }
    }
}