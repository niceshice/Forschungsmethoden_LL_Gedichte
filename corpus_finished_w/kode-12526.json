{
    "title": "kode-12526",
    "gender": "w",
    "author": "Swantje  Lichtenstein",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "KODE Verzerrt verschwinden in der Ähnlichkeit"
        },
        "line.2": {
            "text": "Zellen. Aufgelöste Analyse-Systeme."
        },
        "line.3": {
            "text": "Erkennt irgendwen. Rufe ich an."
        },
        "line.4": {
            "text": "Mit Metaautomaten als Sendestationen,"
        },
        "line.5": {
            "text": "im gleichen Radius rennen wir um den Baum,"
        },
        "line.6": {
            "text": "den Drumherumraum der Rumpelkammer."
        },
        "line.7": {
            "text": "Häppchen mit Stimmgabeln."
        },
        "line.8": {
            "text": "Süppchen mit Lippenlöffeln."
        },
        "line.9": {
            "text": "Liebkost in Lümmelhaltung"
        },
        "line.10": {
            "text": "Zenbuddhisten. Lotsen lotsen:"
        },
        "line.11": {
            "text": "Is-there-any-how, another Howl?"
        },
        "line.12": {
            "text": "In ungeheuerlicher Lauerstellung"
        },
        "line.13": {
            "text": "auf die Lichtwalze, die Sendemasten."
        },
        "line.14": {
            "text": "Filtrierung der Maschinenmaschinen."
        },
        "line.15": {
            "text": "Konkrete Konzeptualismen, sag ich,"
        },
        "line.16": {
            "text": "er denkt, ich wisse nicht, was ich sage,"
        },
        "line.17": {
            "text": "mit welcher Stimme auch immer."
        },
        "line.18": {
            "text": "Klar, trink ein Glas Milch"
        },
        "line.19": {
            "text": "zuck doch, oder zupf den Faden"
        },
        "line.20": {
            "text": "vom Angorajäckchen als Simulation:"
        },
        "line.21": {
            "text": "Guten Morgen, Mitternacht!,"
        },
        "line.22": {
            "text": "ich habe darüber nachgedacht"
        },
        "line.23": {
            "text": "und an Lautsprechern gezerrt,"
        },
        "line.24": {
            "text": "die zieren Wände, sie wissen"
        },
        "line.25": {
            "text": "nicht und haben keine Antwort."
        },
        "line.26": {
            "text": "Der Rasen wächst, den zieht man nicht."
        },
        "line.27": {
            "text": "Sieht so aus. Raschle bloß nicht mit"
        },
        "line.28": {
            "text": "dem Papier."
        }
    }
}