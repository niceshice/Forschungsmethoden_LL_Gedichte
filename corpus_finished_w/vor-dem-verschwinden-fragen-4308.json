{
    "title": "vor-dem-verschwinden-fragen-4308",
    "gender": "w",
    "author": "Andrea Heuser",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "FRAGEN nicht mehr wieder und"
        },
        "line.2": {
            "text": "wieder was, was hat sie"
        },
        "line.3": {
            "text": "sammeln, da waren in all den"
        },
        "line.4": {
            "text": "altkleidersprachen doch auch sommer"
        },
        "line.5": {
            "text": "wiesen, tage, als verschöben nur wolken"
        },
        "line.6": {
            "text": "licht, darin,"
        },
        "line.7": {
            "text": "last & lost"
        },
        "line.8": {
            "text": ", puppenkopf"
        },
        "line.9": {
            "text": "förmchen und pferde, buntstiftblumen, garten"
        },
        "line.10": {
            "text": "gezäunt ihr rock, kniestrumpf und turnschuh, zahn"
        },
        "line.11": {
            "text": "spangenrot, ihr fahrrad im flieder, flirren, das flirren zeit"
        },
        "line.12": {
            "text": "gebreitet über den gleisen warum, was"
        },
        "line.13": {
            "text": "was hast du gesehen,"
        }
    }
}