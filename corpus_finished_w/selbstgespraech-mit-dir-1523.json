{
    "title": "selbstgespraech-mit-dir-1523",
    "gender": "w",
    "author": "Elfriede Gerstl",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "mitten in der ewigkeit"
        },
        "line.2": {
            "text": "kommst du auf mich zu"
        },
        "line.3": {
            "text": "offerierst biskotten zum tee"
        },
        "line.4": {
            "text": "ich esse sie ungern"
        },
        "line.5": {
            "text": "dass ich dich nicht immer gekannt habe"
        },
        "line.6": {
            "text": "geht mir durch den kopf"
        },
        "line.7": {
            "text": "mehrere leben in eines gezw√§ngt"
        },
        "line.8": {
            "text": "kein grund unzufrieden zu sein"
        },
        "line.9": {
            "text": "kein grund zufrieden zu sein"
        },
        "line.10": {
            "text": "ein blatt im wind"
        },
        "line.11": {
            "text": "und nimmt sich so wichtig"
        },
        "line.12": {
            "text": "oktober 2002"
        }
    }
}