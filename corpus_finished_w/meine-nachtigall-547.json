{
    "title": "meine-nachtigall-547",
    "gender": "w",
    "author": "Rose Ausländer",
    "author_birth": "1901",
    "poem": {
        "line.1": {
            "text": "Meine Mutter war einmal ein Reh"
        },
        "line.2": {
            "text": "Die goldbraunen Augen"
        },
        "line.3": {
            "text": "die Anmut"
        },
        "line.4": {
            "text": "blieben ihr aus der Rehzeit"
        },
        "line.5": {
            "text": "Hier war sie"
        },
        "line.6": {
            "text": "halb Engel halb Mensch-"
        },
        "line.7": {
            "text": "die Mitte war Mutter"
        },
        "line.8": {
            "text": "Als ich sie fragte was sie gern geworden wäre"
        },
        "line.9": {
            "text": "sagte sie: eine Nachtigall"
        },
        "line.10": {
            "text": "Jetzt ist sie eine Nachtigall"
        },
        "line.11": {
            "text": "Nacht um Nacht höre ich sie"
        },
        "line.12": {
            "text": "im Garten meines schlaflosen Traumes"
        },
        "line.13": {
            "text": "Sie singt das Zion der Ahnen"
        },
        "line.14": {
            "text": "sie singt das alte Österreich"
        },
        "line.15": {
            "text": "sie singt die Berge und Buchenwälder"
        },
        "line.16": {
            "text": "der Bukowina"
        },
        "line.17": {
            "text": "Wiegenlieder"
        },
        "line.18": {
            "text": "singt mir Nacht um Nacht"
        },
        "line.19": {
            "text": "meine Nachtigall"
        },
        "line.20": {
            "text": "im Garten meines schlaflosen Traumes"
        }
    }
}