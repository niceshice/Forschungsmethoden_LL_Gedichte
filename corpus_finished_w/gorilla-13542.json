{
    "title": "gorilla-13542",
    "gender": "w",
    "author": "Sabine Scho",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "what was the question?"
        },
        "line.2": {
            "text": "I was looking at the big sky"
        },
        "line.3": {
            "text": "kate bush"
        },
        "line.4": {
            "text": "die frage, wo der blick aufhört"
        },
        "line.5": {
            "text": "oder ob und wie er sich verliert"
        },
        "line.6": {
            "text": "ist eine frage nach reichweite"
        },
        "line.7": {
            "text": "die suggeriert, man könne ein"
        },
        "line.8": {
            "text": "auge werfen, wo doch umgekehrt"
        },
        "line.9": {
            "text": "zurückgeworfen wird, licht in"
        },
        "line.10": {
            "text": "wellen / teilchen"
        },
        "line.11": {
            "text": "das auge lässt sich nicht präparieren"
        },
        "line.12": {
            "text": "verhält sich wie pudding, man muss es"
        },
        "line.13": {
            "text": "ersetzen, hauen sie diesem gorilla"
        },
        "line.14": {
            "text": "ein veilchen, das bleibt sicher unbemerkt"
        },
        "line.15": {
            "text": "in seinem schwarzvioletten gesicht, ob der"
        },
        "line.16": {
            "text": "dann sternchen sieht? das seh ich so nicht"
        },
        "line.17": {
            "text": "pax 6, das masterkontrollgen verzapft"
        },
        "line.18": {
            "text": "schöne augen an jeder beliebigen stelle"
        },
        "line.19": {
            "text": "wenn sie so wollen!vieläugige iegen"
        },
        "line.20": {
            "text": "iegen, wenn sie die evolution ganz"
        },
        "line.21": {
            "text": "runterscrollen, augenlosen hybriden"
        },
        "line.22": {
            "text": "hinterher, das könnte ich noch erklären"
        },
        "line.23": {
            "text": "aber kennen sie den"
        },
        "line.24": {
            "text": "sagt ein evolutionsbiologe"
        },
        "line.25": {
            "text": "zum kreationisten: wir haben"
        },
        "line.26": {
            "text": "alle darwin gelesen"
        },
        "line.27": {
            "text": "sagt der kreationist: ich nicht"
        }
    }
}