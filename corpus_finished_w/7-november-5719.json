{
    "title": "7-november-5719",
    "gender": "w",
    "author": "Elfriede Czurda",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "schon wieder nennt das hirn"
        },
        "line.2": {
            "text": "sich selbst wenn es so elend"
        },
        "line.3": {
            "text": "faulenzt heut in diesem turner"
        },
        "line.4": {
            "text": "rumpf knie beugt und streckt"
        },
        "line.5": {
            "text": "und ächzt und stöhnt und statt"
        },
        "line.6": {
            "text": "an die bewegung denkt wie groß"
        },
        "line.7": {
            "text": "die blätter sind der bäume"
        },
        "line.8": {
            "text": "die da draußen jetzt die ganze"
        },
        "line.9": {
            "text": "pracht so kostbar schön statt sie"
        },
        "line.10": {
            "text": "zu horten auf dem blättersparbuch"
        },
        "line.11": {
            "text": "mit jedem windhauch in die gosse"
        },
        "line.12": {
            "text": "werfen und leute trampeln drüber"
        },
        "line.13": {
            "text": "7. november; fitneßstudio"
        }
    }
}