{
    "title": "weltraum-13890",
    "gender": "w",
    "author": "Kerstin Becker",
    "author_birth": "1969",
    "poem": {
        "line.1": {
            "text": "wir scharen uns"
        },
        "line.2": {
            "text": "im Rücken der Fabrik"
        },
        "line.3": {
            "text": "zum Mondrakete baun"
        },
        "line.4": {
            "text": "wir schlagen uns"
        },
        "line.5": {
            "text": "durch Dorntentakel"
        },
        "line.6": {
            "text": "Wurzelkrallen Windenwürmer wir"
        },
        "line.7": {
            "text": "raffen Holz"
        },
        "line.8": {
            "text": "und ausrangierte Kästen aus Metall"
        },
        "line.9": {
            "text": "wir stopfen Glas"
        },
        "line.10": {
            "text": "faserwolle gegen Kosmoskälte"
        },
        "line.11": {
            "text": "und Sternensplitter in lavede Wände"
        },
        "line.12": {
            "text": "passt auf"
        },
        "line.13": {
            "text": "die Fasern gehn beim Atmen in die Lungenblasen"
        },
        "line.14": {
            "text": "schreit der Heizer aus dem schwarzen Fensterloch"
        },
        "line.15": {
            "text": "Holunderbeeren sauen unsre Sachen blau"
        },
        "line.16": {
            "text": "Brennnesseln beißen unsre Beine bis zum Bauch"
        },
        "line.17": {
            "text": "wir steuern unser Schiff mit Schrott"
        },
        "line.18": {
            "text": "wir fülln mit hohlen Händen Pfützenbrühe"
        },
        "line.19": {
            "text": "die schillert in den Tank der leckt"
        },
        "line.20": {
            "text": "wir klotzen unsre Finger kriegen Risse"
        },
        "line.21": {
            "text": "Motorbrummen mit dem Mund wir ziehen steile"
        },
        "line.22": {
            "text": "steigende Spiralen in das All"
        },
        "line.23": {
            "text": "bis wir verschmiert vom Diesseits"
        },
        "line.24": {
            "text": "abgeschnitten in der Kapsel glühn"
        }
    }
}