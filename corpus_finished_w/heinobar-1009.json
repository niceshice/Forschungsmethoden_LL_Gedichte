{
    "title": "heinobar-1009",
    "gender": "w",
    "author": "Ulrike Draesner",
    "author_birth": "1962",
    "poem": {
        "line.1": {
            "text": "insekten in den goldenen locken"
        },
        "line.2": {
            "text": "siegfried in worms in der konditorei."
        },
        "line.3": {
            "text": "bitte nicht so leuchtend starren"
        },
        "line.4": {
            "text": "es ist die heinobar ganz hag-gedeck"
        },
        "line.5": {
            "text": "teppich hünsch: hei!"
        },
        "line.6": {
            "text": "leih mir dein ohr."
        },
        "line.7": {
            "text": "transnationale mücken zischen zierlich:"
        },
        "line.8": {
            "text": "hellebarden langobarden bardenglut"
        },
        "line.9": {
            "text": "afghanistan. siegfried knackt selbst"
        },
        "line.10": {
            "text": "erneuernden heinokeks in zwergenform"
        },
        "line.11": {
            "text": "brütet dem einsatz nach: was sind traumatheorien"
        },
        "line.12": {
            "text": "gegen krümelkummer in der seele"
        },
        "line.13": {
            "text": "saelde"
        },
        "line.14": {
            "text": "(sold)? hellebarden langobarden (falsch)"
        },
        "line.15": {
            "text": "bardengut. siegfried in zwergenform:"
        },
        "line.16": {
            "text": "ist es geheuer keine lust"
        },
        "line.17": {
            "text": "auf umkommen zu hag- hag-"
        },
        "line.18": {
            "text": "haben? heidschi-bumm: tarnkappen-"
        },
        "line.19": {
            "text": "und ohrtransplan-"
        },
        "line.20": {
            "text": "ja, sig, auch so stabt"
        },
        "line.21": {
            "text": "sich TAT."
        },
        "line.22": {
            "text": "bist bloß ... soldat? einfache"
        },
        "line.23": {
            "text": "lösung: kleiner werden, smart dust"
        },
        "line.24": {
            "text": "um einmal noch kämpfend"
        },
        "line.25": {
            "text": "die erde zu umrunden"
        },
        "line.26": {
            "text": "als mythischer staub"
        },
        "line.27": {
            "text": "sig! dein logo steht: resolutes"
        },
        "line.28": {
            "text": "behagen. staub? ach. staben"
        },
        "line.29": {
            "text": "statt schlachten für"
        },
        "line.30": {
            "text": "schland."
        }
    }
}