    "poem": {
        "line.1": {
            "text": "Meine Zunge haftet am Gaum"
        },
        "line.2": {
            "text": "Wenn ich deiner gedenke"
        },
        "line.3": {
            "text": "Jerusalem"
        },
        "line.4": {
            "text": "in Freude kam ich zu dir"
        },
        "line.5": {
            "text": "ich stand im Licht"
        },
        "line.6": {
            "text": "und um mich standen die Tore"
        },
        "line.7": {
            "text": "immer gedenke ich dein"
        },
        "line.8": {
            "text": "drÃ¤ng mich an das Vermauerte"
        },
        "line.9": {
            "text": "du haftest am Gaum"
        },
        "line.10": {
            "text": "bitter und hart"
        }
    }
}