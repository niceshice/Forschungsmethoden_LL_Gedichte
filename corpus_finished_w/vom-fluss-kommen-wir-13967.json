{
    "title": "vom-fluss-kommen-wir-13967",
    "gender": "w",
    "author": "Andra Schwarz",
    "author_birth": "1982",
    "poem": {
        "line.1": {
            "text": "Vom fluss kommen wir an der nahle entlang"
        },
        "line.2": {
            "text": "wo das wasser flach ist durchsichtig & hell"
        },
        "line.3": {
            "text": "wachsen grasnarben schon siebenjährig"
        },
        "line.4": {
            "text": "unter unseren füßen laufende meter"
        },
        "line.5": {
            "text": "schotterflechten vernetzte natur"
        },
        "line.6": {
            "text": "mittendrin güterverkehr fünfmal täglich"
        },
        "line.7": {
            "text": "läuten signale fahren züge vorbei"
        },
        "line.8": {
            "text": "ins blaue hinein gerät eine schwalbe"
        },
        "line.9": {
            "text": "sie flieht vor dem abwind"
        },
        "line.10": {
            "text": "was war"
        }
    }
}