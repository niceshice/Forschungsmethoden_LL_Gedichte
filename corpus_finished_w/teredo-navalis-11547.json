{
    "title": "teredo-navalis-11547",
    "gender": "w",
    "author": "Nora Gomringer",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "In Stockholm, wo man in den 60er Jahren ein Schiff"
        },
        "line.2": {
            "text": "der 1600er barg, wo du sie, immer sie anriefst,"
        },
        "line.3": {
            "text": "sie, die ein Weltgesicht aller hat,"
        },
        "line.4": {
            "text": "da kroch er uns namentlich über den Weg."
        },
        "line.5": {
            "text": "Teredo Navalis"
        },
        "line.6": {
            "text": "ein Name, der Schrecken verbreitet,"
        },
        "line.7": {
            "text": "Frauen und Kinder fürchten macht,"
        },
        "line.8": {
            "text": "aber viel mehr noch: den Käptn."
        },
        "line.9": {
            "text": "Teredo Navalis"
        },
        "line.10": {
            "text": "ein Bewohner der Wasa,"
        },
        "line.11": {
            "text": "ein Gefangener des Elements,"
        },
        "line.12": {
            "text": "eine Stufe des Seins, das noch nicht abgeschlossen."
        },
        "line.13": {
            "text": "Teredo Navalis"
        },
        "line.14": {
            "text": "Schiffsbohrwurm von bis zu 60 Zentimetern Länge."
        },
        "line.15": {
            "text": "Während wir durch das Museum streiften,"
        },
        "line.16": {
            "text": "um ein gesunkenes Großunternehmen herum,"
        },
        "line.17": {
            "text": "sank mein parasitär besetztes Herz"
        },
        "line.18": {
            "text": "mit allen Organen der Flotte."
        },
        "line.19": {
            "text": "Hatte nicht einer Steine mir in den Bauch genäht,"
        },
        "line.20": {
            "text": "als ich offen klaffte vor Eifersucht?"
        },
        "line.21": {
            "text": "Teredo Navalis"
        },
        "line.22": {
            "text": "du Seemannsspulwurm, es steckt in dir"
        },
        "line.23": {
            "text": "wie in allem seither"
        },
        "line.24": {
            "text": "das brackwasserfaule"
        },
        "line.25": {
            "text": "Warum"
        }
    }
}