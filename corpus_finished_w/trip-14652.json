{
    "title": "trip-14652",
    "gender": "w",
    "author": "Lisa Elsässer",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "ich begleite dich nicht"
        },
        "line.2": {
            "text": "auf der fahrt ins schwarze"
        },
        "line.3": {
            "text": "denn in den bäumen"
        },
        "line.4": {
            "text": "hängt das augustlicht"
        },
        "line.5": {
            "text": "die felsen schimmern wie"
        },
        "line.6": {
            "text": "einst unsere wachen augen"
        },
        "line.7": {
            "text": "nun blindes feld und so"
        },
        "line.8": {
            "text": "überholt uns die zukunft"
        },
        "line.9": {
            "text": "länger und länger werdende"
        },
        "line.10": {
            "text": "schatten schmal geworfene"
        },
        "line.11": {
            "text": "umrisse unseres selbst"
        }
    }
}