{
    "title": "archaeopteryx-13541",
    "gender": "w",
    "author": "Sabine Scho",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "übers ohr gehauen für ein"
        },
        "line.2": {
            "text": "platt gemachtes hühnchen"
        },
        "line.3": {
            "text": "ein kuhhandel"
        },
        "line.4": {
            "text": "hört sich bescheiden an"
        },
        "line.5": {
            "text": "am übergang von erdgeprägten"
        },
        "line.6": {
            "text": "geschöpfen zu luftdurchstoßenden"
        },
        "line.7": {
            "text": "tölpeln, vom krallengang zur sturz-"
        },
        "line.8": {
            "text": "bereiten klaue, bei gellendem"
        },
        "line.9": {
            "text": "geschrei, spähend, ready for take off"
        },
        "line.10": {
            "text": "von schuppe zu federkleid, das vielleicht"
        },
        "line.11": {
            "text": "indes nur schmückte, wärmte, zum iegen"
        },
        "line.12": {
            "text": "nichts nützte, und doch, einmal"
        },
        "line.13": {
            "text": "nijinsky"
        },
        "line.14": {
            "text": "sein"
        },
        "line.15": {
            "text": "und faunisch verrenkt diesen einen augenblick"
        },
        "line.16": {
            "text": "gleichgewichtsverloren platte machen"
        }
    }
}