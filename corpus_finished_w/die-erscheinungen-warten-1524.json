{
    "title": "die-erscheinungen-warten-1524",
    "gender": "w",
    "author": "Elfriede Gerstl",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "die erscheinungen warten"
        },
        "line.2": {
            "text": "auf ihre benennung"
        },
        "line.3": {
            "text": "endlich ist wieder ein dichter"
        },
        "line.4": {
            "text": "vorbei gekommen"
        },
        "line.5": {
            "text": "die erscheinungen danken"
        },
        "line.6": {
            "text": "mit ihrem sein"
        }
    }
}