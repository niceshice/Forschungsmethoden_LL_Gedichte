{
    "title": "hummer-13543",
    "gender": "w",
    "author": "Sabine Scho",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "er gilt als schwierige speise"
        },
        "line.2": {
            "text": "sortiert sein besteck"
        },
        "line.3": {
            "text": "links, die gewaltige zwinge"
        },
        "line.4": {
            "text": "nimmt in die zange, das"
        },
        "line.5": {
            "text": "war mal ein helgoländer"
        },
        "line.6": {
            "text": "projekt der nazis"
        },
        "line.7": {
            "text": "greifen und tasten und"
        },
        "line.8": {
            "text": "wild tranchieren lernen"
        },
        "line.9": {
            "text": "menschen von ihm"
        },
        "line.10": {
            "text": "jetzt kriegen sie keinen schreck"
        },
        "line.11": {
            "text": "er berücksichtigt linkshänder"
        },
        "line.12": {
            "text": "hat ein gutes gedächtnis, unter-"
        },
        "line.13": {
            "text": "scheidet irre relevantes von nur"
        },
        "line.14": {
            "text": "mäßig interessantem, das gilt"
        },
        "line.15": {
            "text": "gemeinhin als intelligent"
        },
        "line.16": {
            "text": "wird gekocht bei lebendigem leibe"
        },
        "line.17": {
            "text": "was fühlt ein hummer dabei"
        },
        "line.18": {
            "text": "für gewöhnlich interessiert das"
        },
        "line.19": {
            "text": "hungrige einen dreck"
        },
        "line.20": {
            "text": "auch ein panzerkrebs erhebt"
        },
        "line.21": {
            "text": "anspruch auf sein glück"
        },
        "line.22": {
            "text": "schmerzfreiheiten? was für"
        },
        "line.23": {
            "text": "ausnahmezustände! rezeptoren"
        },
        "line.24": {
            "text": "hat man allein zu dem zweck"
        },
        "line.25": {
            "text": "dass durch die härtesten panzer"
        },
        "line.26": {
            "text": "reize dringen, schauen sie den"
        },
        "line.27": {
            "text": "führerbunker, den kriegte man"
        },
        "line.28": {
            "text": "kaum gesprengt, so gut verschalt"
        },
        "line.29": {
            "text": "und doch drang noch etwas hinein"
        },
        "line.30": {
            "text": "dass in ihm verschanzte wesen"
        },
        "line.31": {
            "text": "roh und von eigener hand"
        },
        "line.32": {
            "text": "aufhörten zu sein"
        }
    }
}