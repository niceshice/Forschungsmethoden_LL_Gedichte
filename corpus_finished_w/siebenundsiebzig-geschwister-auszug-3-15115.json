{
    "title": "siebenundsiebzig-geschwister-auszug-3-15115",
    "gender": "w",
    "author": "Zsuzsanna Gahse",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Heute gehe ich quer"
        },
        "line.2": {
            "text": "durch die Innenstadt,"
        },
        "line.3": {
            "text": "und wen treffe ich auf"
        },
        "line.4": {
            "text": "einem engen Platz?"
        },
        "line.5": {
            "text": "Meine Schwester."
        },
        "line.6": {
            "text": "Was mache ich nun?"
        },
        "line.7": {
            "text": "Bin ratlos. Derzeit"
        },
        "line.8": {
            "text": "sagen auffallend viele,"
        },
        "line.9": {
            "text": "sie seien ratlos, sie"
        },
        "line.10": {
            "text": "seien ratlos, das sage"
        },
        "line.11": {
            "text": "ich auch und stehe"
        },
        "line.12": {
            "text": "vor meiner Schwester."
        },
        "line.13": {
            "text": "Und im Baum hocken, im"
        },
        "line.14": {
            "text": "Kirschbaum baumeln zwischen"
        },
        "line.15": {
            "text": "den Zweigen manchmal sogar"
        },
        "line.16": {
            "text": "Drillinge, kleine Schlitzohren,"
        },
        "line.17": {
            "text": "hübsche Schlitzaugen torkeln"
        },
        "line.18": {
            "text": "abwärts und leuchten, springen"
        },
        "line.19": {
            "text": "gemeinsam vom Ast, ohne"
        },
        "line.20": {
            "text": "die Geschwister aus dem"
        },
        "line.21": {
            "text": "Nest zu zwingen."
        },
        "line.22": {
            "text": "Bin ein Mann, habe"
        },
        "line.23": {
            "text": "einen Bruder, einen"
        },
        "line.24": {
            "text": "Halbbruder, der"
        },
        "line.25": {
            "text": "Halbbruder bin ich,"
        },
        "line.26": {
            "text": "mein Bruder ist ganz."
        },
        "line.27": {
            "text": "Wir sitzen im"
        },
        "line.28": {
            "text": "Nussbaum auf zwei"
        },
        "line.29": {
            "text": "Ästen. Sein Ast bricht."
        },
        "line.30": {
            "text": "Was nun?"
        }
    }
}