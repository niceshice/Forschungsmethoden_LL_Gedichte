{
    "title": "movies-388",
    "gender": "w",
    "author": "Barbara Köhler",
    "author_birth": "1959",
    "poem": {
        "line.1": {
            "text": "Film ist vierundzwanzigmal"
        },
        "line.2": {
            "text": "Wahrheit pro Sekunde"
        },
        "line.3": {
            "text": "Jean-Luc Godard"
        },
        "line.4": {
            "text": "Vierundzwanzigmal pro Sekunde"
        },
        "line.5": {
            "text": "laufe ich mir davon kommt etwas"
        },
        "line.6": {
            "text": "auf mich zu sagt: Ich"
        },
        "line.7": {
            "text": "laufe davon bin fest"
        },
        "line.8": {
            "text": "gehalten in den Bildern"
        },
        "line.9": {
            "text": "die laufen ein Massaker"
        },
        "line.10": {
            "text": "jede Bewegung eine Wendung"
        },
        "line.11": {
            "text": "im Schlaf in vierundzwanzig"
        },
        "line.12": {
            "text": "Stück pro Sekunde Stunden"
        },
        "line.13": {
            "text": "der Tag zerteilt eine gepreßte"
        },
        "line.14": {
            "text": "Stimme die Tonspur sagt: Ich"
        },
        "line.15": {
            "text": "hab mich verlaufen sehe vor"
        },
        "line.16": {
            "text": "lauter Bildern den Film nicht"
        },
        "line.17": {
            "text": "den Stillstand sehe mich vor"
        },
        "line.18": {
            "text": "vierundzwanzig Feststellungen"
        },
        "line.19": {
            "text": "pro Sekunde bewegt die Hand"
        },
        "line.20": {
            "text": "in den Mund gestopft: Leben"
        },
        "line.21": {
            "text": "tut weh Madame beißen Sie zu."
        }
    }
}