{
    "title": "4-januar-5721",
    "gender": "w",
    "author": "Elfriede Czurda",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "was auf den tisch kommt"
        },
        "line.2": {
            "text": "wird auf keinen fall"
        },
        "line.3": {
            "text": "gegessen es wird sortiert"
        },
        "line.4": {
            "text": "gestapelt gelocht zusammen"
        },
        "line.5": {
            "text": "geheftet mit heftiger hand"
        },
        "line.6": {
            "text": "überkritzelt zerknüllt"
        },
        "line.7": {
            "text": "in den papier korb versenkt"
        },
        "line.8": {
            "text": "heraus gefischt glatt"
        },
        "line.9": {
            "text": "gestrichen mit tee voll"
        },
        "line.10": {
            "text": "gekleckert weg damit"
        },
        "line.11": {
            "text": "was auf den tisch kommt"
        },
        "line.12": {
            "text": "wird verdaut wird verdaut"
        },
        "line.13": {
            "text": "4. januar 2004; tisch"
        }
    }
}