{
    "title": "halb-taube-halb-pfau-5-11209",
    "gender": "w",
    "author": "Maren Kames",
    "author_birth": "1984",
    "poem": {
        "line.1": {
            "text": "Das ist das Land. Aha."
        },
        "line.2": {
            "text": "Die Oberfläche des Planeten."
        },
        "line.3": {
            "text": "Die kontinentalen Koordinaten, hier und hier"
        },
        "line.4": {
            "text": "und hier das sind dann wohl die Konstellaren,"
        },
        "line.5": {
            "text": "hier ein paar Planken, hier und hier und"
        },
        "line.6": {
            "text": "hier, das ist das Land. Mhm."
        },
        "line.7": {
            "text": "Ich setz mich einfach auf diese Scholle hier, halte mein offenes Gesicht in den Himmel"
        },
        "line.8": {
            "text": "und trinke diesen Regen."
        },
        "line.9": {
            "text": "Ich setz mich einfach in die Nesseln hier und warte auf Besserung."
        },
        "line.10": {
            "text": "Und vielleicht ist es so: An diesen Schollen ist das Land zusammengenäht,"
        },
        "line.11": {
            "text": "hier wird es reißen."
        },
        "line.12": {
            "text": "Am Ende ist es hologramm, das Land."
        },
        "line.13": {
            "text": "Und am Ende ist es so"
        },
        "line.14": {
            "text": "ich halte hier Reden"
        },
        "line.15": {
            "text": "vom Firn über den Dingen"
        },
        "line.16": {
            "text": "über den Tau im Revier und das Tier"
        },
        "line.17": {
            "text": "den räudigen Zustand und"
        },
        "line.18": {
            "text": "das hier ist Regen"
        },
        "line.19": {
            "text": "das die Lähmung"
        },
        "line.20": {
            "text": "und das der Regen"
        },
        "line.21": {
            "text": "und wo der Kopf das Lot die Route"
        },
        "line.22": {
            "text": "dass alles steht"
        },
        "line.23": {
            "text": "während der Regen in Bewegung"
        },
        "line.24": {
            "text": "über dem Firn über den Dingen"
        },
        "line.25": {
            "text": "und unterm Firn die Rede"
        },
        "line.26": {
            "text": "und die Dinge drehen"
        },
        "line.27": {
            "text": "und die Rede dreht an den Dingen"
        },
        "line.28": {
            "text": "und die Dinge schmelzen"
        },
        "line.29": {
            "text": "und die Rede schmilzt an den Dingen"
        },
        "line.30": {
            "text": "und das geht in Schleifen"
        },
        "line.31": {
            "text": "das geht nicht"
        },
        "line.32": {
            "text": "das bricht"
        },
        "line.33": {
            "text": "ab"
        },
        "line.34": {
            "text": "hier"
        },
        "line.35": {
            "text": "wo anders hin"
        }
    }
}