{
    "title": "chlorophyll-11531",
    "gender": "w",
    "author": "Ulrike Draesner",
    "author_birth": "1962",
    "poem": {
        "line.1": {
            "text": "diese schulstunde blieb, ihr agent"
        },
        "line.2": {
            "text": "lautlos, ihr könig: das blatt"
        },
        "line.3": {
            "text": "frisst licht macht zucker ohne"
        },
        "line.4": {
            "text": "unterlass schlich darauf ich"
        },
        "line.5": {
            "text": "durch"
        },
        "line.6": {
            "text": "sonnenfutterschein"
        },
        "line.7": {
            "text": "nach haus"
        },
        "line.8": {
            "text": "zwischen bäumen stauden gras"
        },
        "line.9": {
            "text": "roh an unsichtbare zitzen licht gesaugt"
        },
        "line.10": {
            "text": "auf der zunge das brechende blatt"
        },
        "line.11": {
            "text": "das füllwort grün für phyl"
        },
        "line.12": {
            "text": "deine augen, mittags, im schilf"
        },
        "line.13": {
            "text": "bei flötenapp, bei offner autotür"
        },
        "line.14": {
            "text": "intimmodul auf gras: einander zärtlich"
        },
        "line.15": {
            "text": "ein wenig ohne lügen zu liegen"
        },
        "line.16": {
            "text": "versucht einander grün"
        },
        "line.17": {
            "text": "in licht das fiel sich drehte wehrte"
        },
        "line.18": {
            "text": "bis zucker wuchs das füllwort"
        },
        "line.19": {
            "text": "fühl bei offner autotür"
        },
        "line.20": {
            "text": "nach flötenapp ging ich allein"
        },
        "line.21": {
            "text": "nach haus einen fleck im rock"
        },
        "line.22": {
            "text": "von gras gesaugt so tief dass"
        },
        "line.23": {
            "text": "jedes licht darüber lief"
        }
    }
}