{
    "title": "vom-kopf-fielen-mir-bilder-16241",
    "gender": "w",
    "author": "Cornelia  Hülmbauer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "vom"
        },
        "line.2": {
            "text": "Kopf"
        },
        "line.3": {
            "text": "fielen mir Bilder"
        },
        "line.4": {
            "text": "Vielleicht"
        },
        "line.5": {
            "text": "zufällig"
        },
        "line.6": {
            "text": "in die Hand"
        },
        "line.7": {
            "text": "mit"
        },
        "line.8": {
            "text": "stein"
        },
        "line.9": {
            "text": "ins"
        },
        "line.10": {
            "text": "Leben"
        },
        "line.11": {
            "text": "gegrüßt"
        },
        "line.12": {
            "text": "Ja u."
        }
    }
}