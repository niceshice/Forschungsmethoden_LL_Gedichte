{
    "title": "liedvoll-deutschyzno-moja-13943",
    "gender": "w",
    "author": "Dagmara Kraus",
    "author_birth": "1981",
    "poem": {
        "line.1": {
            "text": "1"
        },
        "line.2": {
            "text": "millionen flüchtige wörter stehen an"
        },
        "line.3": {
            "text": "der grenze zu diesem gedicht"
        },
        "line.4": {
            "text": "die beine in den bauch sich"
        },
        "line.5": {
            "text": "schlange an der grenze"
        },
        "line.6": {
            "text": "dunkle wörter, dunkle fremde"
        },
        "line.7": {
            "text": "suchen nach zuflucht, wollen hier wohnen"
        },
        "line.8": {
            "text": "verjaschmakt, betschadort, da warten"
        },
        "line.9": {
            "text": "mummen von jenseits der pole"
        },
        "line.10": {
            "text": "es sind welche von ungarn gekommen"
        },
        "line.11": {
            "text": "zupenie niedeutschae sowa"
        },
        "line.12": {
            "text": "drängen sich hier in die futura"
        },
        "line.13": {
            "text": "rce bagaj, bebeten die grenzen"
        },
        "line.14": {
            "text": "deine, deutschyzno moja"
        },
        "line.15": {
            "text": "2"
        },
        "line.16": {
            "text": "aber was soll ein gedicht mit den millionen"
        },
        "line.17": {
            "text": "flüchtigen wörtern nur anfangen"
        },
        "line.18": {
            "text": "es könnte mit ihnen kickern gehen"
        },
        "line.19": {
            "text": "schlägts kickern vor"
        },
        "line.20": {
            "text": "sie einladen zu bier, püree und kohlrouladen"
        },
        "line.21": {
            "text": "oder mal ne reise machen an ein schönes wort"
        },
        "line.22": {
            "text": "und ihnen die gegend zeigen, gegend"
        },
        "line.23": {
            "text": "es könnte millionen wörter einfach schweigen"
        },
        "line.24": {
            "text": "und müsste jetzt nicht gründeln, mörtel, münze"
        },
        "line.25": {
            "text": "wrack und wub erleiden, schlackern, lecker"
        },
        "line.26": {
            "text": "mords- und mergel - wiesz"
        },
        "line.27": {
            "text": "es könnte wieder flippern wie am rastplatz"
        },
        "line.28": {
            "text": "weil die zeit lang wird"
        },
        "line.29": {
            "text": "und mit der tinka auffe kirmes gehn"
        },
        "line.30": {
            "text": "sich in die zukunft sehen lassen"
        },
        "line.31": {
            "text": "vom pinken clafoutisschafott im pelz"
        },
        "line.32": {
            "text": "es könnte schützenfest und adlerschuss verpassen"
        },
        "line.33": {
            "text": "und an der lippe laufen, wo die mimik hüpft"
        },
        "line.34": {
            "text": "dann am frühen winkelwasser"
        },
        "line.35": {
            "text": "in schnöden reimen kurz verschnaufen"
        },
        "line.36": {
            "text": "auch könntes doch ins olle haus"
        },
        "line.37": {
            "text": "das haus am halben mond 1ziehen"
        },
        "line.38": {
            "text": "und dem fallmann hin und wieder"
        },
        "line.39": {
            "text": "den fadengang von buche andiktieren"
        },
        "line.40": {
            "text": "(er soll ihn immer mit zwei fingern nachfahren)"
        },
        "line.41": {
            "text": "dabei könntes aus dem kickern 'skern"
        },
        "line.42": {
            "text": "und all den kiciukitsch entfernen"
        },
        "line.43": {
            "text": "und stattdessen andere wörter weiden"
        },
        "line.44": {
            "text": "vielleicht fremde wörter heimen"
        },
        "line.45": {
            "text": "raffig, diese flüchtigen, mehrbarwigen"
        },
        "line.46": {
            "text": "die völkerball in föderalen hallen spielen"
        },
        "line.47": {
            "text": "- aber millionen"
        },
        "line.48": {
            "text": "abermillionen"
        },
        "line.49": {
            "text": "doch bleibts bloß ein kern am kitsch"
        },
        "line.50": {
            "text": "beim kern am kitsch"
        },
        "line.51": {
            "text": "3"
        },
        "line.52": {
            "text": "abgeschoben ausgespuckt"
        },
        "line.53": {
            "text": "da ist etwas kleinstes blaurosa"
        },
        "line.54": {
            "text": "gepuckt, wie ein taubenschluck"
        },
        "line.55": {
            "text": "leise weints mitten im pulk"
        },
        "line.56": {
            "text": "wo sind denn die aus dem morgenland"
        },
        "line.57": {
            "text": "die drei worte mit den gaben"
        },
        "line.58": {
            "text": "nix myrrhe hier"
        },
        "line.59": {
            "text": "her mit dem schwarz-rot-gold"
        },
        "line.60": {
            "text": "wörter aus dem buch der könige"
        },
        "line.61": {
            "text": "hocken im containerdorf: umfwörter"
        },
        "line.62": {
            "text": "aus saba, noch milchschorf im haar"
        },
        "line.63": {
            "text": "labern babel"
        }
    }
}