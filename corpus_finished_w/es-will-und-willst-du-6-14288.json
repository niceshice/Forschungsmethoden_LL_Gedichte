{
    "title": "es-will-und-willst-du-6-14288",
    "gender": "w",
    "author": "Rozalie Hirs",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "auch wenn du schläfst pegasus"
        },
        "line.2": {
            "text": "denn gibts kindbefund"
        },
        "line.3": {
            "text": "dein sausen aufzuwiegender hände"
        },
        "line.4": {
            "text": "schiehie schiehie schiehie"
        },
        "line.5": {
            "text": "in deides wasser deides wasser herum"
        },
        "line.6": {
            "text": "in deides wasserl wasserlen"
        },
        "line.7": {
            "text": "schiehie"
        },
        "line.8": {
            "text": "bis am innendrinnen"
        },
        "line.9": {
            "text": "im stamm"
        },
        "line.10": {
            "text": "ben grängs mit du begräben wir entblüht"
        },
        "line.11": {
            "text": "umlautiges knichern"
        },
        "line.12": {
            "text": "im umwein schalst du"
        },
        "line.13": {
            "text": "dein umweinen"
        },
        "line.14": {
            "text": "umlaus bis es umwegkippt"
        }
    }
}