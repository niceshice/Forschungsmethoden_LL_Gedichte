{
    "title": "persoenliche-fuerwoerter-1445",
    "gender": "w",
    "author": "Irmela Brender",
    "author_birth": "1935",
    "poem": {
        "line.1": {
            "text": "Du bist der Kakadu"
        },
        "line.2": {
            "text": "und ich der Kaka-ich"
        },
        "line.3": {
            "text": "und sie sind Kaka-ersiees,"
        },
        "line.4": {
            "text": "das ist verwunderlich."
        },
        "line.5": {
            "text": "Kladu-, Kla-ich, Kla-wir,"
        },
        "line.6": {
            "text": "wie passt das zum Getihr?"
        },
        "line.7": {
            "text": "und erst noch zum Gesind?"
        },
        "line.8": {
            "text": "Sehr schwere Sprach, mein Kind!"
        }
    }
}