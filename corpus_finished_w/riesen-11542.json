{
    "title": "riesen-11542",
    "gender": "w",
    "author": "Nora Gomringer",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "In Gradzahlen ahnt die Welt,"
        },
        "line.2": {
            "text": "wo ihr zu finden."
        },
        "line.3": {
            "text": "Es fällt kein Licht"
        },
        "line.4": {
            "text": "In eure Tiefen ein."
        },
        "line.5": {
            "text": "Ich kenne Dichter, die den Moment beschrieben,"
        },
        "line.6": {
            "text": "als sie in eure tellergroßen Augen sahen."
        },
        "line.7": {
            "text": "Man sagt, die Augen ließen tief in Seelen blicken."
        },
        "line.8": {
            "text": "Ich weiß nicht, will ich eure Seelen sehen?"
        },
        "line.9": {
            "text": "Man sagt, ihr wärt wie Vögel unter Wasser,"
        },
        "line.10": {
            "text": "jedoch verschwiegen und trotz der Schnäbel"
        },
        "line.11": {
            "text": "nicht gefiederter, mehr umarmender Natur."
        },
        "line.12": {
            "text": "Und eure Liebe währte mindestens"
        },
        "line.13": {
            "text": "Den einen Tauchgang lang, bis kleinste Bläschen"
        },
        "line.14": {
            "text": "großen Walen dort entstiegen"
        },
        "line.15": {
            "text": "wo eure Messer zwitscherten."
        }
    }
}