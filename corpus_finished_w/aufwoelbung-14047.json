{
    "title": "aufwoelbung-14047",
    "gender": "w",
    "author": "Anja Bayer",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "Aufwölbung"
        },
        "line.2": {
            "text": "des Leibes des Drachens im"
        },
        "line.3": {
            "text": "Schnellrestaurant die Fülle"
        },
        "line.4": {
            "text": "aller Gaben Verheißung"
        },
        "line.5": {
            "text": "der Gegenwart Mittag und Abend"
        },
        "line.6": {
            "text": "preise"
        },
        "line.7": {
            "text": "Morgenlob"
        },
        "line.8": {
            "text": "an Plastiktischen mut"
        },
        "line.9": {
            "text": "machende Erkenntnisse was könnte"
        },
        "line.10": {
            "text": "werden?"
        }
    }
}