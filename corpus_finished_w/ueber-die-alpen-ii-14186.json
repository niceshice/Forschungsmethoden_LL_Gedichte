{
    "title": "ueber-die-alpen-ii-14186",
    "gender": "w",
    "author": "Birgit Kreipe",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "II"
        },
        "line.2": {
            "text": "hätten wir nur nie die alpen überquert!"
        },
        "line.3": {
            "text": "wenn der mond aufging, sah man"
        },
        "line.4": {
            "text": "wie bekümmert die berge waren."
        },
        "line.5": {
            "text": "wir steigen von trümmerfeld zu trümmerfeld."
        },
        "line.6": {
            "text": "die dunkelheit stampfte vorüber ins tal"
        },
        "line.7": {
            "text": "eine herde obskurer elefanten."
        },
        "line.8": {
            "text": "ich fand moose, gelbe sprenkel"
        },
        "line.9": {
            "text": "wie schuppenflechte. stinkende kräuter."
        },
        "line.10": {
            "text": "bekam dich kaum zu gesicht."
        },
        "line.11": {
            "text": "waren wir auch in der gleichen schicht unterwegs"
        },
        "line.12": {
            "text": "so doch nicht in derselben zeit."
        },
        "line.13": {
            "text": "endlich sahen wir ein"
        },
        "line.14": {
            "text": "dass es zwei parallele gebirge waren."
        },
        "line.15": {
            "text": "eines war vielleicht hannibals route"
        },
        "line.16": {
            "text": "und das andere ein asteroidenfeld."
        },
        "line.17": {
            "text": "wenn wir doch einmal zusammen liefen"
        },
        "line.18": {
            "text": "lösten die wolken sich bald wieder auf."
        },
        "line.19": {
            "text": "sie rochen nach schwefel."
        },
        "line.20": {
            "text": "wen kümmerte es, dass das edelweiß grün war"
        },
        "line.21": {
            "text": "grün, besoffen von grüner milch."
        },
        "line.22": {
            "text": "wen kümmerte es in den bergen"
        },
        "line.23": {
            "text": "aus dem gleichen eisen und gestein"
        },
        "line.24": {
            "text": "wie himmelskörper, und doch"
        },
        "line.25": {
            "text": "von ihnen aufgegeben, ungeschlachte kolosse"
        },
        "line.26": {
            "text": "einer geschichte zwischen planeten. nachts"
        },
        "line.27": {
            "text": "träumte ich, sie würden langsam in den himmel ziehen"
        },
        "line.28": {
            "text": "und dort verschwinden, ohne spur"
        },
        "line.29": {
            "text": "einziges signal: alle paar hundert jahre ein meteor."
        }
    }
}