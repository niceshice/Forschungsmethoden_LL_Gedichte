{
    "title": "entgegen-zu-stehen-iii-br-loesen-sich-eingraben-1348",
    "gender": "w",
    "author": "Anja Utler",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "wie ins wuchernde: einfinden wollen"
        },
        "line.2": {
            "text": "sich: haspeln, entgegen, sich: auflegen,"
        },
        "line.3": {
            "text": "-fiedern wie: werden zur: netzhaut"
        },
        "line.4": {
            "text": "für furchen für flöze und: gänzlich sich"
        },
        "line.5": {
            "text": "aufspinnen, netznervig werden so"
        },
        "line.6": {
            "text": "zitternd ein: gitterblick  sehe du"
        },
        "line.7": {
            "text": "ragst du sprichst  sein wie"
        },
        "line.8": {
            "text": "hineingewirkt, -wunden: ins"
        },
        "line.9": {
            "text": "nervenstrang dickicht aus taubnessel"
        },
        "line.10": {
            "text": "erdrauch; ein pochen  vergesse"
        },
        "line.11": {
            "text": "ja höre, vernehme nicht mehr"
        }
    }
}