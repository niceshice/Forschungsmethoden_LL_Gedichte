{
    "title": "gallgesang-358",
    "gender": "w",
    "author": "Anne Duden",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "Wechselkoller abends"
        },
        "line.2": {
            "text": "unter den Zwangsnachbarn."
        },
        "line.3": {
            "text": "In die Balkonzeile sich drängende"
        },
        "line.4": {
            "text": "Brüstungsschwadron"
        },
        "line.5": {
            "text": "zwischen Kontoständen"
        },
        "line.6": {
            "text": "und Zinssätzen"
        },
        "line.7": {
            "text": "wühlender Unterbrustquell"
        },
        "line.8": {
            "text": "spuckt Sand"
        },
        "line.9": {
            "text": "auf die Alters- und Leberflecken"
        },
        "line.10": {
            "text": "und fügt sich nur den Oberrandsalven."
        },
        "line.11": {
            "text": "SIE"
        },
        "line.12": {
            "text": "mit und dagegen Schwundgebeugte"
        },
        "line.13": {
            "text": "zwittrig zersiebt"
        },
        "line.14": {
            "text": "in den Laibungsstürzen."
        },
        "line.15": {
            "text": "Ruheloser Notknochen"
        },
        "line.16": {
            "text": "weiblicher Herkunft"
        },
        "line.17": {
            "text": "doch jetzt noch"
        },
        "line.18": {
            "text": "von Schönheit gezeichnet"
        },
        "line.19": {
            "text": "nach den paar"
        },
        "line.20": {
            "text": "hastigen Atemzügen"
        },
        "line.21": {
            "text": "VOM PLANETEN"
        },
        "line.22": {
            "text": "den zwei drei Sekundenmahlzeiten"
        },
        "line.23": {
            "text": "im Straßengraben."
        },
        "line.24": {
            "text": "Lichtnelkengelehnt"
        },
        "line.25": {
            "text": "mit dem Pannenbesteck"
        },
        "line.26": {
            "text": "zum Aufgabeln"
        },
        "line.27": {
            "text": "des nur noch auswärts Erheischten"
        },
        "line.28": {
            "text": "und sommers."
        },
        "line.29": {
            "text": "Im Zartbruch"
        },
        "line.30": {
            "text": "fallsüchtig erregt"
        },
        "line.31": {
            "text": "Äonen durchgestanden"
        },
        "line.32": {
            "text": "von Wahn und Anprall"
        },
        "line.33": {
            "text": "skelettloser Kinderwald"
        },
        "line.34": {
            "text": "geschachtelt gehalmt"
        },
        "line.35": {
            "text": "auf Schotter und Kiesel"
        },
        "line.36": {
            "text": "hingrünend"
        },
        "line.37": {
            "text": "Bodenatmung"
        },
        "line.38": {
            "text": "blicksame Feinflur"
        },
        "line.39": {
            "text": "verwaschener Tuschhalden."
        },
        "line.40": {
            "text": "Auf"
        },
        "line.41": {
            "text": "in den Mutterschlag"
        },
        "line.42": {
            "text": "die ausgebellt nachheisernde Umkümmerung"
        },
        "line.43": {
            "text": "Blusendürre"
        },
        "line.44": {
            "text": "Krümmermitleid"
        },
        "line.45": {
            "text": "einmal"
        },
        "line.46": {
            "text": "von der Buschzone her"
        },
        "line.47": {
            "text": "nachtgellender Aufruf"
        },
        "line.48": {
            "text": "einer Erdsängerin"
        },
        "line.49": {
            "text": "hoheitsgebietendes Schrillgedicht"
        },
        "line.50": {
            "text": "fürs Bewüten und Bewildern"
        },
        "line.51": {
            "text": "der Trommelfelle"
        },
        "line.52": {
            "text": "das die Mit- und Abfühligkeit"
        },
        "line.53": {
            "text": "streckt und verreißt"
        },
        "line.54": {
            "text": "zerstieben läßt"
        },
        "line.55": {
            "text": "im gelaufenen Tag."
        },
        "line.56": {
            "text": "Minutenlang"
        },
        "line.57": {
            "text": "brandet es ans schmalzige Vertopfte"
        },
        "line.58": {
            "text": "im Zähl- und Aufrechnungsgeviert"
        },
        "line.59": {
            "text": "bestürmt das abgeprügelte Erdreich"
        },
        "line.60": {
            "text": "bis unter die Keller."
        },
        "line.61": {
            "text": "Vorkau"
        },
        "line.62": {
            "text": "galliger Lockruf ins lange schon Tote"
        },
        "line.63": {
            "text": "wechselt den Tonfall unerhört"
        },
        "line.64": {
            "text": "flötet einwärts"
        },
        "line.65": {
            "text": "zum Aufsaugen der angeschlagenen Süße"
        },
        "line.66": {
            "text": "verschluchzt sich"
        },
        "line.67": {
            "text": "kurz nur"
        },
        "line.68": {
            "text": "speist sich bricht ab."
        },
        "line.69": {
            "text": "Ein gelassener Flügelschlag"
        },
        "line.70": {
            "text": "richtungsweisend aus dem unverfrorenen Preßgebiet"
        },
        "line.71": {
            "text": "weg vom eingepelzten Etagengreinen"
        },
        "line.72": {
            "text": "Rührgift"
        },
        "line.73": {
            "text": "und Zickern."
        }
    }
}