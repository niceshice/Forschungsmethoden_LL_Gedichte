{
    "title": "eins-ursprung-5714",
    "gender": "w",
    "author": "Elfriede Czurda",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "hellblau, sphärisch konkav"
        },
        "line.2": {
            "text": "wasser allenthalben, dasselbe"
        },
        "line.3": {
            "text": "wandlose fließen im raum"
        },
        "line.4": {
            "text": "den nichts umschreibt"
        },
        "line.5": {
            "text": "überall nur das einzige"
        },
        "line.6": {
            "text": "unteilbare wasser"
        },
        "line.7": {
            "text": "das den himmel enthält"
        },
        "line.8": {
            "text": "die sonne die wolken"
        },
        "line.9": {
            "text": "über den blühenden kirschen"
        },
        "line.10": {
            "text": "den tau und das monotone"
        },
        "line.11": {
            "text": "rauschen aus dem zentrum"
        },
        "line.12": {
            "text": "lautbare stille, drehung"
        }
    }
}