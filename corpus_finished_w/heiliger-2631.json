{
    "title": "heiliger-2631",
    "gender": "w",
    "author": "Kerstin Hensel",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "Gott, ach einen Affen"
        },
        "line.2": {
            "text": "Brauche ich zu dem ich mich mache, taub"
        },
        "line.3": {
            "text": "Die Menschheit unter der Binde aus Holz"
        },
        "line.4": {
            "text": "Schweigt, da der Wurm drinnen"
        },
        "line.5": {
            "text": "Nicht richtig tickt Zeit Unzeit Wie"
        },
        "line.6": {
            "text": "Kippelt der Sockel darauf ich"
        },
        "line.7": {
            "text": "In Blindheit fern seh"
        }
    }
}