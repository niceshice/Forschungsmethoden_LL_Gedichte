{
    "title": "inventar-11980",
    "gender": "w",
    "author": "Carolin Callies",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "du hast gerinnsel, hälse, male,"
        },
        "line.2": {
            "text": "pflöcke, korken, rot geädert;"
        },
        "line.3": {
            "text": "was warn das falten, marmorschale"
        },
        "line.4": {
            "text": "kragen: da lehnt das letzte aus den händen."
        },
        "line.5": {
            "text": "was warn das hautbeschläge, fingerkuppen,"
        },
        "line.6": {
            "text": "tagealben, linnentrunken"
        },
        "line.7": {
            "text": "& dort am mundstück"
        },
        "line.8": {
            "text": "nicht mehr als deine handvernähten lippen."
        }
    }
}