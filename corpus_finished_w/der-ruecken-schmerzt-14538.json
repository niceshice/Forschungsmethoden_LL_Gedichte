{
    "title": "der-ruecken-schmerzt-14538",
    "gender": "w",
    "author": "Ruth Wolf-Rehfeldt",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "Der Rücken schmerzt"
        },
        "line.2": {
            "text": "Es knacken die Gelenke"
        },
        "line.3": {
            "text": "Mein tränenreiches Auge tropft"
        },
        "line.4": {
            "text": "Wenn ich an Schönheit denke"
        }
    }
}