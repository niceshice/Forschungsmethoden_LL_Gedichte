{
    "title": "woertlich-genommen-10715",
    "gender": "w",
    "author": "Ulla Hahn",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Ich herze dich"
        },
        "line.2": {
            "text": "ich lunge dich"
        },
        "line.3": {
            "text": "ich haute haare"
        },
        "line.4": {
            "text": "pore dich"
        },
        "line.5": {
            "text": "Du baust auf mich"
        },
        "line.6": {
            "text": "du dachst mich spitz"
        },
        "line.7": {
            "text": "palastest mich"
        },
        "line.8": {
            "text": "oasest mich"
        },
        "line.9": {
            "text": "Du meersternst mich"
        },
        "line.10": {
            "text": "du landest mich"
        },
        "line.11": {
            "text": "Ich berg dich"
        },
        "line.12": {
            "text": "tal dich gipfel dich"
        },
        "line.13": {
            "text": "Du freudest mich"
        },
        "line.14": {
            "text": "Ich freude dich"
        },
        "line.15": {
            "text": "Du sehnsuchst mich"
        },
        "line.16": {
            "text": "Ich sternschnupp dich"
        },
        "line.17": {
            "text": "Du brüstest hüftest"
        },
        "line.18": {
            "text": "schenkelst mich"
        },
        "line.19": {
            "text": "Ich zunge zaum"
        },
        "line.20": {
            "text": "ich kehlkopf dich"
        },
        "line.21": {
            "text": "Ich hauch brauch fauch"
        },
        "line.22": {
            "text": "du füllhornst mich"
        },
        "line.23": {
            "text": "Wir atmen amseln amen."
        },
        "line.24": {
            "text": "(aus:"
        },
        "line.25": {
            "text": "Widerworte"
        },
        "line.26": {
            "text": ", 2011)"
        }
    }
}