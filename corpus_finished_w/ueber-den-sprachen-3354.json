{
    "title": "ueber-den-sprachen-3354",
    "gender": "w",
    "author": "Dorothea  Grünzweig",
    "author_birth": "1952",
    "poem": {
        "line.1": {
            "text": "Vjell hat andere Wörter als ich andere Namen"
        },
        "line.2": {
            "text": "wenn ich"
        },
        "line.3": {
            "text": "Wald"
        },
        "line.4": {
            "text": "sage gibt er"
        },
        "line.5": {
            "text": "korpi"
        },
        "line.6": {
            "text": "zurück"
        },
        "line.7": {
            "text": "lumituisku"
        },
        "line.8": {
            "text": "hält er"
        },
        "line.9": {
            "text": "Schneegestöber keväinen"
        },
        "line.10": {
            "text": "Frühlings"
        },
        "line.11": {
            "text": "-entgegen"
        },
        "line.12": {
            "text": "Beispiele könnte ich nennen wie"
        },
        "line.13": {
            "text": "Flocken so viele wir spielen gern mit den Worten"
        },
        "line.14": {
            "text": "gern spielen sie untereinander"
        },
        "line.15": {
            "text": "Leben mit ihnen ist aber auch Kreisen"
        },
        "line.16": {
            "text": "um Fragen wie"
        },
        "line.17": {
            "text": "Verstehen wir sie recht verstehen sie uns"
        },
        "line.18": {
            "text": "sind sie sich gut gehen sie ein aufeinander"
        },
        "line.19": {
            "text": "was nicht immer der Fall ist Kümmern um"
        },
        "line.20": {
            "text": "Kümmernis wegen der Worte"
        },
        "line.21": {
            "text": "bis wir anstatt"
        },
        "line.22": {
            "text": "gelassen zu bleiben ganz wirbelig werden"
        },
        "line.23": {
            "text": "Zeiten gibt es da haben sie Einschluss"
        },
        "line.24": {
            "text": "in uns keiner"
        },
        "line.25": {
            "text": "hört sein eigenes Wort"
        },
        "line.26": {
            "text": "das bringt uns dazu nach"
        },
        "line.27": {
            "text": "unseren Wörtern wie auch nach denen des anderen"
        },
        "line.28": {
            "text": "Heimweh zu haben welches"
        },
        "line.29": {
            "text": "so anschwillt dass wir einander"
        },
        "line.30": {
            "text": "meiden müssen"
        },
        "line.31": {
            "text": "Oder aber und das ist das Schönste versteht ihr"
        },
        "line.32": {
            "text": "erfahren wir lange ans Brückengeländer"
        },
        "line.33": {
            "text": "gelehnt schultervereint Kopf an Kopf"
        },
        "line.34": {
            "text": "die Wortnot als Freiheit Enthebung"
        },
        "line.35": {
            "text": "schiesst das Wasser unter uns fort sind wir"
        },
        "line.36": {
            "text": "ein Boot nordwärts flussab zum schwebenden Meer"
        },
        "line.37": {
            "text": "schweigsam im Rumpf doch jauchzend an den Rändern"
        }
    }
}