{
    "title": "ein-teil-honig-16570",
    "gender": "w",
    "author": "Daniela  Chana",
    "author_birth": "1985",
    "poem": {
        "line.1": {
            "text": "Dein Geburtstagsgeschenk"
        },
        "line.2": {
            "text": "Im letzten Moment besorgt"
        },
        "line.3": {
            "text": "Weil wir gestern noch zerstritten waren"
        },
        "line.4": {
            "text": "Manchmal will ich von mir selbst"
        },
        "line.5": {
            "text": "Wie Honig von einem Löffel tropfen"
        },
        "line.6": {
            "text": "Du bist ein Teil Honig"
        },
        "line.7": {
            "text": "Ein Teil Löffel"
        },
        "line.8": {
            "text": "Von mir"
        },
        "line.9": {
            "text": "Deine Geburtstagstorte"
        },
        "line.10": {
            "text": "Um Mitternacht gebacken"
        },
        "line.11": {
            "text": "Der Teig ist genau, wie du ihn magst"
        },
        "line.12": {
            "text": "Manchmal will ich mich selbst"
        },
        "line.13": {
            "text": "Versäumen wie einen Zug"
        },
        "line.14": {
            "text": "Du bist ein Teil Versäumen"
        },
        "line.15": {
            "text": "Ein Teil Zug"
        },
        "line.16": {
            "text": "Von mir"
        }
    }
}