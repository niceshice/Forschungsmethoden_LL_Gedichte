{
    "title": "leibhaft-lesen-92",
    "gender": "w",
    "author": "Elke Erb",
    "author_birth": "1938",
    "poem": {
        "line.1": {
            "text": "Diese gefügten, gefügigen, Weicheres tragenden"
        },
        "line.2": {
            "text": "Knochen: lesen."
        },
        "line.3": {
            "text": "In Zeilen zerlegter und aufgelegter"
        },
        "line.4": {
            "text": "Gebinde Aufsammlung ab."
        },
        "line.5": {
            "text": "Diese in Bündel gerafften Fasern: entziffern Deute,"
        },
        "line.6": {
            "text": "eräugen zu eigen."
        },
        "line.7": {
            "text": "Diese zehn Zehen im Gehen und Stehen: lesen, laß, daß sie"
        },
        "line.8": {
            "text": "nachher verwesen."
        },
        "line.9": {
            "text": "25.3.1999"
        }
    }
}