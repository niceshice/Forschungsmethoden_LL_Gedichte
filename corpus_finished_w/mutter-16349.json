{
    "title": "mutter-16349",
    "gender": "w",
    "author": "Andrea Grill",
    "author_birth": "1975",
    "poem": {
        "line.1": {
            "text": "Sie isst wie ein Kind"
        },
        "line.2": {
            "text": "sie spricht schön,"
        },
        "line.3": {
            "text": "verwöhnt uns mit"
        },
        "line.4": {
            "text": "den blinden"
        },
        "line.5": {
            "text": "Augenflecken,"
        },
        "line.6": {
            "text": "entwinde dich mir"
        },
        "line.7": {
            "text": "ruhig immer wieder"
        },
        "line.8": {
            "text": "nur gleich"
        },
        "line.9": {
            "text": "schauen werden wir uns"
        },
        "line.10": {
            "text": "ewig, mindestens"
        },
        "line.11": {
            "text": "bis morgen noch"
        },
        "line.12": {
            "text": "könnte man sich"
        },
        "line.13": {
            "text": "eine Nase borgen"
        },
        "line.14": {
            "text": "wäre man fein &"
        },
        "line.15": {
            "text": "heraußen aus den Über-"
        },
        "line.16": {
            "text": "Einstimmungen, ich bin"
        },
        "line.17": {
            "text": "wie ein Kind, ich"
        },
        "line.18": {
            "text": "spreche schön,"
        },
        "line.19": {
            "text": "entwinde mich"
        },
        "line.20": {
            "text": "dir immer wieder"
        },
        "line.21": {
            "text": "neu, diese Scheu"
        },
        "line.22": {
            "text": "vor der Nähe,"
        },
        "line.23": {
            "text": "ertappt zu werden"
        },
        "line.24": {
            "text": "auf unsicheren Fu.en"
        },
        "line.25": {
            "text": "wie in Handschuhen"
        },
        "line.26": {
            "text": "sich zu drehen, meine"
        },
        "line.27": {
            "text": "Mutter wendet immer"
        },
        "line.28": {
            "text": "alles zum Guten, mit dem"
        },
        "line.29": {
            "text": "Futter nach außen"
        }
    }
}