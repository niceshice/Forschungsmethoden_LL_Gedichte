{
    "title": "darss-mission-6055",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "wieder führt es neue wellen aus"
        },
        "line.2": {
            "text": "in schnellen"
        },
        "line.3": {
            "text": "wasserfarben wogen"
        },
        "line.4": {
            "text": "die boote des himmels des abgelegenen"
        },
        "line.5": {
            "text": "lichts am anfang"
        },
        "line.6": {
            "text": "war nichts"
        },
        "line.7": {
            "text": "anderes siehe die glühfäden"
        },
        "line.8": {
            "text": "morgens dies land"
        },
        "line.9": {
            "text": "ist der haken aller"
        },
        "line.10": {
            "text": "modernen hagiographie leckt"
        },
        "line.11": {
            "text": "keinen edison ex weststrande"
        },
        "line.12": {
            "text": "lux elektrifizierung geschieht"
        },
        "line.13": {
            "text": "vor jeder zeit aller augen"
        },
        "line.14": {
            "text": "blicke es wächst"
        },
        "line.15": {
            "text": "in nördlicher richtung"
        }
    }
}