{
    "title": "toilette-eines-cafes-2723",
    "gender": "w",
    "author": "Sabina Naef",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "auf dem weissen Lavabo"
        },
        "line.2": {
            "text": "ein schwarzer Spazierstock"
        },
        "line.3": {
            "text": "als hätte jemand plötzlich"
        },
        "line.4": {
            "text": "das Gehen verlernt"
        }
    }
}