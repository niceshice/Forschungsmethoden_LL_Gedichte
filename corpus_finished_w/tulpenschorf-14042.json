{
    "title": "tulpenschorf-14042",
    "gender": "w",
    "author": "Anja Bayer",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "Tulpenschorf"
        },
        "line.2": {
            "text": "ich in der Dünkeltasse"
        },
        "line.3": {
            "text": "kristallisiere Noch und Nur"
        },
        "line.4": {
            "text": "euer blasses s"
        },
        "line.5": {
            "text": "schöpft kein Wasser mehr"
        },
        "line.6": {
            "text": "aus dem Himmel-"
        },
        "line.7": {
            "text": "hochjauchzend aber"
        },
        "line.8": {
            "text": "ein Docht"
        },
        "line.9": {
            "text": "ist mir zugelaufen"
        },
        "line.10": {
            "text": "die heiligen Marien"
        },
        "line.11": {
            "text": "rühren sich an"
        }
    }
}