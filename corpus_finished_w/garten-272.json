{
    "title": "garten-272",
    "gender": "w",
    "author": "Orsolya Kalász",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "Nicht wahr,"
        },
        "line.2": {
            "text": "uns siehst du bei Sonnenuntergang"
        },
        "line.3": {
            "text": "sitzen"
        },
        "line.4": {
            "text": "in einem der schöneren Gärten"
        },
        "line.5": {
            "text": "mit den abgepellten Doppelschalen"
        },
        "line.6": {
            "text": "unserer Geheimnisse"
        },
        "line.7": {
            "text": "auf dem Tisch"
        },
        "line.8": {
            "text": "noch grüne Nüsse und Weißwein."
        },
        "line.9": {
            "text": "Nicht wahr,"
        },
        "line.10": {
            "text": "schön und sanft"
        },
        "line.11": {
            "text": "genug sind wir,"
        },
        "line.12": {
            "text": "zu singen"
        },
        "line.13": {
            "text": "dein Schlummerlied."
        },
        "line.14": {
            "text": "Aus dem Ungarischen übertragen von Orsolya Kalsz"
        }
    }
}