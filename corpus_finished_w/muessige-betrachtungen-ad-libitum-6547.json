{
    "title": "muessige-betrachtungen-ad-libitum-6547",
    "gender": "w",
    "author": "Ilana Shmueli",
    "author_birth": "1924",
    "poem": {
        "line.1": {
            "text": "1."
        },
        "line.2": {
            "text": "Weiser"
        },
        "line.3": {
            "text": "Alter"
        },
        "line.4": {
            "text": "Zeig mir die Richtung"
        },
        "line.5": {
            "text": "Im Inter-Röhricht"
        },
        "line.6": {
            "text": "Alter sag"
        },
        "line.7": {
            "text": "Wo ist mein Orient"
        },
        "line.8": {
            "text": "(...)"
        },
        "line.9": {
            "text": "3."
        },
        "line.10": {
            "text": "Am Bildschirm"
        },
        "line.11": {
            "text": "Untat und Greuel"
        },
        "line.12": {
            "text": "rasant"
        },
        "line.13": {
            "text": "präsentiert"
        },
        "line.14": {
            "text": "nach Punkten bewertet -"
        },
        "line.15": {
            "text": "4."
        },
        "line.16": {
            "text": "Hierzulande"
        },
        "line.17": {
            "text": "wo so schnell gesiegt wird -"
        },
        "line.18": {
            "text": "gesiegt und besiegt -"
        },
        "line.19": {
            "text": "gebaut und zerstört -"
        },
        "line.20": {
            "text": "gepflanzt und vertilgt"
        },
        "line.21": {
            "text": "ist keine Zeit für das Leid"
        },
        "line.22": {
            "text": "ungelobt"
        },
        "line.23": {
            "text": "loben wir Gott"
        },
        "line.24": {
            "text": "bigott"
        },
        "line.25": {
            "text": "5."
        },
        "line.26": {
            "text": "Wir 'chartern' eilig"
        },
        "line.27": {
            "text": "hin wo es blüht"
        },
        "line.28": {
            "text": "naturschutzbefohlen"
        },
        "line.29": {
            "text": "Atem zu holen"
        },
        "line.30": {
            "text": "unter letzten Fetzen"
        },
        "line.31": {
            "text": "freigelassenen Himmels -"
        },
        "line.32": {
            "text": "stotternd und stolpernd im Inter-Röhrricht"
        },
        "line.33": {
            "text": "suchen wir den Orient."
        }
    }
}