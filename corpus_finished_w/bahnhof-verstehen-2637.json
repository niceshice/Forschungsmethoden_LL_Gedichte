{
    "title": "bahnhof-verstehen-2637",
    "gender": "w",
    "author": "Kerstin Hensel",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "Wir werden zurückbleiben wenn schwer"
        },
        "line.2": {
            "text": "Beladen die Tender"
        },
        "line.3": {
            "text": "Mit dem Schüttgut der Kriege"
        },
        "line.4": {
            "text": "Rangieren, zurückbleiben wenn"
        },
        "line.5": {
            "text": "Erz für die Väter, Koks für die Söhne, die Asche der Enkel"
        },
        "line.6": {
            "text": "Wagen um Wagen besetzen. Eingleisig"
        },
        "line.7": {
            "text": "Fährt mit verspiegelten Scheiben der Glücksexpreß"
        },
        "line.8": {
            "text": "Aus der Welt. Auf verbogenen Zehen"
        },
        "line.9": {
            "text": "Gehen uns die Tauben an"
        },
        "line.10": {
            "text": "Um ein Fressen. Mäuse im Schienenbett"
        },
        "line.11": {
            "text": "Tragen die Meldungen weiter"
        },
        "line.12": {
            "text": "wohin"
        },
        "line.13": {
            "text": "Soll die Reise"
        },
        "line.14": {
            "text": "gehn Russland"
        },
        "line.15": {
            "text": "Schafft Durchzug und Polen steht offen, nur wir"
        },
        "line.16": {
            "text": "Bleiben zurück weil wir nicht wissen"
        },
        "line.17": {
            "text": "Wohin ja wohin"
        },
        "line.18": {
            "text": "Im Fundbüro"
        },
        "line.19": {
            "text": "Wohnen wir zwischen vergessnen Signalen und hören"
        },
        "line.20": {
            "text": "Wer sich davonmacht wenns pfeift."
        }
    }
}