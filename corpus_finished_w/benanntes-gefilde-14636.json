{
    "title": "benanntes-gefilde-14636",
    "gender": "w",
    "author": "Carolin Callies",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "schlag die karte ins land."
        },
        "line.2": {
            "text": "geh die linien, geh den feldern nach."
        },
        "line.3": {
            "text": "benenn's & hol den fluß bei"
        },
        "line.4": {
            "text": "&s moos & die flechten"
        },
        "line.5": {
            "text": "& trotte dich aus. benenn es dir ort."
        },
        "line.6": {
            "text": "niemand im wald,"
        },
        "line.7": {
            "text": "aber ein ungemach & ein vogelsang."
        },
        "line.8": {
            "text": "als käm ein tier aus dem unterholz,"
        },
        "line.9": {
            "text": "nenn's: kommt es nicht."
        },
        "line.10": {
            "text": "das moos ist ein fremder im berg"
        },
        "line.11": {
            "text": "& hellichter tann & nenn es ast,"
        },
        "line.12": {
            "text": "elefantös, nenn's bronzen."
        },
        "line.13": {
            "text": "ich"
        },
        "line.14": {
            "text": "nenn's dir nass."
        },
        "line.15": {
            "text": "komm vom eis, komm vom moos"
        },
        "line.16": {
            "text": "& nenn dich das mütterlein mit dem reisig,"
        },
        "line.17": {
            "text": "kartographiert & beseelt."
        }
    }
}