{
    "title": "undurchdringlichen-augen-zu-sehen-11527",
    "gender": "w",
    "author": "Ulrike Draesner",
    "author_birth": "1962",
    "poem": {
        "line.1": {
            "text": "wie ich mich nicht sehen kann da stehe"
        },
        "line.2": {
            "text": "nicht sehen schlange waran riesenwaran"
        },
        "line.3": {
            "text": "uraltes tüm trümmertüm schwarzes uraltes"
        },
        "line.4": {
            "text": "segnen der erde zirpen der insekten und keine"
        },
        "line.5": {
            "text": "vögel noch fliegen nicht luft nicht nur hahahecheln"
        },
        "line.6": {
            "text": "nur vulkan nur speien nur seen nur zähne riesen"
        },
        "line.7": {
            "text": "nur urtüm urtäler irrtümer mäuler der waharane"
        },
        "line.8": {
            "text": "voller bakterien türme die augen: erscheitern-"
        },
        "line.9": {
            "text": "des TÜM"
        },
        "line.10": {
            "text": "in der ver- in der zuvor- in der kommenheit"
        },
        "line.11": {
            "text": "seiner kurzen glieder seines wagemutes"
        },
        "line.12": {
            "text": "vom wasser an land zu kriechen: über-"
        },
        "line.13": {
            "text": "fällig und stüm"
        },
        "line.14": {
            "text": "reitet auf dem stöpsel"
        },
        "line.15": {
            "text": "der medizinflasche der dakar, heiler"
        },
        "line.16": {
            "text": "der inseln, auf einem wesen, halb pferd"
        },
        "line.17": {
            "text": "halb mensch, herbernsten schützenden"
        },
        "line.18": {
            "text": "gesichtes aus dem neunten jahrhundert"
        },
        "line.19": {
            "text": "herbei, nur einen wimpernschlag"
        },
        "line.20": {
            "text": "weit"
        },
        "line.21": {
            "text": "schau, unter den vögeln"
        },
        "line.22": {
            "text": "bewegt einzig wir"
        },
        "line.23": {
            "text": "das zart behaarte"
        },
        "line.24": {
            "text": "gefältelte auge"
        },
        "line.25": {
            "text": "scha-hau, wie es die grenzen"
        },
        "line.26": {
            "text": "anderer reiche und ihrer"
        },
        "line.27": {
            "text": "tümer"
        },
        "line.28": {
            "text": "bekränzt"
        }
    }
}