{
    "title": "froehliches-gedicht-11646",
    "gender": "w",
    "author": "Odile Kennel",
    "author_birth": "1967",
    "poem": {
        "line.1": {
            "text": "über das Wort"
        },
        "line.2": {
            "text": "fröhlich"
        },
        "line.3": {
            "text": "ein Gedicht schreiben"
        },
        "line.4": {
            "text": "ein fröhliches Gedicht"
        },
        "line.5": {
            "text": "was sonst, über ein"
        },
        "line.6": {
            "text": "zweisilbig hüpfendes"
        },
        "line.7": {
            "text": "Wörtchen, zum"
        },
        "line.8": {
            "text": "e"
        },
        "line.9": {
            "text": "hinziehendes"
        },
        "line.10": {
            "text": "o"
        },
        "line.11": {
            "text": "ist doch nur ein"
        },
        "line.12": {
            "text": "ist doch nur die Eskalation"
        },
        "line.13": {
            "text": "der Erleichterung"
        },
        "line.14": {
            "text": "ein rein persönlich"
        },
        "line.15": {
            "text": "gestörtes Wort-"
        },
        "line.16": {
            "text": "verstörtes Orts-"
        },
        "line.17": {
            "text": "verhältnis"
        },
        "line.18": {
            "text": "ich sagte doch: ein un-"
        },
        "line.19": {
            "text": "kompliziertes Gedicht"
        },
        "line.20": {
            "text": "eins mit schunkelnden"
        },
        "line.21": {
            "text": "Schützen, die rufen auf auf"
        },
        "line.22": {
            "text": "die fröhliche Jagd aus"
        },
        "line.23": {
            "text": "auch Festzelte stehn"
        },
        "line.24": {
            "text": "in diesem Gedicht"
        },
        "line.25": {
            "text": "daraus muntere"
        },
        "line.26": {
            "text": "Marschmusik schallt."
        },
        "line.27": {
            "text": "Heißa, so sing uns"
        },
        "line.28": {
            "text": "ein fröhliches Liedchen"
        },
        "line.29": {
            "text": "hinter dem Jägerzaun"
        },
        "line.30": {
            "text": "gesellig, fidel"
        },
        "line.31": {
            "text": "in diesem Gedicht"
        },
        "line.32": {
            "text": "sind sonntags die Straßen"
        },
        "line.33": {
            "text": "wie in schöneren Dörfern"
        },
        "line.34": {
            "text": "gefegt, ja, das fröhliche"
        },
        "line.35": {
            "text": "Gedicht ist frisch, tüchtig"
        },
        "line.36": {
            "text": "und frei, später"
        },
        "line.37": {
            "text": "schreib ich ein kaum"
        },
        "line.38": {
            "text": "rüdes Gedicht über"
        },
        "line.39": {
            "text": "röhlich,"
        },
        "line.40": {
            "text": "ein"
        },
        "line.41": {
            "text": "lichter-"
        },
        "line.42": {
            "text": "löhliches"
        },
        "line.43": {
            "text": "Gedicht:"
        },
        "line.44": {
            "text": "das hat doch"
        },
        "line.45": {
            "text": "kaum gebrannt"
        }
    }
}