{
    "title": "je-taime-moi-non-plus-4318",
    "gender": "w",
    "author": "Dagmar Leupold",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "Mit meinen Schwestern die Ohren"
        },
        "line.2": {
            "text": "ans Radio gepreßt (damals waren die Radios"
        },
        "line.3": {
            "text": "noch Möbel mit Verlaß) und die Zensur"
        },
        "line.4": {
            "text": "rauschen gehört - süßer als jede Sauerei"
        },
        "line.5": {
            "text": "in der Muttersprache"
        },
        "line.6": {
            "text": "Später, bei günstigen Lichtverhältnissen"
        },
        "line.7": {
            "text": "sagte mal jemand"
        },
        "line.8": {
            "text": "ich sähe aus wie"
        },
        "line.9": {
            "text": "Jane Birkin"
        },
        "line.10": {
            "text": "wo ist Serge, seitdem?"
        },
        "line.11": {
            "text": "Nein, nein"
        },
        "line.12": {
            "text": "das ist keine Klage"
        },
        "line.13": {
            "text": "Nur ein Poem"
        }
    }
}