{
    "title": "hirschuebung-1359",
    "gender": "w",
    "author": "Marion Poschmann",
    "author_birth": "1969",
    "poem": {
        "line.1": {
            "text": "über die Landstraße treibende Streifen,"
        },
        "line.2": {
            "text": "durch Wälder geflößtes, sehr streng"
        },
        "line.3": {
            "text": "bemessenes Weiß,"
        },
        "line.4": {
            "text": "und wir folgten den Sprüngen, den Streckungen"
        },
        "line.5": {
            "text": "dieser durchbrochenen Linie, Wildwechsel,"
        },
        "line.6": {
            "text": "Glätte, Gefälle, an Rastplätzen lagen"
        },
        "line.7": {
            "text": "bedingungslos schlafende Bänke, von Nässe"
        },
        "line.8": {
            "text": "gequollenes Holz, und die braunen gebogenen"
        },
        "line.9": {
            "text": "Rücken der Berge verwundete Fellflächen,"
        },
        "line.10": {
            "text": "Kahlschlag, verendete Pfade, und wir"
        },
        "line.11": {
            "text": "mit Karacho, wie Simulationen von Wind"
        },
        "line.12": {
            "text": "zwischen brusthohen Zweigen,"
        },
        "line.13": {
            "text": "Geweihen in Wattejacken verpackt,"
        },
        "line.14": {
            "text": "mattes Hirngespinst (Fallträume),"
        },
        "line.15": {
            "text": "Fünfender, Fingerzeig:"
        },
        "line.16": {
            "text": "aber"
        },
        "line.17": {
            "text": "wir rochen nach Seife"
        },
        "line.18": {
            "text": "nach Veilchen und Teichwasser, vor uns"
        },
        "line.19": {
            "text": "die furchtbare Vollständigkeit des Kommenden"
        },
        "line.20": {
            "text": "bald harthufig der Frost"
        }
    }
}