{
    "title": "dieser-ort-ist-nur-kulisse-illusion-14899",
    "gender": "w",
    "author": "Martina Weber",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "Dieser Ort ist nur Kulisse, Illusion. Ein Ticket"
        },
        "line.2": {
            "text": "für ein Erinnerungsspiel. Die Geschichte wirft"
        },
        "line.3": {
            "text": "die ausgetrunkenen Gläser weit"
        },
        "line.4": {
            "text": "hinter ihren Rücken."
        },
        "line.5": {
            "text": "My stonewashed brainwashed memory."
        },
        "line.6": {
            "text": "Und deine Lippenbewegung, plötzlich"
        },
        "line.7": {
            "text": "wie hinter Panzerglas. Der Bildschirm"
        },
        "line.8": {
            "text": "zerspringt in zwei Teile. Ich bin"
        },
        "line.9": {
            "text": "kein Groupie (Verschiebung einer Struktur)."
        },
        "line.10": {
            "text": "Eine unbeschriftete Wiese, blue moon."
        },
        "line.11": {
            "text": "Und das Gewicht dieses Farbwerts beachten."
        },
        "line.12": {
            "text": "Die abgestreifte, noch schimmernde Haut"
        },
        "line.13": {
            "text": "einer Schlange am Wegrand. Häuser,"
        },
        "line.14": {
            "text": "komplett aus Licht."
        }
    }
}