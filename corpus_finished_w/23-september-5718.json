{
    "title": "23-september-5718",
    "gender": "w",
    "author": "Elfriede Czurda",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "keine zeile schreibt der schwarze"
        },
        "line.2": {
            "text": "himmel drückt zu boden jeden immer"
        },
        "line.3": {
            "text": "neuen tod schwarzen trauerflor"
        },
        "line.4": {
            "text": "stellt er vor zwei flügel fest den"
        },
        "line.5": {
            "text": "ausschnitt in den weißen rahmen"
        },
        "line.6": {
            "text": "starre schwarze dichte atmet"
        },
        "line.7": {
            "text": "hastig jetzt und nie davor und"
        },
        "line.8": {
            "text": "nachher nicht steht die säule"
        },
        "line.9": {
            "text": "unzerbrechlich vor dem fenster"
        },
        "line.10": {
            "text": "sterbensgelb der himmel plötzlich"
        },
        "line.11": {
            "text": "fährt ein windstoß in den oleander"
        },
        "line.12": {
            "text": "grüne lanzen stürzen auf beton"
        },
        "line.13": {
            "text": "23. september; tante mathilde bricht auf"
        }
    }
}