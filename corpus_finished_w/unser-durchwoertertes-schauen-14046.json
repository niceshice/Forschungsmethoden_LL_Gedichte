    "poem": {
        "line.1": {
            "text": "unser durchw√∂rtertes"
        },
        "line.2": {
            "text": "Schauen hing"
        },
        "line.3": {
            "text": "an einem weiblichen Baum"
        },
        "line.4": {
            "text": "den damals"
        },
        "line.5": {
            "text": "aufgelesenen, gierig"
        },
        "line.6": {
            "text": "veratmeten Rhythmus"
        },
        "line.7": {
            "text": "legtest du nicht mehr"
        },
        "line.8": {
            "text": "ab, lege ich"
        },
        "line.9": {
            "text": "immer wieder an"
        },
        "line.10": {
            "text": "wessen Blick"
        },
        "line.11": {
            "text": "das Nest"
        },
        "line.12": {
            "text": "der Hornisse verstellt"
        },
        "line.13": {
            "text": "darf ihren Angriff"
        },
        "line.14": {
            "text": "erwarten"
        }
    }
}