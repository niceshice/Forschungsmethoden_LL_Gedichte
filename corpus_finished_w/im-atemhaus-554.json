{
    "title": "im-atemhaus-554",
    "gender": "w",
    "author": "Rose Ausländer",
    "author_birth": "1901",
    "poem": {
        "line.1": {
            "text": "Unsichtbare Brücken spannen"
        },
        "line.2": {
            "text": "von dir zu Menschen und Dingen"
        },
        "line.3": {
            "text": "von der Luft zu deinem Atem"
        },
        "line.4": {
            "text": "Mit Blumen sprechen"
        },
        "line.5": {
            "text": "wie mit Menschen"
        },
        "line.6": {
            "text": "die du liebst"
        },
        "line.7": {
            "text": "Im Atemhaus wohnen"
        },
        "line.8": {
            "text": "eine Menschblumenzeit"
        }
    }
}