{
    "title": "uebersetzen-3738",
    "gender": "w",
    "author": "Uljana Wolf",
    "author_birth": "1979",
    "poem": {
        "line.1": {
            "text": "mein freund: das ist"
        },
        "line.2": {
            "text": "unsere schlaglochliebe"
        },
        "line.3": {
            "text": "unser kleiner grenzverkehr"
        },
        "line.4": {
            "text": "holprig unter zungen"
        },
        "line.5": {
            "text": "unser zischgebet"
        },
        "line.6": {
            "text": "und jetzt streichel mich"
        },
        "line.7": {
            "text": "auf diesem stempelkissen"
        },
        "line.8": {
            "text": "bis der zoll kommt"
        },
        "line.9": {
            "text": "mein freund: oder wir"
        },
        "line.10": {
            "text": "schmuggeln flügge"
        },
        "line.11": {
            "text": "geschmacksknospen"
        },
        "line.12": {
            "text": "gazeta wyborcza und"
        },
        "line.13": {
            "text": "münzen münzen"
        },
        "line.14": {
            "text": "in einer flüchtigen"
        },
        "line.15": {
            "text": "mundhöhle randvoll"
        },
        "line.16": {
            "text": "zur stoßzeit"
        }
    }
}