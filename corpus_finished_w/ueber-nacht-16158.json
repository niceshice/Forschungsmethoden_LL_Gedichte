{
    "title": "ueber-nacht-16158",
    "gender": "w",
    "author": "Sandra Hubinger",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "Über Nacht"
        },
        "line.2": {
            "text": "gefror draußen"
        },
        "line.3": {
            "text": "die Wäsche der Kinder"
        },
        "line.4": {
            "text": "junge Stiere atmeten aus"
        },
        "line.5": {
            "text": "Schnee blühte weiß auf Stroh"
        },
        "line.6": {
            "text": "die im Schlaf Vergessenen"
        },
        "line.7": {
            "text": "schlingerten auf der Leine"
        },
        "line.8": {
            "text": "mit Augen glasig"
        },
        "line.9": {
            "text": "darin spiegelten sich"
        },
        "line.10": {
            "text": "Baumstumpf, Axt und leerer Zwinger"
        },
        "line.11": {
            "text": "im Stall war alles wach"
        },
        "line.12": {
            "text": "Über Nacht gefroren draußen"
        },
        "line.13": {
            "text": "die Kämme der Winde"
        },
        "line.14": {
            "text": "junge Stiere traten heraus"
        },
        "line.15": {
            "text": "Schnee schmolz Wasser auf Beton"
        },
        "line.16": {
            "text": "die im Schlaf Verlorenen"
        },
        "line.17": {
            "text": "fingerten nach den Leibern"
        },
        "line.18": {
            "text": "mit Augen garstig"
        },
        "line.19": {
            "text": "darin hielten sich"
        },
        "line.20": {
            "text": "Raub, stumpfe Axt und leere Schlinge"
        },
        "line.21": {
            "text": "im Stall war alles wach"
        },
        "line.22": {
            "text": "Über Nacht die Gefrorenen"
        },
        "line.23": {
            "text": "im Schlaf vergessen"
        },
        "line.24": {
            "text": "Schnee glasig"
        },
        "line.25": {
            "text": "Augen im Zwinger"
        },
        "line.26": {
            "text": "leer und wach"
        }
    }
}