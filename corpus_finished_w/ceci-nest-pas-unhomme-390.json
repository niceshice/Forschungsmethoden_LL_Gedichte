{
    "title": "ceci-nest-pas-unhomme-390",
    "gender": "w",
    "author": "Barbara Köhler",
    "author_birth": "1959",
    "poem": {
        "line.1": {
            "text": "Femmage  Magritte"
        },
        "line.2": {
            "text": "Das hat zwei Beine darauf"
        },
        "line.3": {
            "text": "besteht es glaubt sie wären"
        },
        "line.4": {
            "text": "ihm wichtig je länger je"
        },
        "line.5": {
            "text": "lieber hätt es vielleicht"
        },
        "line.6": {
            "text": "einen Fischschwanz"
        },
        "line.7": {
            "text": "Das hat einen Kopf damit"
        },
        "line.8": {
            "text": "schüttelt es nickt bis"
        },
        "line.9": {
            "text": "ihm schwindelt es stimmt"
        },
        "line.10": {
            "text": "zu & ab nimmt ab & zu:"
        },
        "line.11": {
            "text": "das hat Gewicht"
        },
        "line.12": {
            "text": "Das hat auch eine Sprache"
        },
        "line.13": {
            "text": "gelernt mit seinem Mund"
        },
        "line.14": {
            "text": "sagt es Ich es ist bestimmt"
        },
        "line.15": {
            "text": "komisch es findet sich nicht"
        },
        "line.16": {
            "text": "zum Lachen"
        },
        "line.17": {
            "text": "Das hat Augen das sieht sich"
        },
        "line.18": {
            "text": "vor ihm nach sieht es kommen"
        },
        "line.19": {
            "text": "Das hat Ohren das hört was"
        },
        "line.20": {
            "text": "sich gehört hat es gehört"
        },
        "line.21": {
            "text": "ihm doch nicht"
        },
        "line.22": {
            "text": "Das tut sächlich bloß das"
        },
        "line.23": {
            "text": "hat keinen Zweck das hat seine"
        },
        "line.24": {
            "text": "Regel seine Grammatik ist"
        },
        "line.25": {
            "text": "unpäßlich aber es macht nix"
        },
        "line.26": {
            "text": "das vergeht"
        },
        "line.27": {
            "text": "Das redet auf das Telefon"
        },
        "line.28": {
            "text": "ein als wär es ein Mensch"
        }
    }
}