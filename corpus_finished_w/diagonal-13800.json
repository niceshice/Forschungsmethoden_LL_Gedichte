{
    "title": "diagonal-13800",
    "gender": "w",
    "author": "Yoko Tawada",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "Entlegene Ähnlichkeiten"
        },
        "line.2": {
            "text": "Von einer Hand getragen durch Jahre"
        },
        "line.3": {
            "text": "Ohne Einschreibung"
        },
        "line.4": {
            "text": "Die Schlange sagt:"
        },
        "line.5": {
            "text": "Was"
        },
        "line.6": {
            "text": "Gebärdensprache der Initiale"
        },
        "line.7": {
            "text": "Aus Deinem Namen herausgeschnitten"
        },
        "line.8": {
            "text": "Wann"
        },
        "line.9": {
            "text": "Gestutzte Fragen"
        },
        "line.10": {
            "text": "Kurzschluß in meiner Vokabel"
        },
        "line.11": {
            "text": "Busch der Reibung"
        },
        "line.12": {
            "text": "Millionenfache"
        },
        "line.13": {
            "text": "Überlagerung von Sternbildern"
        },
        "line.14": {
            "text": "Hungrig unermüdlich tanzsüchtig"
        },
        "line.15": {
            "text": "Keine Absprache"
        },
        "line.16": {
            "text": "Über die Tetrapackung für die Milchstraße"
        },
        "line.17": {
            "text": "Ungeteilter Alltag"
        },
        "line.18": {
            "text": "Bei mir und zwischen uns"
        },
        "line.19": {
            "text": "Bin in dieser windigen Stadt"
        },
        "line.20": {
            "text": "Zurückgeblieben"
        },
        "line.21": {
            "text": "Habe Sterne beobachtet"
        },
        "line.22": {
            "text": "Im Bildschirm"
        },
        "line.23": {
            "text": "Vielleicht ein Versuch"
        },
        "line.24": {
            "text": "Das verlorene Netz mit einem Fisch zu fangen"
        },
        "line.25": {
            "text": "Vom Stammbaum hinuntergefallen"
        },
        "line.26": {
            "text": "Die Bauchkabel zerrissen"
        },
        "line.27": {
            "text": "Jede gebärt jede"
        },
        "line.28": {
            "text": "Mutterschaft als Übertreibung"
        },
        "line.29": {
            "text": "Wohin"
        },
        "line.30": {
            "text": "Schlaflose Fenster"
        },
        "line.31": {
            "text": "In einem Spiel wird jedes Rätsel streng gelöst"
        },
        "line.32": {
            "text": "Bügeleisenförmig"
        },
        "line.33": {
            "text": "Ortsnamen sind seßhaft"
        },
        "line.34": {
            "text": "Wohnen im Nirgendwo-Papier"
        },
        "line.35": {
            "text": "Es funkt"
        },
        "line.36": {
            "text": "Sendezeit erfüllt meine Ohren"
        },
        "line.37": {
            "text": "Verschiebt aber den Augenblick der Adressierung"
        },
        "line.38": {
            "text": "Nach der Wiedergeburt als erstes"
        },
        "line.39": {
            "text": "In den Briefkasten hineingeschaut"
        },
        "line.40": {
            "text": "Dann schnell zurück"
        },
        "line.41": {
            "text": "Ins Mischwesen des Buches"
        },
        "line.42": {
            "text": "Ein geschriebener Garten"
        },
        "line.43": {
            "text": "Ein Verstummen auf der Brücke der Aufzählung"
        },
        "line.44": {
            "text": "Abwesenheit der Gänse"
        },
        "line.45": {
            "text": "Ihre Füße sind Wunderkerzen"
        },
        "line.46": {
            "text": "Autoren stehen hinter den Gittern"
        },
        "line.47": {
            "text": "der Autogramme"
        },
        "line.48": {
            "text": "Von Dir her eine heitere Stufe - hellsichtig"
        },
        "line.49": {
            "text": "Unser seltsamer Spaziergang"
        },
        "line.50": {
            "text": "Durch einen augenlosen Fleck"
        },
        "line.51": {
            "text": "Kein Denkmal steht dort"
        },
        "line.52": {
            "text": "Flüchtig erscheint es"
        },
        "line.53": {
            "text": "In einem Blitz"
        },
        "line.54": {
            "text": "Die Einzige"
        },
        "line.55": {
            "text": "Bis jetzt"
        },
        "line.56": {
            "text": "Schreiben"
        },
        "line.57": {
            "text": "In die Arme der Wiederholung"
        },
        "line.58": {
            "text": "Deiner Worte"
        }
    }
}