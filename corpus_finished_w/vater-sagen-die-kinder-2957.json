{
    "title": "vater-sagen-die-kinder-2957",
    "gender": "w",
    "author": "Nora Gomringer",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "Ohne William James"
        },
        "line.2": {
            "text": "Wäre er nicht pragmatisch"
        },
        "line.3": {
            "text": "Er stellt sich hin"
        },
        "line.4": {
            "text": "Und trinkt ein Ei aus der Schale"
        },
        "line.5": {
            "text": "Füchslein sagen die Kinder"
        },
        "line.6": {
            "text": "Stielt ein Ei"
        }
    }
}