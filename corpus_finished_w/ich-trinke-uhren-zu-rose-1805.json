{
    "title": "ich-trinke-uhren-zu-rose-1805",
    "gender": "w",
    "author": "Isabeella Beumer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "ich trinke uhren zur rose"
        },
        "line.2": {
            "text": "oder blattlos verglänzt"
        },
        "line.3": {
            "text": "deine knospe"
        },
        "line.4": {
            "text": "wo sich im spiel der zungen"
        },
        "line.5": {
            "text": "saumfalten öffnen."
        },
        "line.6": {
            "text": "einer weichen fuge"
        },
        "line.7": {
            "text": "schwillt zittern"
        },
        "line.8": {
            "text": "was sich über feuchte"
        },
        "line.9": {
            "text": "gesommerter fackel stülpt"
        },
        "line.10": {
            "text": "ein entgegenatmen um sich"
        },
        "line.11": {
            "text": "darin aufzulösen . . ."
        }
    }
}