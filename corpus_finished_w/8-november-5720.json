{
    "title": "8-november-5720",
    "gender": "w",
    "author": "Elfriede Czurda",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "und leute trampeln drüber ohne"
        },
        "line.2": {
            "text": "einen blick auf diese äderungen"
        },
        "line.3": {
            "text": "und maserungen die grünen ränder"
        },
        "line.4": {
            "text": "die noch dem vergilbten zentrum"
        },
        "line.5": {
            "text": "opponieren der kälte die schon"
        },
        "line.6": {
            "text": "schwarze löcher frißt und käfern"
        },
        "line.7": {
            "text": "die kreisrunden millimeter blatt"
        },
        "line.8": {
            "text": "salat zum abendessen hatten und"
        },
        "line.9": {
            "text": "hier liegt diese schwer lädierte"
        },
        "line.10": {
            "text": "schönheit jetzt im rinnstein lädt"
        },
        "line.11": {
            "text": "mich dringend ein sie aufzuheben"
        },
        "line.12": {
            "text": "mitzunehmen zu den andern schätzen"
        },
        "line.13": {
            "text": "8. november; ein blatt vom taschentuschbaum"
        }
    }
}