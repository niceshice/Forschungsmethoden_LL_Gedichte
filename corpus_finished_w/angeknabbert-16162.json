{
    "title": "angeknabbert-16162",
    "gender": "w",
    "author": "Sandra Hubinger",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "Angeknabbert"
        },
        "line.2": {
            "text": "waren die Triebe von"
        },
        "line.3": {
            "text": "halbzahmen Tieren wir folgten ihrer"
        },
        "line.4": {
            "text": "Bissspur von Baum zu Baum"
        },
        "line.5": {
            "text": "schabten von den Zweigen erst die"
        },
        "line.6": {
            "text": "Äußere Rinde und rollten die innere"
        },
        "line.7": {
            "text": "vorsichtig zusammen den anderen"
        },
        "line.8": {
            "text": "Bäumen nahmen wir die lila Steinfrucht"
        },
        "line.9": {
            "text": "trugen ihren Geruch auf Händen ihre Farbe"
        },
        "line.10": {
            "text": "Unter unseren Fingernägeln nach tagelangem"
        },
        "line.11": {
            "text": "Einweichen ließ sich das unreif Gepflückte"
        },
        "line.12": {
            "text": "schälen gab eine Bitterkeit frei die"
        },
        "line.13": {
            "text": "wir nicht bei uns behielten"
        }
    }
}