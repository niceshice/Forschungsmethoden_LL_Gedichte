{
    "title": "spaerlicher-wortlandschaft-6545",
    "gender": "w",
    "author": "Ilana Shmueli",
    "author_birth": "1924",
    "poem": {
        "line.1": {
            "text": "In spärlicher Wortlandschaft"
        },
        "line.2": {
            "text": "sprachgestört"
        },
        "line.3": {
            "text": "fahrig"
        },
        "line.4": {
            "text": "setz ich das lästige Leid"
        },
        "line.5": {
            "text": "doch noch"
        },
        "line.6": {
            "text": "in Versfüße um"
        }
    }
}