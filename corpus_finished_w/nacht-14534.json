{
    "title": "nacht-14534",
    "gender": "w",
    "author": "Ruth Wolf-Rehfeldt",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "Nacht"
        },
        "line.2": {
            "text": "In geräuschdurchwirktem Gewande"
        },
        "line.3": {
            "text": "Sacht"
        },
        "line.4": {
            "text": "Neigst du dem lärmenden Tage dich zu"
        },
        "line.5": {
            "text": "Blind"
        },
        "line.6": {
            "text": "Folg ich deinen mannigfachen Lauten"
        },
        "line.7": {
            "text": "Wind rauscht in Bäumen"
        },
        "line.8": {
            "text": "Kind schreckt aus Träumen"
        },
        "line.9": {
            "text": "Donner grollt"
        },
        "line.10": {
            "text": "Eisenbahn rollt"
        },
        "line.11": {
            "text": "Quietschende Straßenbahn"
        },
        "line.12": {
            "text": "Fern springt ein Motor an"
        },
        "line.13": {
            "text": "Flugzeuggebrumm"
        },
        "line.14": {
            "text": "Maschinengesumm"
        },
        "line.15": {
            "text": "Der nahen Fabrik"
        },
        "line.16": {
            "text": "Radiomusik"
        },
        "line.17": {
            "text": "Lokomotivengepfeif"
        },
        "line.18": {
            "text": "Weibergekeif"
        },
        "line.19": {
            "text": "Männergekröhl"
        },
        "line.20": {
            "text": "Katzengeheul"
        },
        "line.21": {
            "text": "Mädchengeplapper"
        },
        "line.22": {
            "text": "Schuhgeklapper"
        },
        "line.23": {
            "text": "Schuß fällt dort"
        },
        "line.24": {
            "text": "Kuß erstickt Wort"
        },
        "line.25": {
            "text": "Nacht"
        },
        "line.26": {
            "text": "In geräuschdurchwirktem Gewande"
        },
        "line.27": {
            "text": "Sacht"
        },
        "line.28": {
            "text": "Neigst du dem lärmenden Tage dich zu"
        }
    }
}