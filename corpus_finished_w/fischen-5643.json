{
    "title": "fischen-5643",
    "gender": "w",
    "author": "Ulrike Almut Sandig",
    "author_birth": "1979",
    "poem": {
        "line.1": {
            "text": "mein käptn! / das schiffe versenken"
        },
        "line.2": {
            "text": "im nachmittagstee, das wortkarge"
        },
        "line.3": {
            "text": "falzen an der kante des esstischs,"
        },
        "line.4": {
            "text": "die ecken auf ecken, steuermanns"
        },
        "line.5": {
            "text": "sprüche, gefaltete hände, die seinen"
        },
        "line.6": {
            "text": "auf meinen, der rechte daumen hielt"
        },
        "line.7": {
            "text": "den andern gekreuzt. das"
        },
        "line.8": {
            "text": "fischen"
        },
        "line.9": {
            "text": "nach sätzen, vermeiden von regen,"
        },
        "line.10": {
            "text": "gemurmelten teilchen, glattstreichen"
        },
        "line.11": {
            "text": "gedeckten papiers. ZU TISCH hieß"
        },
        "line.12": {
            "text": "der klappernde löffel im glas, das"
        },
        "line.13": {
            "text": "glitzernde drehen von zucker und"
        },
        "line.14": {
            "text": "segen / wir trieben auf untiefen zu!"
        }
    }
}