{
    "title": "weisses-album-16159",
    "gender": "w",
    "author": "Sandra Hubinger",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "Vom Zaudern und Zupfen"
        },
        "line.2": {
            "text": "an welken Blättern"
        },
        "line.3": {
            "text": "die Enden knipsen"
        },
        "line.4": {
            "text": "Der Spinne Faden"
        },
        "line.5": {
            "text": "fällt vom Licht, schwingt"
        },
        "line.6": {
            "text": "die lockere Faust"
        },
        "line.7": {
            "text": "Im weißen Album"
        },
        "line.8": {
            "text": "ein Blatt sich wellt"
        },
        "line.9": {
            "text": "rollt ein die Enden"
        }
    }
}