{
    "title": "es-faellt-ein-stein-mir-zu-3361",
    "gender": "w",
    "author": "Dorothea  Grünzweig",
    "author_birth": "1952",
    "poem": {
        "line.1": {
            "text": "Es fällt ein Stein mir zu"
        },
        "line.2": {
            "text": "Gedicht genannt ich"
        },
        "line.3": {
            "text": "habe viele Steine"
        },
        "line.4": {
            "text": "kreisen mich ein und"
        },
        "line.5": {
            "text": "weiss nicht ob sie"
        },
        "line.6": {
            "text": "Keile sind die einmal"
        },
        "line.7": {
            "text": "über Bitten und Verstehen auf"
        },
        "line.8": {
            "text": "mich mein Schläfenheil"
        },
        "line.9": {
            "text": "hereinbrechen"
        },
        "line.10": {
            "text": "weiss nicht so weit ob"
        },
        "line.11": {
            "text": "Samen sich aus ihrem Leib"
        },
        "line.12": {
            "text": "befreien in die Erde"
        },
        "line.13": {
            "text": "dringen keimen und"
        },
        "line.14": {
            "text": "dort für das im Augenbloss"
        },
        "line.15": {
            "text": "kaum zu erkennende Schwingen"
        },
        "line.16": {
            "text": "Springen sorgen von"
        },
        "line.17": {
            "text": "Füssen bleiern auf"
        },
        "line.18": {
            "text": "vom Eisgebreit"
        },
        "line.19": {
            "text": "wie soll ichs wissen"
        }
    }
}