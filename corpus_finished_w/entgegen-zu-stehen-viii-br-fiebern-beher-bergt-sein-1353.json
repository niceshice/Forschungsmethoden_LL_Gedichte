{
    "title": "entgegen-zu-stehen-viii-br-fiebern-beher-bergt-sein-1353",
    "gender": "w",
    "author": "Anja Utler",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "regelrecht: luftmassen, vielmals hinweg"
        },
        "line.2": {
            "text": "gefegt übers nässende (..) flügel-"
        },
        "line.3": {
            "text": "schlägen gleich mitten: ins flimmerhaar,"
        },
        "line.4": {
            "text": "bleiben, kaum regen sich: ausgeschabt"
        },
        "line.5": {
            "text": "spröde, so dörrt selbst das: rippenfell,"
        },
        "line.6": {
            "text": "federnd, von algenschicht rinde bespannt"
        },
        "line.7": {
            "text": "überwuchert von"
        },
        "line.8": {
            "text": "krustenflechten"
        },
        "line.9": {
            "text": "mit"
        },
        "line.10": {
            "text": "ihnen in:"
        },
        "line.11": {
            "text": "hungersymbiose"
        },
        "line.12": {
            "text": "so heißt es"
        },
        "line.13": {
            "text": "zu bleiben, dürr aufgesteppt auf den"
        },
        "line.14": {
            "text": "boden so: mehren sie sich brechen schon"
        },
        "line.15": {
            "text": "unterm vogeltritt fingerschlag  vielteilig"
        },
        "line.16": {
            "text": "werden  und krallen sich, staune, ins"
        },
        "line.17": {
            "text": "bauchfell fest"
        }
    }
}