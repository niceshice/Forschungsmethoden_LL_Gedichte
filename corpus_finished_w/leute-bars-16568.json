{
    "title": "leute-bars-16568",
    "gender": "w",
    "author": "Daniela  Chana",
    "author_birth": "1985",
    "poem": {
        "line.1": {
            "text": "Wir beobachten Leute in Bars"
        },
        "line.2": {
            "text": "So sehen sie in der Außenwelt aus"
        },
        "line.3": {
            "text": "Die haben sich angezogen und frisiert"
        },
        "line.4": {
            "text": "Leute mit Weingläsern in der Hand"
        },
        "line.5": {
            "text": "Dabei trinken wir und kauen an den Nägeln"
        },
        "line.6": {
            "text": "Ihre Gesichter verzieht es tornadoförmig"
        },
        "line.7": {
            "text": "Wenn wir betrunken sind"
        },
        "line.8": {
            "text": "Wir beobachten Leute in Bars mit Abstand"
        },
        "line.9": {
            "text": "Hinter einer Krümmung"
        },
        "line.10": {
            "text": "Sehen sie manchmal besser aus"
        },
        "line.11": {
            "text": "Deshalb halten wir"
        },
        "line.12": {
            "text": "Das Weinglas vor uns hin"
        },
        "line.13": {
            "text": "Ist alles, was sie können"
        },
        "line.14": {
            "text": "Ein Ring auf dem Tisch"
        },
        "line.15": {
            "text": "Ein Abdruck auf der Theke"
        },
        "line.16": {
            "text": "Diese Leute in Bars sind nicht dieselben"
        },
        "line.17": {
            "text": "Wie Leute im Supermarkt"
        },
        "line.18": {
            "text": "Leute im Zoo"
        },
        "line.19": {
            "text": "Leute auf Gemälden"
        },
        "line.20": {
            "text": "Außer auf solchen von Leuten in Bars"
        }
    }
}