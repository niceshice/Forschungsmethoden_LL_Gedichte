{
    "title": "nacht-neben-nacht-ueber-tag-359",
    "gender": "w",
    "author": "Anne Duden",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "Mennigrot in kleinsten Portionen"
        },
        "line.2": {
            "text": "aufgesternt"
        },
        "line.3": {
            "text": "Wortanhauch oder flug."
        },
        "line.4": {
            "text": "Gauchheil"
        },
        "line.5": {
            "text": "gegen Sticktau am rissigen Boden."
        },
        "line.6": {
            "text": "Den ausgesparten Abhang entlang"
        },
        "line.7": {
            "text": "endlich September"
        },
        "line.8": {
            "text": "bebend unter Flügeln von Schlupfwespen"
        },
        "line.9": {
            "text": "im Kern geschwärzt lichtsäumige Espen"
        },
        "line.10": {
            "text": "Abzweig von den männlichen Kriegen."
        },
        "line.11": {
            "text": "In England mehrmals auf und ab gegangen."
        },
        "line.12": {
            "text": "Wasserfall eines Teiches im verdüsterten Ohr"
        },
        "line.13": {
            "text": "Tag und Nacht"
        },
        "line.14": {
            "text": "durch Espenlaub"
        },
        "line.15": {
            "text": "für sich oder einwärts."
        },
        "line.16": {
            "text": "Vor Sehnsucht nach einem falben Fußbett unter schmalen"
        },
        "line.17": {
            "text": "Sohlen"
        },
        "line.18": {
            "text": "frühzeitig verblödet."
        },
        "line.19": {
            "text": "Allein die Feldtauben gegenwärtig außerdem."
        },
        "line.20": {
            "text": "Turmhohes Gurren über den Quartieren."
        },
        "line.21": {
            "text": "Im Sitzen klopfte die Erde"
        },
        "line.22": {
            "text": "von Herzschlägen und sprüngen."
        },
        "line.23": {
            "text": "Kein verbindlicher Gedanke an die weit zurückliegenden"
        },
        "line.24": {
            "text": "Knochen"
        },
        "line.25": {
            "text": "den früheren Skelettzusammenhang"
        },
        "line.26": {
            "text": "die Tanzwut"
        },
        "line.27": {
            "text": "sowie die Ernährer und Aufzehrer hinter den Maschinen."
        },
        "line.28": {
            "text": "Stundengemurmel ans Ohr gepreßt"
        },
        "line.29": {
            "text": "Hinsuppendes Hechelndes"
        },
        "line.30": {
            "text": "schon vor den Tatsachen Verpufftes"
        },
        "line.31": {
            "text": "so daß übergreifend weiterhin Dürre sich ausbreitet"
        },
        "line.32": {
            "text": "unbemerkt"
        },
        "line.33": {
            "text": "Wasser- und Luftnot"
        },
        "line.34": {
            "text": "und die sauber ausrasierten Nacken der Jungverbraucher"
        },
        "line.35": {
            "text": "bruchlos in den Tag hinüberragen"
        },
        "line.36": {
            "text": "kopfhoch durch Hemd- oder Blusenkragen"
        },
        "line.37": {
            "text": "und das vollständige Ausbleiben von Stürmen und Gerüst-"
        },
        "line.38": {
            "text": "schlägen"
        },
        "line.39": {
            "text": "in Gehirn- und Augenhöhe."
        },
        "line.40": {
            "text": "Aufdie Halbinseln und Wasserländer"
        },
        "line.41": {
            "text": "ans Hochufer"
        },
        "line.42": {
            "text": "in allerletzter Minute"
        },
        "line.43": {
            "text": "und kurz vor der Versilbung."
        },
        "line.44": {
            "text": "Auf die dumpf durchpulsten Fundamente"
        },
        "line.45": {
            "text": "zwischen Ton und Echo."
        },
        "line.46": {
            "text": "Geworfen oder hingeleckt"
        },
        "line.47": {
            "text": "sich verlängernder Speichelfaden des Sumpfes."
        },
        "line.48": {
            "text": "Ein Rauschen und Strömen der gesunkenen Flüsse im"
        },
        "line.49": {
            "text": "Untergrund"
        },
        "line.50": {
            "text": "Zersetzen der kopfüber in die Weichteile gerammten Wälder"
        },
        "line.51": {
            "text": "Zerfallen Auflösen von Stämmen und Baumkronen"
        },
        "line.52": {
            "text": "bis hinunter zu den Gipfeln untergegangener Gebirge."
        },
        "line.53": {
            "text": "Ein Umkippen der Bestände"
        },
        "line.54": {
            "text": "bis die Toten landen"
        },
        "line.55": {
            "text": "und das Gesichtsfeld der Lebenden zerhauchen"
        },
        "line.56": {
            "text": "ohne Widerhall."
        },
        "line.57": {
            "text": "Gefallene Sätze aufgesprungene Worte"
        },
        "line.58": {
            "text": "mittellos am Leben erhalten."
        },
        "line.59": {
            "text": "Und das Tier in der aufgeschlagenen Abwesenheit"
        },
        "line.60": {
            "text": "jenseits der Verspielung schwer gealtert"
        },
        "line.61": {
            "text": "hart ans Verbliebene gedrückt"
        },
        "line.62": {
            "text": "an Wüsten- oder Zimmerränder"
        },
        "line.63": {
            "text": "in triebhaftem Höhlengehorsam."
        },
        "line.64": {
            "text": "Widergängerin treppunter"
        },
        "line.65": {
            "text": "ohne eigene Röte"
        },
        "line.66": {
            "text": "vorgeschriebene Hitze"
        },
        "line.67": {
            "text": "auf unverdienten Geldern erliegend"
        },
        "line.68": {
            "text": "stückweise und gegenwelthalber."
        },
        "line.69": {
            "text": "Verschlossen im Wartestand"
        },
        "line.70": {
            "text": "den Ausschluß beobachtend."
        },
        "line.71": {
            "text": "EYE IN THE DOLDRUMS"
        },
        "line.72": {
            "text": "Sekundenlöser in der abgewandten Stille."
        },
        "line.73": {
            "text": "Ruheherde sind Steineichen"
        },
        "line.74": {
            "text": "in St. Johns Wood"
        },
        "line.75": {
            "text": "nüchtern unnahbar an die Erde geschmiegt"
        },
        "line.76": {
            "text": "zwischen weißen Küstenfassaden."
        },
        "line.77": {
            "text": "Noch ein Dämmerungsvogel im kahlen Geäst"
        },
        "line.78": {
            "text": "Hocker über der flutenden Themse"
        },
        "line.79": {
            "text": "sang in die bald hundertprozentige Verdünnung hinein."
        },
        "line.80": {
            "text": "Von Kaffee zu Kaffee"
        },
        "line.81": {
            "text": "Fernsehbild zu Fernsehbild"
        },
        "line.82": {
            "text": "Zigarette zu Zigarette"
        },
        "line.83": {
            "text": "erstickt verendet gestorben"
        },
        "line.84": {
            "text": "und Punkt zu Punkt."
        },
        "line.85": {
            "text": "Nicht die Gestorbenen fallen hier ins Gewicht"
        },
        "line.86": {
            "text": "noch diejenigen die die Welt bestimmen"
        },
        "line.87": {
            "text": "auch nicht die immer zahlreicher werdenden Kippsüchtigen"
        },
        "line.88": {
            "text": "oder deren Abwarter"
        },
        "line.89": {
            "text": "aber die AUSHAUCHER"
        },
        "line.90": {
            "text": "jeder für sich"
        },
        "line.91": {
            "text": "die dauerhaft Ohnmächtigen"
        },
        "line.92": {
            "text": "ON DOOMWATCH MISSION"
        },
        "line.93": {
            "text": "auf den ersten Blick"
        },
        "line.94": {
            "text": "in Wahrheit leibhaftig ausgespannt und  gebootet"
        },
        "line.95": {
            "text": "allen Laufrichtungen entschlagen"
        },
        "line.96": {
            "text": "ohne Höhlung oder Hecke"
        },
        "line.97": {
            "text": "unter Baumstümpfen an Tafeln."
        },
        "line.98": {
            "text": "Inbrünstig schwerelos"
        },
        "line.99": {
            "text": "ström- und mündungssüchtig."
        },
        "line.100": {
            "text": "Die Zeit ein Wolkenschub"
        },
        "line.101": {
            "text": "Blitz Schlag oder Schatten"
        },
        "line.102": {
            "text": "giftmischend im Lager der Tätigen."
        },
        "line.103": {
            "text": "Unter dünnsten Augendecken"
        },
        "line.104": {
            "text": "Nick- oder Spürhäuten"
        },
        "line.105": {
            "text": "weitsichtig das opalene Blau"
        },
        "line.106": {
            "text": "obdachlose Landschaft der sich schließenden"
        },
        "line.107": {
            "text": "öffnenden Gegenwelt im Wechsel."
        },
        "line.108": {
            "text": "Ums Verrecken noch Rühren im Versagten"
        },
        "line.109": {
            "text": "Abgebrochenen Entzogenen."
        },
        "line.110": {
            "text": "Ausharren im Januar an Haustierexkrementen entlang."
        },
        "line.111": {
            "text": "ER fortschlingernd in den Eingeweiden."
        },
        "line.112": {
            "text": "Voller Wucht in den Bauch getretener Hund"
        },
        "line.113": {
            "text": "freigeschlagene Wahrheit an beliebigem Ort."
        },
        "line.114": {
            "text": "Eingedickt sichtlos."
        },
        "line.115": {
            "text": "I am sick of streets and people"
        },
        "line.116": {
            "text": "I cant bear the sickness any longer."
        },
        "line.117": {
            "text": "Der Tag schlägt um."
        },
        "line.118": {
            "text": "IS IT I"
        },
        "line.119": {
            "text": "Muttermale Flechte Farn"
        },
        "line.120": {
            "text": "federweicher Flaum"
        },
        "line.121": {
            "text": "unter Zudecken gedrückt."
        },
        "line.122": {
            "text": "Mit einer Drossel über den Sommer zu kommen"
        },
        "line.123": {
            "text": "bei weltweit unerträglicher Nahbarschaft."
        },
        "line.124": {
            "text": "Den Käfig aller Vögel betreten"
        },
        "line.125": {
            "text": "das Schleppnetz alles Aufgehobenen sinken lassen."
        },
        "line.126": {
            "text": "WAS IST DER AUFTRAG"
        },
        "line.127": {
            "text": "nachts um drei zwischen Bett und Badezimmer"
        },
        "line.128": {
            "text": "nachdem die ersten Pflichten"
        },
        "line.129": {
            "text": "nie abgeleistet wurden"
        },
        "line.130": {
            "text": "im Frühstadium tumultuarisch zurückgewiesen"
        },
        "line.131": {
            "text": "kalt umnachtet der Dienst aufgekündigt wurde."
        },
        "line.132": {
            "text": "In der unverantwortlichen Sprache Toter"
        },
        "line.133": {
            "text": "nicht hörbar reden"
        },
        "line.134": {
            "text": "Hapaxlegomena"
        },
        "line.135": {
            "text": "Bitter- Süß- und Senkstoffe"
        },
        "line.136": {
            "text": "auflösen in Hauchbares"
        },
        "line.137": {
            "text": "das an den Tätigen vorbeiströmt"
        },
        "line.138": {
            "text": "nur ausnahmsweise Nerven wäßrige Organe Zerrgelenke"
        },
        "line.139": {
            "text": "streift"
        },
        "line.140": {
            "text": "und schon wieder weitertreibt"
        },
        "line.141": {
            "text": "unteilbar"
        },
        "line.142": {
            "text": "aufs Geflockte zu."
        },
        "line.143": {
            "text": "Herzverschlag"
        },
        "line.144": {
            "text": "pulsierender Müllsack"
        },
        "line.145": {
            "text": "im verschlossenen Korb."
        },
        "line.146": {
            "text": "Reizbar entzündlich"
        },
        "line.147": {
            "text": "mit der Veranlagung zum Feuersturm"
        },
        "line.148": {
            "text": "in den Kanonenofen gestopft."
        },
        "line.149": {
            "text": "Sich verzehrender Schlucker"
        },
        "line.150": {
            "text": "im Dauerrückstand."
        },
        "line.151": {
            "text": "Schlaf jetzt"
        },
        "line.152": {
            "text": "unter den Tritten der Dagewesenen und gleich schon"
        },
        "line.153": {
            "text": "Wiederkommenden."
        },
        "line.154": {
            "text": "Halbwertzeit unbrauchbarer Affekte."
        },
        "line.155": {
            "text": "Wie angewurzelt im Liegen im Stehen"
        },
        "line.156": {
            "text": "vor den Städten in ihnen"
        },
        "line.157": {
            "text": "aber krümmt sich beim kleinsten Geräusch"
        },
        "line.158": {
            "text": "beim leisesten Windzug"
        },
        "line.159": {
            "text": "schreckt auf"
        },
        "line.160": {
            "text": "zuckt zusammen"
        },
        "line.161": {
            "text": "hält sich abwechselnd die Ohren"
        },
        "line.162": {
            "text": "das nicht durchstoßene Auge zu"
        },
        "line.163": {
            "text": "die aufspringenden Wunden"
        },
        "line.164": {
            "text": "berührt die Ränder"
        },
        "line.165": {
            "text": "leckt das überquellende Blut"
        },
        "line.166": {
            "text": "verschluckt erbricht sich"
        },
        "line.167": {
            "text": "sieht die Lymphe Fäden ziehen"
        },
        "line.168": {
            "text": "Haut mit Sand Stein Erde verbinden"
        },
        "line.169": {
            "text": "reißt sich ein Glied aus"
        },
        "line.170": {
            "text": "zittert der Ruhe entgegen."
        },
        "line.171": {
            "text": "Dünnstimmig verkratzt jeweils abends und nachts"
        },
        "line.172": {
            "text": "stumm und tonlos am Tag."
        },
        "line.173": {
            "text": "Jede Regung wird abgefangen"
        },
        "line.174": {
            "text": "eingefroren"
        },
        "line.175": {
            "text": "dann das Ergebnis angestarrt."
        },
        "line.176": {
            "text": "Regen fällt ins allseits Abgeschirmte"
        },
        "line.177": {
            "text": "Vermessene und Verminte."
        },
        "line.178": {
            "text": "In Deutschland auf den Rücken geworfen"
        },
        "line.179": {
            "text": "wenns hochkommt auf die Seite gewälzt"
        },
        "line.180": {
            "text": "die bleiche Unterseite hingespreizt"
        },
        "line.181": {
            "text": "fischblütig lüstern gepfählt"
        },
        "line.182": {
            "text": "von streng verkniffenen Recken"
        },
        "line.183": {
            "text": "und der Geilheit und Putzsucht beflissen vorgelegt:"
        },
        "line.184": {
            "text": "klaffende After und Schlitze"
        },
        "line.185": {
            "text": "Zitzen"
        },
        "line.186": {
            "text": "zahllose Brüste"
        },
        "line.187": {
            "text": "angeschnittene Kehlen gekerbte Gliedmaßen"
        },
        "line.188": {
            "text": "durchbohrte Zungen und Schlünde"
        },
        "line.189": {
            "text": "durchstoßene Schädelrückwände"
        },
        "line.190": {
            "text": "direkt neben dem nervösen Tänzeln der Angestellten"
        },
        "line.191": {
            "text": "der buckelnden Fürbitte der Versprochenen"
        },
        "line.192": {
            "text": "dem Achthaben der Auftraggeber."
        },
        "line.193": {
            "text": "Öffentliche Beschlagnahme"
        },
        "line.194": {
            "text": "durch Fahnenpflanzungen ins"
        },
        "line.195": {
            "text": "lichtscheue Sonnengeflecht."
        },
        "line.196": {
            "text": "Ausgekörperte Lebensziele"
        },
        "line.197": {
            "text": "alleingültig und sattsam dem Endverbraucher zugestoßen."
        },
        "line.198": {
            "text": "Weder bebt der Rocksaum noch zittert die Unterlippe."
        },
        "line.199": {
            "text": "Vielleicht klirren verloren die Beschläge"
        },
        "line.200": {
            "text": "schaben die Scharniere"
        },
        "line.201": {
            "text": "an den ge- oder entladenen Körpern."
        },
        "line.202": {
            "text": "Im kostbaren Moment der Exhibition"
        },
        "line.203": {
            "text": "heilig an Brückenträger gelehnt"
        },
        "line.204": {
            "text": "in Unterführungen gekauert"
        },
        "line.205": {
            "text": "unter Gleise gebeugt."
        },
        "line.206": {
            "text": "Vorschriftsmäßig aufgezäumt"
        },
        "line.207": {
            "text": "vor dick aufgetragenem Gold"
        },
        "line.208": {
            "text": "und insgesamt anberaumt in Richterstuben."
        },
        "line.209": {
            "text": "Kinder rufen ins Verwandte"
        },
        "line.210": {
            "text": "Säuglinge schreien ins Fleisch."
        },
        "line.211": {
            "text": "Einer durchkreuzt beständig die Bläue."
        },
        "line.212": {
            "text": "lehnt jede Speichelübertragung ab."
        },
        "line.213": {
            "text": "Schlägt heimlich mit starren Flügeln einmal pro Jahr"
        },
        "line.214": {
            "text": "vielleicht wenn es heiß wird"
        },
        "line.215": {
            "text": "nimmt auf und kennt keine Notdurft."
        },
        "line.216": {
            "text": "Riecht nicht das Blut"
        },
        "line.217": {
            "text": "nimmt keine Notiz."
        },
        "line.218": {
            "text": "Atmet selten aber nur ein."
        },
        "line.219": {
            "text": "Ist unerwartet behutsam den Schnittstellen verpflichtet"
        },
        "line.220": {
            "text": "und verrechnet sich nicht."
        },
        "line.221": {
            "text": "Unter seinen Blicken schnell noch ein Zubereiten der"
        },
        "line.222": {
            "text": "Tagesration"
        },
        "line.223": {
            "text": "im toten Winkel."
        },
        "line.224": {
            "text": "Dann stillgehalten."
        },
        "line.225": {
            "text": "It is over. IT IS OVER I SAID."
        },
        "line.226": {
            "text": "Schneide stählern im Ohr"
        },
        "line.227": {
            "text": "scharf aufkreischend ins Sägemehl."
        },
        "line.228": {
            "text": "Und erhebt sich in die laufenden Ereignisse"
        },
        "line.229": {
            "text": "läßt ALLES unter sich und zurück."
        },
        "line.230": {
            "text": "Im dämmrigen Garten scheckerts"
        },
        "line.231": {
            "text": "vermißt die Entfernung des Gelichters."
        },
        "line.232": {
            "text": "Zu Hilfe kommt nichts."
        },
        "line.233": {
            "text": "Stickige Trichterverliese"
        },
        "line.234": {
            "text": "liquidierte Zangengeburten."
        },
        "line.235": {
            "text": "Zerr- und Preßschweiß"
        },
        "line.236": {
            "text": "ausgetrieben"
        },
        "line.237": {
            "text": "bis zum nächsten Besuch."
        },
        "line.238": {
            "text": "Kümmern Sie sich nicht weiter darum."
        },
        "line.239": {
            "text": "Das Buttergestirn bleibt fest vernagelt im Kosmos"
        },
        "line.240": {
            "text": "teils inwendig dauerverlobt"
        },
        "line.241": {
            "text": "teils doppelbetthörig angetraut."
        },
        "line.242": {
            "text": "Betrachten Sie sie als erledigt."
        },
        "line.243": {
            "text": "Mahlend die Nacht"
        },
        "line.244": {
            "text": "oder was"
        },
        "line.245": {
            "text": "die toten Zähne"
        },
        "line.246": {
            "text": "eingekübelt im Einzugsbereich"
        },
        "line.247": {
            "text": "jedoch unhaltbar schluck- und speicherversessen."
        },
        "line.248": {
            "text": "Unter dem gehörlosen Dröhnen der Fernaufklärer"
        },
        "line.249": {
            "text": "sichtiges Paarungsgescharre im Zweiminutentakt"
        },
        "line.250": {
            "text": "sowie strahlenförmige Bewaffnung der Einkaufszentren."
        },
        "line.251": {
            "text": "Geduckt wallen die Unterwasserplantagen"
        },
        "line.252": {
            "text": "bevor sie regelmäßig ausgehoben werden."
        },
        "line.253": {
            "text": "Gleich unter den Brüsten"
        },
        "line.254": {
            "text": "sorgfältig freigelegt von Untergebenen"
        },
        "line.255": {
            "text": "ein zweites skrofulöses Augenpaar"
        },
        "line.256": {
            "text": "das aus eigener Entschlußkraft die Lider nicht heben kann."
        },
        "line.257": {
            "text": "KÖNIGINNENBLICK"
        },
        "line.258": {
            "text": "umgeben von Tupferkolonnen"
        },
        "line.259": {
            "text": "befehligt aus der Horizontalen"
        },
        "line.260": {
            "text": "die hochempfindliche aber zähe Liebe."
        },
        "line.261": {
            "text": "Auf den Trottoirs der Kriech- und Standstraßen"
        },
        "line.262": {
            "text": "abgelaufene Gesichter"
        },
        "line.263": {
            "text": "gestürzte Fische"
        },
        "line.264": {
            "text": "posthum aus dem Geschlecht gestoßener Samen."
        },
        "line.265": {
            "text": "Einer will sich erheben"
        },
        "line.266": {
            "text": "kalt und klamm"
        },
        "line.267": {
            "text": "eingeschlagen in Bandagen und Laken"
        },
        "line.268": {
            "text": "bewegungsloser Auftakt"
        },
        "line.269": {
            "text": "des Sichentwindens."
        },
        "line.270": {
            "text": "NACHT NEBEN NACHT ÜBER TAG"
        },
        "line.271": {
            "text": "DESCENT INTO LIMBO"
        },
        "line.272": {
            "text": "HARROWING OF HELL."
        }
    }
}