{
    "title": "atomic-tangerine-5657",
    "gender": "w",
    "author": "Sabine Scho",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "eine tür, was für ein gebrauch"
        },
        "line.2": {
            "text": "ein schloß, scharnier, kragen-"
        },
        "line.3": {
            "text": "stäbchen, damit misst man"
        },
        "line.4": {
            "text": "stahlzargen aus, beton, akten"
        },
        "line.5": {
            "text": "und, ja, akten. so machen wir"
        },
        "line.6": {
            "text": "das licht aus, auf stapeln von"
        },
        "line.7": {
            "text": "papier, aber grauen ist doch auch"
        },
        "line.8": {
            "text": "eine farbe, betont sachlich mit dem"
        },
        "line.9": {
            "text": "besenstiel, beinahe wie zuhaus, nur"
        },
        "line.10": {
            "text": "höher, der dritte teil des karma-"
        },
        "line.11": {
            "text": "spiels fiel aus,"
        },
        "line.12": {
            "text": "no alarms and"
        },
        "line.13": {
            "text": "no surprises"
        },
        "line.14": {
            "text": ", ein korridor, was"
        },
        "line.15": {
            "text": "für ein schlauch, pizza aus der"
        },
        "line.16": {
            "text": "plätzchendose, und da draußen"
        },
        "line.17": {
            "text": "wird es immer weißer, an weih-"
        },
        "line.18": {
            "text": "nachten, heißt es, sei das so brauch"
        },
        "line.19": {
            "text": "glitzerzäune, ein letztes umschlussbier"
        },
        "line.20": {
            "text": "but, please, couldnt you"
        },
        "line.21": {
            "text": "let me out of here"
        }
    }
}