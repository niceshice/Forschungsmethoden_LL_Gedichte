{
    "title": "winter-reise-365",
    "gender": "w",
    "author": "Anne Duden",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "I Aufbruch"
        },
        "line.2": {
            "text": "Aus der Luft"
        },
        "line.3": {
            "text": "am Schub-, am Flugrand"
        },
        "line.4": {
            "text": "abschließend und zwielichtig gesinnt"
        },
        "line.5": {
            "text": "unter der einfrierenden Nickhaut"
        },
        "line.6": {
            "text": "übergreifenden Winternachmittags"
        },
        "line.7": {
            "text": "dem Ent- und Vergehenden"
        },
        "line.8": {
            "text": "halbselig entmachtet"
        },
        "line.9": {
            "text": "plötzlich ans Herz gegriffen."
        },
        "line.10": {
            "text": "Aus- und Rückkehr"
        },
        "line.11": {
            "text": "bloß"
        },
        "line.12": {
            "text": "der Brüll- und Klageagentin"
        },
        "line.13": {
            "text": "in belüfteter Flügelkabine."
        },
        "line.14": {
            "text": "Zu abfällig und entschärft"
        },
        "line.15": {
            "text": "fürs Verenden"
        },
        "line.16": {
            "text": "nach dem Verbacken aller Gefühle"
        },
        "line.17": {
            "text": "zum Qualbestand"
        },
        "line.18": {
            "text": "in dünnwandiger Notgemeinschaft."
        },
        "line.19": {
            "text": "Eingekreist und beschirmt"
        },
        "line.20": {
            "text": "von weißer Beleuchtung"
        },
        "line.21": {
            "text": "und warmer Handreichung"
        },
        "line.22": {
            "text": "zum Essen und Trinken"
        },
        "line.23": {
            "text": "aschenfarben gewiß"
        },
        "line.24": {
            "text": "in die vibrierende Außenbeuge geschmiegt"
        },
        "line.25": {
            "text": "wird das Empfindungsbündel"
        },
        "line.26": {
            "text": "- brummweise und hochgescheit nun"
        },
        "line.27": {
            "text": "Zünglein zwischen den Äonensprachen."
        },
        "line.28": {
            "text": "The day breaks not"
        },
        "line.29": {
            "text": "and it is not my heart."
        },
        "line.30": {
            "text": "Jahr und Tag brachen über dem Wasser."
        },
        "line.31": {
            "text": "Im Auge der Entfernung"
        },
        "line.32": {
            "text": "unter der Hoheit der Fortbewegung"
        },
        "line.33": {
            "text": "und in die Bildstille"
        },
        "line.34": {
            "text": "Brandung verschellender Küsten."
        },
        "line.35": {
            "text": "Feuerschwämme erinnernd"
        },
        "line.36": {
            "text": "hitzig davongestohlen"
        },
        "line.37": {
            "text": "aus dem untragbar Verwandten."
        },
        "line.38": {
            "text": "Trockengewüteter Zunder bis zum Lungenstamm."
        },
        "line.39": {
            "text": "Und bei eisigem Funkensprung"
        },
        "line.40": {
            "text": "aus Xantener Kristall"
        },
        "line.41": {
            "text": "das glühende Blut."
        },
        "line.42": {
            "text": "Klirrende Zuversicht"
        },
        "line.43": {
            "text": "vom Kaffgesims aus:"
        },
        "line.44": {
            "text": "Rutsch-, Zerrbereich"
        },
        "line.45": {
            "text": "zwischen Himmel und Zukunft."
        },
        "line.46": {
            "text": "II Aufenthalt"
        },
        "line.47": {
            "text": "Ein einziges Kehlchen"
        },
        "line.48": {
            "text": "säumt den morgendlichen Abgrund."
        },
        "line.49": {
            "text": "Horchtr ihn aus"
        },
        "line.50": {
            "text": "für die Dunkelhörigen"
        },
        "line.51": {
            "text": "mit irrig entbundenen Ohren."
        },
        "line.52": {
            "text": "Wirft ihn"
        },
        "line.53": {
            "text": "bodenlos vertont"
        },
        "line.54": {
            "text": "auf"
        },
        "line.55": {
            "text": "zaghaft rücknahmebereit"
        },
        "line.56": {
            "text": "über dem Straßenschacht"
        },
        "line.57": {
            "text": "und gibt sich"
        },
        "line.58": {
            "text": "überwach abwartend"
        },
        "line.59": {
            "text": "geschlagen."
        },
        "line.60": {
            "text": "Stellvertreter nur"
        },
        "line.61": {
            "text": "auf knickendem Vogelfuß"
        },
        "line.62": {
            "text": "ins ALLGEMEINE gefugt"
        },
        "line.63": {
            "text": "aus dem"
        },
        "line.64": {
            "text": "weltweit und geschundenermaßen"
        },
        "line.65": {
            "text": "allein die kleinsten Stimmen"
        },
        "line.66": {
            "text": "sich lösen."
        },
        "line.67": {
            "text": "III Einbruch"
        },
        "line.68": {
            "text": "Hier betritt einer"
        },
        "line.69": {
            "text": "vorerst namenlos"
        },
        "line.70": {
            "text": "und abwärts schon"
        },
        "line.71": {
            "text": "den verfrühten und hohlen Morgen"
        },
        "line.72": {
            "text": "seines letzten Tages."
        },
        "line.73": {
            "text": "Allseits durch Mörtel nur"
        },
        "line.74": {
            "text": "getrennt von der noch nachtwarmen Mehrheit."
        },
        "line.75": {
            "text": "Er erhebt sich"
        },
        "line.76": {
            "text": "er fällt"
        },
        "line.77": {
            "text": "TALKING DEATH"
        },
        "line.78": {
            "text": "next to his ANGELCHILD"
        },
        "line.79": {
            "text": "zungenläppisch"
        },
        "line.80": {
            "text": "mit bereits erkaltendem Kopf."
        },
        "line.81": {
            "text": "MORRIS"
        },
        "line.82": {
            "text": "MOSS"
        },
        "line.83": {
            "text": "BRICKLAYER lebenslang"
        },
        "line.84": {
            "text": "verquer in die Verkeilung nun abgesackter"
        },
        "line.85": {
            "text": "Mittelpunkt"
        },
        "line.86": {
            "text": "einer fliehenden"
        },
        "line.87": {
            "text": "dem Haus entstürzenden Piet."
        },
        "line.88": {
            "text": "Letzter Wink seines"
        },
        "line.89": {
            "text": "nackt aufragenden Fußes."
        },
        "line.90": {
            "text": "Jetzt noch knapp drei trudelnde Stunden"
        },
        "line.91": {
            "text": "bis zum Aufprall auf die Ruhe"
        },
        "line.92": {
            "text": "die letzte Umrundung"
        },
        "line.93": {
            "text": "und Ölung und Rückfahrt."
        }
    }
}