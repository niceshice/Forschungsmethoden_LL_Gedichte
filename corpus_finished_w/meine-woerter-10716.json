{
    "title": "meine-woerter-10716",
    "gender": "w",
    "author": "Ulla Hahn",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Meine Wörter hab ich"
        },
        "line.2": {
            "text": "mir ausgezogen"
        },
        "line.3": {
            "text": "bis sie dalagen"
        },
        "line.4": {
            "text": "atmend und nackt"
        },
        "line.5": {
            "text": "mir unter der Zunge."
        },
        "line.6": {
            "text": "Ich dreh sie um"
        },
        "line.7": {
            "text": "spuck sie aus"
        },
        "line.8": {
            "text": "saug sie ein"
        },
        "line.9": {
            "text": "blas sie auf"
        },
        "line.10": {
            "text": "spann sie an"
        },
        "line.11": {
            "text": "von Kopf bis Fu"
        },
        "line.12": {
            "text": "spann sie auf"
        },
        "line.13": {
            "text": "Mach sie groß"
        },
        "line.14": {
            "text": "wie ein Raumschiff zum Mond"
        },
        "line.15": {
            "text": "und klein wie ein Kind."
        },
        "line.16": {
            "text": "Überall suche ich die Zeile"
        },
        "line.17": {
            "text": "die mir sagt"
        },
        "line.18": {
            "text": "wo ich mich find"
        },
        "line.19": {
            "text": "(aus:"
        },
        "line.20": {
            "text": "Herz über Kopf"
        },
        "line.21": {
            "text": ", 1981)"
        }
    }
}