{
    "title": "bukowina-i-545",
    "gender": "w",
    "author": "Rose Ausl√§nder",
    "poem": {
        "line.1": {
            "text": "Grne Mutter"
        },
        "line.2": {
            "text": "Bukowina"
        },
        "line.3": {
            "text": "Schmetterlinge im Haar"
        },
        "line.4": {
            "text": "Trink"
        },
        "line.5": {
            "text": "sagt die Sonne"
        },
        "line.6": {
            "text": "rote Melonenmilch"
        },
        "line.7": {
            "text": "weie Kukuruzmilch"
        },
        "line.8": {
            "text": "ich machte sie s"
        },
        "line.9": {
            "text": "Violette Fhrenzapfen"
        },
        "line.10": {
            "text": "Luftflgel Vgel und Laub"
        },
        "line.11": {
            "text": "Der Karpatenrcken"
        },
        "line.12": {
            "text": "vterlich"
        },
        "line.13": {
            "text": "ldt dich ein"
        },
        "line.14": {
            "text": "dich zu tragen"
        },
        "line.15": {
            "text": "Vier Sprachen"
        },
        "line.16": {
            "text": "Viersprachenlieder"
        },
        "line.17": {
            "text": "Menschen"
        },
        "line.18": {
            "text": "die sich verstehn"
        }
    }
}