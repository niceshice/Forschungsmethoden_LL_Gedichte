{
    "title": "wald-16352",
    "gender": "w",
    "author": "Andrea Grill",
    "poem": {
        "line.1": {
            "text": "sagt sie"
        },
        "line.2": {
            "text": "nicht Bäume oder das Grüne"
        },
        "line.3": {
            "text": "oder aufs Land,"
        },
        "line.4": {
            "text": "hast du gewusst"
        },
        "line.5": {
            "text": "dass eine erschlagene Mücke"
        },
        "line.6": {
            "text": "nach Erde riecht"
        },
        "line.7": {
            "text": "auf der Hand"
        }
    }
}