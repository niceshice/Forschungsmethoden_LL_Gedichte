{
    "title": "nachthonig-13569",
    "categories": [
        "Natur",
        "Landschaft",
        "Tiere"
    ],
    "gender": "w",
    "author": "Mara-Daria Cojocaru",
    "poem": {
        "line.1": {
            "text": "Immer kreuzen unsere Bienenlinien"
        },
        "line.2": {
            "text": "Warum halten diese Lichter"
        },
        "line.3": {
            "text": "Auch nicht still"
        },
        "line.4": {
            "text": "Da kommt der kleine Bär"
        },
        "line.5": {
            "text": "Und da der große"
        },
        "line.6": {
            "text": "Über den Hügel"
        },
        "line.7": {
            "text": "Über den Berg"
        },
        "line.8": {
            "text": "Die räuberischen Sterne fallen"
        },
        "line.9": {
            "text": "Über unsere süßen Träume her"
        }
    }
}