{
    "title": "nachthonig-13569",
    "gender": "w",
    "author": "Mara-Daria Cojocaru",
    "poem": {
        "line.1": {
            "text": "Immer kreuzen unsere Bienenlinien"
        },
        "line.2": {
            "text": "Warum halten diese Lichter"
        },
        "line.3": {
            "text": "Auch nicht still"
        },
        "line.4": {
            "text": "Da kommt der kleine Br"
        },
        "line.5": {
            "text": "Und da der groe"
        },
        "line.6": {
            "text": "ber den Hgel"
        },
        "line.7": {
            "text": "ber den Berg"
        },
        "line.8": {
            "text": "Die ruberischen Sterne fallen"
        },
        "line.9": {
            "text": "ber unsere sen Trume her"
        }
    }
}