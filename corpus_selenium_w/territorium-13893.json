{
    "title": "territorium-13893",
    "gender": "w",
    "author": "Kerstin Becker",
    "poem": {
        "line.1": {
            "text": "die Panzerstrae zwischen den Wldern"
        },
        "line.2": {
            "text": "in denen wir Sommer fr Sommer"
        },
        "line.3": {
            "text": "mit Sprossenschmerzen"
        },
        "line.4": {
            "text": "mit dicken Grinden"
        },
        "line.5": {
            "text": "mit schwarzen Fusohlen"
        },
        "line.6": {
            "text": "aufgehen ist fr die Ewigkeit"
        },
        "line.7": {
            "text": "Staubschleier grauen wir pesen"
        },
        "line.8": {
            "text": "davon eingehllt mit dem Ostwind aus"
        },
        "line.9": {
            "text": "gediente Dachpappefetzen fliegen"
        },
        "line.10": {
            "text": "im slichen Simsongestank"
        },
        "line.11": {
            "text": "Rohre fr lange Leitungen liegen"
        },
        "line.12": {
            "text": "brach wo die weien Birken"
        },
        "line.13": {
            "text": "an Wegerichpisten schieen wir"
        },
        "line.14": {
            "text": "fummeln wir suchen"
        },
        "line.15": {
            "text": "immer heftiger knutschend"
        },
        "line.16": {
            "text": "unsern Zusammenhang"
        }
    }
}