{
    "title": "erdaepfel-13885",
    "gender": "w",
    "author": "Kerstin Becker",
    "poem": {
        "line.1": {
            "text": "Wirbeltiere mit den krummen Rcken"
        },
        "line.2": {
            "text": "bcken wir biegen uns fassen"
        },
        "line.3": {
            "text": "die Khle klebender Klumpen"
        },
        "line.4": {
            "text": "Trauerrnder wir stoppeln"
        },
        "line.5": {
            "text": "Hohlgenagte"
        },
        "line.6": {
            "text": "stochern Musen nach stolpern"
        },
        "line.7": {
            "text": "Ackerschmalz auf den Lippen"
        },
        "line.8": {
            "text": "werfen uns Runde"
        },
        "line.9": {
            "text": "wie Liebesperlen zu beien"
        },
        "line.10": {
            "text": "zwischen den Furchen"
        },
        "line.11": {
            "text": "ins Ungeschlte"
        }
    }
}