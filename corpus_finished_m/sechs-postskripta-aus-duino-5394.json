{
    "title": "sechs-postskripta-aus-duino-5394",
    "gender": "m",
    "author": "Hans Raimund",
    "author_birth": "1945",
    "poem": {
        "line.1": {
            "text": "1"
        },
        "line.2": {
            "text": "Viel Ansprache habe ich hier nicht"
        },
        "line.3": {
            "text": "Die Vögel fehlen mir"
        },
        "line.4": {
            "text": "Auch die Hasen"
        },
        "line.5": {
            "text": "Von Rehen ist hier keine Rede mehr"
        },
        "line.6": {
            "text": "Außer auf den Speisekarten und den Straßenschildern im Karst"
        },
        "line.7": {
            "text": "2"
        },
        "line.8": {
            "text": "Frühmorgens krähen auch hier die Hähne"
        },
        "line.9": {
            "text": "Und heute fiel ein Eichhörnchenein schwarzes"
        },
        "line.10": {
            "text": "Zerzaust aus einem Baum"
        },
        "line.11": {
            "text": "Auf den Asphaltdirekt vor meine Füße"
        },
        "line.12": {
            "text": "3"
        },
        "line.13": {
            "text": "Unlängstspätabendsbei Zitronendropsmond"
        },
        "line.14": {
            "text": "Schauten der Hund und ich einem Igel zu"
        },
        "line.15": {
            "text": "Neben einer Mülltonnedrin Katzen rumorten"
        },
        "line.16": {
            "text": "Fraß er Papierschnitzelstumpfsinnig"
        },
        "line.17": {
            "text": "4"
        },
        "line.18": {
            "text": "Wasser gibt es hier im Überfluß"
        },
        "line.19": {
            "text": "Es macht mir Angst"
        },
        "line.20": {
            "text": "Steht der Wind richtigrieche ich esbis ins Haus"
        },
        "line.21": {
            "text": "5"
        },
        "line.22": {
            "text": "Und die Bora!Die ganze Nacht"
        },
        "line.23": {
            "text": "Klappern die Fensterlädenliegen"
        },
        "line.24": {
            "text": "Morgens aus den Scharnieren gerissen in den Büschen"
        },
        "line.25": {
            "text": "Wetterseitig blättert der Verputz von den Fassaden"
        },
        "line.26": {
            "text": "6"
        },
        "line.27": {
            "text": "Auf den Feldern steht noch der Mais vom Vorjahr"
        },
        "line.28": {
            "text": "Staubigweißraschelt im Wind"
        },
        "line.29": {
            "text": "Wieder ist es zu spät"
        },
        "line.30": {
            "text": "Die Äcker abzubrennen"
        },
        "line.31": {
            "text": "Platz zu machen für NeuesHeuriges"
        },
        "line.32": {
            "text": "(1989)"
        }
    }
}