{
    "title": "falsche-prophezeiungen-i-iauszuegei-817",
    "gender": "m",
    "author": "Christian Loidl",
    "author_birth": "1957",
    "poem": {
        "line.1": {
            "text": "HÖRST DU, HIMMEL,"
        },
        "line.2": {
            "text": "dein gehen"
        },
        "line.3": {
            "text": "auf holz"
        },
        "line.4": {
            "text": "oder haut?"
        },
        "line.5": {
            "text": "hört ihr es,"
        },
        "line.6": {
            "text": "knollen"
        },
        "line.7": {
            "text": "unter dem schnee?"
        },
        "line.8": {
            "text": "IM AUFGANG DER TAGE"
        },
        "line.9": {
            "text": "wird es geschehen:"
        },
        "line.10": {
            "text": "es wird die firma"
        },
        "line.11": {
            "text": "fest gegründet"
        },
        "line.12": {
            "text": "ragen"
        },
        "line.13": {
            "text": "an der spitze"
        },
        "line.14": {
            "text": "der zweige"
        },
        "line.15": {
            "text": "und erhaben"
        },
        "line.16": {
            "text": "über die zapfen"
        },
        "line.17": {
            "text": "ihr"
        },
        "line.18": {
            "text": "entspringen die völker"
        },
        "line.19": {
            "text": "alle"
        },
        "line.20": {
            "text": "völker strömen aus ihr"
        },
        "line.21": {
            "text": "von da"
        },
        "line.22": {
            "text": "pilgern sie aus und"
        },
        "line.23": {
            "text": "singen:"
        },
        "line.24": {
            "text": "auf!"
        },
        "line.25": {
            "text": "laßt uns umziehen"
        },
        "line.26": {
            "text": "zu den honigpumpen!"
        },
        "line.27": {
            "text": "DANN TAUMMELN FLUCHTRASCH UND FLUCHREICH"
        },
        "line.28": {
            "text": "die völker zusammen"
        },
        "line.29": {
            "text": "sie schmieden ihr mythenzeug"
        },
        "line.30": {
            "text": "um zu organen"
        },
        "line.31": {
            "text": "und ihre throne in laptops"
        },
        "line.32": {
            "text": "denn siehe"
        },
        "line.33": {
            "text": "die ungewisse regie"
        },
        "line.34": {
            "text": "entzieht dem städter"
        },
        "line.35": {
            "text": "tabak und spazierstock"
        },
        "line.36": {
            "text": "eine stütze"
        },
        "line.37": {
            "text": "an wasser!"
        },
        "line.38": {
            "text": "ihre männer"
        },
        "line.39": {
            "text": "kriechen in kanonen"
        },
        "line.40": {
            "text": "und ihre söhne"
        },
        "line.41": {
            "text": "in ärsche."
        }
    }
}