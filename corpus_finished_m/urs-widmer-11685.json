{
    "title": "urs-widmer-11685",
    "gender": "m",
    "author": "Franz Hohler",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "Heute Morgen"
        },
        "line.2": {
            "text": "beim Blick aus dem Fenster:"
        },
        "line.3": {
            "text": "der Kirschbaum blüht"
        },
        "line.4": {
            "text": "und bringt mir die Botschaft"
        },
        "line.5": {
            "text": "der Frühling sei da."
        },
        "line.6": {
            "text": "Wenig später"
        },
        "line.7": {
            "text": "im Radio die Meldung"
        },
        "line.8": {
            "text": "Urs Widmer sei gestern"
        },
        "line.9": {
            "text": "gestorben."
        },
        "line.10": {
            "text": "Dem Kirschbaum"
        },
        "line.11": {
            "text": "nehme ich seine Nachricht ab"
        },
        "line.12": {
            "text": "dem Radio nicht."
        },
        "line.13": {
            "text": "Urs ist nicht tot"
        },
        "line.14": {
            "text": "er ist nur aufgebrochen"
        },
        "line.15": {
            "text": "voll Neugier und Wanderlust"
        },
        "line.16": {
            "text": "zu einer grossen Reise"
        },
        "line.17": {
            "text": "bis an den Rand des Universums."
        }
    }
}