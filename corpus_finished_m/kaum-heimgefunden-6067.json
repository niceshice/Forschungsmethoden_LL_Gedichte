{
    "title": "kaum-heimgefunden-6067",
    "gender": "m",
    "author": "Christoph W. Bauer",
    "author_birth": "1968",
    "poem": {
        "line.1": {
            "text": "kaum heimgefunden"
        },
        "line.2": {
            "text": "in unterbelichtete dörfer und"
        },
        "line.3": {
            "text": "felder geschmissen in novemberposen"
        },
        "line.4": {
            "text": "montiert einer"
        },
        "line.5": {
            "text": "am fliessband seiner sprache sich"
        },
        "line.6": {
            "text": "abschiedsformeln in den"
        },
        "line.7": {
            "text": "mund bis eine brise west ihm"
        },
        "line.8": {
            "text": "übern gaumen streicht vokabeln mit prärien"
        },
        "line.9": {
            "text": "verkabelt entlang der"
        },
        "line.10": {
            "text": "liftspannungsseile auf allen vieren"
        },
        "line.11": {
            "text": "gekrochen mit pfeil und"
        },
        "line.12": {
            "text": "bogen unterwegs der begehbaren"
        },
        "line.13": {
            "text": "erde neue fährten abzulesen die bleich"
        },
        "line.14": {
            "text": "gesichtige herkunft mit"
        },
        "line.15": {
            "text": "fremden federn kaschiert blieb er"
        },
        "line.16": {
            "text": "blinder passagier im"
        },
        "line.17": {
            "text": "bahnnetz anderer augen doch wohin"
        },
        "line.18": {
            "text": "ihn deren zugkraft auch trieb stets fuhr ein"
        },
        "line.19": {
            "text": "glasscherbenviertel in seinem"
        },
        "line.20": {
            "text": "windschatten schleicht er noch heute"
        },
        "line.21": {
            "text": "in den spruchbändern der"
        },
        "line.22": {
            "text": "nacht manchmal hinauf in staub"
        },
        "line.23": {
            "text": "beleuchtete speicher voll schachteln"
        },
        "line.24": {
            "text": "durchbrochen von weichen marke märklin"
        }
    }
}