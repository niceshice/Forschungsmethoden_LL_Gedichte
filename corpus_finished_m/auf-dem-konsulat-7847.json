{
    "title": "auf-dem-konsulat-7847",
    "gender": "m",
    "author": "Rolf Haufs",
    "author_birth": "1935",
    "poem": {
        "line.1": {
            "text": "Gehen Sie gleich es dauert"
        },
        "line.2": {
            "text": "Nur eine halbe Stunde"
        },
        "line.3": {
            "text": "Sagen Sie daß Sie nach Princeton"
        },
        "line.4": {
            "text": "Dann brauchen Sie nicht sagt Roehler"
        },
        "line.5": {
            "text": "Zu warten weil da sind immer"
        },
        "line.6": {
            "text": "So an die vierzig die rüber"
        },
        "line.7": {
            "text": "Nur mal so und zum Spaß"
        },
        "line.8": {
            "text": "Die Miss ist freundlich"
        },
        "line.9": {
            "text": "Nehmen Sie ein Bändchen Gedichte mit"
        },
        "line.10": {
            "text": "Nach Zehlendorf eine Station"
        },
        "line.11": {
            "text": "Vor Onkel Toms Hütte"
        },
        "line.12": {
            "text": "Ländliche Gegend mit Kiefern"
        },
        "line.13": {
            "text": "Sie sind der Letzte sagt die Miss"
        },
        "line.14": {
            "text": "Der Letzte der Gruppe und werden"
        },
        "line.15": {
            "text": "Lacht sie doch nicht der Erste sein"
        },
        "line.16": {
            "text": "Sie waren nicht fragt die Miss"
        },
        "line.17": {
            "text": "Mitglied der Kommunistischen Partei"
        },
        "line.18": {
            "text": "Nein sag ich da hab abgeschworn"
        },
        "line.19": {
            "text": "Dem Zeugs von Marx und Compagnie"
        },
        "line.20": {
            "text": "In dreiundfünfzig Nächten Moabit"
        },
        "line.21": {
            "text": "Und Hautfarbe lacht die Miss"
        },
        "line.22": {
            "text": "Ist weiß natürlich weiß ist sie"
        },
        "line.23": {
            "text": "Natürlich sag ich weiß wie nie"
        },
        "line.24": {
            "text": "Ein Verhör ist das nicht wir sind"
        },
        "line.25": {
            "text": "Doch keine Polizei"
        },
        "line.26": {
            "text": "Ein Lichtbild brauch ich auf dem"
        },
        "line.27": {
            "text": "Muß ein Ohr zu sehen sein"
        },
        "line.28": {
            "text": "Bürokratism verstehn Sie ist überall"
        },
        "line.29": {
            "text": "Ob ich Faulkner liebe oder Hemingway"
        },
        "line.30": {
            "text": "Sandburg sag ich will doch"
        },
        "line.31": {
            "text": "Durch Chikago gehn"
        },
        "line.32": {
            "text": "Na dann wünscht die Miss"
        },
        "line.33": {
            "text": "Hoffentlich haben Sie Wetter."
        }
    }
}