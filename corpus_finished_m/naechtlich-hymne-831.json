{
    "title": "naechtlich-hymne-831",
    "gender": "m",
    "author": "Gerhard Rühm",
    "author_birth": "1930",
    "poem": {
        "line.1": {
            "text": "o nimm mich auf die matratze"
        },
        "line.2": {
            "text": "auf das linnen die kissen"
        },
        "line.3": {
            "text": "unter die decke"
        },
        "line.4": {
            "text": "mein bett"
        },
        "line.5": {
            "text": "o nimm mich doch"
        },
        "line.6": {
            "text": "nimm mich auf"
        },
        "line.7": {
            "text": "müde bin ich"
        },
        "line.8": {
            "text": "und werde mich schlaflos auf dir"
        },
        "line.9": {
            "text": "in dir wälzen"
        },
        "line.10": {
            "text": "bett o bett"
        },
        "line.11": {
            "text": "das du mich bettest"
        },
        "line.12": {
            "text": "um mich wenigstens ruhen zu lassen"
        },
        "line.13": {
            "text": "ich verkrieche mich in dich"
        },
        "line.14": {
            "text": "wie in den leib der mutter"
        },
        "line.15": {
            "text": "als sie noch lebte"
        },
        "line.16": {
            "text": "und ich fast noch in ihr war"
        },
        "line.17": {
            "text": "erde ist kein ersatz für dich"
        },
        "line.18": {
            "text": "o mutter"
        },
        "line.19": {
            "text": "kein ersatz für dich"
        },
        "line.20": {
            "text": "o bett"
        },
        "line.21": {
            "text": "ja ich spreche dich an"
        },
        "line.22": {
            "text": "als wärst du ein du"
        },
        "line.23": {
            "text": "das du das mich bettet"
        },
        "line.24": {
            "text": "deckt zudeckt"
        },
        "line.25": {
            "text": "aufnimmt"
        },
        "line.26": {
            "text": "irgendwo muß ich ja sein"
        },
        "line.27": {
            "text": "bei dir ist es warm"
        },
        "line.28": {
            "text": "wenigstens"
        },
        "line.29": {
            "text": "und als umhüllendes"
        },
        "line.30": {
            "text": "bist du mir nah genug"
        },
        "line.31": {
            "text": "wie andere auch"
        },
        "line.32": {
            "text": "mach ich dich"
        },
        "line.33": {
            "text": "zu meinem gefährten"
        },
        "line.34": {
            "text": "meiner gefährtin"
        },
        "line.35": {
            "text": "ach sei nicht so sächlich"
        },
        "line.36": {
            "text": "versinkend in dich"
        },
        "line.37": {
            "text": "erhebe ich dich in den stand"
        },
        "line.38": {
            "text": "süssester weiblichkeit"
        },
        "line.39": {
            "text": "o bett"
        },
        "line.40": {
            "text": "bettin"
        },
        "line.41": {
            "text": "entlohne mich dafür"
        },
        "line.42": {
            "text": "mit dem vergessen"
        },
        "line.43": {
            "text": "des schlafs"
        },
        "line.44": {
            "text": "der bewusstlosigkeit"
        }
    }
}