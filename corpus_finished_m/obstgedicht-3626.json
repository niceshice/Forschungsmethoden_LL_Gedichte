{
    "title": "obstgedicht-3626",
    "gender": "m",
    "author": "Bodo  Hell",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "Giuseppe Arcimboldo (dem Mailänder Maler am Prager Hof Rudolfs II. und"
        },
        "line.2": {
            "text": "Meister des Capriccios wie der Bild-Allegorie) zu Ehren"
        },
        "line.3": {
            "text": "(nach Appetit, Bedarf und Kenntnisstand sowie Beschaffungsmöglichkeit"
        },
        "line.4": {
            "text": "erweiterbar)"
        },
        "line.5": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.6": {
            "text": "verlang deine"
        },
        "line.7": {
            "text": "Äpfel/Aprikosen/Apfelsinen/Asperl/Avocados/Ananas"
        },
        "line.8": {
            "text": "Gold"
        },
        "line.9": {
            "text": "aus Costa Rica im Papier"
        },
        "line.10": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.11": {
            "text": "verlang deine"
        },
        "line.12": {
            "text": "Bananen/Birnen/Bromberen/Berlepsch"
        },
        "line.13": {
            "text": "und"
        },
        "line.14": {
            "text": "Boskop"
        },
        "line.15": {
            "text": "(eigentlich Schöner aus Boskoop, also nach dem"
        },
        "line.16": {
            "text": "gleichnamigen holländischen Ort) im Papier"
        },
        "line.17": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.18": {
            "text": "verlang deine"
        },
        "line.19": {
            "text": "Citrusfrüchte/Chriesi"
        },
        "line.20": {
            "text": "(d.i. schwyzerdütsch für"
        },
        "line.21": {
            "text": "cerises/Kirschen), deine"
        },
        "line.22": {
            "text": "Cox Orange/Corbezzole/Cherimoya"
        },
        "line.23": {
            "text": "(der"
        },
        "line.24": {
            "text": "Geschmack dieser Früchte erinnert an Schlagsahne mit Erdbeer, Banane"
        },
        "line.25": {
            "text": "und Ananas)/"
        },
        "line.26": {
            "text": "Carambolas"
        },
        "line.27": {
            "text": "aus Malaysia (in Scheiben"
        },
        "line.28": {
            "text": "quergeschnitten sehen sie hübsch wie Sterne aus) im Papier"
        },
        "line.29": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.30": {
            "text": "verlang deine"
        },
        "line.31": {
            "text": "Datteln/Dirndln/Drachenfrüchte"
        },
        "line.32": {
            "text": "(die nach wenig"
        },
        "line.33": {
            "text": "schmecken, aber umso mehr kosten) im Papier"
        },
        "line.34": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.35": {
            "text": "verlang deine"
        },
        "line.36": {
            "text": "Erdbeeren"
        },
        "line.37": {
            "text": "und"
        },
        "line.38": {
            "text": "Elsbeeren"
        },
        "line.39": {
            "text": ","
        },
        "line.40": {
            "text": "deine"
        },
        "line.41": {
            "text": "Erdnüsse"
        },
        "line.42": {
            "text": "und"
        },
        "line.43": {
            "text": "Elstar"
        },
        "line.44": {
            "text": "im Papier"
        },
        "line.45": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.46": {
            "text": "verlang deine"
        },
        "line.47": {
            "text": "Feigen"
        },
        "line.48": {
            "text": "/dein"
        },
        "line.49": {
            "text": "faules Obst"
        },
        "line.50": {
            "text": "im Papier"
        },
        "line.51": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.52": {
            "text": "verlang deine"
        },
        "line.53": {
            "text": "Granatäpfel/Gravensteiner/Gloster/Golden Delicious"
        },
        "line.54": {
            "text": "(=Gelber Köstlicher)/"
        },
        "line.55": {
            "text": "Granny Smith/Grapefruits/Guave"
        },
        "line.56": {
            "text": "und"
        },
        "line.57": {
            "text": "Gute Luise"
        },
        "line.58": {
            "text": "im PapierObst im Papiersack fault nicht/klebt"
        },
        "line.59": {
            "text": "nicht/riecht/nicht/näßt nicht"
        },
        "line.60": {
            "text": "verlang deinen"
        },
        "line.61": {
            "text": "Holler"
        },
        "line.62": {
            "text": ", deine"
        },
        "line.63": {
            "text": "Haselnüsse, Hagebutten"
        },
        "line.64": {
            "text": "und"
        },
        "line.65": {
            "text": "Heidelbeeren"
        },
        "line.66": {
            "text": "im Papier"
        },
        "line.67": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.68": {
            "text": "verlang deinen"
        },
        "line.69": {
            "text": "Ingrid-Marie-Apfel"
        },
        "line.70": {
            "text": "und deine"
        },
        "line.71": {
            "text": "Idared"
        },
        "line.72": {
            "text": "(sprich: eidared) im PapierObst im Papiersack fault nicht/klebt"
        },
        "line.73": {
            "text": "nicht/riecht nicht/näßt nicht"
        },
        "line.74": {
            "text": "verlang deine"
        },
        "line.75": {
            "text": "Jaffa Sweetie/Johannisbeeren/Josta/Jonathan"
        },
        "line.76": {
            "text": "und"
        },
        "line.77": {
            "text": "Jonagold"
        },
        "line.78": {
            "text": "im Papier"
        },
        "line.79": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.80": {
            "text": "verlang deine"
        },
        "line.81": {
            "text": "Kriecherl/Kirschen/Kiwi"
        },
        "line.82": {
            "text": "(meist unreif)/"
        },
        "line.83": {
            "text": "Khaki"
        },
        "line.84": {
            "text": "(bei uns nicht winterhart)/"
        },
        "line.85": {
            "text": "Kokosnüsse/Kornelkirschen"
        },
        "line.86": {
            "text": "(jenes"
        },
        "line.87": {
            "text": "vergessene Wildobst, nämlich ovale, rote, aromatisch-säuerliche Früchte"
        },
        "line.88": {
            "text": "auf halbhohen Büschen im Weidekuschelgelände, gelten in Salz eingelegt"
        },
        "line.89": {
            "text": "als Olivenersatz)/"
        },
        "line.90": {
            "text": "Klaräpfel/Kronprinz Rudolph"
        },
        "line.91": {
            "text": "und"
        },
        "line.92": {
            "text": "Krummstiel"
        },
        "line.93": {
            "text": "im Papier"
        },
        "line.94": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.95": {
            "text": "verlang deine"
        },
        "line.96": {
            "text": "Lederäpfel/Limonen/Limetten/Lychees"
        },
        "line.97": {
            "text": "aus"
        },
        "line.98": {
            "text": "Madagaskar (haselnuß- bis pflaumengroß, hochrot mit schuppiger Haut und"
        },
        "line.99": {
            "text": "nicht eßbarem Kern, weiß-glasigem Fruchtfleisch und süßsauerwürzigem"
        },
        "line.100": {
            "text": "Geschmack) im Papier"
        },
        "line.101": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.102": {
            "text": "verlang deine"
        },
        "line.103": {
            "text": "Melonen/Marillen/Maracujas/Macintosh/Maulbeeren"
        },
        "line.104": {
            "text": "(weiß oder schwarz, letztere"
        },
        "line.105": {
            "text": "wohlschmeckender)/"
        },
        "line.106": {
            "text": "Moosbeeren/Mangostinen&Mango"
        },
        "line.107": {
            "text": "s aus"
        },
        "line.108": {
            "text": "Brasilien (es sind das grünrote bis gelbliche ovale Früchte mit"
        },
        "line.109": {
            "text": "orangefarbenem, sehr aromatischem Fleisch um einen langgestreckten"
        },
        "line.110": {
            "text": "flachen Kern) im Papier"
        },
        "line.111": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.112": {
            "text": "verlang deine"
        },
        "line.113": {
            "text": "Nektarinen"
        },
        "line.114": {
            "text": "und deine"
        },
        "line.115": {
            "text": "Nespole"
        },
        "line.116": {
            "text": "aus"
        },
        "line.117": {
            "text": "Spanien, Italien, Griechenland oder der Türkei, verlang deine"
        },
        "line.118": {
            "text": "Naschis"
        },
        "line.119": {
            "text": "aus Israel (das sind grammatikalische Zwitter, nämlich"
        },
        "line.120": {
            "text": "eine Kreuzung zwischen dem Apfel und der Birne, schmecken etwas flau)"
        },
        "line.121": {
            "text": "im Papier Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.122": {
            "text": "verlang deine"
        },
        "line.123": {
            "text": "Orangen/Oliven/Opuntien"
        },
        "line.124": {
            "text": "(die Früchte des"
        },
        "line.125": {
            "text": "Feigenkaktusses, auch Ohrwaschlkaktus genannt, arabisch sabra, Vorsicht"
        },
        "line.126": {
            "text": "vor den haarfeinen Stacheln, sie lassen sich nur mit Nadel und Pinzette aus"
        },
        "line.127": {
            "text": "der Haut entfernen), verlang den sehr späten"
        },
        "line.128": {
            "text": "Ontarioapfel"
        },
        "line.129": {
            "text": "im Papier"
        },
        "line.130": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.131": {
            "text": "verlang deine"
        },
        "line.132": {
            "text": "Pflaumen/Papayas/Pampelmusen/Passionsfrüchte/Persimonen"
        },
        "line.133": {
            "text": "(eine bestimmte japanische Khaki, von Mönchen speziell gepflegt), deine"
        },
        "line.134": {
            "text": "Pomeranzen (siehe auch Apfelsinen, Mandarinen, Rudolfinen), deine"
        },
        "line.135": {
            "text": "Pomelos"
        },
        "line.136": {
            "text": "(ein crossing zwischen Grapefruits&Orangen), verlang"
        },
        "line.137": {
            "text": "deine"
        },
        "line.138": {
            "text": "Pfirsiche"
        },
        "line.139": {
            "text": "und"
        },
        "line.140": {
            "text": "Preiselbeeren"
        },
        "line.141": {
            "text": "im Papier"
        },
        "line.142": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.143": {
            "text": "verlang deine"
        },
        "line.144": {
            "text": "Quitten"
        },
        "line.145": {
            "text": "(siehe auch Quittenkäse&Quittengelee,"
        },
        "line.146": {
            "text": "man unterscheidet zwischen Quittenapfel und Quittenbirne, Quittenduft gilt"
        },
        "line.147": {
            "text": "als saturnisch) im Papier"
        },
        "line.148": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.149": {
            "text": "verlang deine"
        },
        "line.150": {
            "text": "Rosinen/Rosenäpfel/Red Star/Ribisel/Reineclauden"
        },
        "line.151": {
            "text": "ist gleich"
        },
        "line.152": {
            "text": "Ringlotten"
        },
        "line.153": {
            "text": "im Papier"
        },
        "line.154": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.155": {
            "text": "verlang deine"
        },
        "line.156": {
            "text": "Stachelbeeren/Schattenmorellen"
        },
        "line.157": {
            "text": "(keine Fisch-,"
        },
        "line.158": {
            "text": "sondern eine Sauerkirschenart) /"
        },
        "line.159": {
            "text": "Schafsnasen/Schlotteräpfel"
        },
        "line.160": {
            "text": "(Kerne schlottern im Gehäuse)/"
        },
        "line.161": {
            "text": "Schlehen/Sharonfrüchte"
        },
        "line.162": {
            "text": "(khakiähnlich, süß und kernlos), deine"
        },
        "line.163": {
            "text": "Speierlinge"
        },
        "line.164": {
            "text": "und"
        },
        "line.165": {
            "text": "Stinkfrüchte"
        },
        "line.166": {
            "text": "sowie deine"
        },
        "line.167": {
            "text": "Strudler"
        },
        "line.168": {
            "text": "(diese alte"
        },
        "line.169": {
            "text": "Apfelstrudelapfelsorte) im Papier"
        },
        "line.170": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.171": {
            "text": "verlang deine"
        },
        "line.172": {
            "text": "Tutti Frutti, Tamarillos"
        },
        "line.173": {
            "text": "(eiförmig und rot mit"
        },
        "line.174": {
            "text": "süßfruchtigem Aroma, das an Tomaten erinnert) und"
        },
        "line.175": {
            "text": "Tollkirschen"
        },
        "line.176": {
            "text": "im Papier"
        },
        "line.177": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.178": {
            "text": "verlang deine"
        },
        "line.179": {
            "text": "UmeboshiPflaumen"
        },
        "line.180": {
            "text": "(aus Japan) und deine"
        },
        "line.181": {
            "text": "UhudlerTrauben"
        },
        "line.182": {
            "text": "(aus dem Burgenland) im Papier"
        },
        "line.183": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.184": {
            "text": "verlang deine"
        },
        "line.185": {
            "text": "Vogelbeeren"
        },
        "line.186": {
            "text": "und"
        },
        "line.187": {
            "text": "Vogelkirschen"
        },
        "line.188": {
            "text": "sowie die"
        },
        "line.189": {
            "text": "kandierten"
        },
        "line.190": {
            "text": "Veilchen"
        },
        "line.191": {
            "text": "im Papier"
        },
        "line.192": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.193": {
            "text": "verlang deine"
        },
        "line.194": {
            "text": "Wassermelonen/Weichseln/Weintrauben/Williamsbirnen"
        },
        "line.195": {
            "text": "und"
        },
        "line.196": {
            "text": "Wacholderbeeren"
        },
        "line.197": {
            "text": "im Papier"
        },
        "line.198": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.199": {
            "text": "verlang deine"
        },
        "line.200": {
            "text": "X"
        },
        "line.201": {
            "text": "- hamma nix im Papier"
        },
        "line.202": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.203": {
            "text": "verlang deine"
        },
        "line.204": {
            "text": "Ysopwurzel"
        },
        "line.205": {
            "text": "im Papier"
        },
        "line.206": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.207": {
            "text": "verlang deine"
        },
        "line.208": {
            "text": "Zichorien/Zitronen/Zuckermelonen,"
        },
        "line.209": {
            "text": "verlang deine"
        },
        "line.210": {
            "text": "Zwetsche"
        },
        "line.211": {
            "text": "/deine"
        },
        "line.212": {
            "text": "Zwetschge"
        },
        "line.213": {
            "text": "/deine"
        },
        "line.214": {
            "text": "Zwetschke"
        },
        "line.215": {
            "text": "(österr. nur so, nämlich Zwetschke) im Papier"
        },
        "line.216": {
            "text": "Obst im Papiersack fault nicht/klebt nicht/riecht nicht/näßt nicht"
        },
        "line.217": {
            "text": "Papiersack........................reißt"
        }
    }
}