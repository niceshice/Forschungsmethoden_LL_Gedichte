{
    "title": "naturgedicht-7834",
    "gender": "m",
    "author": "Reiner Kunze",
    "author_birth": "1933",
    "poem": {
        "line.1": {
            "text": "Die dinge hören nur, wenn du sie rufst"
        },
        "line.2": {
            "text": "bei ihrem wahren namen"
        },
        "line.3": {
            "text": "Getäuscht sein will allein"
        },
        "line.4": {
            "text": "der mensch"
        },
        "line.5": {
            "text": "Er täuscht sich"
        },
        "line.6": {
            "text": "aus der welt hinaus, die dinge"
        },
        "line.7": {
            "text": "kennen kein verzeihn"
        }
    }
}