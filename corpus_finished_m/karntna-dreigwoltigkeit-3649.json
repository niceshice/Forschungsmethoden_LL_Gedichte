{
    "title": "karntna-dreigwoltigkeit-3649",
    "gender": "m",
    "author": "Axel  Karner",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "de goschn"
        },
        "line.2": {
            "text": "soll i holtn"
        },
        "line.3": {
            "text": "hobns gsogt"
        },
        "line.4": {
            "text": "i hätt do nix valuan"
        },
        "line.5": {
            "text": "i war an ondra"
        },
        "line.6": {
            "text": "nit ana von senen"
        },
        "line.7": {
            "text": "na hamkemmen"
        },
        "line.8": {
            "text": "brauch i neama"
        },
        "line.9": {
            "text": "weil"
        },
        "line.10": {
            "text": "gschegn is bold wos"
        },
        "line.11": {
            "text": "bist nit stüll"
        },
        "line.12": {
            "text": "weast es sicha"
        },
        "line.13": {
            "text": "mia dawischn di noch"
        },
        "line.14": {
            "text": "mia dawischn di sowieso"
        },
        "line.15": {
            "text": "sogns ma"
        },
        "line.16": {
            "text": "jedn tog"
        },
        "line.17": {
            "text": "- - - - -"
        },
        "line.18": {
            "text": "kärntner dreigewaltigkeit"
        },
        "line.19": {
            "text": "das maul"
        },
        "line.20": {
            "text": "soll ich halten"
        },
        "line.21": {
            "text": "haben sie gesagt"
        },
        "line.22": {
            "text": "ich hätte hier nichts verloren"
        },
        "line.23": {
            "text": "ich wäre ein anderer"
        },
        "line.24": {
            "text": "nicht einer von ihnen"
        },
        "line.25": {
            "text": "nein heimkommen"
        },
        "line.26": {
            "text": "brauche ich nicht mehr"
        },
        "line.27": {
            "text": "weil"
        },
        "line.28": {
            "text": "geschehen ist bald etwas"
        },
        "line.29": {
            "text": "bist du nicht still"
        },
        "line.30": {
            "text": "wirst du es sicher"
        },
        "line.31": {
            "text": "wir erwischen dich noch"
        },
        "line.32": {
            "text": "wir erwischen dich sowieso"
        },
        "line.33": {
            "text": "sagen sie mir"
        },
        "line.34": {
            "text": "jeden tag"
        }
    }
}