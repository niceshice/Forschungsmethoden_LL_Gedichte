    "poem": {
        "line.1": {
            "text": "neunundzwanzigsten mai explorer 49"
        },
        "line.2": {
            "text": "mehrhundert meter metallspinne mehrhundert meter"
        },
        "line.3": {
            "text": "metallspinne magnetfeld"
        },
        "line.4": {
            "text": "fühler magnetfeld"
        },
        "line.5": {
            "text": ""
        },
        "line.6": {
            "text": "zwölften juni wird helmut kohl zum vorsitzenden der CDU gewählt"
        },
        "line.7": {
            "text": ""
        },
        "line.8": {
            "text": "vierundzwanzigsten juni explorer 35"
        },
        "line.9": {
            "text": "alter satellit noch aus 67"
        },
        "line.10": {
            "text": ""
        },
        "line.11": {
            "text": "die blüte öffnet sich"
        },
        "line.12": {
            "text": "wir atmen"
        },
        "line.13": {
            "text": "die blüte schliesst sich"
        },
        "line.14": {
            "text": ""
        },
        "line.15": {
            "text": "explorer 49"
        },
        "line.16": {
            "text": "helmut kohl"
        },
        "line.17": {
            "text": "explorer 35"
        },
        "line.18": {
            "text": ""
        },
        "line.19": {
            "text": "magnetfeld magnetfeld magnet"
        },
        "line.20": {
            "text": "feld"
        }
    }
