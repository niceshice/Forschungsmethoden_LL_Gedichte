{
    "title": "allgemeiner-nachrichtendienst-4572",
    "gender": "m",
    "author": "Lutz Seiler",
    "author_birth": "1963",
    "poem": {
        "line.1": {
            "text": "etwas hier unten klingt wie trocken"
        },
        "line.2": {
            "text": "moos den boden überspannt & deine"
        },
        "line.3": {
            "text": "schritte klopfen an. etwas in den bäumen"
        },
        "line.4": {
            "text": "ist von wolkendeutschen helden, die"
        },
        "line.5": {
            "text": "ihre schultern oben aneinander legen"
        },
        "line.6": {
            "text": "im geräusch. & etwas"
        },
        "line.7": {
            "text": "dehnt vom anschlag her"
        },
        "line.8": {
            "text": "der schaukel sich ins wunschgerüst"
        },
        "line.9": {
            "text": "hinter türen, hinters licht. alles"
        },
        "line.10": {
            "text": "ist wies scheint"
        },
        "line.11": {
            "text": "verkleinert unterwegs; es heißt:"
        },
        "line.12": {
            "text": "der ist doch aus der schweiz!"
        },
        "line.13": {
            "text": "nein, der ist gestorben."
        }
    }
}