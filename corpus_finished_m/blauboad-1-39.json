{
    "title": "blauboad-1-39",
    "gender": "m",
    "author": "H. C. Artmann",
    "author_birth": "1921",
    "poem": {
        "line.1": {
            "text": "i bin a ringlgschbüübsizza"
        },
        "line.2": {
            "text": "und hob scho sim weiwa daschlong"
        },
        "line.3": {
            "text": "und eanare gebeina"
        },
        "line.4": {
            "text": "untan schlofzimabon fagrom.."
        },
        "line.5": {
            "text": "heit lod i ma r ei di ochte"
        },
        "line.6": {
            "text": "zu einen libesdraum"
        },
        "line.7": {
            "text": "daun schdöl i owa s oaschestrion ei"
        },
        "line.8": {
            "text": "und bek s me n hakal zaum!"
        },
        "line.9": {
            "text": "so fafoa r e med ole maln"
        },
        "line.10": {
            "text": "wäu ma d easchte en gschdis hod gem"
        },
        "line.11": {
            "text": "das s mii amoe darwischn wean"
        },
        "line.12": {
            "text": "doss wiad kar mendsch darlem!"
        },
        "line.13": {
            "text": "i bin a ringlgschbüübsizza"
        },
        "line.14": {
            "text": "(und schlof en da nocht nua bein liacht"
        },
        "line.15": {
            "text": "wäu i mi waun s so finzta is"
        },
        "line.16": {
            "text": "fua de dodn weiwa fiacht..)"
        }
    }
}