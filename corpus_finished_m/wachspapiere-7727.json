{
    "title": "wachspapiere-7727",
    "gender": "m",
    "author": "Christian Steinbacher",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "Wachspapiere"
        },
        "line.2": {
            "text": "frisch aus Hessen"
        },
        "line.3": {
            "text": "Schlagschüsseln aus bestem Kupfer"
        },
        "line.4": {
            "text": "Levantinischer Naturschwamm"
        },
        "line.5": {
            "text": "Gästetuch aus Zwirnfrottier"
        },
        "line.6": {
            "text": "Alles edel, alles Feinsinn"
        },
        "line.7": {
            "text": "O mein Tontopf aus dem Elsass"
        },
        "line.8": {
            "text": "O ihr Sägen frisch aus Japan"
        },
        "line.9": {
            "text": "Pflanzhacken auch aus Fernost nur"
        },
        "line.10": {
            "text": "Lorbeerseifen aus Aleppo"
        },
        "line.11": {
            "text": "Niedersächsische Sattelseifen"
        },
        "line.12": {
            "text": "Schwäbische Wacholderspäne"
        },
        "line.13": {
            "text": "Eichenholz für Stiefelknechte"
        },
        "line.14": {
            "text": "Westfälische Abflusssiebe"
        },
        "line.15": {
            "text": "Edelstahl für Vierkantreiben"
        },
        "line.16": {
            "text": "Appenzeller Taschentücher"
        },
        "line.17": {
            "text": "Sraußenfedern für die Wedel"
        },
        "line.18": {
            "text": "Ziegenhaar fürn Feinstaubbesen"
        },
        "line.19": {
            "text": "Büffelhorn für Trillerpfeifen"
        },
        "line.20": {
            "text": "Gartenbindegarn aus Schottland"
        },
        "line.21": {
            "text": "Somelierschürzen aus Frankreich"
        },
        "line.22": {
            "text": "Und du Lederfett aus Salzburg"
        },
        "line.23": {
            "text": "Konfektionen, rares Uhrwerk"
        },
        "line.24": {
            "text": "Wo mein Türstopper geschnitzt wird"
        },
        "line.25": {
            "text": "Du mein Deodorant in petto"
        },
        "line.26": {
            "text": "Mein Barett, mein Kofferkuli,"
        },
        "line.27": {
            "text": "Du mein Treffpunkt, du mein Schreibzeug"
        },
        "line.28": {
            "text": "Du mein Hochzeitsveilchenbuschen"
        },
        "line.29": {
            "text": "Du mein o du o du mein"
        },
        "line.30": {
            "text": "Dein Gesinde, hier mein Bachbett"
        },
        "line.31": {
            "text": "Mein Revier, dein Pfeifenputzer"
        },
        "line.32": {
            "text": "Und was sonst als Span mir zufiel"
        },
        "line.33": {
            "text": "Ein Lavoir umrankt von Bimsstein"
        },
        "line.34": {
            "text": "Immer noch ne Babydecke"
        },
        "line.35": {
            "text": "Immer noch ein Fotoalbum"
        },
        "line.36": {
            "text": "Immer noch der alte Schleier"
        },
        "line.37": {
            "text": "Immer noch die alte Mütze"
        },
        "line.38": {
            "text": "Aufbewahrung und Versteckspiel"
        },
        "line.39": {
            "text": "Wann sei was heranzuziehen"
        },
        "line.40": {
            "text": "Trockenblumen, Rosen, Zweige"
        },
        "line.41": {
            "text": "Etiketten frisch vom Fallobst"
        },
        "line.42": {
            "text": "Leere Dosen, Hülsen, Schachteln"
        },
        "line.43": {
            "text": "Hosengurt, Spendierattrappen"
        },
        "line.44": {
            "text": "Batterien für mein Glücksschwein"
        },
        "line.45": {
            "text": "Lilienporzellan für Gäste"
        },
        "line.46": {
            "text": "Reste, Scherben, alte Pullis"
        },
        "line.47": {
            "text": "Funktionslose Gerätschaft"
        },
        "line.48": {
            "text": "Vormals Tisch- und heute Stuhlbein"
        },
        "line.49": {
            "text": "Einzel- und Erinnerungstücke"
        },
        "line.50": {
            "text": "Du mein Türschild, du mein Spanner"
        },
        "line.51": {
            "text": "Und ihr Gummibänder aus Arkansas"
        },
        "line.52": {
            "text": "Und ihr Whiskybecher aus der Lausitz"
        },
        "line.53": {
            "text": "Ein Geschenk ist keine Spende"
        },
        "line.54": {
            "text": "Käsereiben aus Italien"
        },
        "line.55": {
            "text": "Finnländische Waschhandschuhe"
        },
        "line.56": {
            "text": "Alles bestens, alles echt hier"
        },
        "line.57": {
            "text": "Krauseminze nur aus Frankfurt"
        },
        "line.58": {
            "text": "Fußmatten frisch aus der Eifel"
        },
        "line.59": {
            "text": "Stahlstecknadeln nur mit Saummaß"
        },
        "line.60": {
            "text": "Seifenblock mit Schneidedraht nur"
        },
        "line.61": {
            "text": "Und aus Duroplast die Schüssel"
        },
        "line.62": {
            "text": "Filzpantoffel aus dem Etschtal"
        },
        "line.63": {
            "text": "Zimmerbesen nur aus Rosshaar"
        },
        "line.64": {
            "text": "Sieh mein Stofftier, sieh den Abfall"
        },
        "line.65": {
            "text": "In der Falle, auf der Bürste"
        },
        "line.66": {
            "text": "Mein o mein o mein o mein du"
        },
        "line.67": {
            "text": "Und auf Lebzeit, alles hält dir"
        }
    }
}