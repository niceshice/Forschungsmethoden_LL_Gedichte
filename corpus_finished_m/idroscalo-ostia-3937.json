{
    "title": "idroscalo-ostia-3937",
    "gender": "m",
    "author": "Dieter M. Gräf",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "die Schlitzseite schon weg"
        },
        "line.2": {
            "text": "geyachtet: Mall,"
        },
        "line.3": {
            "text": "über der der Hamburger"
        },
        "line.4": {
            "text": "steht. Hier"
        },
        "line.5": {
            "text": "blutet das Ketchup."
        },
        "line.6": {
            "text": "Noch unbehelligtes"
        },
        "line.7": {
            "text": "Zitat einer Nische mit"
        },
        "line.8": {
            "text": "blauer Madonna,"
        },
        "line.9": {
            "text": "ihr dürrer Ständer"
        },
        "line.10": {
            "text": "kaktus, davor; dann"
        },
        "line.11": {
            "text": "doch herunter"
        },
        "line.12": {
            "text": "gelassene Hose der Land"
        },
        "line.13": {
            "text": "schaft, beim auf"
        },
        "line.14": {
            "text": "gelösten Fuß"
        },
        "line.15": {
            "text": "ballfeld, hinter dem"
        },
        "line.16": {
            "text": "auch der Tiber"
        },
        "line.17": {
            "text": "(Für Benot Gran)"
        }
    }
}