{
    "title": "gelsenkirchen-795",
    "gender": "m",
    "author": "Michael Lentz",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "eine alte frau ein gehgestell"
        },
        "line.2": {
            "text": "straßenrand und blanke taube"
        },
        "line.3": {
            "text": "da rollt das gehgestell mal hin die alte frau"
        },
        "line.4": {
            "text": "und mit dem schuh der schlecht zu fuß"
        },
        "line.5": {
            "text": "wird da mal drangegangen"
        },
        "line.6": {
            "text": "mal so nach links gewälzt"
        },
        "line.7": {
            "text": "und angetippt mal so nach rechts"
        },
        "line.8": {
            "text": "der hohle vogel und schon"
        },
        "line.9": {
            "text": "fliegen drin"
        },
        "line.10": {
            "text": "ach so!"
        },
        "line.11": {
            "text": "kaputt"
        },
        "line.12": {
            "text": "na wirklich nicht"
        },
        "line.13": {
            "text": "über die brücke da rast dieser zug"
        },
        "line.14": {
            "text": "und dann?"
        }
    }
}