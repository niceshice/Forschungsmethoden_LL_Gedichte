            "text": "der minister bedauert derartige äusserungen"
        },
        "line.3": {
            "text": "der minister bedauert derartige äusserungen"
        },
        "line.4": {
            "text": "der minister bedauert derartige äusserungen"
        },
        "line.5": {
            "text": "er minister bedauert derartige äusserungen"
        },
        "line.6": {
            "text": "einister bedauert derartige äusserungen"
        },
        "line.7": {
            "text": "ei ister bedauert derartige äusserungen"
        },
        "line.8": {
            "text": "ei ier bedauert derartige äusserungen"
        },
        "line.9": {
            "text": "ei ieedauert derartige äusserungen"
        },
        "line.10": {
            "text": "ei iee auert derartige äusserungen"
        },
        "line.11": {
            "text": "ei iee aueerartige äusserungen"
        },
        "line.12": {
            "text": "ei iee auee artige äusserungen"
        },
        "line.13": {
            "text": "ei iee aueea ige äusserungen"
        },
        "line.14": {
            "text": "ei iee auee ai e äusserungen"
        },
        "line.15": {
            "text": "ei iee auee ai e äuerungen"
        },
        "line.16": {
            "text": "ei iee auee ai e äue ungen"
        },
        "line.17": {
            "text": "ei iee auee ai e äue uen"
        },
        "line.18": {
            "text": "ei iee auee ai e äue ue"
        },
        "line.19": {
            "text": "usw."
        }
    }
