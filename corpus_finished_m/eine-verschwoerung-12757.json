{
    "title": "eine-verschwoerung-12757",
    "gender": "m",
    "author": "F. W. Bernstein",
    "author_birth": "1938",
    "poem": {
        "line.1": {
            "text": "Manche Männer tragen rote Hosen"
        },
        "line.2": {
            "text": "öffentlich im unserer Stadt,"
        },
        "line.3": {
            "text": "und wir kommen zu der großen"
        },
        "line.4": {
            "text": "Frage, welchen Sinn das hat."
        },
        "line.5": {
            "text": "Nein-kein Hosenstall steht offen;"
        },
        "line.6": {
            "text": "keiner kratzt sich KRATZ im Schritt;"
        },
        "line.7": {
            "text": "ältere Männer -nicht besoffen-"
        },
        "line.8": {
            "text": "wer macht warum wozu mit?"
        },
        "line.9": {
            "text": "Hosenrot - das ist das Zeichen!"
        },
        "line.10": {
            "text": "Rote Hosen Weltherrschaft!"
        },
        "line.11": {
            "text": "Ob sie das dies Jahr erreichen'"
        },
        "line.12": {
            "text": "Zweifelhaft!"
        }
    }
}