{
    "title": "fuer-karajan-und-andere-345",
    "gender": "m",
    "author": "Hans Magnus Enzensberger",
    "author_birth": "1929",
    "poem": {
        "line.1": {
            "text": "Drei Männer in steifen Hüten"
        },
        "line.2": {
            "text": "vor dem Kiewer Hauptbahnhof"
        },
        "line.3": {
            "text": "Posaune, Ziehharmonika, Saxophon"
        },
        "line.4": {
            "text": "im Dunst der Oktobernacht,"
        },
        "line.5": {
            "text": "die zwischen zwei Zügen zaudert,"
        },
        "line.6": {
            "text": "zwischen Katastrophe und Katastrophe:"
        },
        "line.7": {
            "text": "vor Ermüdeten spielen sie, die voll Andacht"
        },
        "line.8": {
            "text": "in ihre warmen Piroggen beißen"
        },
        "line.9": {
            "text": "und warten, warten"
        },
        "line.10": {
            "text": "ergreifende Melodien, abgetragen"
        },
        "line.11": {
            "text": "wie ihre Jacken und speckig"
        },
        "line.12": {
            "text": "wie ihre Hüte, und wenn Sie da"
        },
        "line.13": {
            "text": "fröstelnd gestanden wären unter Trinkern,"
        },
        "line.14": {
            "text": "Veteranen, Taschendieben,"
        },
        "line.15": {
            "text": "Sie hätten mir recht gegeben:"
        },
        "line.16": {
            "text": "Salzburg, Bayreuth und die Scala"
        },
        "line.17": {
            "text": "haben dem Bahnhof von Kiew"
        },
        "line.18": {
            "text": "wenig, sehr wenig voraus."
        }
    }
}