{
    "title": "der-vogel-schmerz-7827",
    "gender": "m",
    "author": "Reiner Kunze",
    "author_birth": "1933",
    "poem": {
        "line.1": {
            "text": "Nun bin ich dreißig jahre alt"
        },
        "line.2": {
            "text": "und kenne Deutschland nicht:"
        },
        "line.3": {
            "text": "Die grenzaxt fällt in Deutschland wald"
        },
        "line.4": {
            "text": "O land, das auseinanderbricht"
        },
        "line.5": {
            "text": "im menschen"
        },
        "line.6": {
            "text": "Und alle brücken treiben pfeilerlos"
        },
        "line.7": {
            "text": "Gedicht, steig auf, flieg himmelwärts!"
        },
        "line.8": {
            "text": "Steig auf, gedicht, und sei"
        },
        "line.9": {
            "text": "der vogel Schmerz"
        }
    }
}