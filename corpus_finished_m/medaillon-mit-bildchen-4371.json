{
    "title": "medaillon-mit-bildchen-4371",
    "gender": "m",
    "author": "Christian Schloyer",
    "author_birth": "1976",
    "poem": {
        "line.1": {
            "text": "das kleine haus"
        },
        "line.2": {
            "text": "vermisse ich das kleine haus das nie"
        },
        "line.3": {
            "text": "schläft das kleine haus das sich um seine"
        },
        "line.4": {
            "text": "pferdchen kümmert + kümmert + kümmert"
        },
        "line.5": {
            "text": "das kleine haus"
        },
        "line.6": {
            "text": "ich zähle drei monde, ene mene mu drei"
        },
        "line.7": {
            "text": "sicheln drei lächelnde sicheln"
        },
        "line.8": {
            "text": "das kleine haus unter den"
        },
        "line.9": {
            "text": "sicheln"
        },
        "line.10": {
            "text": "frau maria ist ein felsmassiv, sie wickelt sie gebiert sie lächelt"
        },
        "line.11": {
            "text": "ein kind, die beiden sind eins sind zwei"
        },
        "line.12": {
            "text": "köpfige mitternachtssonnen ineinanderge-"
        },
        "line.13": {
            "text": "säugt vor"
        },
        "line.14": {
            "text": "einem anderen felsmassiv"
        },
        "line.15": {
            "text": "rechts ein wald mit fabrik auf einem"
        },
        "line.16": {
            "text": "unsinnig hohen pappkarton"
        },
        "line.17": {
            "text": "werden auf- + untergänge simuliert, die tage"
        },
        "line.18": {
            "text": "unterscheiden sich"
        },
        "line.19": {
            "text": "frau maria ist ein pferd sie gebiert wie eine"
        },
        "line.20": {
            "text": "fabrik an ihrem einzigen"
        },
        "line.21": {
            "text": "lächeln gebiert sie seit tagen"
        },
        "line.22": {
            "text": "das kleine haus"
        }
    }
}