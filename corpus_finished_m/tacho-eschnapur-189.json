{
    "title": "tacho-eschnapur-189",
    "gender": "m",
    "author": "Oskar Pastior",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "mach ernst mach mach"
        },
        "line.2": {
            "text": "mach überschall vitel-"
        },
        "line.3": {
            "text": "lo tubs mach ungenau"
        },
        "line.4": {
            "text": "gemach mach straub mach"
        },
        "line.5": {
            "text": "futsch provisorat mach"
        },
        "line.6": {
            "text": "ususlasch mich wachtel-"
        },
        "line.7": {
            "text": "fett und damenmatt mach"
        },
        "line.8": {
            "text": "sachen ach am fachwerk-"
        },
        "line.9": {
            "text": "dach und das mit wucht"
        },
        "line.10": {
            "text": "daß tacheles emedokles"
        },
        "line.11": {
            "text": "rachmaninov concordia im"
        },
        "line.12": {
            "text": "tagebau vom stapel clinch"
        },
        "line.13": {
            "text": "mach fluntsch mach krall"
        },
        "line.14": {
            "text": "mach rachengold mach ro-"
        },
        "line.15": {
            "text": "chenkrach mach doch im"
        },
        "line.16": {
            "text": "schacht den schlauch von"
        },
        "line.17": {
            "text": "achternach bis chur zu-"
        },
        "line.18": {
            "text": "gleich dem nachbar auch"
        },
        "line.19": {
            "text": "saug mich in dein know-how"
        },
        "line.20": {
            "text": "strichnin krauch mir zum"
        },
        "line.21": {
            "text": "trachtenball falun rauch"
        },
        "line.22": {
            "text": "schluchtensud am techtel-"
        },
        "line.23": {
            "text": "mech stauch mir den ruch"
        },
        "line.24": {
            "text": "im spachtelholm klau mir"
        },
        "line.25": {
            "text": "den lauschkordon am apfel-"
        },
        "line.26": {
            "text": "strauch und tatsch den"
        },
        "line.27": {
            "text": "ramses noch von cheops"
        },
        "line.28": {
            "text": "mach mandelbrot mach stau"
        },
        "line.29": {
            "text": "mach mir das mumientuch am"
        },
        "line.30": {
            "text": "überhang zum fuchtelbruch"
        },
        "line.31": {
            "text": "im achsendrall synapsen-"
        },
        "line.32": {
            "text": "flau mach huch das tor zum"
        },
        "line.33": {
            "text": "wal ganz loch mach einmal"
        },
        "line.34": {
            "text": "noch die grachten schmal"
        },
        "line.35": {
            "text": "ach tafelblatt streng am ag-"
        },
        "line.36": {
            "text": "nello mir den tubus flach"
        }
    }
}