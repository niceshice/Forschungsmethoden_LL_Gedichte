{
    "title": "yesterdays-12351",
    "gender": "m",
    "author": "David  Krause",
    "author_birth": "1988",
    "poem": {
        "line.1": {
            "text": "Für Rolf Dieter Brinkmann"
        },
        "line.2": {
            "text": "noch immer gibt es baugerüste"
        },
        "line.3": {
            "text": "auf die der schnee fällt immer noch"
        },
        "line.4": {
            "text": "einen architekten voller liebe"
        },
        "line.5": {
            "text": "zum detail vor seinen plänen"
        },
        "line.6": {
            "text": "freitagabends in new york"
        },
        "line.7": {
            "text": "anfang des 21. jahrhunderts"
        },
        "line.8": {
            "text": "im büro so voller schwindel"
        },
        "line.9": {
            "text": "von dem schnee vorm fenster schnee"
        },
        "line.10": {
            "text": "der am ground zero niedergeht schnee"
        },
        "line.11": {
            "text": "von dem die menschen sagen"
        },
        "line.12": {
            "text": "er sei anders nach dem falling man"
        },
        "line.13": {
            "text": "eine ins leere gehende geste"
        },
        "line.14": {
            "text": "er ist im lächeln einer frau"
        },
        "line.15": {
            "text": "die in madrid am bahnhof wartet"
        },
        "line.16": {
            "text": "schnee in ihrem schwarzen haar"
        },
        "line.17": {
            "text": "kurz denkt ein mann an sterne"
        },
        "line.18": {
            "text": "bis sie im zug verschwindet und mit ihrem gang"
        },
        "line.19": {
            "text": "lennie tristano zu spielen beginnt"
        },
        "line.20": {
            "text": "auf mp3 komprimiert"
        },
        "line.21": {
            "text": "jazz und schnee: das passt ja"
        },
        "line.22": {
            "text": "tristano spielt die flocken forte"
        },
        "line.23": {
            "text": "gegen ein londoner schaufenster dann"
        },
        "line.24": {
            "text": "ein leuchtendes legato ins"
        },
        "line.25": {
            "text": "moll ins dunkel einer"
        },
        "line.26": {
            "text": "wohnung in der innenstadt aus"
        },
        "line.27": {
            "text": "offnem fenster schaut der schläfer"
        },
        "line.28": {
            "text": "ein letztes mal"
        },
        "line.29": {
            "text": "schnee fällt"
        },
        "line.30": {
            "text": "an der wall street fällt"
        },
        "line.31": {
            "text": "an den botschaften schnee überschreitet"
        },
        "line.32": {
            "text": "ohne pass die grenzen der länder und"
        },
        "line.33": {
            "text": "ohne uhr die grenzen der stunden"
        },
        "line.34": {
            "text": "er senkt sich im central park"
        },
        "line.35": {
            "text": "auf einen veteranen"
        },
        "line.36": {
            "text": "der solange die spritze hält"
        },
        "line.37": {
            "text": "bis der schnee sie löst"
        },
        "line.38": {
            "text": "auch die schüsse und das feuer"
        },
        "line.39": {
            "text": "wie still es im schnee ist denkt er noch"
        },
        "line.40": {
            "text": "und wie weit doch diese nacht!"
        },
        "line.41": {
            "text": "ein offener band gedichte"
        },
        "line.42": {
            "text": "liegt auf der straße nach und nach"
        },
        "line.43": {
            "text": "zerfließen alle verse"
        },
        "line.44": {
            "text": "und davor steht der zitternde träumer"
        },
        "line.45": {
            "text": "barfuß und schaltet"
        },
        "line.46": {
            "text": "die schwer gewordene kamera aus"
        },
        "line.47": {
            "text": "er geht heim und all der schnee"
        },
        "line.48": {
            "text": "bedeutet nichts mehr"
        },
        "line.49": {
            "text": "nicht die dinge auf die er fällt"
        },
        "line.50": {
            "text": "oder die menschen in ihm"
        },
        "line.51": {
            "text": "und es gibt keinen film mehr"
        },
        "line.52": {
            "text": "keine strömung kein band"
        },
        "line.53": {
            "text": "das im inneren fließt"
        },
        "line.54": {
            "text": "es gibt keinen träumer mehr"
        },
        "line.55": {
            "text": "im gedicht"
        },
        "line.56": {
            "text": "wenn sich die türe schließt."
        },
        "line.57": {
            "text": "____"
        },
        "line.58": {
            "text": "yesterdays: Ein Lied des Cool-Jazz-Pianisten Lennie Tristano."
        }
    }
}