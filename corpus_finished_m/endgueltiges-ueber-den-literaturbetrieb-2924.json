{
    "title": "endgueltiges-ueber-den-literaturbetrieb-2924",
    "gender": "m",
    "author": "Christian Ide  Hintze",
    "author_birth": "1953",
    "poem": {
        "line.1": {
            "text": "Wien, April 1985"
        },
        "line.2": {
            "text": "Da ratsch di huiz, da ratsch di huiz,"
        },
        "line.3": {
            "text": "da Edakteur is hultsch di muiz,"
        },
        "line.4": {
            "text": "da Edakteur zaagt opfi zaa;"
        },
        "line.5": {
            "text": "oh opfi zaa di muiz di huiz"
        },
        "line.6": {
            "text": "Da Utor pfemmt so Rift di muiz,"
        },
        "line.7": {
            "text": "da Reibtisch eglt owa,"
        },
        "line.8": {
            "text": "da Utor zislt s√∂wa hatsch,"
        },
        "line.9": {
            "text": "i ratschu ratsch mi zeega."
        },
        "line.10": {
            "text": "Da Ressere so liliha,"
        },
        "line.11": {
            "text": "Erleger eintsch inzola,"
        },
        "line.12": {
            "text": "Erleger uzlt pressmi au,"
        },
        "line.13": {
            "text": "Erleger eslt ampfm."
        },
        "line.14": {
            "text": "Da sepffa netsch inonaha,"
        },
        "line.15": {
            "text": "rrazunkl maiss no aizza,"
        },
        "line.16": {
            "text": "da Ichter sakra olm di huiz,"
        },
        "line.17": {
            "text": "da Ichter netsch inona."
        },
        "line.18": {
            "text": "Solpetama, solpetama,"
        },
        "line.19": {
            "text": "grollt mosche klebbst mi fafa,"
        },
        "line.20": {
            "text": "grabbscht Ezzesion mi neema gfrast,"
        },
        "line.21": {
            "text": "grabbscht Ernsehen schbeibt mi esl."
        },
        "line.22": {
            "text": "Sol zwugl mella hauzili,"
        },
        "line.23": {
            "text": "sol zwugl mella hoschn,"
        },
        "line.24": {
            "text": "krawutig ebn Ruckerei,"
        },
        "line.25": {
            "text": "sma molle esa uffl,"
        },
        "line.26": {
            "text": "wo alle Leser uffln"
        }
    }
}