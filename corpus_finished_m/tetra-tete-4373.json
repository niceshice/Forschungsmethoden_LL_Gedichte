{
    "title": "tetra-tete-4373",
    "gender": "m",
    "author": "Christian Schloyer",
    "author_birth": "1976",
    "poem": {
        "line.1": {
            "text": "auf spuren schwarzer schnecken"
        },
        "line.2": {
            "text": "münder streich ich chromoplastik"
        },
        "line.3": {
            "text": "stricher schleich ich"
        },
        "line.4": {
            "text": "(unter zuckerzusatz) heimlich"
        },
        "line.5": {
            "text": "meinen heimweg heim"
        },
        "line.6": {
            "text": "unverdunkelt wie geschliffen tritt"
        },
        "line.7": {
            "text": "aus dem geheimbund"
        },
        "line.8": {
            "text": "schatten hinter der laterne mir"
        },
        "line.9": {
            "text": "der antisept entgegen, starrt mich"
        },
        "line.10": {
            "text": "aus der plastiktüte unverhohlen an das mehrweg"
        },
        "line.11": {
            "text": "aug (schon grau) es stiert"
        },
        "line.12": {
            "text": "gebrochen  doch ich bleibe"
        },
        "line.13": {
            "text": "ungeniert (hab nicht einmal"
        },
        "line.14": {
            "text": "daran gerochen)"
        }
    }
}