{
    "title": "fast-haette-ich-ihn-uebersehen-den-zweig-11478",
    "gender": "m",
    "author": "Franz Dodel",
    "author_birth": "1949",
    "poem": {
        "line.1": {
            "text": "fast hätte ich ihn"
        },
        "line.2": {
            "text": "übersehen den Zweig der"
        },
        "line.3": {
            "text": "sich in den Weg neigt"
        },
        "line.4": {
            "text": "im schräg einfallenden Licht"
        },
        "line.5": {
            "text": "noch einmal leuchten"
        },
        "line.6": {
            "text": "die vom Gelb ins Bräunlich sich"
        },
        "line.7": {
            "text": "färbenden Blätter"
        },
        "line.8": {
            "text": "die eng schon geschlossenen"
        },
        "line.9": {
            "text": "Blüten möchten sich"
        },
        "line.10": {
            "text": "noch einmal öffnen reinstes"
        },
        "line.11": {
            "text": "Blau zeigen am Rand"
        },
        "line.12": {
            "text": "schon ein wenig verblichen"
        },
        "line.13": {
            "text": "ein Motorflugzeug"
        },
        "line.14": {
            "text": "hoch über mir durchquert es"
        },
        "line.15": {
            "text": "einsam den Himmel"
        },
        "line.16": {
            "text": "in den schattigen Mulden"
        },
        "line.17": {
            "text": "bleibt träge und kühl"
        },
        "line.18": {
            "text": "die Feuchtigkeit liegen sie"
        },
        "line.19": {
            "text": "kriecht unter das Gras"
        },
        "line.20": {
            "text": "hält sich am matten Kraut fest"
        },
        "line.21": {
            "text": "das der erste Schnee"
        },
        "line.22": {
            "text": "geknickt und niedergedrückt"
        },
        "line.23": {
            "text": "Fäulnisgeruch schwebt"
        },
        "line.24": {
            "text": "säuerlich schwer über dem"
        },
        "line.25": {
            "text": "im Spätherbstnebel"
        },
        "line.26": {
            "text": "dumpf gewordenen Boden"
        },
        "line.27": {
            "text": "ein Efeublatt schwarz"
        },
        "line.28": {
            "text": "glänzt es mir in das Blickfeld"
        },
        "line.29": {
            "text": "es könnte diese"
        },
        "line.30": {
            "text": "Öffnung im Nichts sein in die"
        },
        "line.31": {
            "text": "alles stürzt aus der"
        },
        "line.32": {
            "text": "alles hochsteigt und die heißt"
        },
        "line.33": {
            "text": "Es-gibt der Stille"
        },
        "line.34": {
            "text": "ihr Murmeln meine ich jetzt"
        },
        "line.35": {
            "text": "deutlich zu hören"
        },
        "line.36": {
            "text": "vielleicht kreist hier Gottes Zeit"
        },
        "line.37": {
            "text": "von der es heißt sie"
        },
        "line.38": {
            "text": "sei die allerbeste Zeit"
        },
        "line.39": {
            "text": "eher unsicher"
        },
        "line.40": {
            "text": "scheint mir ob Gott auch mit der"
        },
        "line.41": {
            "text": "zweitbesten Zeit sich"
        },
        "line.42": {
            "text": "abzugeben bereit ist"
        },
        "line.43": {
            "text": "mit unserer Zeit"
        },
        "line.44": {
            "text": "die sich mühsam herausschält"
        },
        "line.45": {
            "text": "aus einer Dauer"
        },
        "line.46": {
            "text": "die wir nicht überblicken"
        },
        "line.47": {
            "text": "und die einen Raum"
        },
        "line.48": {
            "text": "passiert dessen Ränder sich"
        },
        "line.49": {
            "text": "wegbiegen von uns"
        },
        "line.50": {
            "text": "gleichwohl sind wir in Gottes"
        },
        "line.51": {
            "text": "Horizont eine"
        },
        "line.52": {
            "text": "Art Auffälligkeit um die"
        },
        "line.53": {
            "text": "zu kümmern sich lohnt"
        },
        "line.54": {
            "text": "wir bemerken nämlich des"
        },
        "line.55": {
            "text": "Unendlichen Spur"
        },
        "line.56": {
            "text": "sie stört die Ordnung der Welt"
        },
        "line.57": {
            "text": "und zerbricht jede"
        },
        "line.58": {
            "text": "Gegenwart die uns vertraut"
        },
        "line.59": {
            "text": "ich spüre hier denkt"
        },
        "line.60": {
            "text": "das Denken mehr als es denkt"
        },
        "line.61": {
            "text": "und ein Verlangen"
        },
        "line.62": {
            "text": "durchbricht mein Verstehen das"
        },
        "line.63": {
            "text": "richtungslos bleibt von"
        },
        "line.64": {
            "text": "dem ich nicht weiß was es will"
        }
    }
}