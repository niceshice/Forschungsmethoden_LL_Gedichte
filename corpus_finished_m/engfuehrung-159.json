{
    "title": "engfuehrung-159",
    "gender": "m",
    "author": "Paul Celan",
    "author_birth": "1920",
    "poem": {
        "line.1": {
            "text": "*"
        },
        "line.2": {
            "text": "VERBRACHT ins"
        },
        "line.3": {
            "text": "Gelände"
        },
        "line.4": {
            "text": "mit der untrüglichen Spur:"
        },
        "line.5": {
            "text": "Gras, auseinandergeschrieben. Die Steine, weiß,"
        },
        "line.6": {
            "text": "mit den Schatten der Halme:"
        },
        "line.7": {
            "text": "Lies nicht mehr  schau!"
        },
        "line.8": {
            "text": "Schau nicht mehr  geh!"
        },
        "line.9": {
            "text": "Geh, deine Stunde"
        },
        "line.10": {
            "text": "hat keine Schwestern, du bist"
        },
        "line.11": {
            "text": "bist zuhause. Ein Rad, langsam,"
        },
        "line.12": {
            "text": "rollt aus sich selber, die Speichen"
        },
        "line.13": {
            "text": "klettern,"
        },
        "line.14": {
            "text": "klettern auf schwärzlichem Feld, die Nacht"
        },
        "line.15": {
            "text": "braucht keine Sterne, nirgends"
        },
        "line.16": {
            "text": "fragt es nach dir."
        },
        "line.17": {
            "text": "*"
        },
        "line.18": {
            "text": "Nirgends"
        },
        "line.19": {
            "text": "fragt es nach dir"
        },
        "line.20": {
            "text": "Der Ort, wo sie lagen, er hat"
        },
        "line.21": {
            "text": "einen Namen  er hat"
        },
        "line.22": {
            "text": "keinen. Sie lagen nicht dort. Etwas"
        },
        "line.23": {
            "text": "lag zwischen ihnen. Sie"
        },
        "line.24": {
            "text": "sahn nicht hindurch."
        },
        "line.25": {
            "text": "Sahn nicht, nein,"
        },
        "line.26": {
            "text": "redeten von"
        },
        "line.27": {
            "text": "Worten. Keines"
        },
        "line.28": {
            "text": "erwachte, der"
        },
        "line.29": {
            "text": "Schlaf"
        },
        "line.30": {
            "text": "kam über sie."
        },
        "line.31": {
            "text": "*"
        },
        "line.32": {
            "text": "Kam, kam. Nirgends"
        },
        "line.33": {
            "text": "fragt es"
        },
        "line.34": {
            "text": "Ich bins, ich,"
        },
        "line.35": {
            "text": "ich lag zwischen euch, ich war"
        },
        "line.36": {
            "text": "offen, war"
        },
        "line.37": {
            "text": "hörbar, ich tickte euch zu, euer Atem"
        },
        "line.38": {
            "text": "gehorchte, ich"
        },
        "line.39": {
            "text": "bin es noch immer, ihr"
        },
        "line.40": {
            "text": "schlaft ja."
        },
        "line.41": {
            "text": "*"
        },
        "line.42": {
            "text": "Bin es noch immer"
        },
        "line.43": {
            "text": "Jahre."
        },
        "line.44": {
            "text": "Jahre, Jahre, ein Finger"
        },
        "line.45": {
            "text": "tastet hinab und hinan, tastet"
        },
        "line.46": {
            "text": "umher:"
        },
        "line.47": {
            "text": "Nahtstellen, fühlbar, hier"
        },
        "line.48": {
            "text": "klafft es weit auseinander, hier"
        },
        "line.49": {
            "text": "wuchs es wieder zusammen - wer"
        },
        "line.50": {
            "text": "deckte es zu?"
        },
        "line.51": {
            "text": "*"
        },
        "line.52": {
            "text": "Deckte es"
        },
        "line.53": {
            "text": "zu  wer?"
        },
        "line.54": {
            "text": "Kam, kam."
        },
        "line.55": {
            "text": "Kam ein Wort, kam,"
        },
        "line.56": {
            "text": "kam durch die Nacht,"
        },
        "line.57": {
            "text": "wollt leuchten, wollt leuchten."
        },
        "line.58": {
            "text": "Asche."
        },
        "line.59": {
            "text": "Asche, Asche."
        },
        "line.60": {
            "text": "Nacht."
        },
        "line.61": {
            "text": "Nacht-und-Nacht.  Zum"
        },
        "line.62": {
            "text": "Aug geh, zum feuchten."
        },
        "line.63": {
            "text": "*"
        },
        "line.64": {
            "text": "Zum"
        },
        "line.65": {
            "text": "Aug geh,"
        },
        "line.66": {
            "text": "zum feuchten"
        },
        "line.67": {
            "text": "Orkane."
        },
        "line.68": {
            "text": "Orkane, von je,"
        },
        "line.69": {
            "text": "Partikelgestöber, das andre,"
        },
        "line.70": {
            "text": "du"
        },
        "line.71": {
            "text": "weißts ja, wir"
        },
        "line.72": {
            "text": "lasens im Buche, war"
        },
        "line.73": {
            "text": "Meinung."
        },
        "line.74": {
            "text": "War, war"
        },
        "line.75": {
            "text": "Meinung. Wie"
        },
        "line.76": {
            "text": "faßten wir uns"
        },
        "line.77": {
            "text": "an  an mit"
        },
        "line.78": {
            "text": "diesen"
        },
        "line.79": {
            "text": "Händen?"
        },
        "line.80": {
            "text": "Es stand auch geschrieben, daß."
        },
        "line.81": {
            "text": "Wo? Wir"
        },
        "line.82": {
            "text": "taten ein Schweigen darüber,"
        },
        "line.83": {
            "text": "giftgestillt, groß,"
        },
        "line.84": {
            "text": "ein"
        },
        "line.85": {
            "text": "grünes"
        },
        "line.86": {
            "text": "Schweigen, ein Kelchblatt, es"
        },
        "line.87": {
            "text": "hing ein Gedanke an Pflanzliches dran"
        },
        "line.88": {
            "text": "grün, ja"
        },
        "line.89": {
            "text": "hing, ja"
        },
        "line.90": {
            "text": "unter hämischem"
        },
        "line.91": {
            "text": "Himmel."
        },
        "line.92": {
            "text": "An, ja,"
        },
        "line.93": {
            "text": "Pflanzliches."
        },
        "line.94": {
            "text": "Ja."
        },
        "line.95": {
            "text": "Orkane, Par-"
        },
        "line.96": {
            "text": "tikelgestöber, es blieb"
        },
        "line.97": {
            "text": "Zeit, blieb,"
        },
        "line.98": {
            "text": "es beim Stein zu versuchen  er"
        },
        "line.99": {
            "text": "war gastlich, er"
        },
        "line.100": {
            "text": "fiel nicht ins Wort. Wie"
        },
        "line.101": {
            "text": "gut wir es hatten:"
        },
        "line.102": {
            "text": "Körnig,"
        },
        "line.103": {
            "text": "körnig und faserig. Stengelig,"
        },
        "line.104": {
            "text": "dicht;"
        },
        "line.105": {
            "text": "traubig und strahlig; nierig,"
        },
        "line.106": {
            "text": "plattig und"
        },
        "line.107": {
            "text": "klumpig; locker, ver-"
        },
        "line.108": {
            "text": "ästelt : er, es"
        },
        "line.109": {
            "text": "fiel nicht ins Wort, es"
        },
        "line.110": {
            "text": "sprach,"
        },
        "line.111": {
            "text": "sprach gerne zu trockenen Augen, eh es sie schloß."
        },
        "line.112": {
            "text": "Sprach, sprach."
        },
        "line.113": {
            "text": "War, war."
        },
        "line.114": {
            "text": "Wir"
        },
        "line.115": {
            "text": "ließen nicht locker, standen"
        },
        "line.116": {
            "text": "inmitten, ein"
        },
        "line.117": {
            "text": "Porenbau, und"
        },
        "line.118": {
            "text": "es kam."
        },
        "line.119": {
            "text": "Kam auf uns zu, kam"
        },
        "line.120": {
            "text": "hindurch, flickte"
        },
        "line.121": {
            "text": "unsichtbar, flickte"
        },
        "line.122": {
            "text": "an der letzten Membran,"
        },
        "line.123": {
            "text": "und"
        },
        "line.124": {
            "text": "die Welt, ein Tausendkristall,"
        },
        "line.125": {
            "text": "schoß an, schoß an."
        },
        "line.126": {
            "text": "*"
        },
        "line.127": {
            "text": "Schoß an, schoß an."
        },
        "line.128": {
            "text": "Dann"
        },
        "line.129": {
            "text": "Nächte, entmischt. Kreise,"
        },
        "line.130": {
            "text": "grün oder blau, rote"
        },
        "line.131": {
            "text": "Quadrate: die"
        },
        "line.132": {
            "text": "Welt setzt ihr Innerstes ein"
        },
        "line.133": {
            "text": "im Spiel mit den neuen"
        },
        "line.134": {
            "text": "Stunden.  Kreise,"
        },
        "line.135": {
            "text": "rot oder schwarz, helle"
        },
        "line.136": {
            "text": "Quadrate, kein"
        },
        "line.137": {
            "text": "Flugschatten,"
        },
        "line.138": {
            "text": "kein"
        },
        "line.139": {
            "text": "Meßtisch, keine"
        },
        "line.140": {
            "text": "Rauchseele steigt und spielt mit."
        },
        "line.141": {
            "text": "*"
        },
        "line.142": {
            "text": "Steigt und"
        },
        "line.143": {
            "text": "spielt mit -"
        },
        "line.144": {
            "text": "In der Eulenflucht, beim"
        },
        "line.145": {
            "text": "versteinerten Aussatz,"
        },
        "line.146": {
            "text": "bei"
        },
        "line.147": {
            "text": "unsern geflohenen Händen, in"
        },
        "line.148": {
            "text": "der jüngsten Verwerfung,"
        },
        "line.149": {
            "text": "überm"
        },
        "line.150": {
            "text": "Kugelfang an"
        },
        "line.151": {
            "text": "der verschütteten Mauer:"
        },
        "line.152": {
            "text": "sichtbar, aufs"
        },
        "line.153": {
            "text": "neue: die"
        },
        "line.154": {
            "text": "Rillen, die"
        },
        "line.155": {
            "text": "Chöre, damals, die"
        },
        "line.156": {
            "text": "Psalmen. Ho, ho-"
        },
        "line.157": {
            "text": "sianna."
        },
        "line.158": {
            "text": "Also"
        },
        "line.159": {
            "text": "stehen noch Tempel. Ein"
        },
        "line.160": {
            "text": "Stern"
        },
        "line.161": {
            "text": "hat wohl noch Licht."
        },
        "line.162": {
            "text": "Nichts,"
        },
        "line.163": {
            "text": "nichts ist verloren."
        },
        "line.164": {
            "text": "Ho-"
        },
        "line.165": {
            "text": "sianna."
        },
        "line.166": {
            "text": "In der Eulenflucht, hier,"
        },
        "line.167": {
            "text": "die Gespräche, taggrau,"
        },
        "line.168": {
            "text": "der Grundwasserspuren."
        },
        "line.169": {
            "text": "*"
        },
        "line.170": {
            "text": "(  taggrau,"
        },
        "line.171": {
            "text": "der"
        },
        "line.172": {
            "text": "Grundwasserspuren"
        },
        "line.173": {
            "text": "Verbracht"
        },
        "line.174": {
            "text": "ins Gelände"
        },
        "line.175": {
            "text": "mit"
        },
        "line.176": {
            "text": "der untrüglichen"
        },
        "line.177": {
            "text": "Spur:"
        },
        "line.178": {
            "text": "Gras."
        },
        "line.179": {
            "text": "Gras,"
        },
        "line.180": {
            "text": "auseinandergeschrieben.)"
        }
    }
}