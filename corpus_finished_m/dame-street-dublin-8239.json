{
    "title": "dame-street-dublin-8239",
    "gender": "m",
    "author": "Michael Speier",
    "author_birth": "1950",
    "poem": {
        "line.1": {
            "text": "the wild swans at coole (w. b.yeats)"
        },
        "line.2": {
            "text": "und, wildschwant morgens"
        },
        "line.3": {
            "text": "bei bewleys gestöber"
        },
        "line.4": {
            "text": "noch tee in den worten"
        },
        "line.5": {
            "text": "noch licht in der tüte"
        },
        "line.6": {
            "text": "l`ile joyeuse nicht"
        },
        "line.7": {
            "text": "entzweivereint noch"
        },
        "line.8": {
            "text": "umeinander gelegt"
        },
        "line.9": {
            "text": "wünsche, drücke taste schlaf"
        },
        "line.10": {
            "text": "morgens bei bewleys"
        },
        "line.11": {
            "text": "keltischen zwielichts"
        },
        "line.12": {
            "text": "knirschen uns kellner"
        },
        "line.13": {
            "text": "nicht yeats ins gefieder"
        },
        "line.14": {
            "text": "gehör voll wind"
        },
        "line.15": {
            "text": "und fragepartikel"
        },
        "line.16": {
            "text": "aber die sprache"
        },
        "line.17": {
            "text": "morgens bei bewleys"
        },
        "line.18": {
            "text": "die wollte die wußte"
        },
        "line.19": {
            "text": "die nahm mich I`ll put"
        },
        "line.20": {
            "text": "your stick in my mouth"
        },
        "line.21": {
            "text": "nicht zurück soweit du"
        },
        "line.22": {
            "text": "den hals bogst"
        }
    }
}