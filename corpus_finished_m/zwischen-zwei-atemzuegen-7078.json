{
    "title": "zwischen-zwei-atemzuegen-7078",
    "gender": "m",
    "author": "Sepp Mall",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "Draußen gehen Tiere vorbei"
        },
        "line.2": {
            "text": "Mir zerbrechen Bleistifte / in den"
        },
        "line.3": {
            "text": "Fingern / es wird Sommer"
        },
        "line.4": {
            "text": "es wird Herbst"
        },
        "line.5": {
            "text": "Vielleicht / ist ein Wort"
        },
        "line.6": {
            "text": "das niemand hilft"
        },
        "line.7": {
            "text": "Vielleicht werde auch ich"
        },
        "line.8": {
            "text": "mit dem Messer schreiben"
        },
        "line.9": {
            "text": "und etwas wird bleiben"
        },
        "line.10": {
            "text": "von den lichten Stellen"
        },
        "line.11": {
            "text": "die das Jahr / noch hat"
        },
        "line.12": {
            "text": "Draußen gehen Tiere vorbei / wer"
        },
        "line.13": {
            "text": "hält meine Hände im Zaum"
        }
    }
}