{
    "title": "kao-pocetak-i-kraj-16417",
    "gender": "m",
    "author": "Nikola Ćorac (Никола Ћорац)",
    "author_birth": "1991",
    "poem": {
        "line.1": {
            "text": "najprije pronai razlike"
        },
        "line.2": {
            "text": "na lijevom dlanu imam tri oiljka vie"
        },
        "line.3": {
            "text": "na prvi pogled,"
        },
        "line.4": {
            "text": "moje ake su identine"
        },
        "line.5": {
            "text": "ona govori o kretanju"
        },
        "line.6": {
            "text": "uvijek iz iste polazne take,"
        },
        "line.7": {
            "text": "krug je najprije proirena taka"
        },
        "line.8": {
            "text": "kruno je savreno"
        },
        "line.9": {
            "text": "kao poetak i kraj"
        },
        "line.10": {
            "text": "ili gria savjesti"
        }
    }
}