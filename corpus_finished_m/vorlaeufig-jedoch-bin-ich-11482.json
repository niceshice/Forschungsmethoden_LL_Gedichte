{
    "title": "vorlaeufig-jedoch-bin-ich-11482",
    "gender": "m",
    "author": "Franz Dodel",
    "author_birth": "1949",
    "poem": {
        "line.1": {
            "text": "vorläufig jedoch bin ich"
        },
        "line.2": {
            "text": "noch eine Zeit lang"
        },
        "line.3": {
            "text": "beschäftigt mit der Frage:"
        },
        "line.4": {
            "text": "woher kommt sie und"
        },
        "line.5": {
            "text": "wie findet sie mich diese"
        },
        "line.6": {
            "text": "Zuversicht mit der"
        },
        "line.7": {
            "text": "ich wider alle Vernunft"
        },
        "line.8": {
            "text": "leichtsinnig einen"
        },
        "line.9": {
            "text": "Tauschhandel betreibe mit"
        },
        "line.10": {
            "text": "Glühwürmchen und mit"
        },
        "line.11": {
            "text": "grundsätzlich nur lichtschwachen"
        },
        "line.12": {
            "text": "Unwägbarkeiten"
        }
    }
}