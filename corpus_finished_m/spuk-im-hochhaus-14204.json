{
    "title": "spuk-im-hochhaus-14204",
    "gender": "m",
    "author": "Tom Bresemann",
    "author_birth": "1978",
    "poem": {
        "line.1": {
            "text": "1"
        },
        "line.2": {
            "text": "von draußen von hohenschönhausen"
        },
        "line.3": {
            "text": "komm ich her, wo sie zu hause sind,"
        },
        "line.4": {
            "text": "und was berichten sie vom mann"
        },
        "line.5": {
            "text": "der nachts im fenster steht,"
        },
        "line.6": {
            "text": "an heizungsrohre schlägt"
        },
        "line.7": {
            "text": "und in die träume glotzt?"
        },
        "line.8": {
            "text": "die zähne zeig ihm"
        },
        "line.9": {
            "text": "wenn du schlafen kannst"
        },
        "line.10": {
            "text": "(und auch diese fahne"
        },
        "line.11": {
            "text": "ist rot)."
        },
        "line.12": {
            "text": "2"
        },
        "line.13": {
            "text": "darauf eine mitarbeit"
        },
        "line.14": {
            "text": "und einen mitarbeiter"
        },
        "line.15": {
            "text": "mehr pro wohneinheit,"
        },
        "line.16": {
            "text": "arbeit ist ja jetzt überhaupt"
        },
        "line.17": {
            "text": "das wichtigste (ein häkchen"
        },
        "line.18": {
            "text": "an jedes ende des kreuzes)."
        },
        "line.19": {
            "text": "3"
        },
        "line.20": {
            "text": "die nacht ertüchtigt dich im gasbeton."
        },
        "line.21": {
            "text": "nimm die hände aus der tasche,"
        },
        "line.22": {
            "text": "schneid dir mal die haare,"
        },
        "line.23": {
            "text": "pickelrobert unter linden,"
        },
        "line.24": {
            "text": "plattentrottel unter plattentrotteln,"
        },
        "line.25": {
            "text": "zieh nicht leine, das ist deine sache"
        },
        "line.26": {
            "text": "hier, du deutsche sau,"
        },
        "line.27": {
            "text": "du künstler."
        }
    }
}