{
    "title": "zur-insel-im-winter-brzu-fuss-242",
    "gender": "m",
    "author": "Jörg Schieke",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "übers eis. ein weg in die wolken"
        },
        "line.2": {
            "text": "gesunkener schiffe. morgen verjährt"
        },
        "line.3": {
            "text": "mein traurigster marsch & das auf dem schlitten"
        },
        "line.4": {
            "text": "in richtung"
        },
        "line.5": {
            "text": "das meer ist die beste adresse"
        },
        "line.6": {
            "text": "auf reisen geschickte"
        },
        "line.7": {
            "text": "klavier. müde"
        },
        "line.8": {
            "text": "schaut marln zu mir her, hat seitdem ihr piano nicht mehr,"
        },
        "line.9": {
            "text": "auf dem sie,"
        },
        "line.10": {
            "text": "bevor ich es aus dem wohnzimmer trug, stunden-, ja"
        },
        "line.11": {
            "text": "tagelang,"
        },
        "line.12": {
            "text": "immer ein und dieselbe taste anschlug."
        }
    }
}