{
    "title": "aus-der-geschichte-der-koerperdoubles-12340",
    "gender": "m",
    "author": "Ulrich Koch",
    "author_birth": "1966",
    "poem": {
        "line.1": {
            "text": "Wir sprangen vom Beckenrand"
        },
        "line.2": {
            "text": "in uns hinein, manche trieben ab."
        },
        "line.3": {
            "text": "Nach Jahren war das Wasser"
        },
        "line.4": {
            "text": "immer dünnhäutiger geworden."
        },
        "line.5": {
            "text": "Wir memorierten Liebesgedichte"
        },
        "line.6": {
            "text": "und frühkindliche Schattenspiele."
        },
        "line.7": {
            "text": "Morgens erwachten wir selbstaufblasend."
        },
        "line.8": {
            "text": "Nachts entnahmen wir uns Bodenproben."
        },
        "line.9": {
            "text": "Bald bejubelte der Zoo in uns"
        },
        "line.10": {
            "text": "den ersten Nachwuchs in Gefangenschaft."
        },
        "line.11": {
            "text": "Es waren Zwillinge: Zwei Apnoetaucher"
        },
        "line.12": {
            "text": "aus einer der toten Sprachen."
        }
    }
}