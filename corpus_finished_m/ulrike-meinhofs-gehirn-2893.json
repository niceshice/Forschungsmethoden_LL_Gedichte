{
    "title": "ulrike-meinhofs-gehirn-2893",
    "gender": "m",
    "author": "Gregor  Laschen",
    "author_birth": "1941",
    "poem": {
        "line.1": {
            "text": "(Das, was quer-"
        },
        "line.2": {
            "text": "steht in der strudelnden"
        },
        "line.3": {
            "text": "Geschichte, Strudel bildet, nicht"
        },
        "line.4": {
            "text": "mehr spricht und Leben war, das"
        },
        "line.5": {
            "text": "untergeht: und letzte"
        },
        "line.6": {
            "text": "Hand anlegt, selbstbesonnen,"
        },
        "line.7": {
            "text": "fremd zuletzt sich selbst,"
        },
        "line.8": {
            "text": "bei sich.)"
        },
        "line.9": {
            "text": "Dieses verletzte Gehirn, in"
        },
        "line.10": {
            "text": "aller Stille am Ende dem"
        },
        "line.11": {
            "text": "Schädel entnommen, verborgen"
        },
        "line.12": {
            "text": "ein Jahrhundertviertel lang"
        },
        "line.13": {
            "text": "in Heimlichkeit und Formalin,"
        },
        "line.14": {
            "text": "und fremden Händen,"
        },
        "line.15": {
            "text": "Augen, Atem ausgesetzt:"
        },
        "line.16": {
            "text": "Begraben jetzt"
        },
        "line.17": {
            "text": "in aller Stille"
        },
        "line.18": {
            "text": "neben ihrem Grab."
        }
    }
}