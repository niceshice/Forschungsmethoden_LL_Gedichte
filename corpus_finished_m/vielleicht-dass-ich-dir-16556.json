{
    "title": "vielleicht-dass-ich-dir-16556",
    "gender": "m",
    "author": "Sascha Garzetti",
    "author_birth": "1986",
    "poem": {
        "line.1": {
            "text": "Vielleicht, dass ich dir"
        },
        "line.2": {
            "text": "einen Brief schriebe."
        },
        "line.3": {
            "text": "Du könntest ihn"
        },
        "line.4": {
            "text": "zur Seite legen"
        },
        "line.5": {
            "text": "ohne ein Wort"
        },
        "line.6": {
            "text": "darin zu lesen"
        },
        "line.7": {
            "text": "bloß nachsehen"
        },
        "line.8": {
            "text": "wo ich ihn aufgab"
        },
        "line.9": {
            "text": "und mir sagen"
        },
        "line.10": {
            "text": "wo ich zu Hause bin."
        }
    }
}