{
    "title": "suche-nicht-nach-murmeln-im-flussbett-4368",
    "gender": "m",
    "author": "Christian Schloyer",
    "author_birth": "1976",
    "poem": {
        "line.1": {
            "text": "ich wäre gern an der erschaffung der kiesel"
        },
        "line.2": {
            "text": "steine beteiligt gewesen"
        },
        "line.3": {
            "text": "die rundgeschliffenen  münder +"
        },
        "line.4": {
            "text": "nasen  ziehen vorbei"
        },
        "line.5": {
            "text": "& ihr verwässerter blick  fällt"
        },
        "line.6": {
            "text": "in die grund-"
        },
        "line.7": {
            "text": "schwingung gleichfarbener urmeere"
        },
        "line.8": {
            "text": "zurück  bleibt insgeheim mein sehnen nach"
        },
        "line.9": {
            "text": "deiner stimme"
        },
        "line.10": {
            "text": "an ihr hätte ich mich gerne beteiligt oder"
        },
        "line.11": {
            "text": "vergangen"
        },
        "line.12": {
            "text": "die wände"
        },
        "line.13": {
            "text": "hinter den türen in den räumen unserer"
        },
        "line.14": {
            "text": "liebesnächte  alle akren glatt"
        },
        "line.15": {
            "text": "geschliffen im fluss"
        }
    }
}