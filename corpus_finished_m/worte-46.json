{
    "title": "worte-46",
    "gender": "m",
    "author": "Gottfried Benn",
    "author_birth": "1886",
    "poem": {
        "line.1": {
            "text": "Allein: du mit den Worten"
        },
        "line.2": {
            "text": "und das ist wirklich allein,"
        },
        "line.3": {
            "text": "Clairons und Ehrenpforten"
        },
        "line.4": {
            "text": "sind nicht in diesem Sein."
        },
        "line.5": {
            "text": "Du siehst ihnen in die Seele"
        },
        "line.6": {
            "text": "nach Vor- und Urgesicht,"
        },
        "line.7": {
            "text": "Jahre um Jahre - quäle"
        },
        "line.8": {
            "text": "dich ab, du findest nicht."
        },
        "line.9": {
            "text": "Und drüben brennen die Leuchten"
        },
        "line.10": {
            "text": "in sanftem Menschenhort,"
        },
        "line.11": {
            "text": "von Lippen, rosigen, feuchten"
        },
        "line.12": {
            "text": "perlt unbedenklich das Wort."
        },
        "line.13": {
            "text": "Nur deine Jahre vergilben"
        },
        "line.14": {
            "text": "in einem anderen Sinn,"
        },
        "line.15": {
            "text": "bis in die Träume: Silben -"
        },
        "line.16": {
            "text": "doch schweigend gehst du hin."
        }
    }
}