    "author": "Norbert C. Kaser",
    "author_birth": "1947",
    "poem": {
        "line.1": {
            "text": "er reitet auf dem gefesselten"
        },
        "line.2": {
            "text": "dem nackten schiebt er das messer"
        },
        "line.3": {
            "text": "in den hals"
        },
        "line.4": {
            "text": "der sinkt um blut sprudelt er teilt ihn"
        },
        "line.5": {
            "text": "die eingeweide fallen heraus"
        },
        "line.6": {
            "text": "er saeubert sie"
        },
        "line.7": {
            "text": "er zieht dicke haken"
        },
        "line.8": {
            "text": "durch die sehnen"
        },
        "line.9": {
            "text": "der haengt"
        },
        "line.10": {
            "text": "der kopf faellt"
        },
        "line.11": {
            "text": "ins gebiÃŸ wird ein stueck zitrone ge-"
        },
        "line.12": {
            "text": "legt"
        },
        "line.13": {
            "text": "& dabei stehen"
        },
        "line.14": {
            "text": "buddhisten &"
        },
        "line.15": {
            "text": "katholiken &"
        },
        "line.16": {
            "text": "anglikaner &"
        },
        "line.17": {
            "text": "mohammeds juenger &"
        },
        "line.18": {
            "text": "essen seufzend davon"
        },
        "line.19": {
            "text": "28. 8. 68"
        }
    }
}