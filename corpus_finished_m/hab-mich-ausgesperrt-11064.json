{
    "title": "hab-mich-ausgesperrt-11064",
    "gender": "m",
    "author": "Kerstin Preiwuß",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "Hab mich ausgesperrt."
        },
        "line.2": {
            "text": "Für einen Moment sage ich mir"
        },
        "line.3": {
            "text": "der Winter steht vor der Tür."
        },
        "line.4": {
            "text": "Hab mich eingehüllt in ihn"
        },
        "line.5": {
            "text": "und hör nicht mehr viel"
        },
        "line.6": {
            "text": "von der Welt."
        },
        "line.7": {
            "text": "Ich schlaf in ihr."
        },
        "line.8": {
            "text": "Atme kaum dabei."
        },
        "line.9": {
            "text": "Im Traum esse ich süßen Brei."
        },
        "line.10": {
            "text": "Das ist fast zu viel."
        },
        "line.11": {
            "text": "Mein Pelz ist aus Dunkelheit."
        },
        "line.12": {
            "text": "Dunkelheit ist ein schönes Gefühl."
        },
        "line.13": {
            "text": "Nur im Winter werde ich alt."
        }
    }
}