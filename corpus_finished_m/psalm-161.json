{
    "title": "psalm-161",
    "gender": "m",
    "author": "Paul Celan",
    "author_birth": "1920",
    "poem": {
        "line.1": {
            "text": "Niemand knetet uns wieder aus Erde und Lehm,"
        },
        "line.2": {
            "text": "niemand bespricht unseren Staub."
        },
        "line.3": {
            "text": "Niemand."
        },
        "line.4": {
            "text": "Gelobt seist du, Niemand."
        },
        "line.5": {
            "text": "Dir zulieb wollen"
        },
        "line.6": {
            "text": "wir blühn."
        },
        "line.7": {
            "text": "Dir"
        },
        "line.8": {
            "text": "entgegen."
        },
        "line.9": {
            "text": "Ein Nichts"
        },
        "line.10": {
            "text": "waren wir, sind wir, werden"
        },
        "line.11": {
            "text": "wir bleiben, blühend:"
        },
        "line.12": {
            "text": "die Nichts-, die"
        },
        "line.13": {
            "text": "Niemandsrose."
        },
        "line.14": {
            "text": "Mit"
        },
        "line.15": {
            "text": "dem Griffel seelenhell,"
        },
        "line.16": {
            "text": "dem Staubfaden himmelswüst,"
        },
        "line.17": {
            "text": "der Krone rot"
        },
        "line.18": {
            "text": "vom Purpurwort, das wir sangen"
        },
        "line.19": {
            "text": "über, o über"
        },
        "line.20": {
            "text": "dem Dorn."
        }
    }
}