{
    "title": "der-glanzfisch-bleibt-4742",
    "gender": "m",
    "author": "SAID",
    "author_birth": "1947",
    "poem": {
        "line.1": {
            "text": "der glanzfisch bleibt"
        },
        "line.2": {
            "text": "erzählt und berührt"
        },
        "line.3": {
            "text": "ungeachtet der brechungen der zeit"
        },
        "line.4": {
            "text": "und der lärmungen des raums"
        },
        "line.5": {
            "text": "er will seine bilder zu ende denken"
        },
        "line.6": {
            "text": "läßt keinen zirkelschluß zu"
        },
        "line.7": {
            "text": "und wartet auf die versteppung der städte"
        }
    }
}