{
    "title": "habt-ihr-meinen-fisch-gesehen-11059",
    "gender": "m",
    "author": "Kerstin Preiwuß",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "Habt ihr meinen Fisch gesehen?"
        },
        "line.2": {
            "text": "Er wurde von einem Kraken gejagt."
        },
        "line.3": {
            "text": "Der hatte Tentakeln wie ich Finger an jeder Hand."
        },
        "line.4": {
            "text": "Vielleicht hat er meinen Fisch mumifi ziert."
        },
        "line.5": {
            "text": "Vielleicht sitz ich im Bauch des Fischs."
        },
        "line.6": {
            "text": "Vielleicht hat es uns beide berührt."
        },
        "line.7": {
            "text": "Ich hielt mir einmal einen Trilobiten ans Ohr."
        },
        "line.8": {
            "text": "Er kroch langsam hinein."
        },
        "line.9": {
            "text": "Er war ganz behutsam dabei."
        },
        "line.10": {
            "text": "Und so sonor."
        },
        "line.11": {
            "text": "Er wäre lieber draußen geblieben."
        },
        "line.12": {
            "text": "Aber ich ließ ihn ein."
        },
        "line.13": {
            "text": "Später warf ich einen Trilobiten aus meinem Ohr."
        },
        "line.14": {
            "text": "Er wäre lieber drinnen geblieben."
        },
        "line.15": {
            "text": "Aber ich schmiss ihn raus."
        },
        "line.16": {
            "text": "Ich sah ihm eine Weile zu wie er ratlos nach mir suchte."
        },
        "line.17": {
            "text": "Dann ging er langsam ein."
        },
        "line.18": {
            "text": "Noch später kam ein Fisch vorbei."
        },
        "line.19": {
            "text": "Er hatte sich verirrt."
        },
        "line.20": {
            "text": "Er war bleich und blind."
        },
        "line.21": {
            "text": "Er hatte keine Augen aber ein Gespür für Licht."
        },
        "line.22": {
            "text": "Mein drittes Auge sprach der Fisch"
        },
        "line.23": {
            "text": "liegt hinter meiner Stirn."
        },
        "line.24": {
            "text": "Es erlaubt mir mich in völliger Dunkelheit zu befinden."
        },
        "line.25": {
            "text": "Nicht in äußerer Finsternis."
        },
        "line.26": {
            "text": "Ich bin ein Höhlenfisch."
        },
        "line.27": {
            "text": "Meine Wege gehen nach innen."
        },
        "line.28": {
            "text": "Ich kehre nicht zurück."
        },
        "line.29": {
            "text": "Wenn der Krake mich jagt"
        },
        "line.30": {
            "text": "ist jeder Saugnapf ein Auge."
        },
        "line.31": {
            "text": "So sieht er sich satt."
        },
        "line.32": {
            "text": "Nachts wenn alle schlafen ist bei uns beiden Tag."
        }
    }
}