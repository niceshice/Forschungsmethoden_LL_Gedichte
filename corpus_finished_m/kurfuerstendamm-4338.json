{
    "title": "kurfuerstendamm-4338",
    "gender": "m",
    "author": "Wulf Kirsten",
    "author_birth": "1934",
    "poem": {
        "line.1": {
            "text": "Eisenzahnstraße, abgewinkelten arms."
        },
        "line.2": {
            "text": "emaille. gingkobäumchen in reih und glied."
        },
        "line.3": {
            "text": "astlos. vorübergehend an pfähle gebunden,"
        },
        "line.4": {
            "text": "laufen neben den passanten her. so tun,"
        },
        "line.5": {
            "text": "als ob. und wie sie marschieren."
        },
        "line.6": {
            "text": "pomologie, preußisch gedrillt. flexibel"
        },
        "line.7": {
            "text": "gekrümmt bogenlaternen, eisenkunstguß."
        },
        "line.8": {
            "text": "nostalgisch verschnörkelt."
        },
        "line.9": {
            "text": "die geschmacksbildende idee alternierend"
        },
        "line.10": {
            "text": "und eine spur zu absichtlich"
        },
        "line.11": {
            "text": "zwischen die baumschulflüchter gesetzt."
        },
        "line.12": {
            "text": "abgeplatzte platanenrinde: heißt das,"
        },
        "line.13": {
            "text": "ich soll entziffern, was die urbanisierte"
        },
        "line.14": {
            "text": "natur mir achtlos vor die füße wirft?"
        },
        "line.15": {
            "text": "eingemummt in ein giftgrünes brillengestell"
        },
        "line.16": {
            "text": "die bankbesetzerin und ihr ausgesperrtes"
        },
        "line.17": {
            "text": "eigentum. hinter einem wall sparriger"
        },
        "line.18": {
            "text": "bündel das sperrgut mitten auf der avenue."
        },
        "line.19": {
            "text": "die einzige und ihr eigentum. anschauungs-"
        },
        "line.20": {
            "text": "modell. sorgsam verschnürt und gehütet."
        },
        "line.21": {
            "text": "die geheimnisse des lebens. fremdbestimmt."
        },
        "line.22": {
            "text": "die krümel teilt sie mit den tauben, fittich,"
        },
        "line.23": {
            "text": "fußkrank und stahlblau. ein ausgewildertes"
        },
        "line.24": {
            "text": "team. gut eingespielt. auf einem endlosband"
        },
        "line.25": {
            "text": "gestreckt fassadenfrohes grafitti-esperanto."
        },
        "line.26": {
            "text": "der mauerabtrag von spechten handlich"
        },
        "line.27": {
            "text": "geklopft, oder waren die heiligen brocken"
        },
        "line.28": {
            "text": "bloß auf feindwall getrimmt? verhökert"
        },
        "line.29": {
            "text": "an einer straßenecke in Berlin."
        }
    }
}