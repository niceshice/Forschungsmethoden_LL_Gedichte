{
    "title": "abschrankung-isst-wegweiser-188",
    "gender": "m",
    "author": "Oskar Pastior",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "neunzehnhundert / siebenundzwanzig"
        },
        "line.2": {
            "text": "hermannstadt in siebenbürgen"
        },
        "line.3": {
            "text": "muß i denn / muß i dünn"
        },
        "line.4": {
            "text": "kantilene / hindurch"
        },
        "line.5": {
            "text": "schulding / nicht schulding"
        },
        "line.6": {
            "text": "o geboren / o wünschelrut"
        },
        "line.7": {
            "text": "und listen / namen und"
        },
        "line.8": {
            "text": "mitgefangen / hain"
        },
        "line.9": {
            "text": "minderheiten / straßen"
        },
        "line.10": {
            "text": "kaputt / nicht kaputt"
        },
        "line.11": {
            "text": "masse / gewicht"
        },
        "line.12": {
            "text": "krieg / ruh"
        },
        "line.13": {
            "text": "unzug / unzug"
        },
        "line.14": {
            "text": "gepäck / schnitt"
        },
        "line.15": {
            "text": "ballhausgassenspießen"
        },
        "line.16": {
            "text": "zurück / nicht zurück"
        }
    }
}