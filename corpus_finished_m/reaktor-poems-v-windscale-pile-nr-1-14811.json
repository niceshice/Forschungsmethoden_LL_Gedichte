{
    "title": "reaktor-poems-v-windscale-pile-nr-1-14811",
    "gender": "m",
    "author": "Paul-Henri  Campbell",
    "author_birth": "1982",
    "poem": {
        "line.1": {
            "text": "spricht rabbi vineyard : waren londiniums huren teurer"
        },
        "line.2": {
            "text": "bevor dieser h*rr**tt seinen sohn geopfert?"
        },
        "line.3": {
            "text": "indelebilis"
        },
        "line.4": {
            "text": "nun sprich von saurem regen gewölk aus sodom sprich"
        },
        "line.5": {
            "text": "vom stinkenden pesthauch von still schweigenden gasen"
        },
        "line.6": {
            "text": "des schlachtfelds der kammern der undichten herde"
        },
        "line.7": {
            "text": "von vogelschwärmen die über buckingham elsinore"
        },
        "line.8": {
            "text": "o something is foul"
        },
        "line.9": {
            "text": "vogelschwärme flatternde fittiche"
        },
        "line.10": {
            "text": "noch f-f-flogen doch schon über sanssouci vom himmel"
        },
        "line.11": {
            "text": "gefallen wie der gehörnte schuppige höllenfürst selbst"
        },
        "line.12": {
            "text": "in jähen spiralen wie trichter wie lustvoll angezogen"
        },
        "line.13": {
            "text": "vom gierigen grund"
        },
        "line.14": {
            "text": "o mother"
        },
        "line.15": {
            "text": "aller unfälle"
        },
        "line.16": {
            "text": "das hinschmelzen der kerne"
        },
        "line.17": {
            "text": "die tausendzüngigen brände"
        },
        "line.18": {
            "text": "die flürflürflürpschlürfenden ventilele  tiltiltil"
        },
        "line.19": {
            "text": "die kurzgetschtschschlossenen aussetztzer  blackout"
        },
        "line.20": {
            "text": "die spätentdeckten lecke"
        },
        "line.21": {
            "text": "die labyrinthischen risse"
        },
        "line.22": {
            "text": "im satzspiegel der"
        },
        "line.23": {
            "text": "opinions"
        },
        "line.24": {
            "text": "im kurzwellentratsch der plebs"
        },
        "line.25": {
            "text": "ein bisschen brüten über plutonium und wieder ist sinn"
        },
        "line.26": {
            "text": "in den dingen sind sorge skala und zeiger ausgeschlagen"
        },
        "line.27": {
            "text": "die waagschalen unterm sanddorn abgerissen"
        }
    }
}