{
    "title": "ich-will-was-durch-meinen-mund-flattert-11066",
    "gender": "m",
    "author": "Kerstin Preiwuß",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "Ich will was durch meinen Mund flattert"
        },
        "line.2": {
            "text": "nicht aussprechen auch nicht anschreien"
        },
        "line.3": {
            "text": "denn dann ist es in der Welt."
        },
        "line.4": {
            "text": "Es bleibt wo es ist und zahnt blind aus dem Gaumen."
        },
        "line.5": {
            "text": "Wie ein Kind wenn es wächst."
        },
        "line.6": {
            "text": "Dort ist sein Rachen."
        },
        "line.7": {
            "text": "Wie wenn ich lache."
        },
        "line.8": {
            "text": "Ich will aus Licht und Schatten"
        },
        "line.9": {
            "text": "keinen Umriss mehr machen"
        },
        "line.10": {
            "text": "wie bei einem Scherenschnitt."
        },
        "line.11": {
            "text": "Ich will weder jagen gehen noch Jagdwild sein."
        },
        "line.12": {
            "text": "Ich verzehre mich nicht."
        },
        "line.13": {
            "text": "Keinen Happen von mir."
        },
        "line.14": {
            "text": "Keinen Happen für mich."
        },
        "line.15": {
            "text": "Ich hab nichts übrig dafür."
        },
        "line.16": {
            "text": "Es bleibt alles in mir wie es ist."
        },
        "line.17": {
            "text": "Und kehrt in die Erde zurück."
        },
        "line.18": {
            "text": "Wie wenn ein Blitzschlag mich trifft."
        },
        "line.19": {
            "text": "Und nur manchmal ist mir kalt."
        }
    }
}