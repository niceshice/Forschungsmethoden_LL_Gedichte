{
    "title": "steinbruch-13984",
    "gender": "m",
    "author": "Jan Volker Röhnert",
    "author_birth": "1976",
    "poem": {
        "line.1": {
            "text": "Den Tag über"
        },
        "line.2": {
            "text": "den Falken, oben"
        },
        "line.3": {
            "text": "in der Kiefer zugeschaut"
        },
        "line.4": {
            "text": "(meine Großmutter, neunundneunzig"
        },
        "line.5": {
            "text": "wäre sie, pflanzte"
        },
        "line.6": {
            "text": "als Kind diesen Baum):"
        },
        "line.7": {
            "text": "Nest nur zu ahnen"
        },
        "line.8": {
            "text": "im südlichen Wipfelzack"
        },
        "line.9": {
            "text": "von unten schimmernd durch Nadelgrün"
        },
        "line.10": {
            "text": "im obersten Wipfel die Falkin"
        },
        "line.11": {
            "text": "wacht, späht, spreizt"
        },
        "line.12": {
            "text": "zum Fächer den Schwanz"
        },
        "line.13": {
            "text": "der Falke nebenan"
        },
        "line.14": {
            "text": "flog in der Eiche ein"
        },
        "line.15": {
            "text": "würgt aus seinen Fängen"
        },
        "line.16": {
            "text": "die Wühlmaus tief in den Kropf"
        },
        "line.17": {
            "text": "schwingt auf, gleitet"
        },
        "line.18": {
            "text": "rund um die Kiefer"
        },
        "line.19": {
            "text": "drinnen keckert es los"
        },
        "line.20": {
            "text": "die Falkin zum Nest"
        },
        "line.21": {
            "text": "endlich, endlich"
        },
        "line.22": {
            "text": "schwebt heran, lässt"
        },
        "line.23": {
            "text": "zeternd sich nieder, schärft"
        },
        "line.24": {
            "text": "mir und dir und"
        },
        "line.25": {
            "text": "jedem von ihnen ein:"
        },
        "line.26": {
            "text": "dir wird nichts geschehn"
        },
        "line.27": {
            "text": "dir wird nichts geschehn"
        }
    }
}