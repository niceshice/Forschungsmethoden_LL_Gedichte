{
    "title": "auch-deine-stirn-ist-760",
    "gender": "m",
    "author": "Maik Lippert",
    "author_birth": "1966",
    "poem": {
        "line.1": {
            "text": "zerbrechlich"
        },
        "line.2": {
            "text": "laß ruhig die bilder"
        },
        "line.3": {
            "text": "nicht ein"
        },
        "line.4": {
            "text": "in den kopf"
        },
        "line.5": {
            "text": "ein haarriß genügt"
        },
        "line.6": {
            "text": "am radkranz"
        },
        "line.7": {
            "text": "zum entgleisen"
        },
        "line.8": {
            "text": "wer zählt die abteile"
        },
        "line.9": {
            "text": "weiß die knautschzone im voraus"
        },
        "line.10": {
            "text": "nur die puppen"
        },
        "line.11": {
            "text": "bleiben heil in leblosen armen"
        },
        "line.12": {
            "text": "plüschtiere mit dem chip"
        },
        "line.13": {
            "text": "am richtigen fleck"
        },
        "line.14": {
            "text": "können noch sprechen"
        },
        "line.15": {
            "text": "persönliche fahrtenschreiber"
        },
        "line.16": {
            "text": "die schweigend einsatzhelfer dann auflesen"
        },
        "line.17": {
            "text": "doch keiner kann dir die passende"
        },
        "line.18": {
            "text": "mignonzelle reichen"
        },
        "line.19": {
            "text": "damit du verheilst"
        }
    }
}