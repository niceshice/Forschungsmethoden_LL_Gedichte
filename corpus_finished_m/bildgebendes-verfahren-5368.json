    "poem": {
        "line.1": {
            "text": "so leicht durchschaubar"
        },
        "line.2": {
            "text": "die nase"
        },
        "line.3": {
            "text": "(eh nur knorpel) unterschlagen"
        },
        "line.4": {
            "text": "das kettchen"
        },
        "line.5": {
            "text": "um den halsfrei"
        },
        "line.6": {
            "text": "schwebend wie im märchen"
        },
        "line.7": {
            "text": "fesseln"
        },
        "line.8": {
            "text": "an gespenstern"
        },
        "line.9": {
            "text": "in den besten jahren(wir"
        },
        "line.10": {
            "text": "bitten sie"
        },
        "line.11": {
            "text": "vor der aufnahme"
        },
        "line.12": {
            "text": "alles geschmeide"
        },
        "line.13": {
            "text": "abzulegen)"
        },
        "line.14": {
            "text": "morieris"
        },
        "line.15": {
            "text": "und ein bißchen freude"
        },
        "line.16": {
            "text": "am schmuckdie ringe"
        },
        "line.17": {
            "text": "an fingern"
        },
        "line.18": {
            "text": "und ohren sind"
        },
        "line.19": {
            "text": "statements"
        },
        "line.20": {
            "text": "tief"
        },
        "line.21": {
            "text": "schwarze zirkel und"
        },
        "line.22": {
            "text": "hyphens"
        },
        "line.23": {
            "text": "in der syntax"
        },
        "line.24": {
            "text": "des skeletts"
        },
        "line.25": {
            "text": "ein"
        },
        "line.26": {
            "text": "material girl"
        },
        "line.27": {
            "text": "um"
        },
        "line.28": {
            "text": "nicht so"
        },
        "line.29": {
            "text": "schrecklich nackt"
        },
        "line.30": {
            "text": "zu sein"
        },
        "line.31": {
