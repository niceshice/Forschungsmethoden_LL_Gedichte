{
    "title": "stand-der-dinge-7074",
    "gender": "m",
    "author": "Sepp Mall",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "Noch / bleib ich hier"
        },
        "line.2": {
            "text": ": im Zimmer / das sein Herz"
        },
        "line.3": {
            "text": "verbirgt"
        },
        "line.4": {
            "text": "Nenne die Namen der Gegenstände"
        },
        "line.5": {
            "text": "(Bettel / Blume)"
        },
        "line.6": {
            "text": "als gehörten sie mir"
        },
        "line.7": {
            "text": "Immer noch / trag ich Wörter"
        },
        "line.8": {
            "text": "herauf / aus der Kindheit"
        },
        "line.9": {
            "text": "über die Stufen der Jahre"
        },
        "line.10": {
            "text": ": Leuchtzeichen / leichtes"
        },
        "line.11": {
            "text": "Gepäck"
        },
        "line.12": {
            "text": "In ein Haus / das un-"
        },
        "line.13": {
            "text": "beirrt / durch alle Fenster"
        },
        "line.14": {
            "text": "atmet"
        }
    }
}