{
    "title": "laendliche-elegie-3023",
    "gender": "m",
    "author": "Ron Winkler",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "das Sendegerät dieses Tages"
        },
        "line.2": {
            "text": "ist auf den Windkanal eingestellt."
        },
        "line.3": {
            "text": "*"
        },
        "line.4": {
            "text": "in den Weizenfeldern gewinnen"
        },
        "line.5": {
            "text": "Andachtshalme an Geltung."
        },
        "line.6": {
            "text": "*"
        },
        "line.7": {
            "text": "über dem Land Pollenverschickung,"
        },
        "line.8": {
            "text": "gratuit et libre"
        },
        "line.9": {
            "text": "."
        },
        "line.10": {
            "text": "*"
        },
        "line.11": {
            "text": "unverblümt lagern Blüten"
        },
        "line.12": {
            "text": "ihre Aromen aus."
        },
        "line.13": {
            "text": "*"
        },
        "line.14": {
            "text": "einzelne Böen animieren knorrige Sträucher"
        },
        "line.15": {
            "text": "zu verästelten Hymnen."
        },
        "line.16": {
            "text": "*"
        },
        "line.17": {
            "text": "in manchen Glockenblumen"
        },
        "line.18": {
            "text": "schwingen Kirchenversuche."
        },
        "line.19": {
            "text": "*"
        },
        "line.20": {
            "text": "die Katzen unverändert"
        },
        "line.21": {
            "text": "per Sie mit ihrer Umgebung."
        },
        "line.22": {
            "text": "*"
        },
        "line.23": {
            "text": "die Vögel sind überstimmt. sie beschließen"
        },
        "line.24": {
            "text": "den Tag im"
        },
        "line.25": {
            "text": "silent mode"
        },
        "line.26": {
            "text": "."
        },
        "line.27": {
            "text": "*"
        },
        "line.28": {
            "text": "um Mitternacht die Regionalhymne"
        },
        "line.29": {
            "text": "der Frösche am Teich."
        }
    }
}