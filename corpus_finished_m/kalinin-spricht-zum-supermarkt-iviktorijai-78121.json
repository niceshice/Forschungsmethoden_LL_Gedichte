{
    "title": "kalinin-spricht-zum-supermarkt-iviktorijai-78121",
    "gender": "m",
    "author": "Dieter M. Gräf",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "Gelehrtenrepublik?;"
        },
        "line.2": {
            "text": "Hirn im sprung"
        },
        "line.3": {
            "text": "bereiten Löwen, stets"
        },
        "line.4": {
            "text": "dem Osten zugerichtet;"
        },
        "line.5": {
            "text": "der Ostsee Faust?"
        },
        "line.6": {
            "text": "Mag sein, so siehts jetzt aus"
        },
        "line.7": {
            "text": "Kalinin sprichtzum Supermarkt"
        },
        "line.8": {
            "text": "Viktorija"
        },
        "line.9": {
            "text": ";"
        },
        "line.10": {
            "text": "nuntummeln sichin seinervollen"
        },
        "line.11": {
            "text": "SprechblasePrallschutzFrontbügelStoß-Kuh-"
        },
        "line.12": {
            "text": "Bürgerfänger& fingernagelfarbene Reifen,"
        },
        "line.13": {
            "text": "die die Pflanzen umfassen;Königsköpfe, schweben aus"
        },
        "line.14": {
            "text": "ihrer Restaurierung,zurück,"
        },
        "line.15": {
            "text": "es ist der 22. April.Kant & Lenin haben Geburtstag.Ganz"
        },
        "line.16": {
            "text": "Viktorija"
        },
        "line.17": {
            "text": "steht aufStöckelschuh;"
        },
        "line.18": {
            "text": "zwischen der nichts überbrückenden Brücke"
        },
        "line.19": {
            "text": "& der stehengebliebenen Weltzeituhrstellt sich"
        },
        "line.20": {
            "text": "das niedergefickte Königsberg auf,diewandernden"
        },
        "line.21": {
            "text": "Denkmälerhalteninne&die nach Innen"
        },
        "line.22": {
            "text": "gedrehten Grabsteine in der Sockelverkleidungstill;"
        },
        "line.23": {
            "text": "Jungvermählte vor der Kantplatte,Romantischster"
        },
        "line.24": {
            "text": "Erreichbarer OrtMit Autoreifen eingefasste"
        },
        "line.25": {
            "text": "Vorgärten, hungernaber"
        },
        "line.26": {
            "text": "im Garten hüpft aufgehängte Tarnkleidungsanft wie Brüste"
        },
        "line.27": {
            "text": "hoch Stöckelnder,sowjetblond;"
        },
        "line.28": {
            "text": "schlimm aufgerisseneHaut"
        },
        "line.29": {
            "text": "der Straßen,ihr Pech, das die Begriffe verzieht"
        }
    }
}