{
    "title": "bruecke-ueber-die-drina-13203",
    "gender": "m",
    "author": "Max Czollek",
    "author_birth": "1987",
    "poem": {
        "line.1": {
            "text": "auf einer fahrt ist es"
        },
        "line.2": {
            "text": "herbst geworden"
        },
        "line.3": {
            "text": "in wischegrad"
        },
        "line.4": {
            "text": "die häuser tragen"
        },
        "line.5": {
            "text": "einschüsse wie sommersprossen"
        },
        "line.6": {
            "text": "felder reifer kreuze"
        },
        "line.7": {
            "text": "am straßenrand"
        },
        "line.8": {
            "text": "hol die saat ein"
        },
        "line.9": {
            "text": "wer jetzt allein ist"
        },
        "line.10": {
            "text": "weckt erinnerung"
        },
        "line.11": {
            "text": "in gläser ein dreht wachs"
        },
        "line.12": {
            "text": "um isolationsdrähte"
        },
        "line.13": {
            "text": "an den bäumen die granatäpfel"
        },
        "line.14": {
            "text": "leuchtende kinderfäuste"
        }
    }
}