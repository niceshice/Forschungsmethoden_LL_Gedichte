{
    "title": "es-war-2626",
    "gender": "m",
    "author": "Michael Donhauser",
    "author_birth": "1956",
    "poem": {
        "line.1": {
            "text": "Es war"
        },
        "line.2": {
            "text": "es wird"
        },
        "line.3": {
            "text": "und ist"
        },
        "line.4": {
            "text": "und spricht"
        },
        "line.5": {
            "text": "Sagt du"
        },
        "line.6": {
            "text": "und lacht"
        },
        "line.7": {
            "text": "vergeht"
        },
        "line.8": {
            "text": "und fragt"
        },
        "line.9": {
            "text": "Ob ich"
        },
        "line.10": {
            "text": "verliert"
        },
        "line.11": {
            "text": "und jetzt"
        },
        "line.12": {
            "text": "berührt"
        },
        "line.13": {
            "text": "Und still"
        },
        "line.14": {
            "text": "die Hand"
        },
        "line.15": {
            "text": "sagt wann"
        },
        "line.16": {
            "text": "und weiß"
        },
        "line.17": {
            "text": "Daß wir"
        },
        "line.18": {
            "text": "verwandt"
        },
        "line.19": {
            "text": "uns nah"
        },
        "line.20": {
            "text": "und friert"
        },
        "line.21": {
            "text": "Und lacht"
        },
        "line.22": {
            "text": "und sagt"
        },
        "line.23": {
            "text": "und zeigt"
        },
        "line.24": {
            "text": "luzid"
        }
    }
}