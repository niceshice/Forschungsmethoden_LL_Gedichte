{
    "title": "anmerkung-zu-jona-12069",
    "gender": "m",
    "author": "Hendrik  Jackson",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "das Wort, das nicht eintrifft"
        },
        "line.2": {
            "text": "trotzdem, wir gehen nicht in die Immanenz"
        },
        "line.3": {
            "text": "eines dickbäuchigen dunklen, kaum"
        },
        "line.4": {
            "text": "zu erforschenden Lebens sich in sich"
        },
        "line.5": {
            "text": "im weiten Meer des Wals"
        },
        "line.6": {
            "text": "es strömt, läuft"
        },
        "line.7": {
            "text": "über die Augen"
        },
        "line.8": {
            "text": "immer wartest du auf ein Zeichen"
        },
        "line.9": {
            "text": "unsterblicher Stursinn"
        },
        "line.10": {
            "text": "und der ganze Himmel hing voller"
        },
        "line.11": {
            "text": "Leuchttierchen, so träufelten wir"
        },
        "line.12": {
            "text": "die Lüge in Jonas Ohr"
        },
        "line.13": {
            "text": "zieh hinaus in die Welt"
        },
        "line.14": {
            "text": "in die zerzauste Leere"
        }
    }
}