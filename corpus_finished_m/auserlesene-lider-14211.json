{
    "title": "auserlesene-lider-14211",
    "gender": "m",
    "author": "Tom Bresemann",
    "author_birth": "1978",
    "poem": {
        "line.1": {
            "text": "auserlesene lider"
        },
        "line.2": {
            "text": ", in"
        },
        "line.3": {
            "text": "unsere augen     ist"
        },
        "line.4": {
            "text": "viel   eingegangen,"
        },
        "line.5": {
            "text": "viel"
        },
        "line.6": {
            "text": "bittres, viel"
        },
        "line.7": {
            "text": "dunkles,            und die schönheit"
        },
        "line.8": {
            "text": "ruht,              leuchtend"
        },
        "line.9": {
            "text": "in dunkelm grunde,"
        },
        "line.10": {
            "text": "gewisse einlagen,"
        },
        "line.11": {
            "text": "rueltzen und filtze,                flomen und liesen"
        },
        "line.12": {
            "text": "der fliegenpresse, wie echt                          das deutsch klang,"
        },
        "line.13": {
            "text": "daran wol zu spuren,"
        },
        "line.14": {
            "text": "was ist da gesagt"
        },
        "line.15": {
            "text": "und wie"
        },
        "line.16": {
            "text": "verstehet mans? veränderungen"
        },
        "line.17": {
            "text": "im privaten bereich"
        },
        "line.18": {
            "text": "und                    bei der arbeit, erde, und immer"
        },
        "line.19": {
            "text": "wieder       erde wird dir                  maßstab,"
        },
        "line.20": {
            "text": "um nicht der anarchie         gewisser kraft-"
        },
        "line.21": {
            "text": "genialischer                        geberden"
        },
        "line.22": {
            "text": "das wort zu reden,            die ENTstehung"
        },
        "line.23": {
            "text": "dieser keimzellen          aus eigenem erleben"
        },
        "line.24": {
            "text": "heraus:          die heutlichkeit treiben"
        },
        "line.25": {
            "text": "und zu treiben,"
        },
        "line.26": {
            "text": "beger-          und befreiung"
        },
        "line.27": {
            "text": "der individualität, feier"
        },
        "line.28": {
            "text": "des lebens,"
        },
        "line.29": {
            "text": "glanz              der sprache kann nicht"
        },
        "line.30": {
            "text": "wunder nehmen,         weltanschaulicher"
        },
        "line.31": {
            "text": "teil: daraus begreift sich,  was hinter den worten liegt,"
        },
        "line.32": {
            "text": "tausendkuenstiger,        dein tod"
        },
        "line.33": {
            "text": "hat den TOd        zerhauen, die welt"
        },
        "line.34": {
            "text": "sehen?  als soldat."
        }
    }
}