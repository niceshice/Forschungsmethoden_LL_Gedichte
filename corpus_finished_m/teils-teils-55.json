{
    "title": "teils-teils-55",
    "gender": "m",
    "author": "Gottfried Benn",
    "author_birth": "1886",
    "poem": {
        "line.1": {
            "text": "In meinem Elternhaus hingen keine Gainsboroughs"
        },
        "line.2": {
            "text": "wurde auch kein Chopin gespielt"
        },
        "line.3": {
            "text": "ganz amusisches Gedankenleben"
        },
        "line.4": {
            "text": "mein Vater war einmal im Theater gewesen"
        },
        "line.5": {
            "text": "Anfang des Jahrhunderts"
        },
        "line.6": {
            "text": "Wildenbruchs Haubenlerche"
        },
        "line.7": {
            "text": "davon zehrten wir"
        },
        "line.8": {
            "text": "das war alles."
        },
        "line.9": {
            "text": "Nun längst zu Ende"
        },
        "line.10": {
            "text": "graue Herzen, graue Haare"
        },
        "line.11": {
            "text": "der Garten in polnischem Besitz"
        },
        "line.12": {
            "text": "die Gräber teils-teils"
        },
        "line.13": {
            "text": "aber alle slawisch,"
        },
        "line.14": {
            "text": "Oder-Neiße-Linie"
        },
        "line.15": {
            "text": "für Sarginhalte ohne Belang"
        },
        "line.16": {
            "text": "die Kinder denken an sie"
        },
        "line.17": {
            "text": "die Gatten auch noch eine Weile"
        },
        "line.18": {
            "text": "teils-teils"
        },
        "line.19": {
            "text": "bis sie weitermüssen"
        },
        "line.20": {
            "text": "Sela, Psalmenende."
        },
        "line.21": {
            "text": "Heute noch in einer Großstadtnacht"
        },
        "line.22": {
            "text": "Cafterasse"
        },
        "line.23": {
            "text": "Sommersterne,"
        },
        "line.24": {
            "text": "vom Nebentisch"
        },
        "line.25": {
            "text": "Hotelqualitäten in Frankfurt"
        },
        "line.26": {
            "text": "Vergleiche,"
        },
        "line.27": {
            "text": "die Damen unbefriedigt"
        },
        "line.28": {
            "text": "wenn ihre Sehnsucht Gewicht hätte"
        },
        "line.29": {
            "text": "wöge jede drei Zentner."
        },
        "line.30": {
            "text": "Aber ein Fluidum! Heiße Nacht"
        },
        "line.31": {
            "text": "la Reiseprospekt und"
        },
        "line.32": {
            "text": "die Ladies treten aus ihren Bildern:"
        },
        "line.33": {
            "text": "unwahrscheinliche Beauties"
        },
        "line.34": {
            "text": "langbeinig, hoher Wasserfall"
        },
        "line.35": {
            "text": "über ihre Hingabe kann man sich gar nicht"
        },
        "line.36": {
            "text": "erlauben"
        },
        "line.37": {
            "text": "nachzudenken."
        },
        "line.38": {
            "text": "Ehepaare fallen demgegenüber ab,"
        },
        "line.39": {
            "text": "kommen nicht an, Bälle gehn ins Netz,"
        },
        "line.40": {
            "text": "er raucht, sie dreht ihre Ringe,"
        },
        "line.41": {
            "text": "überhaupt nachdenkenswert"
        },
        "line.42": {
            "text": "Verhältnis von Ehe und Mannesschaffen"
        },
        "line.43": {
            "text": "Lähmung oder Hochtrieb."
        },
        "line.44": {
            "text": "Fragen, Fragen! Erinnerungen in einer"
        },
        "line.45": {
            "text": "Sommernacht"
        },
        "line.46": {
            "text": "hingeblinzelt, hingestrichen,"
        },
        "line.47": {
            "text": "in meinem Elternhaus hingen keine"
        },
        "line.48": {
            "text": "Gainsboroughs"
        },
        "line.49": {
            "text": "nun alles abgesunken"
        },
        "line.50": {
            "text": "teils-teils das Ganze"
        },
        "line.51": {
            "text": "Sela, Psalmenende."
        }
    }
}