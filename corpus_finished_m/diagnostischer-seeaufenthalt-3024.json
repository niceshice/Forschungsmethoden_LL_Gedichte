{
    "title": "diagnostischer-seeaufenthalt-3024",
    "gender": "m",
    "author": "Ron Winkler",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "ein flüchtiger Blick auf das Wasser genügt:"
        },
        "line.2": {
            "text": "klassisches"
        },
        "line.3": {
            "text": "Marinett"
        },
        "line.4": {
            "text": "oder"
        },
        "line.5": {
            "text": "Tango marino"
        },
        "line.6": {
            "text": "."
        },
        "line.7": {
            "text": "Bojen markieren die Wellen als Jamben."
        },
        "line.8": {
            "text": "~"
        },
        "line.9": {
            "text": "dem Vernehmen nach werden die Farben der See"
        },
        "line.10": {
            "text": "gelegentlich überreizt."
        },
        "line.11": {
            "text": "~"
        },
        "line.12": {
            "text": "das Wasser hingegen wirkt etwas dünn."
        },
        "line.13": {
            "text": "~"
        },
        "line.14": {
            "text": "die Dinge stehen in Wettbewerb. zwei Strandkiefern"
        },
        "line.15": {
            "text": "ringen um die ästhetischste Neigung."
        },
        "line.16": {
            "text": "~"
        },
        "line.17": {
            "text": "unermüdlich die Brandung: man könnte meinen,"
        },
        "line.18": {
            "text": "hier wird eine größere Schuld abbezahlt."
        },
        "line.19": {
            "text": "~"
        },
        "line.20": {
            "text": "der Wind streicht über die See wie ein energischer Vater."
        },
        "line.21": {
            "text": "~"
        },
        "line.22": {
            "text": "alles macht einen nachhaltigen Eindruck."
        },
        "line.23": {
            "text": "noch die Reiher am Ufer: fishing for consequence."
        }
    }
}