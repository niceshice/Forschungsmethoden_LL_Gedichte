{
    "title": "mittelwelle-12346",
    "gender": "m",
    "author": "David  Krause",
    "author_birth": "1988",
    "poem": {
        "line.1": {
            "text": "das radio mit der antenne"
        },
        "line.2": {
            "text": "trugen wir durch eine landschaft"
        },
        "line.3": {
            "text": "als wünschelrute. wir fanden"
        },
        "line.4": {
            "text": "einen mittelwellensender."
        },
        "line.5": {
            "text": "der spielte stundenlang john cage"
        },
        "line.6": {
            "text": "eine urknallspur."
        },
        "line.7": {
            "text": "die antenne zitterte im wind."
        },
        "line.8": {
            "text": "beim tonwechsel setzte"
        },
        "line.9": {
            "text": "die dämmerung ein"
        },
        "line.10": {
            "text": "schlugen wir die augen auf"
        },
        "line.11": {
            "text": "nach einem langen schlaf im gras"
        },
        "line.12": {
            "text": "trafen sich unsere finger wie flüsse"
        }
    }
}