{
    "title": "sailors-list-songs-5428",
    "gender": "m",
    "author": "Uwe Kolbe",
    "author_birth": "1957",
    "poem": {
        "line.1": {
            "text": "Erst war es beim Fado"
        },
        "line.2": {
            "text": "in Porto, am Fuße der Brücke,"
        },
        "line.3": {
            "text": "am Wasser des Douro."
        },
        "line.4": {
            "text": "Wir tranken schweren Portwein."
        },
        "line.5": {
            "text": "Kaum später Flamenco,"
        },
        "line.6": {
            "text": "Toledo, Gewitter,"
        },
        "line.7": {
            "text": "vom Wasser des Tajo umzingelt."
        },
        "line.8": {
            "text": "Nur einen Sherry genommen."
        },
        "line.9": {
            "text": "Und, logisch, Tango"
        },
        "line.10": {
            "text": "damals in Buenos Aires"
        },
        "line.11": {
            "text": "am trägen Rio de la Plata."
        },
        "line.12": {
            "text": "Es war ein Malbec aus Mendoza."
        },
        "line.13": {
            "text": "Einmal der Blues, der so hieß,"
        },
        "line.14": {
            "text": "im Norden Chicagos"
        },
        "line.15": {
            "text": "am meergroßen See."
        },
        "line.16": {
            "text": "Mit Tränen im Bier."
        },
        "line.17": {
            "text": "Der immerwährende Beat"
        },
        "line.18": {
            "text": "steht in Hamburg,"
        },
        "line.19": {
            "text": "das Wasser der Alsterfleete,"
        },
        "line.20": {
            "text": "ein trockener Kümmel."
        },
        "line.21": {
            "text": "Ein Schiff wird kommen"
        },
        "line.22": {
            "text": "in Magdeburg-Sudenburg,"
        },
        "line.23": {
            "text": "am Elbe-Hafenbecken"
        },
        "line.24": {
            "text": "mit einer Flasche Wermut."
        },
        "line.25": {
            "text": "La Paloma"
        },
        "line.26": {
            "text": "in Birkenwerder nahe Berlin"
        },
        "line.27": {
            "text": "am Briesesee."
        },
        "line.28": {
            "text": "Ein Weinbrand oder zwei."
        },
        "line.29": {
            "text": "Ein Psalm"
        },
        "line.30": {
            "text": "in Budapest im Januar,"
        },
        "line.31": {
            "text": "die Kettenbrücke, die Donau,"
        },
        "line.32": {
            "text": "Marillenbrand (Plinka)."
        },
        "line.33": {
            "text": "Das Volkslied, Dudelsack,"
        },
        "line.34": {
            "text": "im uralten Plovdiv,"
        },
        "line.35": {
            "text": "das flache Bett der Mariza"
        },
        "line.36": {
            "text": "und schwerer Mavrud."
        },
        "line.37": {
            "text": "Einmal ein Choral,"
        },
        "line.38": {
            "text": "in Tübingen"
        },
        "line.39": {
            "text": "am schifflosen Neckar."
        },
        "line.40": {
            "text": "Mit Messwein."
        }
    }
}