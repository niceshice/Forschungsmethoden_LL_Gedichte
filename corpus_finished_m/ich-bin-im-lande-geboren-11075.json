{
    "title": "ich-bin-im-lande-geboren-11075",
    "gender": "m",
    "author": "Valeri Scherstjanoi (Валерий Шерстяной)",
    "author_birth": "1950",
    "poem": {
        "line.1": {
            "text": "das nie meine Heimat war."
        },
        "line.2": {
            "text": "Ich bin im Lande aufgewachsen,"
        },
        "line.3": {
            "text": "das es nicht mehr gibt."
        },
        "line.4": {
            "text": "Ich bin in das Land gegangen,"
        },
        "line.5": {
            "text": "dass es auch nicht mehr gibt."
        },
        "line.6": {
            "text": "Und die Muttersprache meiner Mutter"
        },
        "line.7": {
            "text": "Ist nicht meine Muttersprache."
        },
        "line.8": {
            "text": "Ich bin im Lautland."
        },
        "line.9": {
            "text": "Zwischen Herz und Verstand."
        },
        "line.10": {
            "text": "Hier sind die Stille,"
        },
        "line.11": {
            "text": "die Stimme"
        },
        "line.12": {
            "text": "und die Zunge,"
        },
        "line.13": {
            "text": "der nackte Teufel,"
        },
        "line.14": {
            "text": "die fleischige Waffe im Mund."
        },
        "line.15": {
            "text": "Und die anderen Körper-"
        },
        "line.16": {
            "text": "Sprachen, die anderen"
        },
        "line.17": {
            "text": "Immer da"
        },
        "line.18": {
            "text": "Zwischen"
        },
        "line.19": {
            "text": "Zwischen Russland und Deutschland"
        },
        "line.20": {
            "text": "Dem Herz und dem Verstand"
        },
        "line.21": {
            "text": "In meinem Lautland"
        }
    }
}