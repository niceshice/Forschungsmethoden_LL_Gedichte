{
    "title": "seerinde-2859",
    "gender": "m",
    "author": "Frank  Schablewski",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "das Schlittenholz"
        },
        "line.2": {
            "text": "im Uferschlamm"
        },
        "line.3": {
            "text": "liegt an dem"
        },
        "line.4": {
            "text": "sich Wellen rechen"
        },
        "line.5": {
            "text": "zwischen den Steinen"
        },
        "line.6": {
            "text": "blüht jetzt der Frost aus"
        },
        "line.7": {
            "text": "beschlägt den Spiegel"
        },
        "line.8": {
            "text": "glatt der schale Ton"
        },
        "line.9": {
            "text": "der Seerinde"
        },
        "line.10": {
            "text": "der nicht trägt"
        },
        "line.11": {
            "text": "drei Schritte von dem"
        },
        "line.12": {
            "text": "Leib zu bleiben"
        },
        "line.13": {
            "text": "in den Schädeln"
        },
        "line.14": {
            "text": "in den Sand"
        },
        "line.15": {
            "text": "gesetzt einen Fuß"
        },
        "line.16": {
            "text": "vor den anderen"
        }
    }
}