{
    "title": "oder-so-was-12830",
    "gender": "m",
    "author": "Rainer René Mueller",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "Frühchen im Schnee-,"
        },
        "line.2": {
            "text": ": welche Töne"
        },
        "line.3": {
            "text": "unter den Primeln"
        },
        "line.4": {
            "text": "Orangerie, Strasbourg :"
        },
        "line.5": {
            "text": "Störche klappern, sie"
        },
        "line.6": {
            "text": "müssen's."
        },
        "line.7": {
            "text": "Komischer :"
        },
        "line.8": {
            "text": "der Jud da"
        },
        "line.9": {
            "text": "Wieder Platanen - die"
        },
        "line.10": {
            "text": "(abermals) geplatzte Haut."
        },
        "line.11": {
            "text": "Wo ist: mein ... / :"
        },
        "line.12": {
            "text": "der Körper, dieses Gewächs."
        },
        "line.13": {
            "text": "... ich habe gekniet, mein Liebling"
        },
        "line.14": {
            "text": ": geh weg. / Ich geh weg ..."
        },
        "line.15": {
            "text": "es ist eine Litanei hier oder so was."
        },
        "line.16": {
            "text": ": oder sowas"
        }
    }
}