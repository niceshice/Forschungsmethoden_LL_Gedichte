{
    "title": "protokoll-rimini-15600",
    "gender": "m",
    "author": "Steffen  Mensching",
    "author_birth": "1958",
    "poem": {
        "line.1": {
            "text": "Alle Völker sind anwesend,"
        },
        "line.2": {
            "text": "Alle Fahnen, die Russen"
        },
        "line.3": {
            "text": "Und Belgier, die Polen"
        },
        "line.4": {
            "text": "Und Deutschen, sogar"
        },
        "line.5": {
            "text": "Das grüne Feld Brasiliens,"
        },
        "line.6": {
            "text": "Die Argentinier und Finnen,"
        },
        "line.7": {
            "text": "Norweger und Österreicher,"
        },
        "line.8": {
            "text": "San Marino sowieso und Italien"
        },
        "line.9": {
            "text": "Nicht zu vergessen, nur die rote"
        },
        "line.10": {
            "text": "Fahne (bandiere rossa, bandiere"
        },
        "line.11": {
            "text": "Rossa) bedeutet auch hier"
        },
        "line.12": {
            "text": "Nur noch: Baden verboten!"
        }
    }
}