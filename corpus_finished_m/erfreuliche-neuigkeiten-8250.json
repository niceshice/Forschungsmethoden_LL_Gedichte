{
    "title": "erfreuliche-neuigkeiten-8250",
    "gender": "m",
    "author": "Ror Wolf",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "Ich bin einem Bäcker begegnet,"
        },
        "line.2": {
            "text": "in einer Konditorei."
        },
        "line.3": {
            "text": "Ich glaube, es hatte geregnet,"
        },
        "line.4": {
            "text": "ich ging an dem Bäcker vorbei."
        },
        "line.5": {
            "text": "Es war im April gegen sieben,"
        },
        "line.6": {
            "text": "am Dienstag in Zornheim-Nord."
        },
        "line.7": {
            "text": "Ich habe es niedergeschrieben."
        },
        "line.8": {
            "text": "Dann zog ich von Zornheim fort."
        },
        "line.9": {
            "text": "1983"
        }
    }
}