{
    "title": "verstreut-im-fluss-5672",
    "gender": "m",
    "author": "Manfred Peter Hein",
    "author_birth": "1931",
    "poem": {
        "line.1": {
            "text": "Der Sichelmond hängt schräg"
        },
        "line.2": {
            "text": "überm Minarett dessen"
        },
        "line.3": {
            "text": "Nadelspitze Engel"
        },
        "line.4": {
            "text": "versammelt unsichtbar"
        },
        "line.5": {
            "text": "irr grüßt hinauf"
        },
        "line.6": {
            "text": "der Fremde Nächte"
        },
        "line.7": {
            "text": "voraus seinem Untergang"
        },
        "line.8": {
            "text": "Taggesichte im Blick"
        },
        "line.9": {
            "text": "beschreibend nichts als"
        },
        "line.10": {
            "text": "Gesichter tragende"
        },
        "line.11": {
            "text": "Schatten Blätter Spreu"
        },
        "line.12": {
            "text": "unter der Sichel"
        },
        "line.13": {
            "text": "im Fluß"
        }
    }
}