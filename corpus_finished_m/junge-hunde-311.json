{
    "title": "junge-hunde-311",
    "gender": "m",
    "author": "Marcel Beyer",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "Ach, die Gutgebügelten, junge"
        },
        "line.2": {
            "text": "Luden am Nebentisch, trinken"
        },
        "line.3": {
            "text": "das Panzerpils und tauschen"
        },
        "line.4": {
            "text": "Herrenheftchen. Einer wirft beim"
        },
        "line.5": {
            "text": "Aufstehn die Flasche Bier um,"
        },
        "line.6": {
            "text": "schmiert dann, nach und nach, ein"
        },
        "line.7": {
            "text": "ganzes Paket Tempos über den"
        },
        "line.8": {
            "text": "Plastiksitz. Kurzschnitte, und"
        },
        "line.9": {
            "text": "Pomade. Totes Büffet. Im Jungsklo,"
        },
        "line.10": {
            "text": "schöne Teile. Ein alter Glatzkopf"
        },
        "line.11": {
            "text": "zupft sich etwas von den Lippen."
        },
        "line.12": {
            "text": "Humer-Bursche. Geschlipst. Gewienert."
        },
        "line.13": {
            "text": "Junge Hunde. Fickriges Blau. Im"
        },
        "line.14": {
            "text": "Klappergang, Wien West, verschwitzte"
        },
        "line.15": {
            "text": "Gürteltiere. Rauch schneller, Lude."
        },
        "line.16": {
            "text": "Ohneservice. Vielleicht Ein- bis"
        },
        "line.17": {
            "text": "Zweihundert, in Randbezirken,"
        },
        "line.18": {
            "text": "Arbeiterbeisln, Siebzehnter. Trotte"
        },
        "line.19": {
            "text": "im Regen, aus einem offenen Fenster,"
        },
        "line.20": {
            "text": "obere Etage, Dampf."
        }
    }
}