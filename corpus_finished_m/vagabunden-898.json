{
    "title": "vagabunden-898",
    "gender": "m",
    "author": "Jürgen Theobaldy",
    "author_birth": "1944",
    "poem": {
        "line.1": {
            "text": "Der Gärtner fährt hinaus, ein Lied,"
        },
        "line.2": {
            "text": "auf seiner Mähmaschine weit hinaus,"
        },
        "line.3": {
            "text": "der Morgen singt es, fern die Wolken,"
        },
        "line.4": {
            "text": "und Wege führen in die Felder;"
        },
        "line.5": {
            "text": "die Insel ist ein Floß, es treibt"
        },
        "line.6": {
            "text": "auf flüssigem Gestein und Eisen."
        },
        "line.7": {
            "text": "Vorläufig sind wir sicher, also"
        },
        "line.8": {
            "text": "sind wir vorläufig, Gärtner, he,"
        },
        "line.9": {
            "text": "dort draußen? Du am Zaun, ich hier"
        },
        "line.10": {
            "text": "am Tisch! Dazwischen Pfützen, Korn,"
        },
        "line.11": {
            "text": "es keimt. Das Lied, notiere ich,"
        },
        "line.12": {
            "text": "singt von sich selbst, ich singe mich."
        }
    }
}