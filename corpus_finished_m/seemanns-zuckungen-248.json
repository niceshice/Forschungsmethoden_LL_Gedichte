{
    "title": "seemanns-zuckungen-248",
    "gender": "m",
    "author": "Jörg Schieke",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "auf schlechte späße folgen lange"
        },
        "line.2": {
            "text": "pausen; über wasserdampf"
        },
        "line.3": {
            "text": "gehaltene"
        },
        "line.4": {
            "text": "sätze mit so wunderbar weichen füßen"
        },
        "line.5": {
            "text": "aus japan. und ich konnte nicht anders, vertauschte marln"
        },
        "line.6": {
            "text": "mit marlns zigaretten, ging ein zweites mal"
        },
        "line.7": {
            "text": "duschen & denken:"
        },
        "line.8": {
            "text": "ja; da möchte"
        },
        "line.9": {
            "text": "man reißverschluß sein, da möchte"
        },
        "line.10": {
            "text": "man strände bestaunen & verschwundene"
        },
        "line.11": {
            "text": "städte (schwer"
        },
        "line.12": {
            "text": "zu beschreiben), und man möchte"
        },
        "line.13": {
            "text": "das meer sehn, das meer; den gemütlichen"
        },
        "line.14": {
            "text": "teil"
        },
        "line.15": {
            "text": "der gleise."
        }
    }
}