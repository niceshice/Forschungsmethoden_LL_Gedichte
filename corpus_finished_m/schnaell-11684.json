{
    "title": "schnaell-11684",
    "gender": "m",
    "author": "Franz Hohler",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "schnäll i Chäller"
        },
        "line.2": {
            "text": "schnäll voruse"
        },
        "line.3": {
            "text": "schnäll ufs Hüsli"
        },
        "line.4": {
            "text": "schnäll i Migros"
        },
        "line.5": {
            "text": "schnäll zum Coiffeur"
        },
        "line.6": {
            "text": "schnäll zum Dokter"
        },
        "line.7": {
            "text": "schnäll i d Schtadt"
        },
        "line.8": {
            "text": "schnäll go poschte"
        },
        "line.9": {
            "text": "schnäll go wäsche"
        },
        "line.10": {
            "text": "schnäll go choche"
        },
        "line.11": {
            "text": "schnäll go ässe"
        },
        "line.12": {
            "text": "schnäll go schwümme"
        },
        "line.13": {
            "text": "schnäll go laufe"
        },
        "line.14": {
            "text": "schnäll go luege"
        },
        "line.15": {
            "text": "schnäll go uftue"
        },
        "line.16": {
            "text": "schnäll go grüesse"
        },
        "line.17": {
            "text": "schnäll go bsueche"
        },
        "line.18": {
            "text": "schnäll go hälfe"
        },
        "line.19": {
            "text": "schnäll go läbe"
        }
    }
}