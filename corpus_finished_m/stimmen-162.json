{
    "title": "stimmen-162",
    "gender": "m",
    "author": "Paul Celan",
    "author_birth": "1920",
    "poem": {
        "line.1": {
            "text": "Stimmen"
        },
        "line.2": {
            "text": ", ins Grün"
        },
        "line.3": {
            "text": "der Wasserfläche geritzt."
        },
        "line.4": {
            "text": "Wenn der Eisvogel taucht,"
        },
        "line.5": {
            "text": "sirrt die Sekunde:"
        },
        "line.6": {
            "text": "Was zu dir stand"
        },
        "line.7": {
            "text": "an jedem der Ufer,"
        },
        "line.8": {
            "text": "es tritt"
        },
        "line.9": {
            "text": "gemäht in ein anderes Bild."
        },
        "line.10": {
            "text": "*"
        },
        "line.11": {
            "text": "Stimmen"
        },
        "line.12": {
            "text": "vom Nesselweg her:"
        },
        "line.13": {
            "text": "Komm auf den Händen zu uns."
        },
        "line.14": {
            "text": "Wer mit der Lampe allein ist,"
        },
        "line.15": {
            "text": "hat nur die Hand, draus zu lesen."
        },
        "line.16": {
            "text": "*"
        },
        "line.17": {
            "text": "Stimmen"
        },
        "line.18": {
            "text": ", nachtdurchwachsen, Stränge,"
        },
        "line.19": {
            "text": "an die du die Glocke hängst."
        },
        "line.20": {
            "text": "Wölbe dich, Welt:"
        },
        "line.21": {
            "text": "Wenn die Totenmuschel heranschwimmt,"
        },
        "line.22": {
            "text": "will es hier läuten."
        },
        "line.23": {
            "text": "*"
        },
        "line.24": {
            "text": "Stimmen"
        },
        "line.25": {
            "text": ", vor denen dein Herz"
        },
        "line.26": {
            "text": "ins Herz deiner Mutter zurückweicht."
        },
        "line.27": {
            "text": "Stimmen vom Galgenbaum her,"
        },
        "line.28": {
            "text": "wo Spätholz und Frühholz die Ringe"
        },
        "line.29": {
            "text": "tauschen und tauschen."
        },
        "line.30": {
            "text": "*"
        },
        "line.31": {
            "text": "Stimmen"
        },
        "line.32": {
            "text": ", kehlig, im Grus,"
        },
        "line.33": {
            "text": "darin auch Unendliches schaufelt,"
        },
        "line.34": {
            "text": "(herz-)"
        },
        "line.35": {
            "text": "schleimiges Rinnsal."
        },
        "line.36": {
            "text": "Setz hier die Boote aus, Kind,"
        },
        "line.37": {
            "text": "die ich bemannte:"
        },
        "line.38": {
            "text": "Wenn mittschiffs die Bö sich ins Recht setzt,"
        },
        "line.39": {
            "text": "treten die Klammern zusammen."
        },
        "line.40": {
            "text": "*"
        },
        "line.41": {
            "text": "Jakobsstimme"
        },
        "line.42": {
            "text": ":"
        },
        "line.43": {
            "text": "Die Tränen."
        },
        "line.44": {
            "text": "Die Tränen im Bruderaug."
        },
        "line.45": {
            "text": "Eine blieb hängen, wuchs."
        },
        "line.46": {
            "text": "Wir wohnen darin."
        },
        "line.47": {
            "text": "Atme, daß"
        },
        "line.48": {
            "text": "sie sich löse."
        },
        "line.49": {
            "text": "*"
        },
        "line.50": {
            "text": "Stimmen"
        },
        "line.51": {
            "text": "im Innern der Arche:"
        },
        "line.52": {
            "text": "Es sind"
        },
        "line.53": {
            "text": "nur die Münder"
        },
        "line.54": {
            "text": "geborgen. Ihr"
        },
        "line.55": {
            "text": "Sinkenden, hört"
        },
        "line.56": {
            "text": "auch uns."
        },
        "line.57": {
            "text": "*"
        },
        "line.58": {
            "text": "Keine"
        },
        "line.59": {
            "text": "Stimme"
        },
        "line.60": {
            "text": "ein"
        },
        "line.61": {
            "text": "Spätgeräusch, stundenfremd, deinen"
        },
        "line.62": {
            "text": "Gedanken geschenkt, hier, endlich"
        },
        "line.63": {
            "text": "herbeigewacht: ein"
        },
        "line.64": {
            "text": "Fruchtblatt, augengroß, tief"
        },
        "line.65": {
            "text": "geritzt; es"
        },
        "line.66": {
            "text": "harzt, will nicht"
        },
        "line.67": {
            "text": "vernarben."
        }
    }
}