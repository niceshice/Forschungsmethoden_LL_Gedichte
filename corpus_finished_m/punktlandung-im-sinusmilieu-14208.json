{
    "title": "punktlandung-im-sinusmilieu-14208",
    "gender": "m",
    "author": "Tom Bresemann",
    "author_birth": "1978",
    "poem": {
        "line.1": {
            "text": "I"
        },
        "line.2": {
            "text": "kreative schauprozesse, dergleichen,"
        },
        "line.3": {
            "text": "was man halt macht"
        },
        "line.4": {
            "text": "im nebenleben, wenn schon was mit medien"
        },
        "line.5": {
            "text": "dann in kreuzberg, wenn schon"
        },
        "line.6": {
            "text": "geld in die hand genommen,"
        },
        "line.7": {
            "text": "dann auch handshake."
        },
        "line.8": {
            "text": "hinab mit dem establishment!"
        },
        "line.9": {
            "text": "die matinee frißt ihre mieter"
        },
        "line.10": {
            "text": "zum mittagstisch, die krumen"
        },
        "line.11": {
            "text": "im brechts,"
        },
        "line.12": {
            "text": "hi there, hausverwaltung,"
        },
        "line.13": {
            "text": "would you sign my petition?"
        },
        "line.14": {
            "text": "freiheit ist immer die stipendien"
        },
        "line.15": {
            "text": "der anderen, der basisscore"
        },
        "line.16": {
            "text": "zur punktlandung im sinusmilieu,"
        },
        "line.17": {
            "text": "schieberin."
        },
        "line.18": {
            "text": "II"
        },
        "line.19": {
            "text": "wir erklären das scheitern des businessplans"
        },
        "line.20": {
            "text": "für gescheitert"
        },
        "line.21": {
            "text": "wir wähnen die zeitungen leer davon"
        },
        "line.22": {
            "text": "(selbstverständlich sterben wir gern für interviews zur verfügung)"
        },
        "line.23": {
            "text": "wir fordern ehrlichen lohn für ehrliche armut"
        },
        "line.24": {
            "text": "wir wollen die pleite nach der pleite"
        },
        "line.25": {
            "text": "mit unseren freunden aus der freien szene teilen"
        },
        "line.26": {
            "text": "wir wollen jetzt einmal ganz offen miteinander sein"
        },
        "line.27": {
            "text": "wir verlangen, dass kreuzberg,"
        },
        "line.28": {
            "text": "ehemals kreuzberg, wieder kreuzberg,"
        },
        "line.29": {
            "text": "ehemals kreuzberg, wird"
        },
        "line.30": {
            "text": "wir haben nichts zu meckern"
        },
        "line.31": {
            "text": "wir haben die schnauze voll"
        },
        "line.32": {
            "text": "denn der hass des alltäglichen lebens"
        },
        "line.33": {
            "text": "auf das tägliche reicht uns nicht mehr"
        },
        "line.34": {
            "text": "wir onanieren nicht wir handeln"
        },
        "line.35": {
            "text": "die letzte schlacht sind wir"
        }
    }
}