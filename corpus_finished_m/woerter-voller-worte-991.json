{
    "title": "woerter-voller-worte-991",
    "gender": "m",
    "author": "Franz Mon",
    "author_birth": "1926",
    "poem": {
        "line.1": {
            "text": "pockennarbige wörter"
        },
        "line.2": {
            "text": "kitzeln mir den gaumen"
        },
        "line.3": {
            "text": "salzwasserwörter"
        },
        "line.4": {
            "text": "fahren mir an die gurgel"
        },
        "line.5": {
            "text": "schalltote wörter"
        },
        "line.6": {
            "text": "sprießen mir aus den ohren"
        },
        "line.7": {
            "text": "bauchbindenwörter"
        },
        "line.8": {
            "text": "füllen meinen darm"
        },
        "line.9": {
            "text": "schluckaufwörter"
        },
        "line.10": {
            "text": "fallen mir um den hals"
        },
        "line.11": {
            "text": "schonkostwörter"
        },
        "line.12": {
            "text": "lachen sich ins fäustchen"
        },
        "line.13": {
            "text": "fußbodenwörter"
        },
        "line.14": {
            "text": "faßdaubenwörter"
        },
        "line.15": {
            "text": "fehlfarbenwörter"
        },
        "line.16": {
            "text": "fallen mir ins wort"
        },
        "line.17": {
            "text": "fallen mir in den arm"
        },
        "line.18": {
            "text": "nehmen mich auf den arm"
        },
        "line.19": {
            "text": "schreibtischwörter"
        },
        "line.20": {
            "text": "laufen mir den rücken herab"
        },
        "line.21": {
            "text": "mit langem schwanz"
        },
        "line.22": {
            "text": "fahren mir in die knochen"
        },
        "line.23": {
            "text": "mit langen fingern"
        },
        "line.24": {
            "text": "nagen mir im genick"
        },
        "line.25": {
            "text": "mit langen zähnen"
        },
        "line.26": {
            "text": "blasen mich ans licht"
        },
        "line.27": {
            "text": "mit ihrem langen atem"
        },
        "line.28": {
            "text": "herzensbrecherwörter"
        },
        "line.29": {
            "text": "genickbrecherwörter"
        },
        "line.30": {
            "text": "zungenbrecherwörter"
        },
        "line.31": {
            "text": "wortbrüchige wörter"
        },
        "line.32": {
            "text": "die mir das maul verbrennen"
        },
        "line.33": {
            "text": "auch wenn sie mir auf der zunge zergehen"
        },
        "line.34": {
            "text": "alle diese wörter"
        },
        "line.35": {
            "text": "mit ihrem asthma"
        },
        "line.36": {
            "text": "mit ihrem buckel"
        },
        "line.37": {
            "text": "voller borsten"
        },
        "line.38": {
            "text": "voller brechreiz"
        },
        "line.39": {
            "text": "bleiben mir im rachen stecken"
        },
        "line.40": {
            "text": "wörter mit haaren auf den zähnen"
        },
        "line.41": {
            "text": "streifen mir den rücken"
        },
        "line.42": {
            "text": "schneide ich mir aus dem fleisch"
        },
        "line.43": {
            "text": "schlagen mir überm kopf zusammen"
        },
        "line.44": {
            "text": "pockenwörter in senfsoße"
        },
        "line.45": {
            "text": "pockenwörter mit sodbrennen"
        },
        "line.46": {
            "text": "kohlrabiwörter wie sie im buche stehen"
        },
        "line.47": {
            "text": "pickelhaubenwörter"
        },
        "line.48": {
            "text": "prallaufgeblasene wörter voller worte"
        },
        "line.49": {
            "text": "die mir auf der zunge im mund verdrehen"
        }
    }
}