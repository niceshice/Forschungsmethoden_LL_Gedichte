{
    "title": "tereszin-mit-reisegruppe-2709",
    "gender": "m",
    "author": "Karl Mickel",
    "author_birth": "1935",
    "poem": {
        "line.1": {
            "text": "Jetzt, meine Damen und Herrn, sagte Bohumil"
        },
        "line.2": {
            "text": "Taks, der Reisebegleiter"
        },
        "line.3": {
            "text": "Nähern wir uns Tereszin, auf deutsch"
        },
        "line.4": {
            "text": "Theresienstadt Maidanek Auschwitz, vielleicht"
        },
        "line.5": {
            "text": "Haben Sie davon gehört"
        },
        "line.6": {
            "text": "Grab an Grab und Gräber hinter Gräbern"
        },
        "line.7": {
            "text": "Fußschmal die Wege, Gullies in den Wegen"
        },
        "line.8": {
            "text": "Unter Rosten Schlieren Licht auf schwarzem"
        },
        "line.9": {
            "text": "Wasser: in Höhe der Leichen"
        }
    }
}