{
    "title": "hinter-mir-1527",
    "gender": "m",
    "author": "Wolfgang Hilbig",
    "author_birth": "1941",
    "poem": {
        "line.1": {
            "text": "Hinter meiner Mauer"
        },
        "line.2": {
            "text": "im Rücken meiner Undurchdringlichkeit"
        },
        "line.3": {
            "text": "im Licht"
        },
        "line.4": {
            "text": "drehen sich Gottes Spielzeuge um sich selbst"
        },
        "line.5": {
            "text": "Hinter mir diese Karusselle von Desastern: grell bemalt"
        },
        "line.6": {
            "text": "und von den blauen Netzen der Sonne verhüllt"
        },
        "line.7": {
            "text": "und fern in verstockter Musik."
        }
    }
}