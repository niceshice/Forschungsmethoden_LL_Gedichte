{
    "title": "worueber-sollt-ich-klagen-6088",
    "gender": "m",
    "author": "Johannes Kühn",
    "author_birth": "1934",
    "poem": {
        "line.1": {
            "text": "Silber sah ich wieder genug am Mond,"
        },
        "line.2": {
            "text": "Gold an der Sonne. Beeren sind,"
        },
        "line.3": {
            "text": "Früchte sind gereift. Hoffnung"
        },
        "line.4": {
            "text": "hatte ich im schneeigen Januar,"
        },
        "line.5": {
            "text": "gute Lichtzeit zu erleben"
        },
        "line.6": {
            "text": "in den kommenden Frühlingswochen"
        },
        "line.7": {
            "text": "und im honiggelben Sommerwetter"
        },
        "line.8": {
            "text": "als ein Glücksjüngling"
        },
        "line.9": {
            "text": "noch immer."
        },
        "line.10": {
            "text": "Evangeliensätze von der Kanzel hab ich angestaunt,"
        },
        "line.11": {
            "text": "fromme Männer gesehn,"
        },
        "line.12": {
            "text": "und an ihr Beispiel oft"
        },
        "line.13": {
            "text": "zwiespältig gedacht."
        },
        "line.14": {
            "text": "Eine alte Frau hab ich getroffen,"
        },
        "line.15": {
            "text": "die mir erklärte"
        },
        "line.16": {
            "text": "zahnlosen Mundes,"
        },
        "line.17": {
            "text": "daß ich getanzt hätt mit ihr,"
        },
        "line.18": {
            "text": "als ich noch mit wippenden Beinen ging,"
        },
        "line.19": {
            "text": "statt mit Stecken."
        },
        "line.20": {
            "text": "Es war in einem großen Saal."
        },
        "line.21": {
            "text": "Worüber sollt ich klagen?"
        }
    }
}