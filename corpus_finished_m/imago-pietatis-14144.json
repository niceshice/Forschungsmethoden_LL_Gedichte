{
    "title": "imago-pietatis-14144",
    "gender": "m",
    "author": "Nicolai Kobus",
    "author_birth": "1968",
    "poem": {
        "line.1": {
            "text": "arrieta bitte arretier mich"
        },
        "line.2": {
            "text": "noch ein finales mal da ist"
        },
        "line.3": {
            "text": "doch keiner mehr der wirklich"
        },
        "line.4": {
            "text": "hinsieht auf meine kahle sehr"
        },
        "line.5": {
            "text": "zentrale blanke kehle"
        },
        "line.6": {
            "text": "und den trank schluck doch"
        },
        "line.7": {
            "text": "gefälligst selbst mir rinnen"
        },
        "line.8": {
            "text": "flüssigkeiten durch die finger"
        },
        "line.9": {
            "text": "wie brokatdecken des nachts"
        },
        "line.10": {
            "text": "von unterschenkeln gleiten"
        },
        "line.11": {
            "text": "in träumen als seien auch"
        },
        "line.12": {
            "text": "knochen schon lange"
        },
        "line.13": {
            "text": "kein halt mehr"
        },
        "line.14": {
            "text": "Francisco de Goya"
        },
        "line.15": {
            "text": "Selbstbildnis mit seinem Arzt Arrieta,1820,"
        },
        "line.16": {
            "text": "Öl auf Leinwand, 114,6 x 76,5 cm"
        }
    }
}