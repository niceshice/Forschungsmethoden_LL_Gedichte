{
    "title": "tante-ilses-zimmer-14203",
    "gender": "m",
    "author": "Tom Bresemann",
    "author_birth": "1978",
    "poem": {
        "line.1": {
            "text": "1"
        },
        "line.2": {
            "text": "wo das dorf der onanisten"
        },
        "line.3": {
            "text": "träumt sich reinheit, zierde"
        },
        "line.4": {
            "text": "und zufriedenheit,"
        },
        "line.5": {
            "text": "das glück zu den linnen,"
        },
        "line.6": {
            "text": "den schlüpfrigen"
        },
        "line.7": {
            "text": "stellen der schrift"
        },
        "line.8": {
            "text": "2"
        },
        "line.9": {
            "text": "wer war zuletzt"
        },
        "line.10": {
            "text": "in tante ilses zimmer?"
        },
        "line.11": {
            "text": "die zornigen zeichen."
        },
        "line.12": {
            "text": "das ist verdolmetscht,"
        },
        "line.13": {
            "text": "die im haus sind"
        },
        "line.14": {
            "text": "leuchten nachts zum fenster raus,"
        },
        "line.15": {
            "text": "in tante ilses zimmer mit dem groben"
        },
        "line.16": {
            "text": "tuch im maul,"
        },
        "line.17": {
            "text": "und ärgert dich dein rechtes aug,"
        },
        "line.18": {
            "text": "so reiß es aus und wirfs"
        },
        "line.19": {
            "text": "in tante ilses zimmer."
        },
        "line.20": {
            "text": "3"
        },
        "line.21": {
            "text": "wo das schweineleder hängt,"
        },
        "line.22": {
            "text": "in tante ilses zimmer,"
        },
        "line.23": {
            "text": "stehn die fettkartuschen"
        },
        "line.24": {
            "text": "schmiere in der dunkelheit."
        },
        "line.25": {
            "text": "du liebes herzblatt immertreu,"
        },
        "line.26": {
            "text": "sie sagen tante ilse"
        },
        "line.27": {
            "text": "ist schon lange tot,"
        },
        "line.28": {
            "text": "das schwein."
        }
    }
}