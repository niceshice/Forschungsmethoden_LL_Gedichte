{
    "title": "die-haare-gewuerm-5405",
    "gender": "m",
    "author": "Arne  Rautenberg",
    "author_birth": "1967",
    "poem": {
        "line.1": {
            "text": "die ohren wellhornschnecken"
        },
        "line.2": {
            "text": "die stirn eine riesige kupferplatte"
        },
        "line.3": {
            "text": "die augenbrauen viadukte"
        },
        "line.4": {
            "text": "die augen drehende speichenräder"
        },
        "line.5": {
            "text": "die nase ein schlangenknäuel"
        },
        "line.6": {
            "text": "der mund ein grubenunglück"
        },
        "line.7": {
            "text": "das kinn eine weiße porzellanschale"
        },
        "line.8": {
            "text": "arme beine rumpf geschlecht"
        },
        "line.9": {
            "text": "am boden liegende flicken das herz"
        },
        "line.10": {
            "text": "ein um sich schlagendes feuerwerk"
        }
    }
}