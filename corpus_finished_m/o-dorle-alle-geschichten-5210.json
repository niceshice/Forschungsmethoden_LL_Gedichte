{
    "title": "o-dorle-alle-geschichten-5210",
    "gender": "m",
    "author": "Tom Schulz",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "enden als Kofferträger, zwischen Himmel"
        },
        "line.2": {
            "text": "und Erde, im flüchtigen Akt des nackten"
        },
        "line.3": {
            "text": "realen Wahnsinns,"
        },
        "line.4": {
            "text": "geben Sie ihre Pin"
        },
        "line.5": {
            "text": "Nummer ein"
        },
        "line.6": {
            "text": ", tauschen sie den Wasser"
        },
        "line.7": {
            "text": "Melonenmann, der die Wangen trocknet"
        },
        "line.8": {
            "text": "die Tränen beziehen das Überbett, die Schlaf"
        },
        "line.9": {
            "text": "Brillen blinzeln dazu, die Sterne sind Wagen"
        },
        "line.10": {
            "text": "Standsanzeiger, wir liegen in Abteien"
        },
        "line.11": {
            "text": "die uns erliegen, wir wiegen die Brust"
        },
        "line.12": {
            "text": "die ihre Tränen in Legionen aussendet"
        },
        "line.13": {
            "text": "an eine Schlüsselmutter, die den Absinth"
        },
        "line.14": {
            "text": "verwünscht hat, mit den generalstabsmäßigen"
        },
        "line.15": {
            "text": "Zähnen, immer wieder"
        },
        "line.16": {
            "text": "malmen malmen"
        },
        "line.17": {
            "text": "die Nägel wund in den Steppbetten"
        },
        "line.18": {
            "text": "ganz Filet, ganz bestimmt"
        },
        "line.19": {
            "text": "Fisch ohne Knochen"
        },
        "line.20": {
            "text": "in Anbetracht des Taus waren wir Flüssige"
        },
        "line.21": {
            "text": "du Weiche, du Weichgeköpfte, ich"
        },
        "line.22": {
            "text": "zwei Minuten zu früh Aufgeschlagener"
        },
        "line.23": {
            "text": "in einem Santiago, wo wir die streng"
        },
        "line.24": {
            "text": "komponierten Reisenden einer Zwölfton"
        },
        "line.25": {
            "text": "Musik in Jukeboxes, wir vereinzelte"
        },
        "line.26": {
            "text": "konzessionslose Art-Deco-Kopisten"
        },
        "line.27": {
            "text": "resümieren als Heiratsschwindler in Minne"
        },
        "line.28": {
            "text": "Sängerkostümen, wir ausrangierten Fax"
        },
        "line.29": {
            "text": "Weichen, wir liegen in Weichen"
        },
        "line.30": {
            "text": "(mit dem Bahnhof trinken wir Nescaf"
        },
        "line.31": {
            "text": "auf Eis, der Elsternkreditanstalt winken"
        },
        "line.32": {
            "text": "wir hinterher)"
        },
        "line.33": {
            "text": "o Dorle, die Nerven des Ozeans"
        },
        "line.34": {
            "text": "leuchten aus dem Liebeslautbojengebiet"
        },
        "line.35": {
            "text": "wie Antennen, zerpflückt, dass der Strand"
        },
        "line.36": {
            "text": "Hafer wiehert, so glitzert Sewastopol"
        },
        "line.37": {
            "text": "auf der Schallfolie, noch einmal die Video"
        },
        "line.38": {
            "text": "Piraterie und dann wir, Hafenspieler"
        },
        "line.39": {
            "text": "mit leicht veränderlichen Brauen,"
        },
        "line.40": {
            "text": "das Meer"
        },
        "line.41": {
            "text": "hört nicht auf,"
        },
        "line.42": {
            "text": "hört nicht auf zu senden"
        }
    }
}