{
    "title": "balhisar-5670",
    "gender": "m",
    "author": "Manfred Peter Hein",
    "author_birth": "1931",
    "poem": {
        "line.1": {
            "text": "Schrift über Schrift zugeschrieben der Brut"
        },
        "line.2": {
            "text": "zusammengewürfelter Nacht"
        },
        "line.3": {
            "text": "Kybele"
        },
        "line.4": {
            "text": "Nacht"
        },
        "line.5": {
            "text": "lag ich bei"
        },
        "line.6": {
            "text": "Grimasse für nichts"
        },
        "line.7": {
            "text": "zu vergehn zu erzählen Vergehn"
        },
        "line.8": {
            "text": "Mir"
        },
        "line.9": {
            "text": "und wem gleich"
        },
        "line.10": {
            "text": "Bliebe nichts"
        },
        "line.11": {
            "text": "kräht überm phrygischen Meteoriten der Hahn"
        },
        "line.12": {
            "text": "niedergeschlagen zerstückelte"
        },
        "line.13": {
            "text": "Stimme"
        },
        "line.14": {
            "text": "flußüber Echo"
        },
        "line.15": {
            "text": "unsre Wiederkehr"
        },
        "line.16": {
            "text": "Schrift über Schrift Wer schreibt wer liest"
        },
        "line.17": {
            "text": "die Jahreszeit zu erinnern das Licht das die Nacht trägt"
        },
        "line.18": {
            "text": "die Tage unsre Tage"
        },
        "line.19": {
            "text": "die Nacht die sie gebar"
        },
        "line.20": {
            "text": "Was soll werden über uns"
        },
        "line.21": {
            "text": "aus der Farbe der Farbe"
        },
        "line.22": {
            "text": "des Planeten"
        },
        "line.23": {
            "text": "Kybele"
        },
        "line.24": {
            "text": "Mondkatze niederstürzender"
        },
        "line.25": {
            "text": "Himmelstein"
        },
        "line.26": {
            "text": "Dies meine Stimme Meine Unsichtbarkeit"
        },
        "line.27": {
            "text": "An der Skala des Regenbogens detoniert"
        },
        "line.28": {
            "text": "tauben Ohren engelbitterzüngig das Gras"
        }
    }
}