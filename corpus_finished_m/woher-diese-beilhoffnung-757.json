{
    "title": "woher-diese-beilhoffnung-757",
    "gender": "m",
    "author": "Maik Lippert",
    "author_birth": "1966",
    "poem": {
        "line.1": {
            "text": "woher diese beilhoffnung"
        },
        "line.2": {
            "text": "auf fleisch"
        },
        "line.3": {
            "text": "das der fessel aufwärts"
        },
        "line.4": {
            "text": "steigt"
        },
        "line.5": {
            "text": "ist es die geometrie"
        },
        "line.6": {
            "text": "der fasern"
        },
        "line.7": {
            "text": "die äderung"
        },
        "line.8": {
            "text": "das messer an der sehne"
        },
        "line.9": {
            "text": "hier löst sich das fleisch"
        },
        "line.10": {
            "text": "leicht"
        },
        "line.11": {
            "text": "die zunge ist auch nur ein muskel"
        },
        "line.12": {
            "text": "zum schreien"
        },
        "line.13": {
            "text": "überflüssig"
        }
    }
}