{
    "title": "sehnliches-oder-sehen-2628",
    "gender": "m",
    "author": "Michael Donhauser",
    "author_birth": "1956",
    "poem": {
        "line.1": {
            "text": "Sehnliches oder Sehen, es"
        },
        "line.2": {
            "text": "beugen die Zweige sich und"
        },
        "line.3": {
            "text": "wärmer noch oder bricht"
        },
        "line.4": {
            "text": "von Früchten schwer, was"
        },
        "line.5": {
            "text": "zärtlich entlang im Laub"
        },
        "line.6": {
            "text": "verirrt und leuchtend liegt"
        },
        "line.7": {
            "text": "Denn einsam und mild, nah"
        },
        "line.8": {
            "text": "hieß es, dem letzten Schein"
        },
        "line.9": {
            "text": "sinkt, von Stimmen umspielt"
        },
        "line.10": {
            "text": "das Haupt, die Hand, es"
        },
        "line.11": {
            "text": "war, ich nannte dich und"
        },
        "line.12": {
            "text": "Stille das herbstliche Licht"
        },
        "line.13": {
            "text": "Dein Park, deine Bank mit"
        },
        "line.14": {
            "text": "Gezwitscher, Kastanien, die"
        },
        "line.15": {
            "text": "fallen, die schlagen, auf am"
        },
        "line.16": {
            "text": "Kies, Sand, ich sah deinen"
        },
        "line.17": {
            "text": "Fuß, eine Feder fast weiß"
        },
        "line.18": {
            "text": "schaukeln nieder und ruhn"
        }
    }
}