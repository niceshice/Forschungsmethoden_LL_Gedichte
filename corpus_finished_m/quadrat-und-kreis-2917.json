{
    "title": "quadrat-und-kreis-2917",
    "gender": "m",
    "author": "Eugen  Gomringer",
    "author_birth": "1925",
    "poem": {
        "line.1": {
            "text": "quadrat und kreis"
        },
        "line.2": {
            "text": "sagte sokrates"
        },
        "line.3": {
            "text": "sind an sich schön"
        },
        "line.4": {
            "text": "vier quadrate"
        },
        "line.5": {
            "text": "in einem punkt fixiert"
        },
        "line.6": {
            "text": "darum herum beweglich"
        },
        "line.7": {
            "text": "unterschiedlich geschichtet"
        },
        "line.8": {
            "text": "sind elfmal an sich schön"
        },
        "line.9": {
            "text": "vier kreise"
        },
        "line.10": {
            "text": "in einem punkt fixiert"
        },
        "line.11": {
            "text": "darum herum beweglich"
        },
        "line.12": {
            "text": "unterschiedlich geöffnet"
        },
        "line.13": {
            "text": "sind zweiundzwanzigmal an sich schön"
        },
        "line.14": {
            "text": "vier quadrate und vier kreise"
        },
        "line.15": {
            "text": "in einem punkt fixiert"
        },
        "line.16": {
            "text": "darum herum beweglich"
        },
        "line.17": {
            "text": "unterschiedlich geschichtet"
        },
        "line.18": {
            "text": "unterschiedlich geöffnet"
        },
        "line.19": {
            "text": "sind vierundvierzigmal an sich schön"
        }
    }
}