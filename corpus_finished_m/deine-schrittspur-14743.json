{
    "title": "deine-schrittspur-14743",
    "gender": "m",
    "author": "Jürgen  Nendza",
    "author_birth": "1957",
    "poem": {
        "line.1": {
            "text": "DEINE SCHRITTSPUR ein Reigen"
        },
        "line.2": {
            "text": "aus Augenblicksflächen. Ihr Einwässern"
        },
        "line.3": {
            "text": "infußbreite Spiegel, einbleichend"
        },
        "line.4": {
            "text": "in kurze Himmelstränken,"
        },
        "line.5": {
            "text": "mit Versickern belebt. Ein Spalier"
        },
        "line.6": {
            "text": "aufgesprungener Muscheln, eine Möwe"
        },
        "line.7": {
            "text": "im Schlichtkleid. Du gehst weiter."
        },
        "line.8": {
            "text": "Die Küste gefüttert mit Blicken,"
        },
        "line.9": {
            "text": "die Schuhe in der Hand, die Bänder"
        },
        "line.10": {
            "text": "wie Köder für feste Schleifen."
        }
    }
}