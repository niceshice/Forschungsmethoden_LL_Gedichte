{
    "title": "kaffeehaus-10657",
    "gender": "m",
    "author": "Bas Böttcher",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "Wir sitzen"
        },
        "line.2": {
            "text": "unter Gästen"
        },
        "line.3": {
            "text": "gucken in Kästen"
        },
        "line.4": {
            "text": "trinken aus Tassen"
        },
        "line.5": {
            "text": "klicken den Button"
        },
        "line.6": {
            "text": "incognito unter Nerds und Arbeitslosen"
        },
        "line.7": {
            "text": "in verwaschenen Shirts und Poetenposen"
        },
        "line.8": {
            "text": "im Kaffeehaus zwischen Anzugtyp und Webentwickler"
        },
        "line.9": {
            "text": "sind wir - die anonymen Lyriker"
        },
        "line.10": {
            "text": "Wir bleiben versteckt und entrückt"
        },
        "line.11": {
            "text": "weil sich jedes Caf beglückt"
        },
        "line.12": {
            "text": "mit kleinsten spuren Schmückt"
        },
        "line.13": {
            "text": "Schon Brecht hat hier Käsekuchen verdrückt!"
        },
        "line.14": {
            "text": "Und ganz genau an diesem Orte"
        },
        "line.15": {
            "text": "futterten Genies schon Torte!"
        },
        "line.16": {
            "text": "Kein Caf in Prag wo Kafka kein Kaffee soff"
        },
        "line.17": {
            "text": "Wo Pasternak saß, saß vorher schon Bulgakow"
        },
        "line.18": {
            "text": "Und hier trank Heinrich Heine Wein"
        },
        "line.19": {
            "text": "Und da verschlang der Ringelnatz Kringel"
        },
        "line.20": {
            "text": "Wir sitzen"
        },
        "line.21": {
            "text": "unter Gästen"
        },
        "line.22": {
            "text": "gucken in Kästen"
        },
        "line.23": {
            "text": "trinken aus Tassen"
        },
        "line.24": {
            "text": "klicken den Butten"
        },
        "line.25": {
            "text": "kühne Beobachter, Zyniker und anonyme Lyriker"
        },
        "line.26": {
            "text": "wir lungern in Gaststätten und Kaffee-Ketten"
        },
        "line.27": {
            "text": "hinter Laptoptastaturen und Gazetten"
        },
        "line.28": {
            "text": "und grinsen, wenn jemand verkündet"
        },
        "line.29": {
            "text": "Schon Celan hat hier Zigarren angezündet."
        },
        "line.30": {
            "text": "Wo der Kaffee zwar schlecht, doch das Image echt echt ist"
        },
        "line.31": {
            "text": "bevölkern wir Tische und Theken im Gedächtnis"
        },
        "line.32": {
            "text": "an unsere Helden der Lyrik"
        },
        "line.33": {
            "text": "Die saßen und aßen hier Frühstück!"
        },
        "line.34": {
            "text": "Incognito unter Nerds und Arbeitslosen"
        },
        "line.35": {
            "text": "in verwaschenen Shirts und Poetenposen"
        },
        "line.36": {
            "text": "zwischen den Stühlen und dem Tischkicker"
        },
        "line.37": {
            "text": "lauern wir!"
        },
        "line.38": {
            "text": "Die anonymen Lyriker!"
        }
    }
}