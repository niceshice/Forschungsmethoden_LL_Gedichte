{
    "title": "adriaen-brouwer-1605-1638-15593",
    "gender": "m",
    "author": "Steffen  Mensching",
    "author_birth": "1958",
    "poem": {
        "line.1": {
            "text": "Diogenes Cynicus."
        },
        "line.2": {
            "text": "Trag im Malen, fleißig"
        },
        "line.3": {
            "text": "Im Verzehren."
        },
        "line.4": {
            "text": "Schlechtes Beispiel"
        },
        "line.5": {
            "text": "Fur die Einsamkeit"
        },
        "line.6": {
            "text": "Des Artisten. Schmutzkragen, informeller"
        },
        "line.7": {
            "text": "Mitarbeiter der Lutheraner,"
        },
        "line.8": {
            "text": "Ohne Honorarvertrag. Seine Pfeife soll,"
        },
        "line.9": {
            "text": "Wie Isaac Bullaert berichtet,"
        },
        "line.10": {
            "text": "Nur selten ausgegangen sein."
        },
        "line.11": {
            "text": "Winkelkneipen, Korbmacher, zankische Weiber,"
        },
        "line.12": {
            "text": "Quacksalber, Dirnen, Hunde, Soldaten,"
        },
        "line.13": {
            "text": "Wurfelnd im Dreck, zerbrochene Stuhle, Fasser,"
        },
        "line.14": {
            "text": "Berauschte Raucher, Trinker, Schweine, Dorrfisch,"
        },
        "line.15": {
            "text": "Nach dem Katzen schnappen. Das Leben, im Rahmen"
        },
        "line.16": {
            "text": "Des Moglichen,"
        },
        "line.17": {
            "text": "Wie es halt ablauft, stinkend, normal."
        },
        "line.18": {
            "text": "Unberuhrt von der Erfindung"
        },
        "line.19": {
            "text": "Der"
        },
        "line.20": {
            "text": "Luftpumpe"
        },
        "line.21": {
            "text": ", Krieg, Newtons"
        },
        "line.22": {
            "text": "Thesen, dem Abfall"
        },
        "line.23": {
            "text": "Der Niederlande, Sturz"
        },
        "line.24": {
            "text": "Des Imperiums Spanien, saubert"
        },
        "line.25": {
            "text": "Ein Mensch einen Kinderarsch. Oh"
        },
        "line.26": {
            "text": "Ewigkeit. Wie ihn die Scheiße"
        },
        "line.27": {
            "text": "Auch anstinkt. Trotzdem wischt er sie ab."
        }
    }
}