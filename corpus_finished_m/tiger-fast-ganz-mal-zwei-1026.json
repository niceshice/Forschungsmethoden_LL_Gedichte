{
    "title": "tiger-fast-ganz-mal-zwei-1026",
    "gender": "m",
    "author": "Ferdinand Schmatz",
    "author_birth": "1953",
    "poem": {
        "line.1": {
            "text": "spürt"
        },
        "line.2": {
            "text": "pocht"
        },
        "line.3": {
            "text": "zuckt"
        },
        "line.4": {
            "text": "wähnt, innen, lust"
        },
        "line.5": {
            "text": "sträubt gelb,"
        },
        "line.6": {
            "text": "beutelt die sinne"
        },
        "line.7": {
            "text": "vom teil zum ganzen"
        },
        "line.8": {
            "text": "wild durch"
        },
        "line.9": {
            "text": "ruckt ins viertel vor,"
        },
        "line.10": {
            "text": "zielt volles"
        },
        "line.11": {
            "text": "da"
        },
        "line.12": {
            "text": "prallt im ganzsprung schräge"
        },
        "line.13": {
            "text": "- gebrochen durch strich"
        },
        "line.14": {
            "text": "dumpf auf beute,"
        },
        "line.15": {
            "text": "schlägt"
        },
        "line.16": {
            "text": "reisst"
        },
        "line.17": {
            "text": "fasst, es,"
        },
        "line.18": {
            "text": "gewendet schmerz"
        },
        "line.19": {
            "text": "brennt ihm pulsierendes"
        },
        "line.20": {
            "text": "im nachmoment"
        },
        "line.21": {
            "text": "samt und sonder aller"
        },
        "line.22": {
            "text": "herz"
        }
    }
}