{
    "title": "imago-4372",
    "gender": "m",
    "author": "Christian Schloyer",
    "author_birth": "1976",
    "poem": {
        "line.1": {
            "text": "die jahreszeiten wechseln nun"
        },
        "line.2": {
            "text": "täglich  mit den liebschaften"
        },
        "line.3": {
            "text": "im kopf  steigt stetig der"
        },
        "line.4": {
            "text": "druck, ich bin wie die plötzlich"
        },
        "line.5": {
            "text": "hereinziehende winterlandschaft"
        },
        "line.6": {
            "text": "unüberhörbares atemgeräusch"
        },
        "line.7": {
            "text": "wenn sich die stigmen schließen  zieht der tausendfüßler"
        },
        "line.8": {
            "text": "weiter  ich werde"
        },
        "line.9": {
            "text": "beobachtet  von menschen"
        },
        "line.10": {
            "text": "anhäufungen  verschieben sich"
        },
        "line.11": {
            "text": "durch den reisetrakt, halb +"
        },
        "line.12": {
            "text": "halb verdaut  will ich nichts als verpuppung"
        }
    }
}