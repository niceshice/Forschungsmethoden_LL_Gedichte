{
    "title": "kuehle-religionen-2590",
    "gender": "m",
    "author": "Hans Thill",
    "author_birth": "1954",
    "poem": {
        "line.1": {
            "text": "im kniehohen Kraut wuchs man rasch"
        },
        "line.2": {
            "text": "zu Tannen die beisammenstanden"
        },
        "line.3": {
            "text": "spitze Häuser im Nacken der Berg eine"
        },
        "line.4": {
            "text": "altbekannte Kante"
        },
        "line.5": {
            "text": "das Land eine Unebenheit zwischen"
        },
        "line.6": {
            "text": "Wasserläufen ein Kaffeefleck auf dem"
        },
        "line.7": {
            "text": "allmächtigen weißgedeckten Tisch"
        },
        "line.8": {
            "text": "abschüssige Jahre man weiß nicht"
        },
        "line.9": {
            "text": "von welchem Punkt aus zu vermessen"
        },
        "line.10": {
            "text": "welche Gebirge in einsameren Gegenden"
        },
        "line.11": {
            "text": "aufgeschüttet wurden"
        },
        "line.12": {
            "text": "heiliger Berg klotziger zugiger"
        },
        "line.13": {
            "text": "Brautkörper Gräber weithin zu sehen"
        },
        "line.14": {
            "text": "und Türme alle gleichnamig dazwischen"
        },
        "line.15": {
            "text": "unverständliche Dialekte zu lesen"
        }
    }
}