{
    "title": "jaguar-und-neinguar-1434",
    "gender": "m",
    "author": "Paul Maar",
    "author_birth": "1937",
    "poem": {
        "line.1": {
            "text": "Jaguar und Neinguar,"
        },
        "line.2": {
            "text": "die trafen sich im Februar"
        },
        "line.3": {
            "text": "(am Freitag, um halb sieben)"
        },
        "line.4": {
            "text": "in einem alten Labyrinth."
        },
        "line.5": {
            "text": "Und wenn sie nicht gegangen sind,"
        },
        "line.6": {
            "text": "dann sind sie dort geblieben!"
        }
    }
}