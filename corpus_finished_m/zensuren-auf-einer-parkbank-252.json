{
    "title": "zensuren-auf-einer-parkbank-252",
    "gender": "m",
    "author": "Walther Petri",
    "author_birth": "1940",
    "poem": {
        "line.1": {
            "text": "Frau EINS sitzt kerzengerade"
        },
        "line.2": {
            "text": "und denkt: Wie bin ich schön !"
        },
        "line.3": {
            "text": "Frau ZWEI kaut Schokolade"
        },
        "line.4": {
            "text": "ich seh sie Däumchen drehn"
        },
        "line.5": {
            "text": "dann häkelt sie und mäkelt:"
        },
        "line.6": {
            "text": "Frau EINS wär viel zu schlank"
        },
        "line.7": {
            "text": "und wenn sie weiter Einsen schreibt"
        },
        "line.8": {
            "text": "wird sie bestimmt noch krank"
        },
        "line.9": {
            "text": "Frau DREI mit rundem Hintern"
        },
        "line.10": {
            "text": "sie schaut vom Stricken auf"
        },
        "line.11": {
            "text": "zu ihren frechen Kindern"
        },
        "line.12": {
            "text": "aber leise backen kleine Dreier"
        },
        "line.13": {
            "text": "aus Sand und Wasser Eier"
        },
        "line.14": {
            "text": "kuchen"
        },
        "line.15": {
            "text": "Man hört Frau VIER"
        },
        "line.16": {
            "text": "über sich selber fluchen"
        },
        "line.17": {
            "text": "Nur Frau FÜNF ist aufgestanden:"
        },
        "line.18": {
            "text": "sie will den Lehrer besuchen"
        }
    }
}