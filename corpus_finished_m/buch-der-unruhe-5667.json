{
    "title": "buch-der-unruhe-5667",
    "gender": "m",
    "author": "Manfred Peter Hein",
    "author_birth": "1931",
    "poem": {
        "line.1": {
            "text": "Aufgelassener Ölmühle Schilfdach"
        },
        "line.2": {
            "text": "Schatten der fällt wo ich lese"
        },
        "line.3": {
            "text": "im Buch der Unruhe während"
        },
        "line.4": {
            "text": "langstreift am Strandgemäuer"
        },
        "line.5": {
            "text": "wie gestern das Maultier"
        },
        "line.6": {
            "text": "sich scheuernd am Ölbaum"
        },
        "line.7": {
            "text": "Stimme des Schattens schürft"
        },
        "line.8": {
            "text": "im Schatten"
        },
        "line.9": {
            "text": "zu gleicher Stunde"
        },
        "line.10": {
            "text": "Wo"
        },
        "line.11": {
            "text": "mögen die wahrhaft"
        },
        "line.12": {
            "text": "in Wahrheit lebendig"
        },
        "line.13": {
            "text": "Lebenden sein"
        }
    }
}