{
    "title": "zahnschliff-jetzt-1023",
    "gender": "m",
    "author": "Ferdinand Schmatz",
    "author_birth": "1953",
    "poem": {
        "line.1": {
            "text": "es - hahn -, feilt"
        },
        "line.2": {
            "text": "im zahn schliff"
        },
        "line.3": {
            "text": "auf sehen zu lesen"
        },
        "line.4": {
            "text": "der ernte brief, auf:"
        },
        "line.5": {
            "text": "lippenblatt"
        },
        "line.6": {
            "text": "wink und stoss,"
        },
        "line.7": {
            "text": "stappelt des"
        },
        "line.8": {
            "text": "bambusbambus (=bambus)"
        },
        "line.9": {
            "text": "baumkuss"
        },
        "line.10": {
            "text": "am salztag und wendet"
        },
        "line.11": {
            "text": "der süssnacht bestand,"
        },
        "line.12": {
            "text": "allen ganges zu sammeln:"
        },
        "line.13": {
            "text": "schattengüsse,"
        },
        "line.14": {
            "text": "fein schliff wesen zu rahmen,"
        },
        "line.15": {
            "text": "der münder zu"
        },
        "line.16": {
            "text": "- zahnblut kapselt -"
        },
        "line.17": {
            "text": "lautet:"
        },
        "line.18": {
            "text": "wortwort"
        },
        "line.19": {
            "text": "zu wenden vokale"
        },
        "line.20": {
            "text": "zu tönen kon-, sorten,"
        },
        "line.21": {
            "text": "zu lippen überm palmfuss"
        },
        "line.22": {
            "text": "nageln die liedfrucht -"
        },
        "line.23": {
            "text": "halb winpernd,"
        },
        "line.24": {
            "text": "die kauhand (=nichts)"
        },
        "line.25": {
            "text": "schliessen,"
        },
        "line.26": {
            "text": "zu kosten die stunde,"
        },
        "line.27": {
            "text": "aus: augen"
        },
        "line.28": {
            "text": "aus: sonne"
        },
        "line.29": {
            "text": "aus: tag"
        },
        "line.30": {
            "text": "weg lesen, ver"
        },
        "line.31": {
            "text": "- salzen, -sprechen und stehen"
        },
        "line.32": {
            "text": "zu raten"
        },
        "line.33": {
            "text": "spruchspruch,"
        },
        "line.34": {
            "text": "welche berinnen"
        },
        "line.35": {
            "text": "die blattblatt,"
        },
        "line.36": {
            "text": "zu wandeln takt -"
        },
        "line.37": {
            "text": "los jagend im furchtstrom"
        },
        "line.38": {
            "text": "gemeinsam!"
        }
    }
}