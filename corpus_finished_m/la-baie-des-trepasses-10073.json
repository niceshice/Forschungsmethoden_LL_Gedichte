{
    "title": "la-baie-des-trepasses-10073",
    "gender": "m",
    "author": "Andreas Neeser",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "Die Brise im Ohr überschlägt sich"
        },
        "line.2": {
            "text": "im Rausch"
        },
        "line.3": {
            "text": "wirft sie Wellen aus schaumigem Grün."
        },
        "line.4": {
            "text": "Beim Leuchtturm drei Möwen"
        },
        "line.5": {
            "text": "Sehnsucht"
        },
        "line.6": {
            "text": "geflügeltes Wort."
        },
        "line.7": {
            "text": "Die sinkende Sonne schickt Finger"
        },
        "line.8": {
            "text": "ein Blick in die Blendung"
        },
        "line.9": {
            "text": "ein Gischtrest im Mund"
        },
        "line.10": {
            "text": "von der Insel dein Duft."
        },
        "line.11": {
            "text": "Einmal erwachen mit Schwimmhaut"
        },
        "line.12": {
            "text": "am Herz."
        }
    }
}