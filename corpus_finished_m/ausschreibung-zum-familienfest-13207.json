{
    "title": "ausschreibung-zum-familienfest-13207",
    "gender": "m",
    "author": "Max Czollek",
    "author_birth": "1987",
    "poem": {
        "line.1": {
            "text": "ich wende mich an diejenigen"
        },
        "line.2": {
            "text": "die gebrauchte zahnbürsten kaufen"
        },
        "line.3": {
            "text": "für den bürgersteig und merken"
        },
        "line.4": {
            "text": "die arbeit ist bereits getan"
        },
        "line.5": {
            "text": "die an regenschirme glauben"
        },
        "line.6": {
            "text": "auch bei bombenwetter"
        },
        "line.7": {
            "text": "und werden vom schweiß ganz nass"
        },
        "line.8": {
            "text": "die einen bauchschuss vortäuschen"
        },
        "line.9": {
            "text": "um durch die musterung zu kommen"
        },
        "line.10": {
            "text": "und dabei so überzeugend sind"
        },
        "line.11": {
            "text": "dass sie verbluten"
        },
        "line.12": {
            "text": "die sich freuten"
        },
        "line.13": {
            "text": "würden fremde ihr gesicht besetzen"
        },
        "line.14": {
            "text": "ohne erst um erlaubnis zu bitten"
        },
        "line.15": {
            "text": ""
        },
        "line.16": {
            "text": "ich wende mich an diejenigen"
        },
        "line.17": {
            "text": "die als väter auszogen"
        },
        "line.18": {
            "text": "und zurückkehrten als söhne"
        },
        "line.19": {
            "text": "die im lidl standen und glaubten"
        },
        "line.20": {
            "text": "es sei zeit zu den waffeln zu greifen"
        },
        "line.21": {
            "text": "die in der morgendämmerung"
        },
        "line.22": {
            "text": "eine tür eintraten"
        },
        "line.23": {
            "text": "und sich anschließend entschuldigten"
        },
        "line.24": {
            "text": "die sich auf keinen fall"
        },
        "line.25": {
            "text": "lebendig fangen lassen, deren blutkörper"
        },
        "line.26": {
            "text": "kugelsichere westen tragen"
        },
        "line.27": {
            "text": ""
        },
        "line.28": {
            "text": "ich wende mich an die letzten minuten"
        },
        "line.29": {
            "text": "auf der wilhelm gustloff, an die brennende heimat"
        },
        "line.30": {
            "text": "ich wende mich an diejenigen"
        },
        "line.31": {
            "text": "die die zündschnur nicht bemerken"
        },
        "line.32": {
            "text": "die von den worten in die vergangenheit reicht"
        },
        "line.33": {
            "text": "die mit einem stuhlbein auf der ostsee treiben"
        },
        "line.34": {
            "text": "die im tauwasser der eisberge geweihten"
        },
        "line.35": {
            "text": "die nach der sturmflut hühnergötter suchen"
        },
        "line.36": {
            "text": "und bernstein, die in erdgeschichte bewanderten"
        },
        "line.37": {
            "text": "die immer seetang finden und keine haare"
        },
        "line.38": {
            "text": ""
        },
        "line.39": {
            "text": "ich wende mich an diejenigen"
        },
        "line.40": {
            "text": "die im fischmarkt einen aal ersteigern"
        },
        "line.41": {
            "text": "ihn wieder in die elbe schmeißen"
        },
        "line.42": {
            "text": "denen sonst nichts einfällt"
        },
        "line.43": {
            "text": "zum thema freiheit"
        },
        "line.44": {
            "text": "die hollywoodschaukeln besitzen"
        },
        "line.45": {
            "text": "und genügend aufgänge"
        },
        "line.46": {
            "text": "ein leben treppen zu steigen"
        },
        "line.47": {
            "text": "die ihre hände strecken zur kapitulation"
        },
        "line.48": {
            "text": "und dabei behaupten"
        },
        "line.49": {
            "text": "sie würden nach sternen greifen"
        },
        "line.50": {
            "text": "die nordsee für ein geräusch halten"
        },
        "line.51": {
            "text": "die sich eine flasche"
        },
        "line.52": {
            "text": "auf die augen drücken"
        },
        "line.53": {
            "text": "die den einsatz verdoppeln"
        },
        "line.54": {
            "text": "aussteigen, ohne ihr blatt zu zeigen"
        },
        "line.55": {
            "text": ""
        },
        "line.56": {
            "text": "ich wende mich an diejenigen"
        },
        "line.57": {
            "text": "die keine lastwagen an der ampel sehen"
        },
        "line.58": {
            "text": "sondern deutsche in osteuropa"
        },
        "line.59": {
            "text": "die keine flugzeuge am himmel"
        },
        "line.60": {
            "text": "ohne new york, damaskus, kabul, caracas"
        },
        "line.61": {
            "text": "seoul, ramallah, belgrad, tel aviv, heiligendamm"
        },
        "line.62": {
            "text": "ich wende mich an diejenigen"
        },
        "line.63": {
            "text": "die im schatten der giganten marschieren"
        },
        "line.64": {
            "text": "denen angenehm kühl ist dabei"
        },
        "line.65": {
            "text": "an jene in geliehenen anzügen"
        },
        "line.66": {
            "text": "in ubahnen, schauläden, leeren gläsern"
        },
        "line.67": {
            "text": "in himmelbetten, bester erinnerung"
        },
        "line.68": {
            "text": "in meinen gebeten"
        },
        "line.69": {
            "text": ""
        },
        "line.70": {
            "text": "ich wende mich an die zugvögel"
        },
        "line.71": {
            "text": "denen ich glauben schenken möchte"
        },
        "line.72": {
            "text": "von ganzem herzen"
        },
        "line.73": {
            "text": "an die tanzenden, ich versuche sie"
        },
        "line.74": {
            "text": "mir in die dämmerung zu stellen, mit ihren"
        },
        "line.75": {
            "text": "beinen aus carbon"
        },
        "line.76": {
            "text": "ich wende mich an die adern"
        },
        "line.77": {
            "text": "meines schläfendeltas, deren verlauf ich"
        },
        "line.78": {
            "text": "im kalklicht der besuchertoilette einer gedenkstätte"
        },
        "line.79": {
            "text": "zu orten versuche"
        },
        "line.80": {
            "text": "und dabei merke, dass ich bereits alles"
        },
        "line.81": {
            "text": "wieder vergessen habe"
        },
        "line.82": {
            "text": ""
        },
        "line.83": {
            "text": "ich wende mich an diejenigen"
        },
        "line.84": {
            "text": "die schon immer mal lucky strikes"
        },
        "line.85": {
            "text": "an einer tankstelle rauchen wollten"
        },
        "line.86": {
            "text": "die sich bärte wachsen lassen"
        },
        "line.87": {
            "text": "um dahinter ihre zähne zu verstecken"
        },
        "line.88": {
            "text": "die keine angst mehr haben vor den ärzten"
        },
        "line.89": {
            "text": "eine geheimwaffe erfinden"
        },
        "line.90": {
            "text": "und die baupläne auf dem weg zum treffpunkt"
        },
        "line.91": {
            "text": "unter ihrer sitzbank vergessen"
        },
        "line.92": {
            "text": "wende mich an diejenigen"
        },
        "line.93": {
            "text": "die gürtel besitzen für ihren bauch"
        },
        "line.94": {
            "text": "und einen koffer für stark frequentierte orte"
        },
        "line.95": {
            "text": "hauptbahnhöfe, große freiheit, stelenfeld"
        },
        "line.96": {
            "text": ""
        },
        "line.97": {
            "text": "ich wende mich an diejenigen"
        },
        "line.98": {
            "text": "die keine albträume haben"
        },
        "line.99": {
            "text": "denen die welt auch so eine kugel gibt"
        },
        "line.100": {
            "text": "deren geballte faust die eigenen"
        },
        "line.101": {
            "text": "finger bricht, die im schlaf"
        },
        "line.102": {
            "text": "schon über dachkanten hinaus"
        },
        "line.103": {
            "text": "ich wende mich an diejenigen"
        },
        "line.104": {
            "text": "unter birkengestöber"
        },
        "line.105": {
            "text": "für die es im frühjahr schon schneit"
        },
        "line.106": {
            "text": "die schulen besetzen"
        },
        "line.107": {
            "text": "weil sie nicht mehr weiter können"
        },
        "line.108": {
            "text": "die in meiner scheiße waten"
        },
        "line.109": {
            "text": "die ich vergessen habe"
        },
        "line.110": {
            "text": ""
        },
        "line.111": {
            "text": "ich wende mich an diejenigen"
        },
        "line.112": {
            "text": "deren auszug für heute geplant ist"
        },
        "line.113": {
            "text": "die wissen, wo sie ihre bücher lassen"
        },
        "line.114": {
            "text": "die lieder kennen"
        },
        "line.115": {
            "text": "von denen keiner mehr sagen will"
        },
        "line.116": {
            "text": "er habe sie vorher nicht gemocht"
        },
        "line.117": {
            "text": "an diejenigen, die erdfrüchte"
        },
        "line.118": {
            "text": "in salzwasser tunken, die fröhlich sind"
        },
        "line.119": {
            "text": "und wissen, es ist doch ohne grund"
        },
        "line.120": {
            "text": "die den göttern ihre erwartungen opfern"
        },
        "line.121": {
            "text": "die nur noch lyrik verstehen und sonst nichts"
        },
        "line.122": {
            "text": ""
        },
        "line.123": {
            "text": "schließlich wende ich mich an diejenigen"
        },
        "line.124": {
            "text": "die glauben, nachts warten die leeren spiegel"
        },
        "line.125": {
            "text": "auf ihr verlassenes gesicht"
        },
        "line.126": {
            "text": "diejenigen, die die verweinten augen"
        },
        "line.127": {
            "text": "ihrer eltern vergruben und dabei unheilbar waren"
        },
        "line.128": {
            "text": "wie himmel am abend"
        },
        "line.129": {
            "text": "diejenigen, die sind wie die schwalben"
        },
        "line.130": {
            "text": "die schlafen im flug"
        },
        "line.131": {
            "text": "die am ende vom himmel fallen"
        },
        "line.132": {
            "text": "ohne viel geschrei"
        }
    }
}