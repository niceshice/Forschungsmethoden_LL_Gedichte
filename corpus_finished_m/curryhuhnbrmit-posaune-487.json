    "poem": {
        "line.1": {
            "text": "Was kann"
        },
        "line.2": {
            "text": "bei einem Hurrikan"
        },
        "line.3": {
            "text": "ein Curryhuhn"
        },
        "line.4": {
            "text": "tun?"
        },
        "line.5": {
            "text": "Es nimmt sich einen"
        },
        "line.6": {
            "text": "Curryhahn,"
        },
        "line.7": {
            "text": "dem kann"
        },
        "line.8": {
            "text": "der Hurrikan doch nichts tun?"
        }
    }
}