{
    "title": "woerterpalindromblock-186",
    "gender": "m",
    "author": "Oskar Pastior",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "abnimmtvollmond.neumondnimmtzu. schreibtawie"
        },
        "line.2": {
            "text": "abwärtsundzetim zeigersinn wo daumen rechtshänder links"
        },
        "line.3": {
            "text": "führtundblinddarm den herzenswunsch verkehrt - danngibt"
        },
        "line.4": {
            "text": "esdie courtoisie der xenien, andere sprachen anderemonde,"
        },
        "line.5": {
            "text": "sindschriftenmit ein und aus und abund aufhäuserund"
        },
        "line.6": {
            "text": "parteienunterflügelwindeintrabantenpornogegenge-"
        },
        "line.7": {
            "text": "schlechtswut-lunabeschreibtabschaumweitung,sichel"
        },
        "line.8": {
            "text": "schildertzunderhalde, atemlos nagt symmetrieaufwiderruf"
        },
        "line.9": {
            "text": "amallgemeinen:immondlichtscheintvorderhandschwarz"
        },
        "line.10": {
            "text": "auf"
        },
        "line.11": {
            "text": "weiß"
        },
        "line.12": {
            "text": "auf schwarzvorderhand, scheintmondlichtim all-"
        },
        "line.13": {
            "text": "gemeinen amwiderruf auf, symmetrienagtatemloszunder -"
        },
        "line.14": {
            "text": "halde -schildertsichelabschaumweitung, beschreibt luna"
        },
        "line.15": {
            "text": "geschlechtswutgegentrabantenporno, einflügelwindunter"
        },
        "line.16": {
            "text": "parteien und häuser; auf und ab und aus und ein mit schriften"
        },
        "line.17": {
            "text": "sind monde anderesprachen,anderexeniender courtoisie;"
        },
        "line.18": {
            "text": "die es gibt; dannverkehrt herzenswunsch denblinddarmund"
        },
        "line.19": {
            "text": "führt links rechtshänder-daumenwozeigersinnimzetund"
        },
        "line.20": {
            "text": "abwärtswie a schreibt.zu nimmtneumond,vollmondnimmt"
        },
        "line.21": {
            "text": "ab"
        }
    }
}