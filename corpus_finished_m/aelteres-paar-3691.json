{
    "title": "aelteres-paar-3691",
    "gender": "m",
    "author": "Yaak  Karsunke",
    "author_birth": "1934",
    "poem": {
        "line.1": {
            "text": "für Ingrid"
        },
        "line.2": {
            "text": "auf einem handtellergroßen"
        },
        "line.3": {
            "text": "kupferstich von 1543"
        },
        "line.4": {
            "text": "siehst du sie stehen"
        },
        "line.5": {
            "text": "eine frau & einen mann"
        },
        "line.6": {
            "text": "nicht eben schlank nicht mehr jung"
        },
        "line.7": {
            "text": "die erfinder der liebe"
        },
        "line.8": {
            "text": "das tappen der feigenbaumblätter"
        },
        "line.9": {
            "text": "die sich aneinander rieben"
        },
        "line.10": {
            "text": "ließ Eva nicht schlafen"
        },
        "line.11": {
            "text": "& als Adam den apfel nahm sah er"
        },
        "line.12": {
            "text": "ihre brüste mit neuen"
        },
        "line.13": {
            "text": "augen vor seinen"
        },
        "line.14": {
            "text": "sich öffnenden lippen"
        },
        "line.15": {
            "text": "(kurz darauf waren beide"
        },
        "line.16": {
            "text": "von erkenntnis heftig bewegt)"
        },
        "line.17": {
            "text": "die schlange züngelt"
        },
        "line.18": {
            "text": "aus dem paradies & gleitet"
        },
        "line.19": {
            "text": "geschmeidig auf dich & mich zu -"
        },
        "line.20": {
            "text": "lass uns in den garten gehen"
        },
        "line.21": {
            "text": "& ihren altar"
        },
        "line.22": {
            "text": "mit granatäpfeln schmücken"
        }
    }
}