{
    "title": "der-tanz-10655",
    "gender": "m",
    "author": "Bas Böttcher",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "Magisch magnetisch geschmeidig im Takt"
        },
        "line.2": {
            "text": "Fleisch live in Schleifenbewegung unplugged"
        },
        "line.3": {
            "text": "Tanzen in Trance ohne Pflanzenextrakt"
        },
        "line.4": {
            "text": "Rhythmisch geschmiedeter Nichtangriffspakt"
        },
        "line.5": {
            "text": "Wir fühlen uns unverwundbar wunderbar"
        },
        "line.6": {
            "text": "Und sind so wunderbar unverwundbar"
        },
        "line.7": {
            "text": "Körperfunktionen und Technik intakt"
        },
        "line.8": {
            "text": "Bässe in Bassboxen bouncen exakt"
        },
        "line.9": {
            "text": "Pobacken, Bäuche und Busen halbnackt"
        },
        "line.10": {
            "text": "Funkeln im flackernden Licht  Blickkontakt"
        },
        "line.11": {
            "text": "Wir fühlen uns unverwundbar wunderbar"
        },
        "line.12": {
            "text": "Und sind so wunderbar unverwundbar"
        },
        "line.13": {
            "text": "Dass das das krasseste Fest ist, ist Fakt"
        },
        "line.14": {
            "text": "Publikum dicht aneinander gepackt"
        },
        "line.15": {
            "text": "Tanzsilhouetten im Flutlicht abstrakt"
        },
        "line.16": {
            "text": "Feiert noch schön! Das war mein Text  letzter Akt"
        },
        "line.17": {
            "text": "Wir fühlen uns unverwundbar wunderbar"
        },
        "line.18": {
            "text": "Und sind so wunderbar und verwundbar"
        }
    }
}