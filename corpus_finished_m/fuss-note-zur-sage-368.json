{
    "title": "fuss-note-zur-sage-368",
    "gender": "m",
    "author": "Paul Wühr",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "Fuß"
        },
        "line.2": {
            "text": "Note zur Sage mit Ohren"
        },
        "line.3": {
            "text": "an ihrem Fuß"
        },
        "line.4": {
            "text": "damit hört sie wohin"
        },
        "line.5": {
            "text": "es geht woher"
        },
        "line.6": {
            "text": "sie gekommen hier heißt"
        },
        "line.7": {
            "text": "sie sich selber"
        },
        "line.8": {
            "text": "die Namen unterschlagen nur"
        },
        "line.9": {
            "text": "ein Garten"
        },
        "line.10": {
            "text": "schlägt sich noch nieder und"
        },
        "line.11": {
            "text": "seine Rosen"
        },
        "line.12": {
            "text": "nicht die Frau und die Toten"
        },
        "line.13": {
            "text": "die alle in uns"
        },
        "line.14": {
            "text": "heißen und unsere Sprache"
        },
        "line.15": {
            "text": "ausrufen"
        },
        "line.16": {
            "text": "die deutsche in allen Wörtern"
        },
        "line.17": {
            "text": "bis keines mehr"
        },
        "line.18": {
            "text": "in Unschuld gesagt werden"
        },
        "line.19": {
            "text": "kann"
        }
    }
}