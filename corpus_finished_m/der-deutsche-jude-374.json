{
    "title": "der-deutsche-jude-374",
    "gender": "m",
    "author": "Paul Wühr",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "Um"
        },
        "line.2": {
            "text": "es so dumm anzufangen wie"
        },
        "line.3": {
            "text": "möglich Bonn Weizmann"
        },
        "line.4": {
            "text": "im Bundestag gesagt mußten"
        },
        "line.5": {
            "text": "wir uns das sein lassen"
        },
        "line.6": {
            "text": "nicht schon wieder sollten hier"
        },
        "line.7": {
            "text": "Juden leben"
        },
        "line.8": {
            "text": "wer will heute deutsch sein"
        },
        "line.9": {
            "text": "nach allem was hier geschah"
        },
        "line.10": {
            "text": "ein Deutscher doch nur weil"
        },
        "line.11": {
            "text": "er muß"
        },
        "line.12": {
            "text": "er kann sich von Deutschland"
        },
        "line.13": {
            "text": "nicht lossprechen ein solcher"
        },
        "line.14": {
            "text": "sollte auch nicht"
        },
        "line.15": {
            "text": "nachdenken darüber ob dieser"
        },
        "line.16": {
            "text": "Weizmann recht hat"
        },
        "line.17": {
            "text": "also grundsätzlich da allen"
        },
        "line.18": {
            "text": "Juden das Recht hier zu leben"
        },
        "line.19": {
            "text": "zu keiner Zeit abgesprochen"
        },
        "line.20": {
            "text": "werden kann"
        },
        "line.21": {
            "text": "wir sollten da gar nicht von"
        },
        "line.22": {
            "text": "Recht sprechen"
        },
        "line.23": {
            "text": "wenn er in einem Land leben"
        },
        "line.24": {
            "text": "will in dem es bald keine"
        },
        "line.25": {
            "text": "Christen mehr gibt"
        },
        "line.26": {
            "text": "wenn es je welche gab will"
        },
        "line.27": {
            "text": "ich meinen"
        },
        "line.28": {
            "text": "lasse ich Gotthold Ephraim"
        },
        "line.29": {
            "text": "Lessing hier sagen"
        },
        "line.30": {
            "text": "Der"
        },
        "line.31": {
            "text": "redet in der Zukunft so"
        },
        "line.32": {
            "text": "in der seinen"
        },
        "line.33": {
            "text": "ich habe ihn dorthin erzählt"
        },
        "line.34": {
            "text": "hoffentlich bleibt es was meine"
        },
        "line.35": {
            "text": "Person betrifft nicht nur bei"
        },
        "line.36": {
            "text": "mir wie gesagt"
        },
        "line.37": {
            "text": "neben Lessing stelle ich mir"
        },
        "line.38": {
            "text": "den Mendelssohn vor"
        },
        "line.39": {
            "text": "und mich hinter den beiden"
        },
        "line.40": {
            "text": "Theodor Lessing hat jetzt"
        },
        "line.41": {
            "text": "das Wort"
        },
        "line.42": {
            "text": "Dieser"
        },
        "line.43": {
            "text": "fragt ob die Juden wie"
        },
        "line.44": {
            "text": "seinerzeit"
        },
        "line.45": {
            "text": "in Berlin immer noch beweisen"
        },
        "line.46": {
            "text": "müssen heute daß sie als"
        },
        "line.47": {
            "text": "Juden doch keine Juden sind"
        },
        "line.48": {
            "text": "ob sie also immer noch ihre"
        },
        "line.49": {
            "text": "Glaubensbrüder und nicht"
        },
        "line.50": {
            "text": "nur diese sondern das ganze"
        },
        "line.51": {
            "text": "Volk verraten"
        },
        "line.52": {
            "text": "wenn sie sind was sie sein"
        },
        "line.53": {
            "text": "wollen"
        },
        "line.54": {
            "text": "Als"
        },
        "line.55": {
            "text": "Bürger eines Staates mitten"
        },
        "line.56": {
            "text": "in Europa der"
        },
        "line.57": {
            "text": "Name desselben verdürbe"
        },
        "line.58": {
            "text": "mir nur die Sache"
        },
        "line.59": {
            "text": "um die es mir geht sagt"
        },
        "line.60": {
            "text": "ein Lessing"
        },
        "line.61": {
            "text": "der hier aus einer Zukunft"
        },
        "line.62": {
            "text": "zurückspricht in"
        },
        "line.63": {
            "text": "der wir eines Tages Menschen"
        },
        "line.64": {
            "text": "sein wollen"
        },
        "line.65": {
            "text": "kenne ich keine Brüder"
        },
        "line.66": {
            "text": "im Glauben ein"
        },
        "line.67": {
            "text": "Volk kann ich auch nicht"
        },
        "line.68": {
            "text": "verraten"
        },
        "line.69": {
            "text": "Als"
        },
        "line.70": {
            "text": "deutsche Juden standen wir"
        },
        "line.71": {
            "text": "auf unserem"
        },
        "line.72": {
            "text": "deutschen Standpunkt in"
        },
        "line.73": {
            "text": "den Lagern sagt"
        },
        "line.74": {
            "text": "Theodor Lessing in einem"
        },
        "line.75": {
            "text": "Gotthold der"
        },
        "line.76": {
            "text": "jenen aus der Zukunft"
        },
        "line.77": {
            "text": "zurück sagen läßt"
        },
        "line.78": {
            "text": "wir waren typische Deutsche"
        },
        "line.79": {
            "text": "keine Juden wie"
        },
        "line.80": {
            "text": "wir sie jetzt in unserem"
        },
        "line.81": {
            "text": "Staat sind in"
        },
        "line.82": {
            "text": "dem wir nichts mehr anderes"
        },
        "line.83": {
            "text": "sein wollen als"
        },
        "line.84": {
            "text": "Juden was die kollektive"
        },
        "line.85": {
            "text": "Selbsterhaltung"
        },
        "line.86": {
            "text": "unseres Volkes wahrhaftig"
        },
        "line.87": {
            "text": "gezeigt hat"
        },
        "line.88": {
            "text": "Daraufhin"
        },
        "line.89": {
            "text": "schwiegen die beiden"
        },
        "line.90": {
            "text": "Lessings nämlich auch"
        },
        "line.91": {
            "text": "der andere sagte dazu"
        },
        "line.92": {
            "text": "kein Wort"
        },
        "line.93": {
            "text": "Moses Mendelssohn stieß"
        },
        "line.94": {
            "text": "seinen Freund so oft in"
        },
        "line.95": {
            "text": "die Seite bis dann doch"
        },
        "line.96": {
            "text": "einer von beiden aus dem"
        },
        "line.97": {
            "text": "größeren Lessing heraus"
        },
        "line.98": {
            "text": "sagte"
        },
        "line.99": {
            "text": "uns beiden kann das"
        },
        "line.100": {
            "text": "eine Volk in einem Staat"
        },
        "line.101": {
            "text": "gestohlen bleiben"
        },
        "line.102": {
            "text": "Es"
        },
        "line.103": {
            "text": "sei ja immer nur um den Frieden"
        },
        "line.104": {
            "text": "herum gegangen ein bekennender"
        },
        "line.105": {
            "text": "Gläubiger sei gut und zwar"
        },
        "line.106": {
            "text": "besonders nämlich außerdem"
        },
        "line.107": {
            "text": "auch noch als Mensch"
        },
        "line.108": {
            "text": "sagt Gotthold Ephraim Lessing"
        },
        "line.109": {
            "text": "immer im festen Glauben daran"
        },
        "line.110": {
            "text": "das Menschliche siege am"
        },
        "line.111": {
            "text": "Ende über den Krieg"
        },
        "line.112": {
            "text": "den die Bekenntnisse doch zu"
        },
        "line.113": {
            "text": "erklären noch immer nicht"
        },
        "line.114": {
            "text": "aufhören könnten"
        },
        "line.115": {
            "text": "und wann fragt sich Lessing"
        },
        "line.116": {
            "text": "wird allen endlich klar daß"
        },
        "line.117": {
            "text": "Bekenntnisse aufhören müssen"
        },
        "line.118": {
            "text": "Wie"
        },
        "line.119": {
            "text": "wäre es wenn wir den"
        },
        "line.120": {
            "text": "Juden nicht mehr"
        },
        "line.121": {
            "text": "sagt Theodor Lessing"
        },
        "line.122": {
            "text": "und Gotthold Ephraim"
        },
        "line.123": {
            "text": "sagt und den Christen"
        },
        "line.124": {
            "text": "gar nicht mehr heraus"
        },
        "line.125": {
            "text": "kehren würden"
        },
        "line.126": {
            "text": "nur noch reden mit"
        },
        "line.127": {
            "text": "einander meinte"
        },
        "line.128": {
            "text": "Mendelssohn und"
        },
        "line.129": {
            "text": "essen"
        },
        "line.130": {
            "text": "und trinken riefen"
        },
        "line.131": {
            "text": "die Lessings"
        },
        "line.132": {
            "text": "und lieben seufzte"
        },
        "line.133": {
            "text": "besonders der Mann"
        },
        "line.134": {
            "text": "aus Wolfenbüttel"
        },
        "line.135": {
            "text": "und nur noch bekennen"
        },
        "line.136": {
            "text": "wie gut es tut hier"
        },
        "line.137": {
            "text": "überall zusammen"
        },
        "line.138": {
            "text": "zu sein"
        },
        "line.139": {
            "text": "um eines Tages sagen"
        },
        "line.140": {
            "text": "zu können sagt Seume"
        },
        "line.141": {
            "text": "die Erde hat den Himmel"
        },
        "line.142": {
            "text": "verdorben"
        },
        "line.143": {
            "text": "Ein"
        },
        "line.144": {
            "text": "Deutscher muß kein bekennender"
        },
        "line.145": {
            "text": "Jude sein lebt er im deutschen"
        },
        "line.146": {
            "text": "Staat er kann aber"
        },
        "line.147": {
            "text": "sagt Moses"
        },
        "line.148": {
            "text": "ein Israeli das müßte doch"
        },
        "line.149": {
            "text": "der Weizmann sagen müsse"
        },
        "line.150": {
            "text": "kein bekennender Jude sein"
        },
        "line.151": {
            "text": "wenn er im jüdischen Staat"
        },
        "line.152": {
            "text": "leben will"
        },
        "line.153": {
            "text": "aber er kann"
        },
        "line.154": {
            "text": "das meinte der Gotthold"
        },
        "line.155": {
            "text": "ob er warf Theodor in die"
        },
        "line.156": {
            "text": "Runde verletze mit seinem"
        },
        "line.157": {
            "text": "Bekenntnis er hoffe ein"
        },
        "line.158": {
            "text": "jeder Staat ließe ihn Bürger"
        },
        "line.159": {
            "text": "sein auch wenn er Niedersachse"
        },
        "line.160": {
            "text": "sei und zwar ein bekennender"
        },
        "line.161": {
            "text": "ja sagte Gotthold"
        },
        "line.162": {
            "text": "Ansonsten"
        },
        "line.163": {
            "text": "aber sagt Gotthold Ephraim"
        },
        "line.164": {
            "text": "Lessing wie anders könnte"
        },
        "line.165": {
            "text": "es Weizmann gemeint"
        },
        "line.166": {
            "text": "haben"
        },
        "line.167": {
            "text": "einen Deutschen der an dem"
        },
        "line.168": {
            "text": "Juden in seiner Person nicht"
        },
        "line.169": {
            "text": "hängt"
        },
        "line.170": {
            "text": "wie ich an dem Deutschen in"
        },
        "line.171": {
            "text": "mir nicht"
        },
        "line.172": {
            "text": "kann das Staatsoberhaupt"
        },
        "line.173": {
            "text": "Israels hier nicht einmal"
        },
        "line.174": {
            "text": "in Gedanken ausweisen"
        },
        "line.175": {
            "text": "wollen und"
        },
        "line.176": {
            "text": "schon gar nicht heimholen"
        },
        "line.177": {
            "text": "Nun"
        },
        "line.178": {
            "text": "lebt hier ein Mann in"
        },
        "line.179": {
            "text": "diesem Land"
        },
        "line.180": {
            "text": "das wir wohl oder übel"
        },
        "line.181": {
            "text": "immer"
        },
        "line.182": {
            "text": "Deutschland nennen"
        },
        "line.183": {
            "text": "werden müssen"
        },
        "line.184": {
            "text": "der mir hier soufflierte"
        },
        "line.185": {
            "text": "als ich den zwei"
        },
        "line.186": {
            "text": "Lessings dem Seume"
        },
        "line.187": {
            "text": "dem Mendelssohn"
        },
        "line.188": {
            "text": "flüsterte was sie uns"
        },
        "line.189": {
            "text": "vordenken"
        },
        "line.190": {
            "text": "sollten ich spreche von"
        },
        "line.191": {
            "text": "Rafael Seligmann"
        },
        "line.192": {
            "text": "der über sich selbst sagt"
        },
        "line.193": {
            "text": "Ich bin ein deutscher"
        },
        "line.194": {
            "text": "Jude"
        }
    }
}