{
    "title": "anderkatt-15138",
    "gender": "m",
    "author": "Georg Leß",
    "author_birth": "1981",
    "poem": {
        "line.1": {
            "text": "Gänsehaut, weil du von Haut"
        },
        "line.2": {
            "text": "bevor du vom Fell sprichst"
        },
        "line.3": {
            "text": "äußertest du einen Mungo"
        },
        "line.4": {
            "text": "aufgeschwemmt und fürchterlich"
        },
        "line.5": {
            "text": "und unterm Haar treibt lose ein Gesicht"
        },
        "line.6": {
            "text": "seins oder deins, ich schneide das nie wieder"
        },
        "line.7": {
            "text": "dass das kein Mungo sei, versichern selbst die Eltern"
        },
        "line.8": {
            "text": "das, nein, ist niemals unser"
        },
        "line.9": {
            "text": "du hast dich geborgen gefühlt"
        },
        "line.10": {
            "text": "wie von einer Schere umarmt, unser Fell"
        },
        "line.11": {
            "text": "sprießt dazwischen noch voller"
        },
        "line.12": {
            "text": "und unsere Luftnot darunter gemischt"
        },
        "line.13": {
            "text": "weil du von der Nordsee, von Expeditionen"
        },
        "line.14": {
            "text": "bevor du vom Schiff sprichst"
        },
        "line.15": {
            "text": "das Fell schlägt darüber zusammen, hält dicht"
        },
        "line.16": {
            "text": "am Ufer Nummer 3 mit einer wunden Sprache nur"
        },
        "line.17": {
            "text": "einer Gattung und"
        },
        "line.18": {
            "text": "Export von Logik, was ich nicht auszusprechen weiß"
        },
        "line.19": {
            "text": "nehme ich trotzdem in den Mund"
        }
    }
}