{
    "title": "einmal-mehr-haben-uns-die-worte-356",
    "gender": "m",
    "author": "Kurt  Aebli",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "E"
        },
        "line.2": {
            "text": "inmal mehr haben uns die Worte"
        },
        "line.3": {
            "text": "von außen gesehen vergessene"
        },
        "line.4": {
            "text": "Worte haben gesehen daß sich"
        },
        "line.5": {
            "text": "die Dinge nicht so abgespielt haben"
        },
        "line.6": {
            "text": "und das ist das Wort mit dem wir ein"
        },
        "line.7": {
            "text": "kontinuierliches und zielgerichtetes"
        },
        "line.8": {
            "text": "Weggehen einleiten stellvertretend"
        },
        "line.9": {
            "text": "sagte er für ein anderes mechanisches"
        },
        "line.10": {
            "text": "Experiment"
        }
    }
}