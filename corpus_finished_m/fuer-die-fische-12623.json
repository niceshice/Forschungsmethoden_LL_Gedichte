{
    "title": "fuer-die-fische-12623",
    "gender": "m",
    "author": "Ulrich Zieger",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "was geschähe tatsächlich"
        },
        "line.2": {
            "text": "würde es schneien,"
        },
        "line.3": {
            "text": "das licht in den häusern würde verändert sein,"
        },
        "line.4": {
            "text": "das geschähe tatsächlich"
        },
        "line.5": {
            "text": "würde es schneien,"
        },
        "line.6": {
            "text": "was geschähe tatsächlich"
        },
        "line.7": {
            "text": "würde es hageln,"
        },
        "line.8": {
            "text": "wir würden die fenster mit brettern vernageln,"
        },
        "line.9": {
            "text": "das geschähe tatsächlich"
        },
        "line.10": {
            "text": "würde es hageln,"
        },
        "line.11": {
            "text": "was geschähe tatsächlich"
        },
        "line.12": {
            "text": "würde es regnen,"
        },
        "line.13": {
            "text": "zwei würden sich unter zwei schirmen begegnen,"
        },
        "line.14": {
            "text": "das geschähe tatsächlich"
        },
        "line.15": {
            "text": "würde es regnen,"
        },
        "line.16": {
            "text": "was geschähe tatsächlich"
        },
        "line.17": {
            "text": "würde es spucken,"
        },
        "line.18": {
            "text": "man würde es zuerst nicht glauben können,"
        },
        "line.19": {
            "text": "das geschähe tatsächlich"
        },
        "line.20": {
            "text": "würde es spucken."
        }
    }
}