{
    "title": "vielleicht-ging-ein-summen-16559",
    "gender": "m",
    "author": "Sascha Garzetti",
    "author_birth": "1986",
    "poem": {
        "line.1": {
            "text": "Vielleicht ging ein Summen"
        },
        "line.2": {
            "text": "durch dein Herzskelett"
        },
        "line.3": {
            "text": "wie an der Schnur gezogen."
        },
        "line.4": {
            "text": "Ich lege die Hand auf die Brust"
        },
        "line.5": {
            "text": "Jahre spater, ein Herzschlag"
        },
        "line.6": {
            "text": "als hatte einer einen Brief eingeworfen."
        },
        "line.7": {
            "text": "Zwischen geborgten Erinnerungen"
        },
        "line.8": {
            "text": "verlaufen fein gespannte Netze"
        },
        "line.9": {
            "text": "darin Zeilen, die ich zeitlebens"
        },
        "line.10": {
            "text": "nicht schreiben werde"
        },
        "line.11": {
            "text": "die aufscheinen in fremden Satzen"
        },
        "line.12": {
            "text": "hasenherzig, zag."
        }
    }
}