{
    "title": "wenn-ich-gross-bin-13205",
    "gender": "m",
    "author": "Max Czollek",
    "author_birth": "1987",
    "poem": {
        "line.1": {
            "text": "wenn ich groß bin"
        },
        "line.2": {
            "text": "fürchte ich mich nicht"
        },
        "line.3": {
            "text": "vor überlandflügen"
        },
        "line.4": {
            "text": "spiele ich ein lied"
        },
        "line.5": {
            "text": "auf der wirbelsäulenflöte"
        },
        "line.6": {
            "text": "meiner ersten liebe"
        },
        "line.7": {
            "text": "wenn ich groß bin"
        },
        "line.8": {
            "text": "fange ich dorftrottel"
        },
        "line.9": {
            "text": "mit diesen vogelhänden"
        },
        "line.10": {
            "text": "lasse sie eine autobahn"
        },
        "line.11": {
            "text": "ausrollen auf das meer"
        },
        "line.12": {
            "text": "für meine wolkenpanzer"
        }
    }
}