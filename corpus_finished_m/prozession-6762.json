{
    "title": "prozession-6762",
    "gender": "m",
    "author": "Jan Volker Röhnert",
    "author_birth": "1976",
    "poem": {
        "line.1": {
            "text": "Diesen Morgen zogen"
        },
        "line.2": {
            "text": "die Patriarchen aus"
        },
        "line.3": {
            "text": "Sonne auf dem Tramwaygleis"
        },
        "line.4": {
            "text": "und die Mädchen waren schön"
        },
        "line.5": {
            "text": "in den Ecken Hunde"
        },
        "line.6": {
            "text": "in ihren Pelz gerollt"
        },
        "line.7": {
            "text": "an den Rändern Schnee gefroren"
        },
        "line.8": {
            "text": "und in Nebeldunst die Farben"
        },
        "line.9": {
            "text": "die Drähte klirrten und das Glockenspiel"
        },
        "line.10": {
            "text": "ein kleines Wunder"
        },
        "line.11": {
            "text": "eine Spur von Blattgold"
        },
        "line.12": {
            "text": "auf dem Straßenteer"
        }
    }
}