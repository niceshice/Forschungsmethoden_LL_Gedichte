{
    "title": "die-haeuser-4741",
    "gender": "m",
    "author": "SAID",
    "author_birth": "1947",
    "poem": {
        "line.1": {
            "text": "die häuser"
        },
        "line.2": {
            "text": "schweigen und bleiben"
        },
        "line.3": {
            "text": "jede straße"
        },
        "line.4": {
            "text": "eine sprache ohne wiederkehr"
        },
        "line.5": {
            "text": "denn jede flucht beginnt mit einer täuschung"
        }
    }
}