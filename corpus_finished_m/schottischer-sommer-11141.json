{
    "title": "schottischer-sommer-11141",
    "gender": "m",
    "author": "Peter Huchel",
    "author_birth": "1903",
    "poem": {
        "line.1": {
            "text": "\"What seemed corporeal melted äs breath into the wind'"
        },
        "line.2": {
            "text": "Shakespeare,"
        },
        "line.3": {
            "text": "Macbeth"
        },
        "line.4": {
            "text": "Schottischer Sommer,"
        },
        "line.5": {
            "text": "unter der Eiche"
        },
        "line.6": {
            "text": "zopftrocken"
        },
        "line.7": {
            "text": "sitzend die Weiber aus Cawdor,"
        },
        "line.8": {
            "text": "manche verborgen im Licht der Wolken,"
        },
        "line.9": {
            "text": "abgeblühte Nesseln im Sand."
        },
        "line.10": {
            "text": "Über die Felsen herab"
        },
        "line.11": {
            "text": "Trompetenstösse, ein Klirren"
        },
        "line.12": {
            "text": "wühlt die Brandung auf."
        },
        "line.13": {
            "text": "Nebel, der sie erzeugte,"
        },
        "line.14": {
            "text": "bald ist es Winter,"
        },
        "line.15": {
            "text": "dünnes, nie ruhendes Holz,"
        },
        "line.16": {
            "text": "der Schnee fegt hin und her"
        },
        "line.17": {
            "text": "und stäubt die Öde an."
        },
        "line.18": {
            "text": "Dürr und düster"
        },
        "line.19": {
            "text": "vor der goldenen Naht des Abends"
        },
        "line.20": {
            "text": "hocken sie auf zerrissenen Fellen."
        },
        "line.21": {
            "text": "Wenn der Mond"
        },
        "line.22": {
            "text": "die Zeiger verrückt am Turm,"
        },
        "line.23": {
            "text": "starren sie mit erloschenen Augen."
        },
        "line.24": {
            "text": "Unbewohnbar die Trauer,"
        },
        "line.25": {
            "text": "die an den Klippen verebbt."
        }
    }
}