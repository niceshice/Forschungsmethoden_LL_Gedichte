{
    "title": "mergelgrube-5661",
    "gender": "m",
    "author": "Manfred Peter Hein",
    "author_birth": "1931",
    "poem": {
        "line.1": {
            "text": "Der tote Hund"
        },
        "line.2": {
            "text": "sag ich warum"
        },
        "line.3": {
            "text": "jagst du"
        },
        "line.4": {
            "text": "ihm nach"
        },
        "line.5": {
            "text": "Hund im Vorstadtgemäuer"
        },
        "line.6": {
            "text": "Gemergel von Ephesos"
        },
        "line.7": {
            "text": "Der Hafen"
        },
        "line.8": {
            "text": "das Meer"
        },
        "line.9": {
            "text": "verlandet"
        },
        "line.10": {
            "text": "sieben Buchstaben"
        },
        "line.11": {
            "text": "sieben Meilen Land"
        },
        "line.12": {
            "text": "Blachfeld November"
        },
        "line.13": {
            "text": "und wann ist März"
        },
        "line.14": {
            "text": "in Abschiedskreisen"
        },
        "line.15": {
            "text": "die Störche"
        }
    }
}