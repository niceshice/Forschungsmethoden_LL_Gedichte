{
    "title": "wien-heldenplatz-1229",
    "gender": "m",
    "author": "Ernst Jandl",
    "author_birth": "1925",
    "poem": {
        "line.1": {
            "text": "der glanze heldenplatz zirka"
        },
        "line.2": {
            "text": "versaggerte in maschenhaftem männchenmeere"
        },
        "line.3": {
            "text": "drunter auch frauen die ans maskelknie"
        },
        "line.4": {
            "text": "zu heften heftig sich versuchten, hoffensdick"
        },
        "line.5": {
            "text": "und brüllzten wesentlich."
        },
        "line.6": {
            "text": "verwogener stirnscheitelunterschwang"
        },
        "line.7": {
            "text": "nach nöten nördlich, kechelte"
        },
        "line.8": {
            "text": "mit zu-nummernder aufs bluten feilzer stimme"
        },
        "line.9": {
            "text": "hinsensend sämmertliche eigenwäscher."
        },
        "line.10": {
            "text": "pirsch!"
        },
        "line.11": {
            "text": "döppelte der gottelbock von Sa-Atz zu Sa-Atz"
        },
        "line.12": {
            "text": "mit hünig sprenkem stimmstummel."
        },
        "line.13": {
            "text": "balzerig würmelte es im männechensee"
        },
        "line.14": {
            "text": "und den weibern ward so pfingstig ums heil"
        },
        "line.15": {
            "text": "zumahn: wenn ein knie-ender sie hirschelte."
        }
    }
}