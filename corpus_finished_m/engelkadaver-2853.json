{
    "title": "engelkadaver-2853",
    "gender": "m",
    "author": "Frank  Schablewski",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "zu Füßen von weitem auszunehmen die Fressen"
        },
        "line.2": {
            "text": "vom Kopf das Haar jemandem graut"
        },
        "line.3": {
            "text": "vor Kiefer"
        },
        "line.4": {
            "text": "fern"
        },
        "line.5": {
            "text": "Spannern einen Flügelschlag entfernt"
        },
        "line.6": {
            "text": "mit letztem Körpereinsatz den Ruheschmerz"
        },
        "line.7": {
            "text": "hoch gewürgt zu gelegenen Mäuler"
        },
        "line.8": {
            "text": "himmelschreiend jeder Blüte gleich"
        },
        "line.9": {
            "text": "hinter der anderen eine unersättlich scheint"
        },
        "line.10": {
            "text": "der Engelkadaver steht bis zum Hals hinauf"
        },
        "line.11": {
            "text": "geschlagen das gebrochene herzugeben"
        },
        "line.12": {
            "text": "all"
        },
        "line.13": {
            "text": "es fällt schwer aufeinander die Lippen"
        },
        "line.14": {
            "text": "blutleer gepreßt in die Seite jeder nach ging"
        },
        "line.15": {
            "text": "in der Hand des anderen auf"
        },
        "line.16": {
            "text": "hören zu leben"
        },
        "line.17": {
            "text": "von anderen der Schock sich zeitlich versetzt"
        },
        "line.18": {
            "text": "im Wechselstrom zu Schwingungen"
        },
        "line.19": {
            "text": "in der Sekunde jemand die Spannung"
        },
        "line.20": {
            "text": "auf den Nullpunkt legt die Finger hinein"
        },
        "line.21": {
            "text": "den Rest aus den Augen zu schließen"
        },
        "line.22": {
            "text": "in einem löst das Gebundene die Betäubung"
        },
        "line.23": {
            "text": "unfurchtbar gegenüber dem blütenbeschmiert"
        },
        "line.24": {
            "text": "den Damm unterspült in Sichverliebenden"
        }
    }
}