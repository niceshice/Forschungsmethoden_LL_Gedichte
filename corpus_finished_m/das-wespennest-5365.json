{
    "title": "das-wespennest-5365",
    "gender": "m",
    "author": "Nicolai Kobus",
    "author_birth": "1968",
    "poem": {
        "line.1": {
            "text": "DAS WESPENNEST ein fester rest von bauwerk"
        },
        "line.2": {
            "text": "im zwischenraum der gaube als letzter"
        },
        "line.3": {
            "text": "test des überlebens vor dem winter"
        },
        "line.4": {
            "text": "ich glaube die ritzen der rigipsplatten"
        },
        "line.5": {
            "text": "durchsickert kein kleistersekret"
        },
        "line.6": {
            "text": "sondern geronnenes blut so sterben"
        },
        "line.7": {
            "text": "gestreifte körper in röhren als material"
        },
        "line.8": {
            "text": "und werkzeug zugleich in diensten"
        },
        "line.9": {
            "text": "der königin ich höre das fressen"
        },
        "line.10": {
            "text": "ein ohrenbetäubendes flüstern im dach"
        },
        "line.11": {
            "text": "der kammerjäger vermittelt gelassenheit"
        },
        "line.12": {
            "text": "angesichts einjähriger nester spätestens"
        },
        "line.13": {
            "text": "nach dem ersten frost ist ruhe da oben"
        },
        "line.14": {
            "text": "vergleichsweise kurzfristige staatenbildung"
        },
        "line.15": {
            "text": "[aus der serie"
        },
        "line.16": {
            "text": "kleintiere"
        },
        "line.17": {
            "text": "]"
        }
    }
}