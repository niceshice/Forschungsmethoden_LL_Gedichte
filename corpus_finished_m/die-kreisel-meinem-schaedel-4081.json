{
    "title": "die-kreisel-meinem-schaedel-4081",
    "gender": "m",
    "author": "Norbert C. Kaser",
    "author_birth": "1947",
    "poem": {
        "line.1": {
            "text": "die kreisel in meinem schaedel"
        },
        "line.2": {
            "text": "sind wieder angerollt noch"
        },
        "line.3": {
            "text": "bluehen die stra√üenlampen"
        },
        "line.4": {
            "text": "im morgen nach der nacht"
        },
        "line.5": {
            "text": "des vollen mondes. die zeit"
        },
        "line.6": {
            "text": "hab ich ueberschlafen und"
        },
        "line.7": {
            "text": "mein lob ist traum mit"
        },
        "line.8": {
            "text": "zorn. von den gipfelkreuzen"
        },
        "line.9": {
            "text": "ueber die spiegelnden fenster"
        },
        "line.10": {
            "text": "der hoeher gelegenen hoefe"
        },
        "line.11": {
            "text": "klettert langsam das"
        },
        "line.12": {
            "text": "schneeverbrennende"
        },
        "line.13": {
            "text": "sonnenlicht in den tal"
        },
        "line.14": {
            "text": "kessel noch bluehen die"
        },
        "line.15": {
            "text": "lampen die kreisel in"
        },
        "line.16": {
            "text": "meinem schaedel sind"
        },
        "line.17": {
            "text": "angerollt."
        },
        "line.18": {
            "text": "[31. 1. 69]"
        }
    }
}