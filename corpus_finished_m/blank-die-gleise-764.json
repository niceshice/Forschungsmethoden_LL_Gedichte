{
    "title": "blank-die-gleise-764",
    "gender": "m",
    "author": "Maik Lippert",
    "author_birth": "1966",
    "poem": {
        "line.1": {
            "text": "zieht die sichel"
        },
        "line.2": {
            "text": "über menschen wie"
        },
        "line.3": {
            "text": "wasserzeichen"
        },
        "line.4": {
            "text": "sie haben dich vergessen"
        },
        "line.5": {
            "text": "die geheimschrift mit zitronensaft"
        },
        "line.6": {
            "text": "die nachts dein licht"
        },
        "line.7": {
            "text": "auf leckenden gußheizungen"
        },
        "line.8": {
            "text": "dechiffrierte"
        },
        "line.9": {
            "text": "den brief auf karopapier"
        },
        "line.10": {
            "text": "liebste"
        },
        "line.11": {
            "text": "diana selene birgit"
        },
        "line.12": {
            "text": "semmelgesicht"
        },
        "line.13": {
            "text": "ich zähle in gedanken"
        },
        "line.14": {
            "text": "an deinen schläfen"
        },
        "line.15": {
            "text": "die windpockennarben"
        },
        "line.16": {
            "text": "nur ich mond"
        },
        "line.17": {
            "text": "narr"
        },
        "line.18": {
            "text": "dein sabbernder attis"
        },
        "line.19": {
            "text": "ich schelle für dich"
        },
        "line.20": {
            "text": "mit konservenblech"
        },
        "line.21": {
            "text": "mond ich"
        },
        "line.22": {
            "text": "tanze für dich"
        },
        "line.23": {
            "text": "mit brokkoli & porzellan"
        },
        "line.24": {
            "text": "in den einkaufstaschen"
        },
        "line.25": {
            "text": "unter wartenden am bahndamm"
        },
        "line.26": {
            "text": "in der appellinie der straßenbeleuchtung"
        },
        "line.27": {
            "text": "menschen wie"
        },
        "line.28": {
            "text": "wasserzeichen"
        }
    }
}