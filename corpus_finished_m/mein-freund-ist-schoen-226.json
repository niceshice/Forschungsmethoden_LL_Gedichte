{
    "title": "mein-freund-ist-schoen-226",
    "gender": "m",
    "author": "Detlev Meyer",
    "author_birth": "1948",
    "poem": {
        "line.1": {
            "text": "Mein Freund ist schön"
        },
        "line.2": {
            "text": "wie ein Hawaii-Toast und"
        },
        "line.3": {
            "text": "klug wie ein Schnürsenkel."
        },
        "line.4": {
            "text": "Ich bin sehr glücklich"
        },
        "line.5": {
            "text": "mit meinem Freund."
        },
        "line.6": {
            "text": "Als ich ihn traf,"
        },
        "line.7": {
            "text": "war er ein Sportstudio;"
        },
        "line.8": {
            "text": "nun führt er das Leben"
        },
        "line.9": {
            "text": "eines Sofakissens."
        },
        "line.10": {
            "text": "Ich bin sehr glücklich"
        },
        "line.11": {
            "text": "mit meinem Freund."
        },
        "line.12": {
            "text": "Ich halte auf ihm"
        },
        "line.13": {
            "text": "meine Siesta."
        }
    }
}