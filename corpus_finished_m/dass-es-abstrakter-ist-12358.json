{
    "title": "dass-es-abstrakter-ist-12358",
    "gender": "m",
    "author": "Martin  Piekar",
    "author_birth": "1990",
    "poem": {
        "line.1": {
            "text": "dass es abstrakter ist"
        },
        "line.2": {
            "text": ", wenn ich"
        },
        "line.3": {
            "text": "vom Ficken spreche, weißt nur du"
        },
        "line.4": {
            "text": "es ist ein geschollener Traum"
        },
        "line.5": {
            "text": "zwischen uns"
        },
        "line.6": {
            "text": "mit Widerhaken häkeln wir in Emotionen"
        },
        "line.7": {
            "text": "und widersprechen Epikur"
        },
        "line.8": {
            "text": "so ein bisschen punkromantisch"
        },
        "line.9": {
            "text": "die panchromatische Frankfurternacht"
        },
        "line.10": {
            "text": "stellt die Zeit aufs Abstellgleis"
        },
        "line.11": {
            "text": "und uns die Weichen, frivol und sorglos"
        },
        "line.12": {
            "text": "zu entgleisen in Nebengassen"
        },
        "line.13": {
            "text": "Schattengraffito tätowiert"
        },
        "line.14": {
            "text": "und die kredenzen uns ein Pendelschwingen"
        },
        "line.15": {
            "text": "von Ideen, bis wir vom Grat der Straßenwoge"
        },
        "line.16": {
            "text": "in den Morgen schlingern"
        },
        "line.17": {
            "text": "ich wünschte Caravaggio könnte uns so malen"
        },
        "line.18": {
            "text": "nein, nur er, nachts vertanzt besoffen"
        },
        "line.19": {
            "text": "hey  hey du, deine Träume"
        },
        "line.20": {
            "text": "wandern wie Spinnen ausm Kopf"
        },
        "line.21": {
            "text": "durch dein wehendes Haar als Schraffur"
        }
    }
}