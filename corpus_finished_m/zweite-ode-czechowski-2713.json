{
    "title": "zweite-ode-czechowski-2713",
    "gender": "m",
    "author": "Karl Mickel",
    "author_birth": "1935",
    "poem": {
        "line.1": {
            "text": "Auch über Lebende"
        },
        "line.2": {
            "text": "Wird es doch heißen"
        },
        "line.3": {
            "text": "Müssen: wie gut"
        },
        "line.4": {
            "text": "Daß Der auf der Erde gewesen ist."
        },
        "line.5": {
            "text": "Traust Du dem Frieden?"
        },
        "line.6": {
            "text": "Ich sage"
        },
        "line.7": {
            "text": "Besser sogleich über Dich:"
        },
        "line.8": {
            "text": "Gut, daß er da ist."
        },
        "line.9": {
            "text": "Zu Deinem Lobe"
        },
        "line.10": {
            "text": "In höchsten Tönen"
        },
        "line.11": {
            "text": "Vier Wörter mit i:"
        },
        "line.12": {
            "text": "Er isset, trinket, dichtet und fickt-"
        },
        "line.13": {
            "text": "Wenn aus dem Fenster"
        },
        "line.14": {
            "text": "Flüchtig"
        },
        "line.15": {
            "text": "Anderthalb Takte"
        },
        "line.16": {
            "text": "Hat er das ganze"
        },
        "line.17": {
            "text": "Wider sich streitende Stück"
        },
        "line.18": {
            "text": "Hält er zusammen"
        },
        "line.19": {
            "text": "Schlottern ihm die Gelenke auch"
        },
        "line.20": {
            "text": "Und die Seele flattert"
        },
        "line.21": {
            "text": "Und unsre Freunde"
        },
        "line.22": {
            "text": "Die hin sind -"
        },
        "line.23": {
            "text": "Hier das Nordlicht"
        },
        "line.24": {
            "text": "Dort das Kreuz des Südens -"
        },
        "line.25": {
            "text": "Ist es woanders"
        },
        "line.26": {
            "text": "Anders?"
        },
        "line.27": {
            "text": "Ist es hier denn"
        },
        "line.28": {
            "text": "Anders als anderswo?"
        },
        "line.29": {
            "text": "All"
        },
        "line.30": {
            "text": "Gemeine Allgemeine Krise"
        },
        "line.31": {
            "text": "Vom scheiternden Schiff"
        },
        "line.32": {
            "text": "Die scheiternden Schiffe"
        },
        "line.33": {
            "text": "Betrachten-"
        },
        "line.34": {
            "text": "Ja! darum"
        },
        "line.35": {
            "text": "Leiden, Du mußt nur die Hälfte sehn"
        },
        "line.36": {
            "text": "Menschen Symmetrisches:"
        },
        "line.37": {
            "text": "Streu Deinen Samen"
        },
        "line.38": {
            "text": "Sagt ich; wenige Jahre, und"
        },
        "line.39": {
            "text": "Meine Söhne"
        },
        "line.40": {
            "text": "Schießen einander"
        }
    }
}