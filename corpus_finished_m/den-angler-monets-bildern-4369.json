{
    "title": "den-angler-monets-bildern-4369",
    "gender": "m",
    "author": "Christian Schloyer",
    "author_birth": "1976",
    "poem": {
        "line.1": {
            "text": "merk dir nie an den wolken (wenn"
        },
        "line.2": {
            "text": "da ein meer ist  & da"
        },
        "line.3": {
            "text": "ist"
        },
        "line.4": {
            "text": "ein meer) wo du die fisch"
        },
        "line.5": {
            "text": "falle versenkst, merk dir immer das über"
        },
        "line.6": {
            "text": "fließende blau (merks dir am"
        },
        "line.7": {
            "text": "über) am fluss"
        },
        "line.8": {
            "text": "vom himmel"
        },
        "line.9": {
            "text": "ins meer"
        },
        "line.10": {
            "text": ", merk dir genau wann"
        },
        "line.11": {
            "text": "du das meer in den himmel"
        },
        "line.12": {
            "text": "versenkst, merk dir"
        },
        "line.13": {
            "text": "kein meer"
        },
        "line.14": {
            "text": "an den wolken"
        },
        "line.15": {
            "text": "(& es gibt diese"
        },
        "line.16": {
            "text": "wolken  nicht alle sind blau), merk dir am besten"
        },
        "line.17": {
            "text": "den fisch"
        }
    }
}