{
    "title": "obstsalatbrmit-posaune-478",
    "gender": "m",
    "author": "Günter Saalmann",
    "author_birth": "1936",
    "poem": {
        "line.1": {
            "text": "Ein Apfelstück,"
        },
        "line.2": {
            "text": "ein Birnenstück,"
        },
        "line.3": {
            "text": "Bananenscheibchen,"
        },
        "line.4": {
            "text": "nicht zu dick,"
        },
        "line.5": {
            "text": "Rosinchen,"
        },
        "line.6": {
            "text": "Mandarinchen"
        },
        "line.7": {
            "text": "stopf ich mir rein,"
        },
        "line.8": {
            "text": "das ist gesund,"
        },
        "line.9": {
            "text": "nun hab ich"
        },
        "line.10": {
            "text": "Obffwawap"
        },
        "line.11": {
            "text": "im Mump."
        }
    }
}