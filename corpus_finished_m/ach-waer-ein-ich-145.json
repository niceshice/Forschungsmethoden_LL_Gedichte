{
    "title": "ach-waer-ein-ich-145",
    "gender": "m",
    "author": "Peter Rühmkorf",
    "author_birth": "1929",
    "poem": {
        "line.1": {
            "text": "Capriccio über den Trennungsstrich in Liebe und Lyrik"
        },
        "line.2": {
            "text": "Ach, wär ein Ich"
        },
        "line.3": {
            "text": "und nicht nur dieses Äch-"
        },
        "line.4": {
            "text": "zen von gestanz-"
        },
        "line.5": {
            "text": "tem und verspanntem Blech,"
        },
        "line.6": {
            "text": "nicht nur das Quiet-"
        },
        "line.7": {
            "text": "schen ausgefranster Bowdenzüge,"
        },
        "line.8": {
            "text": "lieber ein Lied-"
        },
        "line.9": {
            "text": "chen, das mich außer Landes trüge,"
        },
        "line.10": {
            "text": "am liebsten Liebe, die"
        },
        "line.11": {
            "text": "- wie kurz sie sei -"
        },
        "line.12": {
            "text": "statt hier bei Brunch mit Lie-"
        },
        "line.13": {
            "text": "und Bi-Bedienerei,"
        },
        "line.14": {
            "text": "Mundwinkelküssen,"
        },
        "line.15": {
            "text": "achtlos appliziert,"
        },
        "line.16": {
            "text": "auf Stehimbissen, wie?"
        },
        "line.17": {
            "text": "von wem? ich weiß nicht, eingeführt -"
        },
        "line.18": {
            "text": "Ach, wär ein Ich,"
        },
        "line.19": {
            "text": "und Ihr, Madame, mit mir zusammen im Gesträuch:"
        },
        "line.20": {
            "text": "ich e i n m a l rich-"
        },
        "line.21": {
            "text": "tig in und Ihr gesammelt außer Euch."
        }
    }
}