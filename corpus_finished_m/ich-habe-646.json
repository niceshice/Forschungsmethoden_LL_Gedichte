{
    "title": "ich-habe-646",
    "gender": "m",
    "author": "Martin Auer",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "Ich hab einen Kopf,"
        },
        "line.2": {
            "text": "und ich hab einen Bauch."
        },
        "line.3": {
            "text": "Ich hab 'nen Popo,"
        },
        "line.4": {
            "text": "ein Pipi hab ich auch."
        },
        "line.5": {
            "text": "Ich habe zwei Füße."
        },
        "line.6": {
            "text": "Ich habe zwei Hände -"
        },
        "line.7": {
            "text": "damit ist's noch lange nicht zu Ende."
        },
        "line.8": {
            "text": "Ich habe zwei Arme."
        },
        "line.9": {
            "text": "Ich habe zwei Beine."
        },
        "line.10": {
            "text": "Ich habe zwei Ohren"
        },
        "line.11": {
            "text": "(und nicht kleine)."
        },
        "line.12": {
            "text": "Ich habe zwei Augen."
        },
        "line.13": {
            "text": "Ich hab einen Mund."
        },
        "line.14": {
            "text": "Ich hab Haar und Zähne und Zunge und"
        },
        "line.15": {
            "text": "ich habe zwei Wangen."
        },
        "line.16": {
            "text": "Ich hab eine Nase."
        },
        "line.17": {
            "text": "Ich hab einen Darm."
        },
        "line.18": {
            "text": "Und ich hab eine Blase."
        },
        "line.19": {
            "text": "Ich habe ein Hirn."
        },
        "line.20": {
            "text": "Und ich habe ein Herz."
        },
        "line.21": {
            "text": "Ich hab manchmal Spaß."
        },
        "line.22": {
            "text": "Und ich hab manchmal Schmerz."
        },
        "line.23": {
            "text": "Ich habe Gefühle."
        },
        "line.24": {
            "text": "Ich hab einen Geist."
        },
        "line.25": {
            "text": "Ich hab auch Gedanken"
        },
        "line.26": {
            "text": "(zumindest zumeist)."
        },
        "line.27": {
            "text": "Ich hab hunderttausend Nervenkanäle."
        },
        "line.28": {
            "text": "Ich hab einen Körper."
        },
        "line.29": {
            "text": "Ich hab eine Seele."
        },
        "line.30": {
            "text": "Ich hab eine Menge, sicherlich!"
        },
        "line.31": {
            "text": "Aber, wo bin Ich?"
        }
    }
}