{
    "title": "es-fallt-aus-dem-takt-16565",
    "gender": "m",
    "author": "Sascha Garzetti",
    "author_birth": "1986",
    "poem": {
        "line.1": {
            "text": "Paris, Notre-Dame"
        },
        "line.2": {
            "text": "Es fallt aus dem Takt,"
        },
        "line.3": {
            "text": "von der Hand des Vaters,"
        },
        "line.4": {
            "text": "kniet sich hin"
        },
        "line.5": {
            "text": "nach einem Kiesel,"
        },
        "line.6": {
            "text": "nach einem zweiten,"
        },
        "line.7": {
            "text": "beugt sich vor"
        },
        "line.8": {
            "text": "uber das Wasser"
        },
        "line.9": {
            "text": "zwischen Pflastersteinen,"
        },
        "line.10": {
            "text": "erblickt darin nicht"
        },
        "line.11": {
            "text": "die Fratzen der Wasserspeier"
        },
        "line.12": {
            "text": "am Rand der Balustrade,"
        },
        "line.13": {
            "text": "der Spiegel zu klein,"
        },
        "line.14": {
            "text": "sein Gesicht"
        },
        "line.15": {
            "text": "ein Abbild fast,"
        },
        "line.16": {
            "text": "sieht hinab"
        },
        "line.17": {
            "text": "wie in einen Brunnenschacht,"
        },
        "line.18": {
            "text": "darin grast eine Herde"
        },
        "line.19": {
            "text": "wei√üer Kuhe,"
        },
        "line.20": {
            "text": "sieht allein,"
        },
        "line.21": {
            "text": "wie sie davonsteigen,"
        },
        "line.22": {
            "text": "sobald es ein Steinchen"
        },
        "line.23": {
            "text": "fallen lasst"
        },
        "line.24": {
            "text": "und ein zweites."
        }
    }
}