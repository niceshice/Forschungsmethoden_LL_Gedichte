{
    "title": "kavafis-el-iskandariya-5562",
    "gender": "m",
    "author": "José F. A. Oliver",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "koptische ikonen. Über dem bett"
        },
        "line.2": {
            "text": "ihr schrein & 1 verzeiten in griechischen amphoren"
        },
        "line.3": {
            "text": "die silberrostspur eines spiegels"
        },
        "line.4": {
            "text": "im Arbeitszimmer gegenüber. Im oval"
        },
        "line.5": {
            "text": "Mohameds augen blick: 1 aus"
        },
        "line.6": {
            "text": "gehöhltes auto unterm fenster / 1 rot"
        },
        "line.7": {
            "text": "skelett / in der Rue Lepsius [damals"
        },
        "line.8": {
            "text": "-wort]. Es wird lackiert"
        },
        "line.9": {
            "text": "in der Sharm-El-Sheik-Straße"
        },
        "line.10": {
            "text": "als er mir erklärte"
        },
        "line.11": {
            "text": "was ein"
        },
        "line.12": {
            "text": "hamil"
        },
        "line.13": {
            "text": "sei & die pantinen"
        },
        "line.14": {
            "text": "der frauen heim beten. Kavafis sprach"
        },
        "line.15": {
            "text": "vom"
        },
        "line.16": {
            "text": "Haus der Seele"
        },
        "line.17": {
            "text": "vom"
        },
        "line.18": {
            "text": "Haus des Körpers"
        },
        "line.19": {
            "text": "& von"
        },
        "line.20": {
            "text": "dem des Fleisches"
        },
        "line.21": {
            "text": "& wuchsen in den spiegel"
        },
        "line.22": {
            "text": "die offenen dächer der häuser / nacht"
        },
        "line.23": {
            "text": "geweitet & im rückzug des tages"
        },
        "line.24": {
            "text": "die zum turban aufgetürmteapfelsinen. Sein schlaf"
        },
        "line.25": {
            "text": "zimmer wie unverlassen. Ich zog"
        },
        "line.26": {
            "text": "die gebetsschuhe des dichters an"
        },
        "line.27": {
            "text": "& den geruch der unbesungenen liebhaber"
        }
    }
}