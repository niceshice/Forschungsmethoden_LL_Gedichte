{
    "title": "cadenabbia-15523",
    "gender": "m",
    "author": "André Schinkel",
    "author_birth": "1972",
    "poem": {
        "line.1": {
            "text": "Der Wald gerinnt, die Kämme funkeln"
        },
        "line.2": {
            "text": "Von oben her, von unten rauf;"
        },
        "line.3": {
            "text": "Die Sonne spinnt, die Kähne schunkeln,"
        },
        "line.4": {
            "text": "Die Villen gehn wie Sterne auf."
        },
        "line.5": {
            "text": "Der See liegt grün, ein welkes Wesen"
        },
        "line.6": {
            "text": "Aus Schönheit und Verlassenheit:"
        },
        "line.7": {
            "text": "Wir sprechen noch vom Blick-Genesen,"
        },
        "line.8": {
            "text": "Jedoch schon in Vergangenheit."
        },
        "line.9": {
            "text": "Die Tage sind: gezählt  verronnen,"
        },
        "line.10": {
            "text": "Im Rausch, vom Paradies erzeugt;"
        },
        "line.11": {
            "text": "Die Häuser drängen sich, benommen,"
        },
        "line.12": {
            "text": "Im Schatten, den der Felsen beugt."
        },
        "line.13": {
            "text": "Wir stehen dort, das Wasser leuchtet"
        },
        "line.14": {
            "text": "Der Wind dreht von Bellagio her;"
        },
        "line.15": {
            "text": "Das Licht verharrt, die Welle feuchtet:"
        },
        "line.16": {
            "text": "Idyllisch sind die Köpfe leer."
        }
    }
}