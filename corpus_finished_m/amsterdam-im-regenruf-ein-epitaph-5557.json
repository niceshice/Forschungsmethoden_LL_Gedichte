{
    "title": "amsterdam-im-regenruf-ein-epitaph-5557",
    "gender": "m",
    "author": "José F. A. Oliver",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "wie zärtlichkeiten"
        },
        "line.2": {
            "text": "ins vergessen, häuserzeilen"
        },
        "line.3": {
            "text": "in de Pijp - wirft lichtstreu"
        },
        "line.4": {
            "text": "schemen aufs trottoir/ die räder"
        },
        "line.5": {
            "text": "nackt in stellung"
        },
        "line.6": {
            "text": "die speichen fächern"
        },
        "line.7": {
            "text": "zeit zurück - an eine mitte"
        },
        "line.8": {
            "text": "um ein fenster/ der regenwind"
        },
        "line.9": {
            "text": "septemberwärts radiert der stadt"
        },
        "line.10": {
            "text": "Goyescas"
        },
        "line.11": {
            "text": "skizziert die luft-"
        },
        "line.12": {
            "text": "gravur Caprichos, getränkt"
        },
        "line.13": {
            "text": "in aquatinta"
        },
        "line.14": {
            "text": "die äugen herbsten unter"
        },
        "line.15": {
            "text": "schlupf/ ein fahrrad hingestolpert"
        },
        "line.16": {
            "text": "mit wundgeschürftem"
        },
        "line.17": {
            "text": "knie der sommer - war auf dem sims"
        },
        "line.18": {
            "text": "vergangen/ die mutter"
        },
        "line.19": {
            "text": "brach Oregano/ der vater"
        },
        "line.20": {
            "text": "roch nach meertau"
        },
        "line.21": {
            "text": "im dämmerwetter"
        },
        "line.22": {
            "text": "Amsterdam/ zieht haut"
        },
        "line.23": {
            "text": "das kopfsteinpflaster/ verinnert"
        },
        "line.24": {
            "text": "jenen sommertag/ die mutter"
        },
        "line.25": {
            "text": "bricht Oregano/ das kind"
        },
        "line.26": {
            "text": "sagt ros:marin -"
        },
        "line.27": {
            "text": "und stürzt"
        },
        "line.28": {
            "text": "im speichenkreuz der vater//"
        },
        "line.29": {
            "text": "sein tod"
        },
        "line.30": {
            "text": "liegt unterm meertau"
        },
        "line.31": {
            "text": "für G. in Zeiten der Trauer"
        }
    }
}