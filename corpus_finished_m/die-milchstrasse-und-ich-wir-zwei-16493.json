{
    "title": "die-milchstrasse-und-ich-wir-zwei-16493",
    "gender": "m",
    "author": "Dominik Dombrowski",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "Ich schwimme ich krieche ich rolle ich fliege ich"
        },
        "line.2": {
            "text": "lasse mich in ein Foto fallen"
        },
        "line.3": {
            "text": "Ich möchte mich an einem Meer"
        },
        "line.4": {
            "text": "und an den Bergen beteiligen"
        },
        "line.5": {
            "text": "Nie ist dort etwas"
        },
        "line.6": {
            "text": "zu sehen von dem"
        },
        "line.7": {
            "text": "was jemand festhalten will:"
        },
        "line.8": {
            "text": "Ich hab kein Gehirn"
        },
        "line.9": {
            "text": "ich hab"
        },
        "line.10": {
            "text": "Gestirne im Kopf"
        },
        "line.11": {
            "text": "Von den Gezeiten"
        },
        "line.12": {
            "text": "will ich nichts wissen"
        },
        "line.13": {
            "text": "Vor den Gesetzen"
        },
        "line.14": {
            "text": "sind wir beide bleich"
        },
        "line.15": {
            "text": "Ich kenne jetzt Mondbäume"
        },
        "line.16": {
            "text": "Ich nehme ein Blatt vor den Mund"
        }
    }
}