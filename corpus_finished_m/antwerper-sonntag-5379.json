{
    "title": "antwerper-sonntag-5379",
    "gender": "m",
    "author": "Richard Pietraß",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Die Autos standen wie Blei."
        },
        "line.2": {
            "text": "Die Füße lernten zu gehen."
        },
        "line.3": {
            "text": "Die Augen lernten zu sehen."
        },
        "line.4": {
            "text": "Die Ohren drehten bei."
        },
        "line.5": {
            "text": "Die Füße waren wie Blei."
        },
        "line.6": {
            "text": "Die Augen lernten zu gehen."
        },
        "line.7": {
            "text": "Die Ohren lernten verstehen."
        },
        "line.8": {
            "text": "Die Autos hatten frei."
        },
        "line.9": {
            "text": "Die Lider hingen wie Blei."
        },
        "line.10": {
            "text": "Die Ohren lernten zu sehen."
        },
        "line.11": {
            "text": "Die Füße bekamen Zehen."
        },
        "line.12": {
            "text": "Die Daumen drehten bei."
        },
        "line.13": {
            "text": "Die Ohren verloren ihr Blei."
        },
        "line.14": {
            "text": "Die Füße gingen auf Zehen."
        },
        "line.15": {
            "text": "Den Augen kam Hören und Sehen."
        },
        "line.16": {
            "text": "Die Schiffe drehten bei."
        },
        "line.17": {
            "text": "(2oo4)"
        }
    }
}