{
    "title": "urin-urin-756",
    "gender": "m",
    "author": "Maik Lippert",
    "author_birth": "1966",
    "poem": {
        "line.1": {
            "text": "urin urin"
        },
        "line.2": {
            "text": "fließt nicht ab"
        },
        "line.3": {
            "text": "zwischen den bordsteinen"
        },
        "line.4": {
            "text": "riechst du das nicht"
        },
        "line.5": {
            "text": "urin urin"
        },
        "line.6": {
            "text": "und du kriechst"
        },
        "line.7": {
            "text": "mit dem feuerzeug"
        },
        "line.8": {
            "text": "auf und ab"
        },
        "line.9": {
            "text": "dein glück fiel"
        },
        "line.10": {
            "text": "in eine der fugen"
        },
        "line.11": {
            "text": "ein krumen in stanniol"
        },
        "line.12": {
            "text": "woher nimmst du"
        },
        "line.13": {
            "text": "diese nacht"
        },
        "line.14": {
            "text": "dein synapsenfeuer"
        },
        "line.15": {
            "text": "urin urin"
        },
        "line.16": {
            "text": "fließt nicht ab"
        },
        "line.17": {
            "text": "zwischen den bordsteinen"
        }
    }
}