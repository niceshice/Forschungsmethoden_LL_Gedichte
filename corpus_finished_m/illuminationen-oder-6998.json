{
    "title": "illuminationen-oder-6998",
    "gender": "m",
    "author": "Hendrik  Jackson",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "Häuserstufen, nachts."
        },
        "line.2": {
            "text": "(Abstand  Irrlichter)"
        },
        "line.3": {
            "text": "heraufgeschaut: Ahasver Ahasver"
        },
        "line.4": {
            "text": "immerblau die Sphären"
        },
        "line.5": {
            "text": "(clair)"
        },
        "line.6": {
            "text": "überwolkt"
        },
        "line.7": {
            "text": "unterströmt. welch ein Sturzbach... Materie, Luftleere: Schizo-Lötung? wer?"
        },
        "line.8": {
            "text": "sirrende Drähte"
        },
        "line.9": {
            "text": "(Boden - Himmel)"
        },
        "line.10": {
            "text": "zerreißend aufgespannt. Frequenzen: Polk"
        },
        "line.11": {
            "text": "da was auf am Stein"
        },
        "line.12": {
            "text": "(Gestammel)"
        },
        "line.13": {
            "text": "kristallen, leis"
        },
        "line.14": {
            "text": "Entsetzen, Bilderfluß. an Häusern verströmt und in den Hinterhof Abendvolk"
        },
        "line.15": {
            "text": "ein Ärmel streift, reibt"
        },
        "line.16": {
            "text": "(Farben  Schemen)"
        },
        "line.17": {
            "text": "flink bewegt oder einfach wie in Eis"
        },
        "line.18": {
            "text": "gelegt. Summen"
        },
        "line.19": {
            "text": "(Bienen)"
        },
        "line.20": {
            "text": "von unten: stalak"
        },
        "line.21": {
            "text": "ein Schwarz. mauliges, unwirsches Abtun. diese Müdigkeit, Sinn-Verschleiß"
        },
        "line.22": {
            "text": "Erinnerung faltet auf"
        },
        "line.23": {
            "text": "(Abstand - Unsichtbares)"
        },
        "line.24": {
            "text": "entfällt, aber: Lehm Erde Darg"
        },
        "line.25": {
            "text": "die Imme stirbt und Ahasver"
        },
        "line.26": {
            "text": "(Phantasma)"
        },
        "line.27": {
            "text": "wogt"
        },
        "line.28": {
            "text": "finster oder gelb das Feld, Geringstes ging über, verflüchtigte, der Rest  hart"
        }
    }
}