{
    "title": "techne-waehrend-und-7572",
    "gender": "m",
    "author": "Andre Rudolph",
    "author_birth": "1975",
    "poem": {
        "line.1": {
            "text": "täglich, und ganz unbeobachtet,"
        },
        "line.2": {
            "text": "macht die seele ihre"
        },
        "line.3": {
            "text": "backups"
        },
        "line.4": {
            "text": ""
        },
        "line.5": {
            "text": "und sage niemand, es"
        },
        "line.6": {
            "text": "handle sich dabei um eine"
        },
        "line.7": {
            "text": "durchsichtige technik; lange"
        },
        "line.8": {
            "text": "hat man angenommen,"
        },
        "line.9": {
            "text": "es wäre eine simple traumroutine,"
        },
        "line.10": {
            "text": "die da abläuft, inzwischen"
        },
        "line.11": {
            "text": "ist ziemlich sicher, dass"
        },
        "line.12": {
            "text": "es am späten abend geschieht,"
        },
        "line.13": {
            "text": "während du einschläfst, und"
        },
        "line.14": {
            "text": "fünf, höchstens zehn sekunden"
        },
        "line.15": {
            "text": "dauert; während der mond"
        },
        "line.16": {
            "text": "in den fenstern steht,"
        },
        "line.17": {
            "text": "ja, der mond, in den fenstern,"
        },
        "line.18": {
            "text": "seit hundert jahren sind sie mit-"
        },
        "line.19": {
            "text": "einander verschaltet; alles"
        },
        "line.20": {
            "text": "hast du versucht, ein break rein-"
        },
        "line.21": {
            "text": "zulöten,"
        },
        "line.22": {
            "text": "aber der kontakt steht"
        },
        "line.23": {
            "text": ","
        },
        "line.24": {
            "text": "den chip zu killen,"
        },
        "line.25": {
            "text": "aber"
        },
        "line.26": {
            "text": "der kontakt steht"
        },
        "line.27": {
            "text": ", sogar das ganze"
        },
        "line.28": {
            "text": "ding in seine einzelteile zu zer-"
        },
        "line.29": {
            "text": "legen,"
        },
        "line.30": {
            "text": "aber der kontakt"
        },
        "line.31": {
            "text": "so wie deine träume übrigens,"
        },
        "line.32": {
            "text": "die vorgeben, am morgen"
        },
        "line.33": {
            "text": "den körper zu verlassen, während"
        },
        "line.34": {
            "text": "aber die glasröhre des tages"
        },
        "line.35": {
            "text": "zerbricht, und die träume ihre"
        },
        "line.36": {
            "text": "quecksilberkugeln in dein muskel-"
        },
        "line.37": {
            "text": "fleisch schießen und die systeme"
        },
        "line.38": {
            "text": "lahmlegen, während dein"
        },
        "line.39": {
            "text": "körper völlig unbeeindruckt"
        },
        "line.40": {
            "text": "seinen geheimen kalender auf-"
        },
        "line.41": {
            "text": "blättert und die programmdaten"
        },
        "line.42": {
            "text": "abgleicht, und"
        },
        "line.43": {
            "text": "der kontakt"
        },
        "line.44": {
            "text": "aber,"
        },
        "line.45": {
            "text": "monströs und präzise,"
        },
        "line.46": {
            "text": "steht"
        },
        "line.47": {
            "text": "."
        }
    }
}