{
    "title": "die-liebe-im-september-11832",
    "gender": "m",
    "author": "Steffen  Jacobs",
    "author_birth": "1968",
    "poem": {
        "line.1": {
            "text": "Auf dem Frühweg zum Bahnhof,"
        },
        "line.2": {
            "text": "spätsommermorgens halb vier:"
        },
        "line.3": {
            "text": "Geschoßeinschläge jäh ringsum,"
        },
        "line.4": {
            "text": "auf Straßenpflaster, auf Motorhauben."
        },
        "line.5": {
            "text": "Kastanien sind es, ach, Kastanien,"
        },
        "line.6": {
            "text": "die da fallen, knallen und platzen."
        },
        "line.7": {
            "text": "Das alte Geschäft der Vermehrung"
        },
        "line.8": {
            "text": "ist es: Vermehrung, Vermehrung."
        },
        "line.9": {
            "text": "So stürze ich zu dir, Liebe, durch"
        },
        "line.10": {
            "text": "städtisches Feld, ein frisch vom Baum"
        },
        "line.11": {
            "text": "gesprungener, endlich für fruchtbar"
        },
        "line.12": {
            "text": "befundener Held: Meine Versehrung!"
        }
    }
}