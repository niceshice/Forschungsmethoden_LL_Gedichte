{
    "title": "alexandria-i-hautverspuren-5563",
    "gender": "m",
    "author": "José F. A. Oliver",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "kavafishaut"
        },
        "line.2": {
            "text": ". Im sicheren schlaf"
        },
        "line.3": {
            "text": "noch einmal den geruch auf die zunge legen"
        },
        "line.4": {
            "text": ": 1 lippenzittern & lippenaugen schauend. Kein arm"
        },
        "line.5": {
            "text": "der uns nicht wurde"
        },
        "line.6": {
            "text": "& abschied atmen mit jeder lust"
        },
        "line.7": {
            "text": "& stranden im gedächtnis"
        },
        "line.8": {
            "text": "aus aller zeit gerissen"
        },
        "line.9": {
            "text": "verloren wir die stunden & folgte nicht 1 tag"
        },
        "line.10": {
            "text": "hier lagen wir. Im sicheren schlaf"
        },
        "line.11": {
            "text": "2 unbekannte 1 SEELENWORT"
        },
        "line.12": {
            "text": "& uferlos"
        },
        "line.13": {
            "text": "& aufgehoben"
        },
        "line.14": {
            "text": "die sich vergreifende ferne"
        },
        "line.15": {
            "text": "aus ich & ich. Im sicheren schlaf"
        },
        "line.16": {
            "text": "noch einmal schweigen. Wir waren 1"
        },
        "line.17": {
            "text": "bevor wir sind"
        }
    }
}