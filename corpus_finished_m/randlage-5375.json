{
    "title": "randlage-5375",
    "gender": "m",
    "author": "Richard Pietraß",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Die letzte Saat"
        },
        "line.2": {
            "text": "Des Felds ist aufgegangen."
        },
        "line.3": {
            "text": "Ellenbogen"
        },
        "line.4": {
            "text": "Vermessen das Land."
        },
        "line.5": {
            "text": "Pflöcke stückeln"
        },
        "line.6": {
            "text": "Roggenschläge."
        },
        "line.7": {
            "text": "Zins- und Pferdefuß"
        },
        "line.8": {
            "text": "Gehen zur Hand."
        },
        "line.9": {
            "text": "Am Rapsrain"
        },
        "line.10": {
            "text": "Trapst die Nachtigall."
        },
        "line.11": {
            "text": "Die Säge singt"
        },
        "line.12": {
            "text": "Im Holunder."
        },
        "line.13": {
            "text": "Die Bauern motten"
        },
        "line.14": {
            "text": "Pflüge ein."
        },
        "line.15": {
            "text": "Dem Schafstall"
        },
        "line.16": {
            "text": "Blüht ein Quellewunder."
        },
        "line.17": {
            "text": "Wohin ich Habnicht sehe"
        },
        "line.18": {
            "text": "Sieht mich das Ende an."
        },
        "line.19": {
            "text": "Ich stehe und verstehe"
        },
        "line.20": {
            "text": "Wende sich, wer kann."
        },
        "line.21": {
            "text": "(1992)"
        }
    }
}