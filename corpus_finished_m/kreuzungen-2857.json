{
    "title": "kreuzungen-2857",
    "gender": "m",
    "author": "Frank  Schablewski",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "mit Fischreihen im Hinterland"
        },
        "line.2": {
            "text": "als Wassertank"
        },
        "line.3": {
            "text": "Warten von Vögeln"
        },
        "line.4": {
            "text": "aus anders gesehen die Ufer"
        },
        "line.5": {
            "text": "von morgen früh verbreitet"
        },
        "line.6": {
            "text": "regeln an Kreuzungen Lichtzeichen"
        },
        "line.7": {
            "text": "alles weitere ruht fast gänzlich"
        },
        "line.8": {
            "text": "ungeschützt aus dem Verkehr"
        },
        "line.9": {
            "text": "zieht oasisch das Grün Brandblasen"
        },
        "line.10": {
            "text": "zwischen Bleikristallen liegt einer"
        },
        "line.11": {
            "text": "am anderen und scheint darin"
        },
        "line.12": {
            "text": "sich zu erkennen zu geben jedem"
        },
        "line.13": {
            "text": "einzelnen bleibt die Hand auf"
        },
        "line.14": {
            "text": "demHerz wie ein Stoß von Fingern"
        },
        "line.15": {
            "text": "in den Rippen zurück"
        },
        "line.16": {
            "text": "gelassen"
        },
        "line.17": {
            "text": "einen Teil des Weges aufeinander"
        },
        "line.18": {
            "text": "los geht jeder für sich"
        }
    }
}