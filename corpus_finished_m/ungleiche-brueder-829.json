{
    "title": "ungleiche-brueder-829",
    "gender": "m",
    "author": "Gerhard Rühm",
    "author_birth": "1930",
    "poem": {
        "line.1": {
            "text": "kain erwarb ein schloss,"
        },
        "line.2": {
            "text": "abel s erdgeschoss."
        },
        "line.3": {
            "text": "kain fuhr mit dem bus,"
        },
        "line.4": {
            "text": "abel ging zu fuss."
        },
        "line.5": {
            "text": "kain die nacht durchwachte,"
        },
        "line.6": {
            "text": "abel s tagwerk machte."
        },
        "line.7": {
            "text": "kain schlief mit den schwestern,"
        },
        "line.8": {
            "text": "abel war von gestern."
        },
        "line.9": {
            "text": "kain ass hirsch und has,"
        },
        "line.10": {
            "text": "abel kaute gras."
        },
        "line.11": {
            "text": "kain trank bier und wein,"
        },
        "line.12": {
            "text": "abel wässerlein."
        },
        "line.13": {
            "text": "kain zog an zigarren,"
        },
        "line.14": {
            "text": "abel schwere karren."
        },
        "line.15": {
            "text": "kain nahm kokain,"
        },
        "line.16": {
            "text": "abel sah man knien."
        },
        "line.17": {
            "text": "kain zu lachen wusste,"
        },
        "line.18": {
            "text": "abel weinen musste."
        },
        "line.19": {
            "text": "kain ward alt, uralt,"
        },
        "line.20": {
            "text": "abel starb schon bald."
        }
    }
}