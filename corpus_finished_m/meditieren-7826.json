{
    "title": "meditieren-7826",
    "gender": "m",
    "author": "Reiner Kunze",
    "author_birth": "1933",
    "poem": {
        "line.1": {
            "text": "Was das sei, tochter?"
        },
        "line.2": {
            "text": "Gegen morgen"
        },
        "line.3": {
            "text": "noch am schreibtisch sitzen, am hosenbein"
        },
        "line.4": {
            "text": "einen nachtfalter der"
        },
        "line.5": {
            "text": "schläft"
        },
        "line.6": {
            "text": "und keiner weiß vom anderen"
        }
    }
}