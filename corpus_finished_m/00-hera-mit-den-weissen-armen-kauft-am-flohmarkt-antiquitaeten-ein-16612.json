{
    "title": "00-hera-mit-den-weissen-armen-kauft-am-flohmarkt-antiquitaeten-ein-16612",
    "gender": "m",
    "author": "Stefan Schmitzer",
    "author_birth": "1979",
    "poem": {
        "line.1": {
            "text": "kleinfamilien"
        },
        "line.2": {
            "text": "überall kleinfamilien"
        },
        "line.3": {
            "text": "ihr selbstzufriedenes bums"
        },
        "line.4": {
            "text": "ihre kinderwägen"
        },
        "line.5": {
            "text": "ihre kinderwägen"
        },
        "line.6": {
            "text": "überall ihre kinderwägen"
        },
        "line.7": {
            "text": "ganz besondere kinderwägen"
        },
        "line.8": {
            "text": "wo sie fahren wächst erstmal nichts mehr alter"
        },
        "line.9": {
            "text": "schneisen im pflaster als wie die schienen altrömischer dampflokomotiven"
        },
        "line.10": {
            "text": "überall kinderwägen"
        },
        "line.11": {
            "text": "schnittige kleinfamilien"
        },
        "line.12": {
            "text": "schnittige schamlippen"
        },
        "line.13": {
            "text": "und schnittige pimmel"
        },
        "line.14": {
            "text": "die alle nach fenchel und dill und limonade riechen"
        },
        "line.15": {
            "text": "stromlinienförmige pimmel"
        },
        "line.16": {
            "text": "und stromlinienförmige vulven"
        },
        "line.17": {
            "text": "denen stromlinienförmige babies entgleiten"
        },
        "line.18": {
            "text": "schnittige babies überall so schnittige menschen"
        },
        "line.19": {
            "text": "die alle nach fenchel und dill und limonade riechen"
        },
        "line.20": {
            "text": "gruppieret in kleinfamilien"
        },
        "line.21": {
            "text": "mit ihren kinderwägen"
        },
        "line.22": {
            "text": "die wie dampflokomotiven aus der zeit kaiser traians über alles drüberfahren"
        },
        "line.23": {
            "text": "alter wie die drüberfahren"
        },
        "line.24": {
            "text": "da wächst erst mal nichts mehr"
        },
        "line.25": {
            "text": "das gibt den barbaren hier die spurbreite vor für die nächsten paartausend mal rumfummeln"
        },
        "line.26": {
            "text": "barbarbarbarbarbarbarbar"
        },
        "line.27": {
            "text": "es breitet sich ein nebel aus"
        },
        "line.28": {
            "text": "eine sturmwolke über der eisenbahntrasse"
        },
        "line.29": {
            "text": "wohin geht der zug"
        },
        "line.30": {
            "text": "wohin geht die wolke"
        },
        "line.31": {
            "text": "barbarbarbarbarbar"
        }
    }
}