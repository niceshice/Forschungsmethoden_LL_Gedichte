{
    "title": "alp-11834",
    "gender": "m",
    "author": "Steffen  Jacobs",
    "author_birth": "1968",
    "poem": {
        "line.1": {
            "text": "Wir sind der Welt"
        },
        "line.2": {
            "text": "aufs Dach gestiegen. Die"
        },
        "line.3": {
            "text": "Aussicht war nicht schlecht."
        },
        "line.4": {
            "text": "Wir sind nicht lange"
        },
        "line.5": {
            "text": "dort geblieben. Der"
        },
        "line.6": {
            "text": "Schwindel war zu echt."
        },
        "line.7": {
            "text": "Es fehlte wohl am"
        },
        "line.8": {
            "text": "langen Atem. Es fehlte"
        },
        "line.9": {
            "text": "wohl an dem und dem."
        },
        "line.10": {
            "text": "Wir sind der Sonne"
        },
        "line.11": {
            "text": "nah geraten. Jetzt"
        },
        "line.12": {
            "text": "stocken wir im Le-, im Lehm."
        }
    }
}