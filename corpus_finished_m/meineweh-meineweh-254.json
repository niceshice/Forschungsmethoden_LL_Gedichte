{
    "title": "meineweh-meineweh-254",
    "gender": "m",
    "author": "Walther Petri",
    "author_birth": "1940",
    "poem": {
        "line.1": {
            "text": "In Meineweh, in Meineweh"
        },
        "line.2": {
            "text": "tut allen Leuten etwas weh"
        },
        "line.3": {
            "text": "der Fu√ü"
        },
        "line.4": {
            "text": "der Kopf"
        },
        "line.5": {
            "text": "der kleine Zeh"
        },
        "line.6": {
            "text": "in Meineweh, der Jammerstadt"
        },
        "line.7": {
            "text": "sogar die Maus"
        },
        "line.8": {
            "text": "Zahnschmerzen hat"
        },
        "line.9": {
            "text": "und machst du dort"
        },
        "line.10": {
            "text": "ein froh Gesicht"
        },
        "line.11": {
            "text": "ruft jedermann: da"
        },
        "line.12": {
            "text": "stimmt was nicht!"
        },
        "line.13": {
            "text": "selbst wenn die Sonne"
        },
        "line.14": {
            "text": "herrlich scheint"
        },
        "line.15": {
            "text": "da wird in Meineweh"
        },
        "line.16": {
            "text": "geweint"
        },
        "line.17": {
            "text": "doch gestern tats mir weh"
        },
        "line.18": {
            "text": "in beiden Ohren"
        },
        "line.19": {
            "text": "bin ich denn auch"
        },
        "line.20": {
            "text": "in Meineweh geboren?"
        }
    }
}