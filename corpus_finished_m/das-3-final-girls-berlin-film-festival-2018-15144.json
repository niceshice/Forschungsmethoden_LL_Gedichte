{
    "title": "das-3-final-girls-berlin-film-festival-2018-15144",
    "gender": "m",
    "author": "Georg Leß",
    "author_birth": "1981",
    "poem": {
        "line.1": {
            "text": "an einem Tisch sitzen aus sieben* Ländern sieben"
        },
        "line.2": {
            "text": "Horrorfilmregisseurinnen, entspinnen"
        },
        "line.3": {
            "text": "sieben von vielen, kennst du die Listen? sieben"
        },
        "line.4": {
            "text": "Horrorfilmregisseurinnen mit sieben"
        },
        "line.5": {
            "text": "verschiedenen Ländern und ferneren Zielen"
        },
        "line.6": {
            "text": "an einem Tisch sitzen in sieben Ländern"
        },
        "line.7": {
            "text": "Horrorfilmregisseurinnen mit sieben"
        },
        "line.8": {
            "text": "Akzenten und Vorsätzen oder Äxten"
        },
        "line.9": {
            "text": "aus sieben Ländern, infizieren mit Eigenlieben"
        },
        "line.10": {
            "text": "an einem Tisch sitzen aus acht Ländern"
        },
        "line.11": {
            "text": "Horrorfilmregisseurinnen, ändern, sacht"
        },
        "line.12": {
            "text": "wehrt er sich"
        },
        "line.13": {
            "text": "* es waren neun"
        }
    }
}