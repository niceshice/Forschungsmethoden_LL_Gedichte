{
    "title": "aus-der-brunnengasse-14205",
    "gender": "m",
    "author": "Tom Bresemann",
    "author_birth": "1978",
    "poem": {
        "line.1": {
            "text": "auch jesus war weiß und mittelschicht."
        },
        "line.2": {
            "text": "ist das jetzt sarkastisch oder amerikanisch"
        },
        "line.3": {
            "text": "zu verstehen?"
        },
        "line.4": {
            "text": "zeig nicht so mit dem finger herum."
        },
        "line.5": {
            "text": "ich sollte mein leben ändern, wenigstens"
        },
        "line.6": {
            "text": "mal das wasser wechseln,"
        },
        "line.7": {
            "text": "meinesgleichen"
        },
        "line.8": {
            "text": "sind nicht meine leute,"
        },
        "line.9": {
            "text": "kurwa,"
        },
        "line.10": {
            "text": "und ich wandle"
        },
        "line.11": {
            "text": "unter euch als bruder, brüder."
        },
        "line.12": {
            "text": "wisst ihr,"
        },
        "line.13": {
            "text": "von den birkenmännern"
        },
        "line.14": {
            "text": "ist alles herden- oder hütervieh."
        },
        "line.15": {
            "text": "wie das kluge fleisch weiß"
        },
        "line.16": {
            "text": "den bannerträger der genossenschaft,"
        },
        "line.17": {
            "text": "so gib schon"
        },
        "line.18": {
            "text": "deutsches pfötchen, bitch."
        },
        "line.19": {
            "text": "höre ich dich"
        },
        "line.20": {
            "text": "das sagen in deiner sportpalaststimme"
        },
        "line.21": {
            "text": "oder meiner?"
        },
        "line.22": {
            "text": "(das zur gnade"
        },
        "line.23": {
            "text": "der späten replik)"
        },
        "line.24": {
            "text": "zeig her die letzte ehrliche haut"
        },
        "line.25": {
            "text": "des oder-neiße-friedensreichs"
        },
        "line.26": {
            "text": "weil mein schatz im untergrund ist."
        },
        "line.27": {
            "text": "meine gleichnisse sind spaltbar"
        },
        "line.28": {
            "text": "wie die achtzigerjahre, mein haus"
        },
        "line.29": {
            "text": "der häuser mittleres, mein blut"
        },
        "line.30": {
            "text": "regt schaurig nachts die liebe,"
        },
        "line.31": {
            "text": "kälter als der tod."
        },
        "line.32": {
            "text": "geh halt nach österreich"
        },
        "line.33": {
            "text": "ein jeder"
        },
        "line.34": {
            "text": "kennt so einen antisemiten"
        },
        "line.35": {
            "text": "den er mag."
        }
    }
}