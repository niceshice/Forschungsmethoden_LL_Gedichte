{
    "title": "feder-stein-7076",
    "gender": "m",
    "author": "Sepp Mall",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "Und manchmal / ein heller Nach-"
        },
        "line.2": {
            "text": "mittag"
        },
        "line.3": {
            "text": "mit Wolken / weiter oben"
        },
        "line.4": {
            "text": "und weiß"
        },
        "line.5": {
            "text": "die ihre Bahn ziehen / den"
        },
        "line.6": {
            "text": "bloßen Himmel entlang"
        },
        "line.7": {
            "text": "Augen-"
        },
        "line.8": {
            "text": "blicke / die aus Licht"
        },
        "line.9": {
            "text": "bestehn / und fernen Rufen"
        },
        "line.10": {
            "text": "(jenseits der Häuser)"
        },
        "line.11": {
            "text": "Und Eidechsen / die"
        },
        "line.12": {
            "text": "an den Mauern gehn: ein-"
        },
        "line.13": {
            "text": "fach so"
        }
    }
}