{
    "title": "november-8424",
    "gender": "m",
    "author": "Mirko Bonné",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "Zerrüttete Gehöfte"
        },
        "line.2": {
            "text": "aus Vögeln, Vorwinterluft,"
        },
        "line.3": {
            "text": "und alle Fenster sind beschlagen"
        },
        "line.4": {
            "text": "mit heulendem Wasserblech."
        },
        "line.5": {
            "text": "Nur die Feldwege wechseln"
        },
        "line.6": {
            "text": "Brauntöne und fassen das Laub."
        },
        "line.7": {
            "text": "Wolken, die zu Wolkendecken werden,"
        },
        "line.8": {
            "text": "machen deutlich, was es heißt,"
        },
        "line.9": {
            "text": "Augen zu haben."
        },
        "line.10": {
            "text": "Einige schlafen, andere"
        },
        "line.11": {
            "text": "machen die Betten, löschen"
        },
        "line.12": {
            "text": "unschlüssig E-Mails oder spielen"
        },
        "line.13": {
            "text": "Schlagzeug gegen die Stille."
        },
        "line.14": {
            "text": "Zwischen heute und morgen"
        },
        "line.15": {
            "text": "ist keine Grenze. November, ein Name."
        },
        "line.16": {
            "text": "Geh übers Gras. Im Nebel die elf Pappeln,"
        },
        "line.17": {
            "text": "Wegmarken. Meinetwegen als letzter"
        },
        "line.18": {
            "text": "sei unterwegs zu wem immer."
        }
    }
}