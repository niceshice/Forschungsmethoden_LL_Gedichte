{
    "title": "zweigstaelle-2852",
    "gender": "m",
    "author": "Frank  Schablewski",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "wieder kauernd unter Alten gelegen"
        },
        "line.2": {
            "text": "Brust an Brustkorb geflochten"
        },
        "line.3": {
            "text": "aus Buschgräten aufgezogen"
        },
        "line.4": {
            "text": "an einer Schnur"
        },
        "line.5": {
            "text": "gerade gleich"
        },
        "line.6": {
            "text": "einem Ei dem anderen schön gefärbt"
        },
        "line.7": {
            "text": "in leeren Zweigställen"
        },
        "line.8": {
            "text": "auf halbmast"
        },
        "line.9": {
            "text": "Bäume wie tot ausgesät"
        },
        "line.10": {
            "text": "unter Rostgrenzen in der Luft"
        },
        "line.11": {
            "text": "hängend mit brandrotem Laub"
        },
        "line.12": {
            "text": "an geschlagenen Hölzern unbewegt"
        },
        "line.13": {
            "text": "dunkel ohne Schnee zwischen Tannen"
        },
        "line.14": {
            "text": "verschmilzt grün und blau"
        },
        "line.15": {
            "text": "Fäule vor Nässe"
        },
        "line.16": {
            "text": "in verschwommenen Augenblicken kriechen"
        },
        "line.17": {
            "text": "zögernd aufgelöst Traumbilder von Flecken"
        },
        "line.18": {
            "text": "Erde verschmiert schweissverklebtes Haar"
        },
        "line.19": {
            "text": "über Gebindeweben auf Gänsehäuten"
        },
        "line.20": {
            "text": "fingerbreit vom Herzen entfernt"
        },
        "line.21": {
            "text": "sich erinnert gefühlt an Ostermarschen"
        },
        "line.22": {
            "text": "mit Gehäusen unter Gasmatten bedeckt"
        },
        "line.23": {
            "text": "Hand in Hand alles aus der Nähe"
        },
        "line.24": {
            "text": "wirkt wie ein Zwirngespinst"
        },
        "line.25": {
            "text": "an ausgehobenen Armen jedes"
        },
        "line.26": {
            "text": "Ende offen mit Blütenständern"
        }
    }
}