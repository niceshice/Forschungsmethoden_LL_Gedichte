{
    "title": "abc-683",
    "gender": "m",
    "author": "Jürg Schubiger",
    "author_birth": "1936",
    "poem": {
        "line.1": {
            "text": "Apfelmus und Apfelbrei,"
        },
        "line.2": {
            "text": "am Ende ist das einerlei."
        },
        "line.3": {
            "text": "Butterbrote werden ranzig,"
        },
        "line.4": {
            "text": "Bagdad liegt sehr weit von Danzig."
        },
        "line.5": {
            "text": "Carlo übt, es ist zum Weinen,"
        },
        "line.6": {
            "text": "Chorgesänge mit den Seinen."
        },
        "line.7": {
            "text": "Doch bedenkt, bedenkt, ruft Schorsch:"
        },
        "line.8": {
            "text": "Dachs ist Dachs und Dorsch ist Dorsch."
        },
        "line.9": {
            "text": "Elfen sind sehr wohlgestaltet,"
        },
        "line.10": {
            "text": "Echsen wirken leicht veraltet."
        },
        "line.11": {
            "text": "Freiheit ist ein schönes Wort,"
        },
        "line.12": {
            "text": "Friede, Freude und so fort."
        },
        "line.13": {
            "text": "Gernegross sind kleine Leute,"
        },
        "line.14": {
            "text": "Grau sind Elefantenhäute."
        },
        "line.15": {
            "text": "Hektor heisst mein alter Hecht,"
        },
        "line.16": {
            "text": "Heisere versteht man schlecht."
        },
        "line.17": {
            "text": "Inseln sind beliebte Orte,"
        },
        "line.18": {
            "text": "Igel leben ohne Worte."
        },
        "line.19": {
            "text": "Jakob ist ein froher Junge,"
        },
        "line.20": {
            "text": "jauchzt aus voller Lust und Lunge."
        },
        "line.21": {
            "text": "Kauderwelsch ist schwer verständlich,"
        },
        "line.22": {
            "text": "kch zum Beispiel heisst unendlich."
        },
        "line.23": {
            "text": "Lügen haben kurze Beine,"
        },
        "line.24": {
            "text": "Lachsforellen haben keine."
        },
        "line.25": {
            "text": "Monster möchten uns erschrecken,"
        },
        "line.26": {
            "text": "mit Gebrüll und Zähneblecken."
        },
        "line.27": {
            "text": "Nordwärts zieht die Herde grasend,"
        },
        "line.28": {
            "text": "Nobert folgt ihr, Flöte blasend."
        },
        "line.29": {
            "text": "Oma sagt: Mein Urgrossvater"
        },
        "line.30": {
            "text": "Oskar sang im Wiener Prater."
        },
        "line.31": {
            "text": "Pappeln stehen ernst und stramm,"
        },
        "line.32": {
            "text": "Purzelbäumen fehlt der Stamm."
        },
        "line.33": {
            "text": "Quitten duften, Quellen sprudeln,"
        },
        "line.34": {
            "text": "Quallen kommen oft in Rudeln."
        },
        "line.35": {
            "text": "Rosa ist nicht ganz bei Trost,"
        },
        "line.36": {
            "text": "röstet lächelnd Tost auf Tost."
        },
        "line.37": {
            "text": "Samt und Seide trägt Susanne,"
        },
        "line.38": {
            "text": "sogar in der Badewanne."
        },
        "line.39": {
            "text": "Tom bläst Trübsal und Trombone,"
        },
        "line.40": {
            "text": "teils mit Eifer und teils ohne."
        },
        "line.41": {
            "text": "Ur- passt gut zu Wald und Kunde"
        },
        "line.42": {
            "text": "und zu Laub, sagt Kunigunde."
        },
        "line.43": {
            "text": "Vrohe schrieb mit V statt F"
        },
        "line.44": {
            "text": "Verien Frieda ihrem Chef."
        },
        "line.45": {
            "text": "Wale suchen in den Meeren"
        },
        "line.46": {
            "text": "wie verrückt nach Heidelbeeren."
        },
        "line.47": {
            "text": "Xaver schien, als er am Reck war,"
        },
        "line.48": {
            "text": "x-beliebig beug- und streckbar."
        },
        "line.49": {
            "text": "Yoga, und zwar zweimal täglich,"
        },
        "line.50": {
            "text": "you will see, das stärkt unsäglich."
        },
        "line.51": {
            "text": "Zecken haben uns befallen,"
        },
        "line.52": {
            "text": "zeigen schon den Bauch, den prallen."
        }
    }
}