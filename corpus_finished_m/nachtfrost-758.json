{
    "title": "nachtfrost-758",
    "gender": "m",
    "author": "Maik Lippert",
    "author_birth": "1966",
    "poem": {
        "line.1": {
            "text": "nachtfrost"
        },
        "line.2": {
            "text": "und die fliegen am fenster"
        },
        "line.3": {
            "text": "erhängen sich"
        },
        "line.4": {
            "text": "müde matrosen"
        },
        "line.5": {
            "text": "an der takelage"
        },
        "line.6": {
            "text": "pünktlich zum blätterfall"
        },
        "line.7": {
            "text": "wer gab den befehl"
        },
        "line.8": {
            "text": "zum herbsttod"
        },
        "line.9": {
            "text": "gardinen abhängen"
        },
        "line.10": {
            "text": "und waschen"
        },
        "line.11": {
            "text": "so landen sie doch nicht"
        },
        "line.12": {
            "text": "im meer"
        },
        "line.13": {
            "text": "nur in der lauge"
        },
        "line.14": {
            "text": "und kreiseln"
        },
        "line.15": {
            "text": "verwunschene kompaßnadeln"
        },
        "line.16": {
            "text": "in chitinpanzern"
        }
    }
}