{
    "title": "die-muecke-475",
    "gender": "m",
    "author": "Lutz Rathenow",
    "author_birth": "1952",
    "poem": {
        "line.1": {
            "text": "Die Mücke saugt"
        },
        "line.2": {
            "text": "Zeigefingerblut."
        },
        "line.3": {
            "text": "Meins. Mückenruhig."
        },
        "line.4": {
            "text": "Das bekam ihr gar nicht gut."
        },
        "line.5": {
            "text": "Schon fliegt die nächste ran."
        },
        "line.6": {
            "text": "Und fängt zu saugen an."
        },
        "line.7": {
            "text": "Menschensaft pur,"
        },
        "line.8": {
            "text": "saugt sie stur."
        },
        "line.9": {
            "text": "Eitel meldet sich das Gewissen:"
        },
        "line.10": {
            "text": "Du bist, für Mücken, ein Leckerbissen."
        },
        "line.11": {
            "text": "Nein, ich töte keine Mücken mehr."
        },
        "line.12": {
            "text": "Denn sie verehren mich zu sehr."
        }
    }
}