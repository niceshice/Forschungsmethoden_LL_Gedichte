{
    "title": "ueberquert-die-alpeni-taipeh-3935",
    "gender": "m",
    "author": "Dieter M. Gräf",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "naturbelassene Hosen des"
        },
        "line.2": {
            "text": "Eroberers der ausländischen"
        },
        "line.3": {
            "text": "Welt, Rasiermesser, Schach"
        },
        "line.4": {
            "text": "figuren, mit denen er auf"
        },
        "line.5": {
            "text": "St. Helena noch spielte,"
        },
        "line.6": {
            "text": "nun auf diese Insel gesperrt."
        },
        "line.7": {
            "text": "Sein chinesengroßes Feld"
        },
        "line.8": {
            "text": "bett. Vergrößerung: dann,"
        },
        "line.9": {
            "text": "ein Nebenflügel, auf Stoff"
        },
        "line.10": {
            "text": "blumen erscheinende eigene"
        },
        "line.11": {
            "text": "Geschichte, Minischrift"
        },
        "line.12": {
            "text": "zeichen auf Eierschalen"
        },
        "line.13": {
            "text": "stele im Schaukasten mit"
        },
        "line.14": {
            "text": "Lupeneinsatz, oder, in der"
        },
        "line.15": {
            "text": "Eingangshalle, der Kaiser"
        },
        "line.16": {
            "text": "bezwinger als Bronzekoloss."
        },
        "line.17": {
            "text": "Der Starre steife Pausen, wenn"
        },
        "line.18": {
            "text": "ein Bediensteter der Ehren"
        },
        "line.19": {
            "text": "wache den Nacken massiert,"
        },
        "line.20": {
            "text": "kann sich entpuppen,"
        },
        "line.21": {
            "text": "bevor sie, mit versetzten"
        },
        "line.22": {
            "text": "Schritten, abgelöst wird."
        },
        "line.23": {
            "text": "Im Freien, am Teich,"
        },
        "line.24": {
            "text": "bewegt womöglich ein"
        },
        "line.25": {
            "text": "Baum das Chi eines"
        },
        "line.26": {
            "text": "Mannes, der übt: seine"
        },
        "line.27": {
            "text": "Alpen überqueren, und"
        },
        "line.28": {
            "text": "dabei hineinbröckeln."
        }
    }
}