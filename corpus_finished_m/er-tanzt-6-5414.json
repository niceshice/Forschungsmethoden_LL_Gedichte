{
    "title": "er-tanzt-6-5414",
    "gender": "m",
    "author": "Hans Raimund",
    "author_birth": "1945",
    "poem": {
        "line.1": {
            "text": "AUS DER EINSAMKEIT HERAUS"
        },
        "line.2": {
            "text": "Nimmt er sich"
        },
        "line.3": {
            "text": "Das Recht"
        },
        "line.4": {
            "text": "Den Atem anzuhalten"
        },
        "line.5": {
            "text": "Hat er"
        },
        "line.6": {
            "text": "Seit dem ersten Schrei"
        },
        "line.7": {
            "text": "Ein für alle Mal"
        },
        "line.8": {
            "text": "Die Unwillkürlichkeit"
        },
        "line.9": {
            "text": "Verweigert"
        },
        "line.10": {
            "text": "Unter Elis Knieer"
        },
        "line.11": {
            "text": "Sehnte Nähe   Erdung"
        },
        "line.12": {
            "text": "Weißes Wissen"
        },
        "line.13": {
            "text": "Kresse"
        },
        "line.14": {
            "text": "Wirft die Samenkörner aus"
        },
        "line.15": {
            "text": "Zwischen Fingern funken"
        },
        "line.16": {
            "text": "Schlägt  Geruch von nassen"
        },
        "line.17": {
            "text": "Nadeln"
        },
        "line.18": {
            "text": "Rosmarin"
        }
    }
}