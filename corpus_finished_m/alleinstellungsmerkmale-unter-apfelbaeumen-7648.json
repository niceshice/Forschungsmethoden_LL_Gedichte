{
    "title": "alleinstellungsmerkmale-unter-apfelbaeumen-7648",
    "gender": "m",
    "author": "Tom Schulz",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "erst opferten wir ein Pferd, dann sangen wir"
        },
        "line.2": {
            "text": "Baum Blume Gras"
        },
        "line.3": {
            "text": "leg dich längs auf den Boden"
        },
        "line.4": {
            "text": "& werde ein Freund der Erde"
        },
        "line.5": {
            "text": "in den Bankenprospekt schnippten wir"
        },
        "line.6": {
            "text": "die kleinen Finger, die mit den Wärme"
        },
        "line.7": {
            "text": "Meldern hakelten, Wärmefelder"
        },
        "line.8": {
            "text": "aus Butan, sie tauschten"
        },
        "line.9": {
            "text": "die Tiere in Anteile von etwas"
        },
        "line.10": {
            "text": "das einen hohen Zaun errichtet hatte"
        },
        "line.11": {
            "text": "Baum Blume Gras"
        },
        "line.12": {
            "text": "erst opfern wir eine Tangente, dann singen wir"
        },
        "line.13": {
            "text": "im Teich die Enten recken die Köpfchen"
        },
        "line.14": {
            "text": "als stünden sie zur Abstimmung, als würden sie"
        },
        "line.15": {
            "text": "aus der Welt gewählt"
        },
        "line.16": {
            "text": "leg dich längs auf den Boden"
        },
        "line.17": {
            "text": "& werde ein Freund der Erde"
        },
        "line.18": {
            "text": "Baum Blume Gras"
        },
        "line.19": {
            "text": "wir schliefen im Gehen"
        },
        "line.20": {
            "text": "vor & hinter der industriellen"
        },
        "line.21": {
            "text": "Zone lag ein Verwertungsmodus"
        },
        "line.22": {
            "text": "Raffinerien, die das Sprudelnde"
        },
        "line.23": {
            "text": "in Zähes verwandelten, der gelbe"
        },
        "line.24": {
            "text": "Strom an der Uraltankstelle"
        },
        "line.25": {
            "text": "Baum Blume Gras"
        },
        "line.26": {
            "text": "leg dich leicht auf den Boden"
        },
        "line.27": {
            "text": "& werde zu Erde"
        },
        "line.28": {
            "text": "sei ein Freund aller Gestirne"
        },
        "line.29": {
            "text": "die sich drehen & drehen & drehen"
        },
        "line.30": {
            "text": "um keinen Marktplatz"
        },
        "line.31": {
            "text": "erst opfern wir eine Muscheltankstelle, dann singen wir"
        }
    }
}