{
    "title": "daemmerung-10069",
    "gender": "m",
    "author": "Andreas Neeser",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "Krähen im Anflug zum Schlafbaum"
        },
        "line.2": {
            "text": "die Saat ist geerntet, die Sonne"
        },
        "line.3": {
            "text": "geht unter um sechs."
        },
        "line.4": {
            "text": "Galgenvögel"
        },
        "line.5": {
            "text": "geben Nachricht vom Himmel"
        },
        "line.6": {
            "text": "sie rufen und scheiden die Geister"
        },
        "line.7": {
            "text": "im Halblicht"
        },
        "line.8": {
            "text": "schärft sich ihr Blick"
        },
        "line.9": {
            "text": "die Hellsicht fasst steiniges Feld."
        },
        "line.10": {
            "text": "Und krächzten sie Worte"
        },
        "line.11": {
            "text": "wir hörten sie nicht."
        }
    }
}