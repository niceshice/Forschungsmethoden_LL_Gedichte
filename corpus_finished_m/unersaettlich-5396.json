{
    "title": "unersaettlich-5396",
    "gender": "m",
    "author": "Hans Raimund",
    "author_birth": "1945",
    "poem": {
        "line.1": {
            "text": "Auf den Klippen"
        },
        "line.2": {
            "text": "Festnadelnd mit den Augen"
        },
        "line.3": {
            "text": "Das Segelwinzig am Horizont"
        },
        "line.4": {
            "text": "Die Gischtkräusel auf den Wellen"
        },
        "line.5": {
            "text": "Die Leuchtspur der Sonne"
        },
        "line.6": {
            "text": "Die Rauchpilzeregenbogenbunt"
        },
        "line.7": {
            "text": "Über den Schloten"
        },
        "line.8": {
            "text": "Auf den Klippen"
        },
        "line.9": {
            "text": "Nasenflügel geblähtwitternd"
        },
        "line.10": {
            "text": "- Naftamoderndes TreibholzTang"
        },
        "line.11": {
            "text": "Teerungeklärte AbwässerFischlaich"
        },
        "line.12": {
            "text": "Brennendes LaubZiegenköttel"
        },
        "line.13": {
            "text": "Tausnasse PinienzapfenSchwefel -"
        },
        "line.14": {
            "text": "SchniefendAtem anhaltendniesend"
        },
        "line.15": {
            "text": "Auf den Klippen"
        },
        "line.16": {
            "text": "Im Ohr das Getöse der Autobahn"
        },
        "line.17": {
            "text": "Die Pfiffe der Lokomotiven"
        },
        "line.18": {
            "text": "Das Tuckern der Fischerboote"
        },
        "line.19": {
            "text": "Den Schrei der Möwejäh"
        },
        "line.20": {
            "text": "Stößt sie niederaufs Wasser"
        },
        "line.21": {
            "text": "Schon glitzert im Schnabel der Fisch"
        },
        "line.22": {
            "text": "Auf den Klippen"
        },
        "line.23": {
            "text": "Salzige Luft von den Lippen leckend"
        },
        "line.24": {
            "text": "Im After ziehend zuckend die Angst"
        },
        "line.25": {
            "text": "Vor dem falschen Schrittdem Sturz"
        },
        "line.26": {
            "text": "Vor dem Windstoß hinterrücks aus dem Karst"
        },
        "line.27": {
            "text": "Vor dem Blitz aus dem Kaiserwetterhimmel"
        },
        "line.28": {
            "text": "Dem Biß der Viper"
        },
        "line.29": {
            "text": "Auf den Klippen"
        },
        "line.30": {
            "text": "Sehendhörendriechendspürend"
        },
        "line.31": {
            "text": "Wahrgenommenes speichernd"
        },
        "line.32": {
            "text": "Im Nu verwandelt"
        },
        "line.33": {
            "text": "EmpfindungBildKlangDuft"
        },
        "line.34": {
            "text": "Quälend vagkaum noch enträtselbar"
        },
        "line.35": {
            "text": "Oder jederzeit abrufbarErinnerung"
        },
        "line.36": {
            "text": "Stets jedoch Hunger"
        },
        "line.37": {
            "text": "Nie zu stillender"
        },
        "line.38": {
            "text": "(1989)"
        }
    }
}