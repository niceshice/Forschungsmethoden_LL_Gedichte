{
    "title": "unter-dem-glassturz-3761",
    "gender": "m",
    "author": "Norbert  Hummelt",
    "author_birth": "1962",
    "poem": {
        "line.1": {
            "text": "noch stehn die magnolien nicht"
        },
        "line.2": {
            "text": "im zenit doch ist ein druck"
        },
        "line.3": {
            "text": "auf meinem linken augenlid"
        },
        "line.4": {
            "text": "u. sendet schauder bis in"
        },
        "line.5": {
            "text": "mein gehirn als sei der flieder"
        },
        "line.6": {
            "text": "schon an mir vorbeigegangen"
        },
        "line.7": {
            "text": "die glocke soll sich nur nicht"
        },
        "line.8": {
            "text": "wieder senken, um gottes"
        },
        "line.9": {
            "text": "willen bitte, nicht um mich"
        },
        "line.10": {
            "text": "ich will auch immer atmen daß"
        },
        "line.11": {
            "text": "ich luft bekomme, mit tropfen"
        },
        "line.12": {
            "text": "künstlich die pupillen weiten"
        },
        "line.13": {
            "text": "daß ich das blühen einmal nicht"
        },
        "line.14": {
            "text": "versäume, das weiße blitzen"
        },
        "line.15": {
            "text": "der kastanienbäume"
        },
        "line.16": {
            "text": "von dem ich eingekapselt"
        },
        "line.17": {
            "text": "schon u. immerfort mit"
        },
        "line.18": {
            "text": "allen venen u. arterien träume"
        }
    }
}