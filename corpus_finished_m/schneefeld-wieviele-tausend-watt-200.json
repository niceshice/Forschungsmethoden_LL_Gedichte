{
    "title": "schneefeld-wieviele-tausend-watt-200",
    "gender": "m",
    "author": "Peter Waterhouse",
    "author_birth": "1956",
    "poem": {
        "line.1": {
            "text": "Schneefeld"
        },
        "line.2": {
            "text": "wieviele tausend Watt"
        },
        "line.3": {
            "text": "Über die Kuppe"
        },
        "line.4": {
            "text": "gehen sie. Wer?"
        },
        "line.5": {
            "text": "Die Luftgestalten"
        },
        "line.6": {
            "text": "Im Himmel sind sie"
        },
        "line.7": {
            "text": "Wer?"
        },
        "line.8": {
            "text": "Die Raketen"
        },
        "line.9": {
            "text": "Der Wagen fährt fort"
        },
        "line.10": {
            "text": "Ich bleibe hier"
        },
        "line.11": {
            "text": "Was ist das also hier"
        },
        "line.12": {
            "text": "Milchstraße?"
        },
        "line.13": {
            "text": "Milchfelder?"
        },
        "line.14": {
            "text": "'Ichleb vom Leben'"
        },
        "line.15": {
            "text": "'He, schau"
        },
        "line.16": {
            "text": "1  2  3 - 4'"
        },
        "line.17": {
            "text": "Jetzt sieht er mich"
        },
        "line.18": {
            "text": "Soeben ist die Welt entstanden"
        },
        "line.19": {
            "text": "und entsteht"
        },
        "line.20": {
            "text": "entsteht"
        },
        "line.21": {
            "text": "Endlich ist der"
        },
        "line.22": {
            "text": "Parkplatz voll von Wagen"
        },
        "line.23": {
            "text": "es ist Montag"
        },
        "line.24": {
            "text": "Ein Dorf verschneit"
        },
        "line.25": {
            "text": "schön geschrieben"
        },
        "line.26": {
            "text": "wie eine Apfelblüte"
        },
        "line.27": {
            "text": "Ich gleiche allen"
        },
        "line.28": {
            "text": "Wem?"
        },
        "line.29": {
            "text": "Allen Frauen und Männern"
        },
        "line.30": {
            "text": "Ich fahre in der Eisenbahn"
        },
        "line.31": {
            "text": "durch große Buchstaben von Eis"
        },
        "line.32": {
            "text": "Dort ist Schatten"
        },
        "line.33": {
            "text": "'Schatten wovon?'"
        },
        "line.34": {
            "text": "Schatten von Wald"
        },
        "line.35": {
            "text": "In uns träumen Blumen"
        },
        "line.36": {
            "text": "Die Straße führt nach Göttingen hin"
        },
        "line.37": {
            "text": "Bist du wer"
        },
        "line.38": {
            "text": "Und diese Straße"
        },
        "line.39": {
            "text": "geht in den Nebel"
        },
        "line.40": {
            "text": "Bist du Nebel"
        },
        "line.41": {
            "text": "Letztes Lichtfenster"
        },
        "line.42": {
            "text": "erlischt"
        },
        "line.43": {
            "text": "Jetzt ist Nacht"
        },
        "line.44": {
            "text": "Das Eis zischt"
        },
        "line.45": {
            "text": "wie leiser"
        },
        "line.46": {
            "text": "Straßenverkehr"
        },
        "line.47": {
            "text": "Namen Namen"
        },
        "line.48": {
            "text": "Namen Namen"
        },
        "line.49": {
            "text": "in der Ambulanz des Krankenhauses"
        },
        "line.50": {
            "text": "Stock"
        },
        "line.51": {
            "text": "'ich habe Freude damit'"
        },
        "line.52": {
            "text": "Abschied"
        },
        "line.53": {
            "text": "der Kopf"
        },
        "line.54": {
            "text": "zuckt nach hinten"
        },
        "line.55": {
            "text": "Marktplätzchen"
        },
        "line.56": {
            "text": "kenne ich hier alle"
        },
        "line.57": {
            "text": "kenne ich hier keinen"
        },
        "line.58": {
            "text": "Plötzlich da"
        },
        "line.59": {
            "text": "der Mund des Vaters geschwungen"
        },
        "line.60": {
            "text": "wie dort der Mund der jungen Frau"
        },
        "line.61": {
            "text": "Der Körper"
        },
        "line.62": {
            "text": "von Kohlestaub geschwächt"
        },
        "line.63": {
            "text": "und ich schreibe das auf"
        },
        "line.64": {
            "text": "Plastik und Papier"
        },
        "line.65": {
            "text": "auf dem Bahndamm"
        },
        "line.66": {
            "text": "hier knospt etwas"
        },
        "line.67": {
            "text": "Alte Dinge"
        },
        "line.68": {
            "text": "knospende Stellen"
        },
        "line.69": {
            "text": "Soviel Licht"
        },
        "line.70": {
            "text": "über Wagram"
        },
        "line.71": {
            "text": "und die Feldwege leben"
        },
        "line.72": {
            "text": "Die Zaunstangen stehen"
        },
        "line.73": {
            "text": "in einer Hülle aus Sonne"
        },
        "line.74": {
            "text": "Da"
        },
        "line.75": {
            "text": "etwas Weißes:"
        },
        "line.76": {
            "text": "Sonne-und-Haus"
        },
        "line.77": {
            "text": "Neben jedem Graslicht"
        },
        "line.78": {
            "text": "ein"
        },
        "line.79": {
            "text": "Schatten"
        },
        "line.80": {
            "text": "Hasen sitzen"
        },
        "line.81": {
            "text": "da sammelt sich das Licht"
        },
        "line.82": {
            "text": "in ihren Fellen"
        },
        "line.83": {
            "text": "Zaunstange"
        },
        "line.84": {
            "text": "das ist"
        },
        "line.85": {
            "text": "einer der Grundstoffe der Welt"
        },
        "line.86": {
            "text": "Hühner Gänse"
        },
        "line.87": {
            "text": "Hunde Kinder"
        },
        "line.88": {
            "text": "Gemisch"
        },
        "line.89": {
            "text": "Schulkinder"
        },
        "line.90": {
            "text": "gehen durch eine Schar Gänse"
        },
        "line.91": {
            "text": "das ist Lebensspeise"
        },
        "line.92": {
            "text": "Die Kinder rufen"
        },
        "line.93": {
            "text": "die Gänse sind weiß"
        },
        "line.94": {
            "text": "alles ist Brot"
        },
        "line.95": {
            "text": "Grüne Hosen Streifen"
        },
        "line.96": {
            "text": "rosa Hemd blauer Anorak"
        },
        "line.97": {
            "text": "das macht mich gesund"
        },
        "line.98": {
            "text": "Anbauten"
        },
        "line.99": {
            "text": "Hütten Antennen"
        },
        "line.100": {
            "text": "mein Mittagessen"
        },
        "line.101": {
            "text": "Das Dorf"
        },
        "line.102": {
            "text": "das Dorf, ja aber"
        },
        "line.103": {
            "text": "ich spür mich geformt werden"
        },
        "line.104": {
            "text": "Schlamm"
        },
        "line.105": {
            "text": "ich sehe den Schlamm nicht"
        },
        "line.106": {
            "text": "die Gänse laufen durch den Schlamm"
        },
        "line.107": {
            "text": "Der Lastwagen"
        },
        "line.108": {
            "text": "spritzt Schlamm auf mich"
        },
        "line.109": {
            "text": "also viele ... viele was?"
        },
        "line.110": {
            "text": "Zweiter Tagesanbruch"
        },
        "line.111": {
            "text": "um die Hausecken"
        },
        "line.112": {
            "text": "kommen die Schulkinder"
        },
        "line.113": {
            "text": "Rosa Schulkind"
        },
        "line.114": {
            "text": "du schmückst mich"
        },
        "line.115": {
            "text": "fast unmerklich"
        },
        "line.116": {
            "text": "Dann gehe ich"
        },
        "line.117": {
            "text": "auf einer Straße"
        },
        "line.118": {
            "text": "lasse mich schmücken von der Straße"
        },
        "line.119": {
            "text": "Straße"
        },
        "line.120": {
            "text": "die ich gehe"
        },
        "line.121": {
            "text": "jetzt ist sie mein Kleid"
        },
        "line.122": {
            "text": "Ich auf der Landstraße"
        },
        "line.123": {
            "text": "ich gehe"
        },
        "line.124": {
            "text": "bin Schöpfung"
        },
        "line.125": {
            "text": "Name dieses Landes"
        },
        "line.126": {
            "text": "in dem ich mit der Eisenbahn fahr"
        },
        "line.127": {
            "text": "Eisenbahnland"
        },
        "line.128": {
            "text": "Ein Birkenwäldchen"
        },
        "line.129": {
            "text": "wie Menschen"
        },
        "line.130": {
            "text": "(senkrechte lebhafte Tote)"
        },
        "line.131": {
            "text": "Die Sonne sinkt"
        },
        "line.132": {
            "text": "und"
        },
        "line.133": {
            "text": "jetzt wird der Wald heller"
        },
        "line.134": {
            "text": "Seltsame Dinge"
        },
        "line.135": {
            "text": "in dieser Landschaft"
        },
        "line.136": {
            "text": "zum Beispiel ein Berg"
        },
        "line.137": {
            "text": "Licht Licht"
        },
        "line.138": {
            "text": "aus der Flughafenhalle Schöneberg"
        },
        "line.139": {
            "text": "Nacht Nacht vom Himmel"
        }
    }
}