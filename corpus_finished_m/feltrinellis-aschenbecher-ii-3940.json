{
    "title": "feltrinellis-aschenbecher-ii-3940",
    "gender": "m",
    "author": "Dieter M. Gräf",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "sich aus"
        },
        "line.2": {
            "text": "drücken in ein glänzen"
        },
        "line.3": {
            "text": "des Nichts;"
        },
        "line.4": {
            "text": "in Autobahntunnel ein"
        },
        "line.5": {
            "text": "tauchendes Haimaul,"
        },
        "line.6": {
            "text": "noch immer am Steuer,"
        },
        "line.7": {
            "text": "hin zu den alten Partisanen,"
        },
        "line.8": {
            "text": "die Abendnachrichten über"
        },
        "line.9": {
            "text": "blenden"
        },
        "line.10": {
            "text": "*"
        },
        "line.11": {
            "text": "Weizenfeld, auf dem sich der Strommast erhebt"
        },
        "line.12": {
            "text": "ein als Wohnmobil ausgebauter grauer"
        },
        "line.13": {
            "text": "VW-Bus mit Gardinen an den Fenstern"
        },
        "line.14": {
            "text": "Nulla lucente"
        },
        "line.15": {
            "text": "hintere Hosentasche, in ihr findet sich:"
        },
        "line.16": {
            "text": "eine Zigarettenschachtel, eine einfache Hand*"
        },
        "line.17": {
            "text": "* granate, gefüllt mit gepresstem"
        }
    }
}