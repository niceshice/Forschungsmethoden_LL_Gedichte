{
    "title": "federn-blasen-15762",
    "gender": "m",
    "author": "Günter Grass",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "Das war im Mai, als Willy zurücktrat."
        },
        "line.2": {
            "text": "Ich hatte mit Möwenfedern den sechsten tagsüber"
        },
        "line.3": {
            "text": "mich gezeichnet: ältlich schon und gebraucht,"
        },
        "line.4": {
            "text": "doch immer noch Federn blasend,"
        },
        "line.5": {
            "text": "wie ich als Junge (zur Luftschiffzeit)"
        },
        "line.6": {
            "text": "und auch zuvor,"
        },
        "line.7": {
            "text": "soweit ich mich denke (vorchristlich steinzeitlich)"
        },
        "line.8": {
            "text": "Federn, drei vier zugleich,"
        },
        "line.9": {
            "text": "den Flaum, Wünsche, das Glück"
        },
        "line.10": {
            "text": "liegend laufend geblasen"
        },
        "line.11": {
            "text": "und in Schwebe (ein Menschenalter) gehalten habe."
        },
        "line.12": {
            "text": "Willy auch."
        },
        "line.13": {
            "text": "Sein bestaunt langer Atem."
        },
        "line.14": {
            "text": "Woher er ihn holte."
        },
        "line.15": {
            "text": "Seit dem Lübecker Pausenhof."
        },
        "line.16": {
            "text": "Meine Federn  einige waren seine  ermatten."
        },
        "line.17": {
            "text": "Zufällig liegen sie, wie gewöhnlich."
        },
        "line.18": {
            "text": "Draußen, ich weiß, bläht die Macht ihre Backen;"
        },
        "line.19": {
            "text": "doch keine Feder,"
        },
        "line.20": {
            "text": "kein Traum wird ihr tanzen."
        }
    }
}