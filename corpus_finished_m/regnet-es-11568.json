{
    "title": "regnet-es-11568",
    "gender": "m",
    "author": "Michael Donhauser",
    "author_birth": "1956",
    "poem": {
        "line.1": {
            "text": "Regnet es"
        },
        "line.2": {
            "text": "waren es"
        },
        "line.3": {
            "text": "Tropfen"
        },
        "line.4": {
            "text": "Silben"
        },
        "line.5": {
            "text": "welche da"
        },
        "line.6": {
            "text": "als ein Gruß"
        },
        "line.7": {
            "text": "zogen"
        },
        "line.8": {
            "text": "fielen"
        },
        "line.9": {
            "text": "alles war"
        },
        "line.10": {
            "text": "etwas spät"
        },
        "line.11": {
            "text": "horchte"
        },
        "line.12": {
            "text": "wiegend"
        },
        "line.13": {
            "text": "wie die Nacht"
        },
        "line.14": {
            "text": "dunkel als"
        },
        "line.15": {
            "text": "Woge"
        },
        "line.16": {
            "text": "liebte"
        },
        "line.17": {
            "text": "alle Not"
        },
        "line.18": {
            "text": "es verlor"
        },
        "line.19": {
            "text": "jeder Sinn"
        },
        "line.20": {
            "text": "sein Gebot"
        }
    }
}