{
    "title": "klare-konturen-das-licht-1345",
    "gender": "m",
    "author": "Nico Bleutge",
    "author_birth": "1972",
    "poem": {
        "line.1": {
            "text": "klare konturen. das licht"
        },
        "line.2": {
            "text": "lag hinter den booten, der hafen"
        },
        "line.3": {
            "text": "hielt die dämmerung fest und die hügel"
        },
        "line.4": {
            "text": "traten am anderen ufer auf-"
        },
        "line.5": {
            "text": "gefaltet vors auge"
        }
    }
}