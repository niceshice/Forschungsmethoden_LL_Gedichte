{
    "title": "das-schwarzbuch-die-farbfotos-x-die-zeche-war-ein-altes-mass-12493",
    "gender": "m",
    "author": "Christoph  Wenzel",
    "author_birth": "1979",
    "poem": {
        "line.1": {
            "text": "X"
        },
        "line.2": {
            "text": "DIE ZECHE WAR EIN ALTES MASS und jetzt"
        },
        "line.3": {
            "text": "die rechnung bitte  wir sind abgebrannt:"
        },
        "line.4": {
            "text": "auf halde nur das katzengold und gruben-"
        },
        "line.5": {
            "text": "socken so"
        },
        "line.6": {
            "text": "manch guten kameraden deckt"
        },
        "line.7": {
            "text": "schon längst der grüne rasen"
        },
        "line.8": {
            "text": "oder diese"
        },
        "line.9": {
            "text": "wolkendecke matte wetter auch bei feier-"
        },
        "line.10": {
            "text": "schichten ein würfelspiel am kneipentisch"
        },
        "line.11": {
            "text": "und draußen orgelt ein kadett und springt"
        },
        "line.12": {
            "text": "und springt und springt nicht an"
        }
    }
}