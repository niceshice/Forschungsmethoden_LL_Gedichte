{
    "title": "schliere-164",
    "gender": "m",
    "author": "Paul Celan",
    "author_birth": "1920",
    "poem": {
        "line.1": {
            "text": "Schliere im Aug:"
        },
        "line.2": {
            "text": "von den Blicken auf halbem"
        },
        "line.3": {
            "text": "Weg erschautes Verloren."
        },
        "line.4": {
            "text": "Wirklichgesponnenes Niemals,"
        },
        "line.5": {
            "text": "wiedergekehrt."
        },
        "line.6": {
            "text": "Wege, halb  und die längsten."
        },
        "line.7": {
            "text": "Seelenbeschrittene Fäden,"
        },
        "line.8": {
            "text": "Glasspur,"
        },
        "line.9": {
            "text": "rückwärtsgerollt"
        },
        "line.10": {
            "text": "und nun"
        },
        "line.11": {
            "text": "vom Augen-Du auf dem steten"
        },
        "line.12": {
            "text": "Stern über dir"
        },
        "line.13": {
            "text": "weiß überschleiert."
        },
        "line.14": {
            "text": "Schliere im Aug:"
        },
        "line.15": {
            "text": "daß bewahrt sei"
        },
        "line.16": {
            "text": "ein durchs Dunkel getragenes Zeichen,"
        },
        "line.17": {
            "text": "vom Sand (oder Eis?) einer fremden"
        },
        "line.18": {
            "text": "Zeit für ein fremderes Immer"
        },
        "line.19": {
            "text": "belebt und als stumm"
        },
        "line.20": {
            "text": "vibrierender Mitlaut gestimmt."
        }
    }
}