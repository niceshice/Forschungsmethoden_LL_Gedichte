{
    "title": "salzburg-auf-dem-moenchsberg-stehend-7828",
    "gender": "m",
    "author": "Reiner Kunze",
    "author_birth": "1933",
    "poem": {
        "line.1": {
            "text": "Nach ankunft im Westen Europas"
        },
        "line.2": {
            "text": "Wiederzukehren"
        },
        "line.3": {
            "text": "hierher, können von nun an mich hindern"
        },
        "line.4": {
            "text": "armut nur, krankheit"
        },
        "line.5": {
            "text": "und tod"
        },
        "line.6": {
            "text": "Im kupferlaub der dächer geht der blick"
        },
        "line.7": {
            "text": "den abend ab"
        },
        "line.8": {
            "text": "Heimat haben und welt,"
        },
        "line.9": {
            "text": "und nie mehr der lüge"
        },
        "line.10": {
            "text": "den ring küssen müssen"
        }
    }
}