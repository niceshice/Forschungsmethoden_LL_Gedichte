{
    "title": "mich-deprimiert-das-gefuehl-11481",
    "gender": "m",
    "author": "Franz Dodel",
    "author_birth": "1949",
    "poem": {
        "line.1": {
            "text": "mich deprimiert das Gefühl"
        },
        "line.2": {
            "text": "das Eigentliche"
        },
        "line.3": {
            "text": "halte sich in einer stets"
        },
        "line.4": {
            "text": "gleichen Entfernung"
        },
        "line.5": {
            "text": "eine unbestimmte Art"
        },
        "line.6": {
            "text": "Zuständigkeit nimmt"
        },
        "line.7": {
            "text": "verlässlich Abstand von mir"
        },
        "line.8": {
            "text": "doch ich fühle mich"
        },
        "line.9": {
            "text": "den Unzulänglichkeiten"
        },
        "line.10": {
            "text": "jeder Art ohne"
        },
        "line.11": {
            "text": "je zu begreifen warum"
        },
        "line.12": {
            "text": "dankbar verbunden"
        },
        "line.13": {
            "text": "nie werde ich wissen was"
        },
        "line.14": {
            "text": "dieses Etwas ist"
        },
        "line.15": {
            "text": "das mich anpeilt und durchdringt"
        },
        "line.16": {
            "text": "und das mich als ein"
        },
        "line.17": {
            "text": "Anderes wieder verlässt"
        }
    }
}