{
    "title": "vom-gehoer-12823",
    "gender": "m",
    "author": "Rainer René Mueller",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "vom Gehör her, zu-"
        },
        "line.2": {
            "text": "handen : ein Exil :"
        },
        "line.3": {
            "text": "im Eisen am Halleschen Tor"
        },
        "line.4": {
            "text": "schleift sich das ab"
        },
        "line.5": {
            "text": "von Gesicht zu Gesicht"
        },
        "line.6": {
            "text": "ein Drecksgurren am Kanal"
        },
        "line.7": {
            "text": "setzt sich als Hitze fest:"
        },
        "line.8": {
            "text": "jagt's ihn\""
        },
        "line.9": {
            "text": "vor's Hirn schlägt Wasser"
        },
        "line.10": {
            "text": "eh einer zugrund' geht"
        },
        "line.11": {
            "text": "vom Hören / Sagen hergezerrt"
        },
        "line.12": {
            "text": ": dass ein Gruß sei"
        },
        "line.13": {
            "text": "dass, wenn die Olschliere"
        },
        "line.14": {
            "text": "die Stille verlädt, ein Lärm"
        },
        "line.15": {
            "text": "oben über's Gleis fährt,"
        },
        "line.16": {
            "text": "ein Gesicht"
        },
        "line.17": {
            "text": "abschleift. Ein Drecksgurren am"
        },
        "line.18": {
            "text": "Eisen, vom Gehör her :"
        },
        "line.19": {
            "text": "zu Schanden /"
        },
        "line.20": {
            "text": "als bliebe, ..."
        }
    }
}