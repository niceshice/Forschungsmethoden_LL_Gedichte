{
    "title": "prag-14918",
    "gender": "m",
    "author": "Marcus Roloff",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "märztag, herbsttag"
        },
        "line.2": {
            "text": "pfützenscherben im gesprungenen"
        },
        "line.3": {
            "text": "straßenpflaster, am wenzelsplatz"
        },
        "line.4": {
            "text": "gerinnen die reflexe meines leeren atems"
        },
        "line.5": {
            "text": "zu einer prachtpostkarte."
        },
        "line.6": {
            "text": "nichts bleibt hängen in der"
        },
        "line.7": {
            "text": "klumpreuse, schachtelglocke, ausgeworfener"
        },
        "line.8": {
            "text": "sehvorgang (abgebrochen), ich"
        },
        "line.9": {
            "text": "kann das verweilen nicht halten."
        },
        "line.10": {
            "text": "von türmen schreib ich nichts, von einem"
        },
        "line.11": {
            "text": "punkt in der zeit, der LUCERNA-"
        },
        "line.12": {
            "text": "PASSAGE z.b. (im kino"
        },
        "line.13": {
            "text": "gewesen, geweint), der"
        },
        "line.14": {
            "text": "sich summiert. viele. viele grüße."
        }
    }
}