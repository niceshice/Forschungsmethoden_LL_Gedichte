{
    "title": "nach-shikoku-12702",
    "gender": "m",
    "author": "Jürgen Theobaldy",
    "author_birth": "1944",
    "poem": {
        "line.1": {
            "text": "Gegen Ende November"
        },
        "line.2": {
            "text": "schiebt sich die Fähre"
        },
        "line.3": {
            "text": "in die tiefer stehende Sonne."
        },
        "line.4": {
            "text": "Außer uns kaum Reisende."
        },
        "line.5": {
            "text": "Rucksack, mein Kopfkissenwort"
        },
        "line.6": {
            "text": "auf leeren Reisstrohmatten."
        },
        "line.7": {
            "text": "Kein Schnee, kein Silberhauch,"
        },
        "line.8": {
            "text": "im Tuckern der Motoren"
        },
        "line.9": {
            "text": "nicht ein Laut von Sturmgespenstern."
        },
        "line.10": {
            "text": "Ich hätte ihnen in die Augen geblickt,"
        },
        "line.11": {
            "text": "denn wenn, dann haben sie Augen."
        },
        "line.12": {
            "text": "Aber sein muss es nicht."
        },
        "line.13": {
            "text": "Die Fernsehschirme blind und stumm,"
        },
        "line.14": {
            "text": "als fehlte es an Strom."
        },
        "line.15": {
            "text": "Und das ist selten hier,"
        },
        "line.16": {
            "text": "wie Übermut beim Übersetzen."
        }
    }
}