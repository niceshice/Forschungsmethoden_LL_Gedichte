{
    "title": "er-tanzt-5-5413",
    "gender": "m",
    "author": "Hans Raimund",
    "author_birth": "1945",
    "poem": {
        "line.1": {
            "text": "KNOSPENDER FROST"
        },
        "line.2": {
            "text": "Auf der Stirndie"
        },
        "line.3": {
            "text": "Leise blutet"
        },
        "line.4": {
            "text": "Ein Bild"
        },
        "line.5": {
            "text": "Eines von vielen"
        },
        "line.6": {
            "text": "Mit andern gekreuzten"
        },
        "line.7": {
            "text": "zerfließendverästelnd"
        },
        "line.8": {
            "text": "An Rändernhinein"
        },
        "line.9": {
            "text": "In Tiefenwo es"
        },
        "line.10": {
            "text": "Atemverschlagend"
        },
        "line.11": {
            "text": "An nie ins Auge"
        },
        "line.12": {
            "text": "Gestochenen Stellen"
        },
        "line.13": {
            "text": "Sprießtdie Schichten"
        },
        "line.14": {
            "text": "Eises sprengend"
        },
        "line.15": {
            "text": "bevor es"
        },
        "line.16": {
            "text": "Welkvor dem Aufgang"
        },
        "line.17": {
            "text": "Der Blüteverblassend"
        },
        "line.18": {
            "text": "Sich wieder zurücknimmt"
        }
    }
}