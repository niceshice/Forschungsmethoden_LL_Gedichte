{
    "title": "schwoaze-maenna-3655",
    "gender": "m",
    "author": "Axel  Karner",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "schwoaze männa"
        },
        "line.2": {
            "text": "hobnd im föld draußn"
        },
        "line.3": {
            "text": "a loch gegrobn"
        },
        "line.4": {
            "text": "tiaf ausm bodn"
        },
        "line.5": {
            "text": "es lebn gsuacht"
        },
        "line.6": {
            "text": "mit de bloaßn finga"
        },
        "line.7": {
            "text": "de toatn"
        },
        "line.8": {
            "text": "aus da eadn"
        },
        "line.9": {
            "text": "gekrotzt"
        },
        "line.10": {
            "text": "und aufghänkt"
        },
        "line.11": {
            "text": "zan steabn"
        },
        "line.12": {
            "text": "- - - - -"
        },
        "line.13": {
            "text": "schwarze männer"
        },
        "line.14": {
            "text": "schwarze männer"
        },
        "line.15": {
            "text": "haben im feld draußen"
        },
        "line.16": {
            "text": "ein loch gegraben"
        },
        "line.17": {
            "text": "tief aus dem boden"
        },
        "line.18": {
            "text": "das leben gesucht"
        },
        "line.19": {
            "text": "mit den bloßen fingern"
        },
        "line.20": {
            "text": "die toten"
        },
        "line.21": {
            "text": "aus der erde"
        },
        "line.22": {
            "text": "gekratzt"
        },
        "line.23": {
            "text": "und aufgehängt"
        },
        "line.24": {
            "text": "zum sterben"
        }
    }
}