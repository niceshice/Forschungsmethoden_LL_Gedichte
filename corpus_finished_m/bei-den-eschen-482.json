{
    "title": "bei-den-eschen-482",
    "gender": "m",
    "author": "Günter Saalmann",
    "author_birth": "1936",
    "poem": {
        "line.1": {
            "text": "Bei den Eschen"
        },
        "line.2": {
            "text": "schrie das Häschen"
        },
        "line.3": {
            "text": "- es begoß sich dort das Näschen:"
        },
        "line.4": {
            "text": "Gebt den Fröschen"
        },
        "line.5": {
            "text": "niemals Höschen,"
        },
        "line.6": {
            "text": "eitlen Putz und Puderdöschen!"
        },
        "line.7": {
            "text": "Den Geräuschen"
        },
        "line.8": {
            "text": "lauscht das Mäuschen"
        },
        "line.9": {
            "text": "angsterfüllt, ganz aus dem Häuschen."
        },
        "line.10": {
            "text": "Bat das Gänschen"
        },
        "line.11": {
            "text": "es zum Tänzchen."
        },
        "line.12": {
            "text": "Niedlich, niedlich,"
        },
        "line.13": {
            "text": "schrien die Menschen."
        }
    }
}