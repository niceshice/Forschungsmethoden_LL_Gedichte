{
    "title": "signal-11683",
    "gender": "m",
    "author": "Franz Hohler",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "Auf der Fluh"
        },
        "line.2": {
            "text": "steht einer"
        },
        "line.3": {
            "text": "und schwingt"
        },
        "line.4": {
            "text": "eine Fackel"
        },
        "line.5": {
            "text": "hin und her"
        },
        "line.6": {
            "text": "dass die Nacht"
        },
        "line.7": {
            "text": "sich erhellt"
        },
        "line.8": {
            "text": "für flüchtige Zeit"
        },
        "line.9": {
            "text": "mit Funken von Rot."
        },
        "line.10": {
            "text": "Die Fackel bin ich."
        },
        "line.11": {
            "text": "Der sie schwingt"
        },
        "line.12": {
            "text": "ist der Tod."
        }
    }
}