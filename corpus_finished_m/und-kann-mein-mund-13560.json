{
    "title": "und-kann-mein-mund-13560",
    "gender": "m",
    "author": "Jörg Schieke",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "Und kann mein Mund"
        },
        "line.2": {
            "text": "dich schon nicht zeigen, in jenem"
        },
        "line.3": {
            "text": "wie auch immer, Stories weit"
        },
        "line.4": {
            "text": "verteilt, auf alle, die hier"
        },
        "line.5": {
            "text": "sitzen, unterschreiben, kirrn die Insekten"
        },
        "line.6": {
            "text": "mit dem Schummerton, Formationen"
        },
        "line.7": {
            "text": "von Sterbehaltungen, von Rots"
        },
        "line.8": {
            "text": "ins Violette spielt, die Köpfe"
        },
        "line.9": {
            "text": "rücken halbe Stunden weiter, viel Zucker"
        },
        "line.10": {
            "text": "wenig Milch, Licht, übertragen"
        },
        "line.11": {
            "text": "auf Küsten, Tänze, solche Leute"
        },
        "line.12": {
            "text": "- die neue ergeben; zeitlich"
        },
        "line.13": {
            "text": "begrenzte Küsten sind das."
        }
    }
}