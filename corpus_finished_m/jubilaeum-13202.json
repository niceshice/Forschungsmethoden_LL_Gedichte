{
    "title": "jubilaeum-13202",
    "gender": "m",
    "author": "Max Czollek",
    "author_birth": "1987",
    "poem": {
        "line.1": {
            "text": "durch meine wohnung"
        },
        "line.2": {
            "text": "wandern schatten"
        },
        "line.3": {
            "text": "trinken auf gestern"
        },
        "line.4": {
            "text": "hinter den wänden"
        },
        "line.5": {
            "text": "ruft es nach ihnen"
        },
        "line.6": {
            "text": "kann sein: schweigen"
        },
        "line.7": {
            "text": "oj, ich habe flaschen"
        },
        "line.8": {
            "text": "geleert für das meer"
        },
        "line.9": {
            "text": "morgen fahre ich hin"
        },
        "line.10": {
            "text": "bis dahin sterben"
        },
        "line.11": {
            "text": "meine väter in weiß"
        },
        "line.12": {
            "text": "sie werden das nicht verstehen"
        }
    }
}