{
    "title": "ciel-15725",
    "gender": "w",
    "author": "Marie T.  Martin",
    "author_birth": "2021",
    "poem": {
        "line.1": {
            "text": "Ist das der Himmel oder sind das"
        },
        "line.2": {
            "text": "die Wellen, fädiges Flimmern, die"
        },
        "line.3": {
            "text": "Gischt, die dich an Land bringt,"
        },
        "line.4": {
            "text": "birgt dich eine Höhlung oder holt"
        },
        "line.5": {
            "text": "dich ein loses Flattern?"
        },
        "line.6": {
            "text": "Ist es ein Gewebe, eine bewegte"
        },
        "line.7": {
            "text": "Ebene aus Wasser und Licht oder"
        },
        "line.8": {
            "text": "atmende Härchen, durch die etwas"
        },
        "line.9": {
            "text": "strömt von Zelle zu Zelle. Ist es"
        },
        "line.10": {
            "text": "dein Erleben oder das Sehen von"
        },
        "line.11": {
            "text": "dir Selbst mit inneren Augen?"
        },
        "line.12": {
            "text": "Dringen diese Poren in offene"
        },
        "line.13": {
            "text": "Wunden, durchsiebt dich durch"
        },
        "line.14": {
            "text": "die Ohren ein Sirren von Verlorenem?"
        },
        "line.15": {
            "text": "Ist das Jetzt jetzt?"
        },
        "line.16": {
            "text": "Nach einer Arbeit von Georgia Russell"
        }
    }
}