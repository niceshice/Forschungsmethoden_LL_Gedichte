{
    "title": "host-ma-3654",
    "gender": "m",
    "author": "Axel  Karner",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "host ma"
        },
        "line.2": {
            "text": "de augn vadraht"
        },
        "line.3": {
            "text": "de sun vabotn"
        },
        "line.4": {
            "text": "vatrocknet da mund"
        },
        "line.5": {
            "text": "lei es schrein"
        },
        "line.6": {
            "text": "auf da tenn"
        },
        "line.7": {
            "text": "a ongnoglts kind"
        },
        "line.8": {
            "text": "dos de ondan"
        },
        "line.9": {
            "text": "vatreibt"
        },
        "line.10": {
            "text": "- - - - -"
        },
        "line.11": {
            "text": "hast mir"
        },
        "line.12": {
            "text": "hast mir"
        },
        "line.13": {
            "text": "die augen verdreht"
        },
        "line.14": {
            "text": "die sonne verboten"
        },
        "line.15": {
            "text": "vertrocknet den mund"
        },
        "line.16": {
            "text": "nur das schreien"
        },
        "line.17": {
            "text": "auf der tenne"
        },
        "line.18": {
            "text": "ein angenageltes kind"
        },
        "line.19": {
            "text": "das die anderen"
        },
        "line.20": {
            "text": "vertreibt"
        }
    }
}