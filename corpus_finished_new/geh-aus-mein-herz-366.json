{
    "title": "geh-aus-mein-herz-366",
    "gender": "m",
    "author": "Paul Wühr",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "GEH AUS MEIN HERZ und"
        },
        "line.2": {
            "text": "suche Freud schön ist"
        },
        "line.3": {
            "text": "die Welt die mir"
        },
        "line.4": {
            "text": "von dannen ziehet"
        },
        "line.5": {
            "text": "das muß ein Schlächter"
        },
        "line.6": {
            "text": "sein die Tier die seh"
        },
        "line.7": {
            "text": "ich springen vom Mittag"
        },
        "line.8": {
            "text": "weht es lau"
        },
        "line.9": {
            "text": "vom Blute haben wirs"
        },
        "line.10": {
            "text": "gelernt das hat nicht"
        },
        "line.11": {
            "text": "Rast noch Ruh wer hat"
        },
        "line.12": {
            "text": "gezählet des Müllers"
        },
        "line.13": {
            "text": "Lust"
        },
        "line.14": {
            "text": "daß ihm keine fehlet"
        }
    }
}