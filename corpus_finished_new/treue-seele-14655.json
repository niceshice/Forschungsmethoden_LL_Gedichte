    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "es ist november deine sprache"
        },
        "line.2": {
            "text": "auf meiner verdrossenen so klar"
        },
        "line.3": {
            "text": "das angebot des flusses sicht bis"
        },
        "line.4": {
            "text": "auf den grund unter irrendem"
        },
        "line.5": {
            "text": "wasser jeder stein dem andern"
        },
        "line.6": {
            "text": "naher freund in kalter zeit"
        }
    }
}