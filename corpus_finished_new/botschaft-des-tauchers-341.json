{
    "title": "botschaft-des-tauchers-341",
    "gender": "m",
    "author": "Hans Magnus Enzensberger",
    "author_birth": "1929",
    "poem": {
        "line.1": {
            "text": "unter der silbernen glocke, hangend"
        },
        "line.2": {
            "text": "im tang, die maske vor dem gesicht,"
        },
        "line.3": {
            "text": "den elektrischen rüssel, angehalftert"
        },
        "line.4": {
            "text": "an runzligen nabelschnüren, taumelnd"
        },
        "line.5": {
            "text": "im milchigen abgrund, allein"
        },
        "line.6": {
            "text": "mit seinem herzen wie ein prophet,"
        },
        "line.7": {
            "text": "allein mit seinem schweiß im gegurgel:"
        },
        "line.8": {
            "text": "oben im licht macht die jury den tod"
        },
        "line.9": {
            "text": "und den kultursalat an, jupiter"
        },
        "line.10": {
            "text": "verkauft sich unter den lampen,"
        },
        "line.11": {
            "text": "es jubeln posaunenchöre, überall"
        },
        "line.12": {
            "text": "pfeifen die schiedsrichter schon"
        },
        "line.13": {
            "text": "zum letzten elfmeter, die sender"
        },
        "line.14": {
            "text": "morsen bullen zum harakiri:"
        },
        "line.15": {
            "text": "unten im dunkel in seiner rüstung,"
        },
        "line.16": {
            "text": "in seiner brennenden reuse schwebt,"
        },
        "line.17": {
            "text": "in der dünung der algen,"
        },
        "line.18": {
            "text": "saumselig, in seinen sielen"
        },
        "line.19": {
            "text": "aus kupfer und gummi, blind,"
        },
        "line.20": {
            "text": "der ruhmlose taucher, und ruft"
        },
        "line.21": {
            "text": "in das rauschende sprechgeschirr:"
        },
        "line.22": {
            "text": "CQ CQ an alle! an alle!"
        },
        "line.23": {
            "text": "ich bin auf dem grund allein,"
        },
        "line.24": {
            "text": "wo niemand recht hat von uns"
        },
        "line.25": {
            "text": "und von euch, vernäht in mein ende:"
        },
        "line.26": {
            "text": "die stumme muschel hat recht"
        },
        "line.27": {
            "text": "und der herrliche hummer allein,"
        },
        "line.28": {
            "text": "recht hat der sinnreiche seestern."
        },
        "line.29": {
            "text": "ich wiederhole: laßt ab,"
        },
        "line.30": {
            "text": "laßt ab von uns und von euch"
        },
        "line.31": {
            "text": "und von mir!"
        },
        "line.32": {
            "text": "kurz-kurz-kurz"
        },
        "line.33": {
            "text": "lang"
        },
        "line.34": {
            "text": "kurz-lang"
        }
    }
}