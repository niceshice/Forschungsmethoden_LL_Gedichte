{
    "title": "israel-550",
    "gender": "w",
    "author": "Rose Ausländer",
    "author_birth": "1901",
    "poem": {
        "line.1": {
            "text": "Zurück"
        },
        "line.2": {
            "text": "ins zukünftige"
        },
        "line.3": {
            "text": "Meinland Deinland"
        },
        "line.4": {
            "text": "Hier"
        },
        "line.5": {
            "text": "heißt der Stein"
        },
        "line.6": {
            "text": "Zeder Zitrone"
        },
        "line.7": {
            "text": "Unvergeßlich"
        },
        "line.8": {
            "text": "die stählernen Brüder"
        },
        "line.9": {
            "text": "vergaßen den Schlaf"
        },
        "line.10": {
            "text": "Nicht ins Schlaraffenland"
        },
        "line.11": {
            "text": "komm"
        },
        "line.12": {
            "text": "ins stachlige Hier"
        },
        "line.13": {
            "text": "Auf rebellischem Boden"
        },
        "line.14": {
            "text": "verläßlich die Hüter"
        },
        "line.15": {
            "text": "pflanzen"
        },
        "line.16": {
            "text": "beständigen Traum"
        },
        "line.17": {
            "text": "Komm"
        },
        "line.18": {
            "text": "ins Zurück"
        },
        "line.19": {
            "text": "die Stacheln grünen"
        },
        "line.20": {
            "text": "Saft"
        },
        "line.21": {
            "text": "aus dem Stein"
        },
        "line.22": {
            "text": "schlägt der"
        },
        "line.23": {
            "text": "Mosessohn"
        }
    }
}