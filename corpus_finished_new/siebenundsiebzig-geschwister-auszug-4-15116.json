{
    "title": "siebenundsiebzig-geschwister-auszug-4-15116",
    "gender": "w",
    "author": "Zsuzsanna Gahse",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Lalalala, tralala,"
        },
        "line.2": {
            "text": "sagt mein Bruder."
        },
        "line.3": {
            "text": "A che bell vivere."
        },
        "line.4": {
            "text": "Er hört mir nicht zu,"
        },
        "line.5": {
            "text": "hört nicht mehr zu."
        },
        "line.6": {
            "text": "Schau dich mal um,"
        },
        "line.7": {
            "text": "sagt er, schau dich"
        },
        "line.8": {
            "text": "doch einfach um!"
        },
        "line.9": {
            "text": "Am Ende gab es"
        },
        "line.10": {
            "text": "Geschwister schon"
        },
        "line.11": {
            "text": "vor dem Urknall."
        },
        "line.12": {
            "text": "Am Anfang war, noch"
        },
        "line.13": {
            "text": "vor dem Urknall, das"
        },
        "line.14": {
            "text": "Ei oder sogar zwei Eier,"
        },
        "line.15": {
            "text": "Eines warf das andere"
        },
        "line.16": {
            "text": "aus dem Nest, aber wo"
        },
        "line.17": {
            "text": "sich jetzt das gefallene"
        },
        "line.18": {
            "text": "Ei befindet, ist unklar."
        }
    }
}