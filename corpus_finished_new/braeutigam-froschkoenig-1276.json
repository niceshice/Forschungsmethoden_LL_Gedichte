{
    "title": "braeutigam-froschkoenig-1276",
    "gender": "w",
    "author": "Marie Luise Kaschnitz",
    "author_birth": "1901",
    "poem": {
        "line.1": {
            "text": "Wie häßlich ist"
        },
        "line.2": {
            "text": "Dein Bräutigam"
        },
        "line.3": {
            "text": "Jungfrau Leben"
        },
        "line.4": {
            "text": "Eine Rüsselmaske sein Antlitz"
        },
        "line.5": {
            "text": "Eine Patronentasche sein Gürtel"
        },
        "line.6": {
            "text": "Ein Flammenwerfer"
        },
        "line.7": {
            "text": "Seine Hand"
        },
        "line.8": {
            "text": "Dein Bräutigam Froschkönig"
        },
        "line.9": {
            "text": "Fährt mit Dir"
        },
        "line.10": {
            "text": "(Ein Rad fleigt hierhin, eins dorthin)"
        },
        "line.11": {
            "text": "Über die Häuser der Toten"
        },
        "line.12": {
            "text": "Zwischen zwei"
        },
        "line.13": {
            "text": "Weltuntergängen"
        },
        "line.14": {
            "text": "Preßt er sich"
        },
        "line.15": {
            "text": "In Deinen Schoß"
        },
        "line.16": {
            "text": "Im Dunkeln nur"
        },
        "line.17": {
            "text": "Ertastest Du"
        },
        "line.18": {
            "text": "Sein feuchtes Haar"
        },
        "line.19": {
            "text": "Im Morgengrauen"
        },
        "line.20": {
            "text": "Nur im"
        },
        "line.21": {
            "text": "Morgengrauen"
        },
        "line.22": {
            "text": "Nur im"
        },
        "line.23": {
            "text": "Erblickst Du seine"
        },
        "line.24": {
            "text": "Traurigen"
        },
        "line.25": {
            "text": "Schönen"
        },
        "line.26": {
            "text": "Augen."
        }
    }
}