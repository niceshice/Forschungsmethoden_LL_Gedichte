{
    "title": "mouches-volantes-14141",
    "gender": "m",
    "author": "Nicolai Kobus",
    "author_birth": "1968",
    "poem": {
        "line.1": {
            "text": "mouches volantes stets blut"
        },
        "line.2": {
            "text": "am lidrand einfärbung im eiklar"
        },
        "line.3": {
            "text": "vorzugsweise vor blanken wänden"
        },
        "line.4": {
            "text": "treiben flimmertierchen durch den"
        },
        "line.5": {
            "text": "kahlen vers den einen im blinzeln"
        },
        "line.6": {
            "text": "getrübten clair das weiße im auge"
        },
        "line.7": {
            "text": "näher kommst du dir nie"
        }
    }
}