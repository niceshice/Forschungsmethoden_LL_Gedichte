{
    "title": "ueberraschungsgast-1526",
    "gender": "w",
    "author": "Elfriede Gerstl",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "das gedicht kommt"
        },
        "line.2": {
            "text": "wie eine katze"
        },
        "line.3": {
            "text": "rufen kannst es nicht"
        },
        "line.4": {
            "text": "folgsam folge seiner gangart"
        },
        "line.5": {
            "text": "(wer weiss wann es dich"
        },
        "line.6": {
            "text": "wieder besucht)"
        },
        "line.7": {
            "text": "ob du's hirnzauberei nennst"
        },
        "line.8": {
            "text": "oder spiritueller moment"
        },
        "line.9": {
            "text": "ein anlass zum staunen"
        },
        "line.10": {
            "text": "5. j√§nner 2003"
        }
    }
}