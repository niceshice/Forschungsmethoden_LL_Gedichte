{
    "title": "kennt-ich-die-wurzel-14547",
    "gender": "w",
    "author": "Ruth Wolf-Rehfeldt",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "Kennt ich die Wurzel"
        },
        "line.2": {
            "text": "Und diese von innen"
        },
        "line.3": {
            "text": "Wäre ich bei mir"
        },
        "line.4": {
            "text": "Säße ich drinnen"
        },
        "line.5": {
            "text": "So such ich draußen"
        },
        "line.6": {
            "text": "Fremd und von Sinnen"
        }
    }
}