{
    "title": "wulkan-1005",
    "gender": "w",
    "author": "Ulrike Draesner",
    "author_birth": "1962",
    "poem": {
        "line.1": {
            "text": "weißt du wie es ist wenn man"
        },
        "line.2": {
            "text": "schleudert (das fallende laub) fragte er"
        },
        "line.3": {
            "text": "mit weicher polnischer ruhe:"
        },
        "line.4": {
            "text": "wulkan."
        },
        "line.5": {
            "text": "die lage der kanten das porzellan"
        },
        "line.6": {
            "text": "sagte er alle gegenstände des hauses"
        },
        "line.7": {
            "text": "erinnerten an eigene bedürfnisse: fotos"
        },
        "line.8": {
            "text": "von kindstaufen eine blau träumende"
        },
        "line.9": {
            "text": "kommode mit stolzierendem emaillepfau"
        },
        "line.10": {
            "text": "der wein vom hochzeitsjahr"
        },
        "line.11": {
            "text": "der anderen."
        },
        "line.12": {
            "text": "noch warm ihre deutschen lippen"
        },
        "line.13": {
            "text": "noch auf den gläsern"
        },
        "line.14": {
            "text": "der spüle. wir schämten"
        },
        "line.15": {
            "text": "uns nicht des nehmens"
        },
        "line.16": {
            "text": "des sehens wohl. so"
        },
        "line.17": {
            "text": "kamen wir"
        },
        "line.18": {
            "text": "nicht an."
        },
        "line.19": {
            "text": "wulkan."
        },
        "line.20": {
            "text": "lebensbild"
        },
        "line.21": {
            "text": "hergebracht. gemälde"
        },
        "line.22": {
            "text": "aller schatten an der wand"
        },
        "line.23": {
            "text": "schichten aus mensch gestapelt"
        },
        "line.24": {
            "text": "fruchtbar heiß, erstarrt. im eigenen"
        },
        "line.25": {
            "text": "dreißig jahre auf gepackten koffern"
        },
        "line.26": {
            "text": "gesessen: was fliehen"
        },
        "line.27": {
            "text": "in geflohenes"
        },
        "line.28": {
            "text": "heißt."
        },
        "line.29": {
            "text": "zersägten"
        },
        "line.30": {
            "text": "das bett die anderen"
        },
        "line.31": {
            "text": "um zu heizen den einen geretteten"
        },
        "line.32": {
            "text": "sack das holzbein des toten sohns."
        },
        "line.33": {
            "text": "ich kann ihre hände sehen vater"
        },
        "line.34": {
            "text": "großvater großmutter ihre nägel"
        },
        "line.35": {
            "text": "sie hatten nicht alles verloren"
        },
        "line.36": {
            "text": "fast noch alle teile des körpers"
        },
        "line.37": {
            "text": "bei sich noch etwas seele"
        },
        "line.38": {
            "text": "vielleicht"
        },
        "line.39": {
            "text": "man legt sich nieder und liebt sich"
        },
        "line.40": {
            "text": "in europäischem gras. ein turm"
        },
        "line.41": {
            "text": "ragt auf, aus eisen und rekonstruktion"
        },
        "line.42": {
            "text": "das ist normal. eine straßenbahn"
        },
        "line.43": {
            "text": "fährt. das herz wulkan"
        },
        "line.44": {
            "text": "weich gegen die wände"
        },
        "line.45": {
            "text": "huft in einem brüchigen"
        },
        "line.46": {
            "text": "polnischen stall."
        }
    }
}