{
    "title": "bin-troedeliges-heute-huehnerfleisch-2877",
    "gender": "w",
    "author": "Elisabeth  Wandeler-Deck",
    "author_birth": "1939",
    "poem": {
        "line.1": {
            "text": "(bin trödeliges heute Hühnerfleisch)"
        },
        "line.2": {
            "text": "Was solls."
        },
        "line.3": {
            "text": "Es gibt."
        },
        "line.4": {
            "text": "Bitte warten."
        },
        "line.5": {
            "text": "Stopp jetzt. Ich will mich ja ausruhen von gestrig presslaubigem Abend. Achso."
        },
        "line.6": {
            "text": "Tropft greift."
        },
        "line.7": {
            "text": "Nein, nur kein Monument herstellen. Ich möchte von niemandem grimmig üble Rede führen."
        },
        "line.8": {
            "text": "Ohh."
        },
        "line.9": {
            "text": "Bin ja neu hier."
        },
        "line.10": {
            "text": "Kippt schiebt."
        },
        "line.11": {
            "text": "Wuchtet."
        },
        "line.12": {
            "text": "(Aber ich könnte. )"
        },
        "line.13": {
            "text": "(Zwischen abbrechend grünen Stillen und lang bröckelnden Geranien.)"
        },
        "line.14": {
            "text": "(Das ist vom kunterbunt gestreckten Singen nicht wahrer geworden. )"
        },
        "line.15": {
            "text": "Tropft stopft!"
        },
        "line.16": {
            "text": "Nimmt nippt."
        },
        "line.17": {
            "text": "Ich lasse das buntscheckig gewieselte Auge fallen."
        }
    }
}