{
    "title": "tierleben-8-4017",
    "gender": "m",
    "author": "Felix Philipp Ingold",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "8"
        },
        "line.2": {
            "text": "Die Ufer sind"
        },
        "line.3": {
            "text": "mehr als die Flüsse. Auch"
        },
        "line.4": {
            "text": "ist das Nu  steht's"
        },
        "line.5": {
            "text": "im Begriff  dem Unicorn"
        },
        "line.6": {
            "text": "numerisch unterlegen."
        },
        "line.7": {
            "text": "Wer aber wäre"
        },
        "line.8": {
            "text": "wessen Wappentier. Das"
        },
        "line.9": {
            "text": "ewig stampft und"
        },
        "line.10": {
            "text": "harzt wo immer noch"
        },
        "line.11": {
            "text": "die Sonne wurzelt. Nur"
        },
        "line.12": {
            "text": "wozu. Lebendige"
        },
        "line.13": {
            "text": "Zahlen jedenfalls verspeist"
        },
        "line.14": {
            "text": "das Vieh wie"
        },
        "line.15": {
            "text": "nichts. Im Eis verscharrt's"
        },
        "line.16": {
            "text": "(Zitat:) sein Ei."
        },
        "line.17": {
            "text": "(\"Die Ufer stehn öfter"
        },
        "line.18": {
            "text": "im Begriff als"
        },
        "line.19": {
            "text": "die Flüsse. Numerisch"
        },
        "line.20": {
            "text": "ist das Nu"
        },
        "line.21": {
            "text": "der Summe unterlegen."
        },
        "line.22": {
            "text": "Wie das Unicorn"
        },
        "line.23": {
            "text": "das stampft und schnieft"
        },
        "line.24": {
            "text": "und die Zahlen"
        },
        "line.25": {
            "text": "lebendig verscharrt"
        },
        "line.26": {
            "text": "dem Wappentier. Im Muster"
        },
        "line.27": {
            "text": "der Tapete harrt's.\")"
        }
    }
}