{
    "title": "zb-bananen-761",
    "gender": "m",
    "author": "Maik Lippert",
    "author_birth": "1966",
    "poem": {
        "line.1": {
            "text": "esse ich noch immer"
        },
        "line.2": {
            "text": "am liebsten überreif"
        },
        "line.3": {
            "text": "wie damals"
        },
        "line.4": {
            "text": "fruchtkörper"
        },
        "line.5": {
            "text": "mit geborstenen schalen"
        },
        "line.6": {
            "text": "gestrandete boote"
        },
        "line.7": {
            "text": "am boden der nylontasche"
        },
        "line.8": {
            "text": "mutter noch in der schürze der verkaufsstellenleiterin"
        },
        "line.9": {
            "text": "gestärkte kragenflügel"
        },
        "line.10": {
            "text": "im küchenhimmel"
        },
        "line.11": {
            "text": "die essenz"
        },
        "line.12": {
            "text": "mein ambrosius ein leicht flüchtiges ester"
        },
        "line.13": {
            "text": "isobutylazetat"
        },
        "line.14": {
            "text": "in jedem chemiebuch nachzulesen"
        },
        "line.15": {
            "text": "und doch vergaß und vergesse ich mich noch heute"
        },
        "line.16": {
            "text": "beim lösen der schalen"
        },
        "line.17": {
            "text": "mit jedem biß ins mark"
        },
        "line.18": {
            "text": "und ich gestehe"
        },
        "line.19": {
            "text": "nicht an juanita gedacht zu haben"
        },
        "line.20": {
            "text": "nicht an ihren vater miguel"
        },
        "line.21": {
            "text": "nicht an jaime julio und atahualpa"
        },
        "line.22": {
            "text": "ich weiß deren schule"
        },
        "line.23": {
            "text": "sind geschwollene hände"
        },
        "line.24": {
            "text": "und das modernde alphabet der stauden"
        },
        "line.25": {
            "text": "und ich esse noch immer bananen"
        },
        "line.26": {
            "text": "am liebsten überreif"
        },
        "line.27": {
            "text": "in der küche sitzt mutter"
        },
        "line.28": {
            "text": "die flügel am kragen sind müde"
        },
        "line.29": {
            "text": "wenn du abreist"
        },
        "line.30": {
            "text": "vergiß die bananen nicht"
        },
        "line.31": {
            "text": "und ich kaue gewissenlos"
        },
        "line.32": {
            "text": "im gebiß tastet die zunge"
        },
        "line.33": {
            "text": "vergeblich nach sehnsucht"
        },
        "line.34": {
            "text": "zur buße"
        }
    }
}