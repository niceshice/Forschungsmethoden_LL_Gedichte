{
    "title": "wer-haette-das-gedacht-280",
    "gender": "w",
    "author": "Orsolya Kalász",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "Hier steht dem Anfang sein Ende bevor"
        },
        "line.2": {
            "text": "das sich verhindern möchte und"
        },
        "line.3": {
            "text": "nach draußen, zu den Kettenblumen flüchten,"
        },
        "line.4": {
            "text": "den Kettenblumen,"
        },
        "line.5": {
            "text": "die die Sprache absperren."
        },
        "line.6": {
            "text": "Es bleibt mir überlassen"
        },
        "line.7": {
            "text": "auch ihn zu verlassen"
        },
        "line.8": {
            "text": "mir bleibt es überlassen,"
        },
        "line.9": {
            "text": "mich auch ihm zu überlassen"
        },
        "line.10": {
            "text": "in Kettenblumen."
        },
        "line.11": {
            "text": "An der Schwelle"
        },
        "line.12": {
            "text": "die ich dachte,"
        },
        "line.13": {
            "text": "einen Anfang"
        },
        "line.14": {
            "text": "mit dem Ende machen."
        },
        "line.15": {
            "text": "Und alles, was sprießt"
        },
        "line.16": {
            "text": "aus den Spalten"
        },
        "line.17": {
            "text": "Rissen und Trichtern,"
        },
        "line.18": {
            "text": "aus den Kissen"
        },
        "line.19": {
            "text": "auffliegt und siegt"
        },
        "line.20": {
            "text": "alles was wird"
        },
        "line.21": {
            "text": "will seinen Strauch."
        },
        "line.22": {
            "text": "Deutsche Fassung von Orsolya Kalsz"
        }
    }
}