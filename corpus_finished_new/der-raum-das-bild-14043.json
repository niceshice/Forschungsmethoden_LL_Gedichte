{
    "title": "der-raum-das-bild-14043",
    "gender": "w",
    "author": "Anja Bayer",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "der Raum, das Bild"
        },
        "line.2": {
            "text": "ich suche dich"
        },
        "line.3": {
            "text": "bleibt offen stehn"
        },
        "line.4": {
            "text": "lass dein Auge"
        },
        "line.5": {
            "text": "fallen"
        }
    }
}