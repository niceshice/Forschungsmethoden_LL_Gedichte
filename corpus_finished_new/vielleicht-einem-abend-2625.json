{
    "title": "vielleicht-einem-abend-2625",
    "gender": "m",
    "author": "Michael Donhauser",
    "author_birth": "1956",
    "poem": {
        "line.1": {
            "text": "Vielleicht an einem Abend, an"
        },
        "line.2": {
            "text": "einem Abend spät vielleicht"
        },
        "line.3": {
            "text": "Ein Glas gefüllt mit Anis und"
        },
        "line.4": {
            "text": "eine Stimme, die weint"
        },
        "line.5": {
            "text": "Vielleicht, daß eine Stimme"
        },
        "line.6": {
            "text": "weint"
        },
        "line.7": {
            "text": "Ein Glas an einem Abend spät"
        },
        "line.8": {
            "text": "vielleicht"
        },
        "line.9": {
            "text": "Ich gehe nicht, nicht mehr"
        },
        "line.10": {
            "text": "sehr weit"
        },
        "line.11": {
            "text": "Zu sehr, zu sehr, nicht mehr"
        },
        "line.12": {
            "text": "zu weit"
        }
    }
}