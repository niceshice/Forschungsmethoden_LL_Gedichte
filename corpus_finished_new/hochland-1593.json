{
    "title": "hochland-1593",
    "gender": "w",
    "author": "Erika Burkart",
    "author_birth": "1922",
    "poem": {
        "line.1": {
            "text": "In meinen Erinnerungen"
        },
        "line.2": {
            "text": "ist immer Winter,"
        },
        "line.3": {
            "text": "wir wohnen in einem Hochland,"
        },
        "line.4": {
            "text": "nachts fällt der Schnee,"
        },
        "line.5": {
            "text": "der mich zudeckt."
        },
        "line.6": {
            "text": "Ihr, meine Toten,"
        },
        "line.7": {
            "text": "mit eurem Anruf"
        },
        "line.8": {
            "text": "im Erdgeschoss, vor der Tür -"
        },
        "line.9": {
            "text": "Worten aus Briefen,"
        },
        "line.10": {
            "text": "Stimmen aus Büchern,"
        },
        "line.11": {
            "text": "euren traurigen"
        },
        "line.12": {
            "text": "und seligen Augen!"
        },
        "line.13": {
            "text": "Wie ihr durch mich hindurchschaut"
        },
        "line.14": {
            "text": "auf die, die ich war."
        }
    }
}