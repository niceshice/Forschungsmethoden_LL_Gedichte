{
    "title": "mut-6085",
    "gender": "m",
    "author": "Johannes Kühn",
    "author_birth": "1934",
    "poem": {
        "line.1": {
            "text": "Melden wieder viele Zeitungsseiten"
        },
        "line.2": {
            "text": "von Erfolgreichen,"
        },
        "line.3": {
            "text": "die gehn im Park des Lebens."
        },
        "line.4": {
            "text": "Ich sitze vor der Wildnis,"
        },
        "line.5": {
            "text": "einer dornenreichen,"
        },
        "line.6": {
            "text": "in die kein Weg führt."
        },
        "line.7": {
            "text": "Hier lärmt es spottend."
        },
        "line.8": {
            "text": "Die Axt ist stumpf"
        },
        "line.9": {
            "text": "und auch die Säge,"
        },
        "line.10": {
            "text": "ich klage,"
        },
        "line.11": {
            "text": "kein Adler, der mich hochzög"
        },
        "line.12": {
            "text": "an den Haaren"
        },
        "line.13": {
            "text": "und mich fortbrächt"
        },
        "line.14": {
            "text": "in ein andres Land,"
        },
        "line.15": {
            "text": "erscheint aus einer Höhe,"
        },
        "line.16": {
            "text": "keiner fliegt von einer Seite her."
        },
        "line.17": {
            "text": "Sage ja zu deiner Ödnis,"
        },
        "line.18": {
            "text": "kaure in dir selbst, ein Elendsbündel,"
        },
        "line.19": {
            "text": "übersteh die Woche!"
        }
    }
}