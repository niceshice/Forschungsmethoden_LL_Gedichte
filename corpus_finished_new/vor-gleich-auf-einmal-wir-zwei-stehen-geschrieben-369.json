    "title": "vor-gleich-auf-einmal-wir-zwei-stehen-geschrieben-369",
    "gender": "m",
    "author": "Paul WÃ¼hr",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "Vor"
        },
        "line.2": {
            "text": "Gleich auf einmal wir zwei"
        },
        "line.3": {
            "text": "stehen geschrieben"
        },
        "line.4": {
            "text": "so steht es"
        },
        "line.5": {
            "text": "wir spielen uns vor dem Leben"
        },
        "line.6": {
            "text": "vom Blatt"
        },
        "line.7": {
            "text": "so wird gespielt"
        },
        "line.8": {
            "text": "wo wir auch immer uns"
        },
        "line.9": {
            "text": "gleichen"
        },
        "line.10": {
            "text": "so ist es"
        },
        "line.11": {
            "text": "nur Trennung"
        },
        "line.12": {
            "text": "so ist es"
        },
        "line.13": {
            "text": "nichts anderes steht geschrieben"
        },
        "line.14": {
            "text": "so steht es"
        },
        "line.15": {
            "text": "so sind wir verschieden"
        }
    }
}