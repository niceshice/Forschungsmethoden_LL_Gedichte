{
    "title": "kabutzkes-liebeslied-1428",
    "gender": "m",
    "author": "Fredrik Vahle",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "Ach, wie schön,"
        },
        "line.2": {
            "text": "dass es mich gibt"
        },
        "line.3": {
            "text": "griesegrau vorm Himmelsblau."
        },
        "line.4": {
            "text": "Ach, wie schön, dass es mich gibt,"
        },
        "line.5": {
            "text": "drum bin ich"
        },
        "line.6": {
            "text": "in dich verliebt."
        },
        "line.7": {
            "text": "Ohne mich"
        },
        "line.8": {
            "text": "fehlt was für dich"
        },
        "line.9": {
            "text": "griesegrau vorm Himmelsblau."
        },
        "line.10": {
            "text": "Ach, wie schön,"
        },
        "line.11": {
            "text": "dass es dich gibt"
        },
        "line.12": {
            "text": "griesegrau vorm Himmelsblau."
        },
        "line.13": {
            "text": "Siehste wohl,"
        },
        "line.14": {
            "text": "ich hab dich lieb,"
        },
        "line.15": {
            "text": "piep."
        }
    }
}