{
    "title": "cambridge-den-291005-8389",
    "gender": "w",
    "author": "Barbara Köhler",
    "author_birth": "1959",
    "poem": {
        "line.1": {
            "text": "benannte dinge  am ende"
        },
        "line.2": {
            "text": "der allerseelengasse der"
        },
        "line.3": {
            "text": "friedhof zur himmelfahrt"
        },
        "line.4": {
            "text": "an einem sommertag gegen"
        },
        "line.5": {
            "text": "ende oktober autumn sagt"
        },
        "line.6": {
            "text": "man hier & anderswo fall"
        },
        "line.7": {
            "text": "in gleicher sprache eine"
        },
        "line.8": {
            "text": "welt dreht sich sagt man"
        },
        "line.9": {
            "text": "& turning leaves wie sie"
        },
        "line.10": {
            "text": "tanzen die blätter & das"
        },
        "line.11": {
            "text": "wetter umschlägt wie sie"
        },
        "line.12": {
            "text": "schweben fallen auffahrn"
        },
        "line.13": {
            "text": "mit dem wind: sie fallen"
        },
        "line.14": {
            "text": "sie falln sind alles was"
        },
        "line.15": {
            "text": "auffliegt laut gibt laub"
        },
        "line.16": {
            "text": "was wirbelt was flüstert"
        },
        "line.17": {
            "text": "sich wendet & dreht sich"
        },
        "line.18": {
            "text": "um blatt um blatt sprich"
        },
        "line.19": {
            "text": "ein name zwei zahlen ein"
        },
        "line.20": {
            "text": "bindestrich eine winzige"
        },
        "line.21": {
            "text": "leiter ins nichts an den"
        },
        "line.22": {
            "text": "himmel gelehnt in ranken"
        },
        "line.23": {
            "text": "in efeu verankert heiter"
        },
        "line.24": {
            "text": "gestellt & nichts weiter"
        },
        "line.25": {
            "text": "steht da der stein liegt"
        },
        "line.26": {
            "text": "versunken im gras als ob"
        },
        "line.27": {
            "text": "er & blühte die flechten"
        }
    }
}