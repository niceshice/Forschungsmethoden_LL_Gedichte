{
    "title": "du-bist-wie-moderne-lyrik-16567",
    "gender": "w",
    "author": "Daniela  Chana",
    "author_birth": "1985",
    "poem": {
        "line.1": {
            "text": "Du bist wie moderne Lyrik"
        },
        "line.2": {
            "text": "Ein bisschen hässlich, ein bisschen kalt"
        },
        "line.3": {
            "text": "Und ich versteh dich nicht"
        },
        "line.4": {
            "text": "Irgendwie hab ich dich trotzdem gern"
        },
        "line.5": {
            "text": "Du wiederholst dich oft"
        },
        "line.6": {
            "text": "Und vollendest deine Gedanken nicht"
        },
        "line.7": {
            "text": "Du wiederholst dich oft"
        },
        "line.8": {
            "text": "Und vollendest deine"
        },
        "line.9": {
            "text": "Deine Ideen springen wild umher"
        },
        "line.10": {
            "text": "Ich soll dich nicht verstehen"
        },
        "line.11": {
            "text": "Deine Ideen springen wild umher"
        },
        "line.12": {
            "text": "Du schälst Kartoffeln ordentlicher als ich"
        },
        "line.13": {
            "text": "Manchmal will ich, dass du schöner wärst"
        },
        "line.14": {
            "text": "Du sagst: Ich bin so durcheinander"
        },
        "line.15": {
            "text": "Durch und ein und ander und so"
        },
        "line.16": {
            "text": "Ich beobachte dich beim Kartoffelschälen"
        },
        "line.17": {
            "text": "Neben dem Herd in meiner Küche"
        },
        "line.18": {
            "text": "Und liebe das Alte in dir"
        },
        "line.19": {
            "text": "Egal wie du dich veränderst"
        },
        "line.20": {
            "text": "Du hast immer etwas von deinen Eltern"
        },
        "line.21": {
            "text": "Deine Arme wissen noch"
        },
        "line.22": {
            "text": "Wie man früher Wärme gab"
        }
    }
}