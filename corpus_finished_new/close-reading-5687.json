{
    "title": "close-reading-5687",
    "gender": "m",
    "author": "Thien Tran",
    "author_birth": "1979",
    "poem": {
        "line.1": {
            "text": "diese Linien im Kopf"
        },
        "line.2": {
            "text": "sind die Linien der Flucht"
        },
        "line.3": {
            "text": "keine Fluchtlinien. der Eingang"
        },
        "line.4": {
            "text": "nach innen ist die Oberfläche des Ausgangs"
        },
        "line.5": {
            "text": "und der Ausgang"
        },
        "line.6": {
            "text": "liegt innen. manchmal brechen sie"
        },
        "line.7": {
            "text": "manchmal landen sie. manchmal versinken sie"
        },
        "line.8": {
            "text": "das Hier-sein-Dort-sein ist das Hier-sein-Dort-sein"
        },
        "line.9": {
            "text": "der Linien. Wort an Wort"
        },
        "line.10": {
            "text": "Satz an Satz. die Zoommodulierfunktionen"
        },
        "line.11": {
            "text": "sind die Funktionen der Funktionen"
        },
        "line.12": {
            "text": "man denke an die Haut"
        },
        "line.13": {
            "text": "an die Einheit von Signifikant"
        },
        "line.14": {
            "text": "und Signifikat. Signifikantenströme und Free Time-"
        },
        "line.15": {
            "text": "Akrobaten untrennbar ineinander"
        },
        "line.16": {
            "text": "vermischt. unendlich klein also die Kugelformen"
        },
        "line.17": {
            "text": "dieser"
        },
        "line.18": {
            "text": "reality relation"
        },
        "line.19": {
            "text": ". Kugelschreiber"
        },
        "line.20": {
            "text": "und Bleistifte wie Metalle"
        },
        "line.21": {
            "text": "miteinander vereint. ein jedes für sich"
        },
        "line.22": {
            "text": "von seiner eigenen Biographie"
        },
        "line.23": {
            "text": "vorangetrieben."
        }
    }
}