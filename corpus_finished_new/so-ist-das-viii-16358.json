{
    "title": "so-ist-das-viii-16358",
    "gender": "w",
    "author": "Andrea Grill",
    "author_birth": "1975",
    "poem": {
        "line.1": {
            "text": "VIII."
        },
        "line.2": {
            "text": "den Tod halte ich bei keinem aus"
        },
        "line.3": {
            "text": "mit dem ich ein Geheimnis habe sagte sie sage ich"
        },
        "line.4": {
            "text": "Todeszeit des Helden: 17 Uhr 20 Minuten, Abflug"
        },
        "line.5": {
            "text": "das Atmen erwischte uns gedankenlos"
        },
        "line.6": {
            "text": "kann unsere Gesichter hinter"
        },
        "line.7": {
            "text": "zwei einfache Hände retten: blickverdeckt den Spähern"
        },
        "line.8": {
            "text": "zaghafte neugeborene Rehe"
        },
        "line.9": {
            "text": "in die Ferne gehend"
        },
        "line.10": {
            "text": "jeden Jäger entwaffnet"
        },
        "line.11": {
            "text": "deine Selbstverständlichkeit;"
        },
        "line.12": {
            "text": "wollte Sekunden teilen können"
        },
        "line.13": {
            "text": "in ihre Ursprünge ab und zu"
        }
    }
}