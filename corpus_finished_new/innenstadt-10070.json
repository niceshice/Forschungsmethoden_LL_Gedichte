{
    "title": "innenstadt-10070",
    "gender": "m",
    "author": "Andreas Neeser",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "Aufgeschlagen"
        },
        "line.2": {
            "text": "der Koffer mit lauter Zuhause"
        },
        "line.3": {
            "text": "Kostüme für lichte Figuren."
        },
        "line.4": {
            "text": "Du ziehst das erfahrenste Hemd an"
        },
        "line.5": {
            "text": "und machst dir ein Bild von der Stadt"
        },
        "line.6": {
            "text": "sie zeichnet sich ab durch die Stunden"
        },
        "line.7": {
            "text": "mit feinerem Strich"
        },
        "line.8": {
            "text": "legst du Linien und ziehst die Konturen"
        },
        "line.9": {
            "text": "bleibende Farben"
        },
        "line.10": {
            "text": "Fragment zu Fragment nach dem Aufbruch"
        },
        "line.11": {
            "text": "zur Ankunft."
        }
    }
}