{
    "title": "kann-uns-leere-14544",
    "gender": "w",
    "author": "Ruth Wolf-Rehfeldt",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "Kann uns Leere"
        },
        "line.2": {
            "text": "Wie lastende Schwere"
        },
        "line.3": {
            "text": "Beugen und drücken ?"
        },
        "line.4": {
            "text": "Brauchen wir Krücken"
        },
        "line.5": {
            "text": "Für das Nichts ?"
        }
    }
}