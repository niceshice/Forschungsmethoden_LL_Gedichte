{
    "title": "das-innerste-afrika-239",
    "gender": "m",
    "author": "Volker Braun",
    "author_birth": "1939",
    "poem": {
        "line.1": {
            "text": "Komm in ein wärmeres Land"
        },
        "line.2": {
            "text": "mit Rosenwetter"
        },
        "line.3": {
            "text": "Und grünen laubigen Türen"
        },
        "line.4": {
            "text": "Wo unverkleidete Männer"
        },
        "line.5": {
            "text": "Deine Genossen sind."
        },
        "line.6": {
            "text": "Dahin! Dahin"
        },
        "line.7": {
            "text": "Möcht ich mit dir, Geliebter"
        },
        "line.8": {
            "text": "Komm"
        },
        "line.9": {
            "text": "aus deinem Bau deinem lebenslänglichen Planjahr ewigen"
        },
        "line.10": {
            "text": "Schnee / Wartesaal wo die Geschichte auf den vergilbten"
        },
        "line.11": {
            "text": "FahrplanstarrtdieReisendenranzig/ Truppengelände"
        },
        "line.12": {
            "text": "TRAUNERN IST NICHT GESTATTET"
        },
        "line.13": {
            "text": "Unter die sachten Tamarisken"
        },
        "line.14": {
            "text": "In den Tropenregen, der die Losungen"
        },
        "line.15": {
            "text": "Abwäscht, trockenen Protokolle."
        },
        "line.16": {
            "text": "Sieh das Meer, das dagegen ist"
        },
        "line.17": {
            "text": "Mit fröhlichen Wellen, und ins Offene geht"
        },
        "line.18": {
            "text": "dahin"
        },
        "line.19": {
            "text": "Dahin führt kein Weg."
        },
        "line.20": {
            "text": "Wenn du gehst, hebt die Zeit ihre Flügel."
        },
        "line.21": {
            "text": "Nimm den Pfad gleich links durch die Brust"
        },
        "line.22": {
            "text": "Und überschreite die Grenze."
        },
        "line.23": {
            "text": "Wo die Zitronen blühn"
        },
        "line.24": {
            "text": ", piff paff!"
        },
        "line.25": {
            "text": "En quelque soir,par exemple,le touristenafEUROPA"
        },
        "line.26": {
            "text": "SACKBAHNHOF die verdunkelten Züge aus der vierten"
        },
        "line.27": {
            "text": "Welt vor Hungerberstend / hinterderZeitmauer Getöse"
        },
        "line.28": {
            "text": "unverständlicheSchreie/BlutsickertausdenNähten"
        },
        "line.29": {
            "text": "derNiederlage/Zukunftsgraupelund fast will/Mir es"
        },
        "line.30": {
            "text": "scheinen,essei,als in der bleiernen Zeit"
        },
        "line.31": {
            "text": "Sie können dich töten, aber vielleicht"
        },
        "line.32": {
            "text": "Kommst du davon"
        },
        "line.33": {
            "text": "Ledig und unbestimmt"
        },
        "line.34": {
            "text": "komm! ins Offene, Freund!"
        },
        "line.35": {
            "text": "Nicht im Süden liegt es, Ausland nicht"
        },
        "line.36": {
            "text": "Wo unverkleidete Männer"
        },
        "line.37": {
            "text": "Wo der Regen"
        },
        "line.38": {
            "text": "Denn nicht Mächtiges ists, zum Leben aber gehört es"
        },
        "line.39": {
            "text": "Was wir wollen"
        },
        "line.40": {
            "text": "wo dich keiner"
        },
        "line.41": {
            "text": "Das innerste Land, die Fremde"
        },
        "line.42": {
            "text": "Erwartet. Du mußt die Grenze überschreiten"
        },
        "line.43": {
            "text": "Mit deinem gültigen Gesicht."
        },
        "line.44": {
            "text": "Dein rotes Spanien, dein Libanon"
        },
        "line.45": {
            "text": "Erreiche es vor der Rente."
        },
        "line.46": {
            "text": "Wir befinden uns, sagte er, auf einer schiefen Ebne.Alles"
        },
        "line.47": {
            "text": "deutet darauf hin, daß es abwärts geht.Schließen Sie ein-"
        },
        "line.48": {
            "text": "mal die Augen und hören Sie, wie es knirscht.Das ist das"
        },
        "line.49": {
            "text": "Ende.Warten wir ab,wir werden es erleben.Wirsind"
        },
        "line.50": {
            "text": "auf dembestenWeg.Wir brauchen nur fortzufahren mit"
        },
        "line.51": {
            "text": "der Übung.Vor einiger Zeit konnten wirz. B.dasBrett"
        },
        "line.52": {
            "text": "oder wie man es nennen will zurückwippen über den Null-"
        },
        "line.53": {
            "text": "punkt und sagen:esgehtaufwärts!Jetzt ist es eine end-"
        },
        "line.54": {
            "text": "gültige Schräge in den Keller.Zu den Kakerlaken,meine"
        },
        "line.55": {
            "text": "DamenundHerrn.BleibenSieruhig,gehnSieindie"
        },
        "line.56": {
            "text": "Firma,wickeln Sie sich in die Plane,fassen Sie sich kurz."
        },
        "line.57": {
            "text": "WirhabendiefurchtbareNachrichtvernommen,wir"
        },
        "line.58": {
            "text": "habennichtshinzuzusetzen.Adieu.SagtederMann in"
        },
        "line.59": {
            "text": "Itzehoe und glitt hinter dem Fenster hinab."
        },
        "line.60": {
            "text": "Non!wir werden den Sommer nicht mehr in diesem geiz-"
        },
        "line.61": {
            "text": "igenLandverbringen,wowirimmernur einander ver-"
        },
        "line.62": {
            "text": "sprochene Waisen sind,"
        },
        "line.63": {
            "text": "komm"
        },
        "line.64": {
            "text": "Steckmuscheln, Zikaden"
        },
        "line.65": {
            "text": "Mach dich auf"
        },
        "line.66": {
            "text": "Lebenslänglicher Leib:"
        },
        "line.67": {
            "text": "SIEH DAS MEER, DAS DAGEGEN IST."
        },
        "line.68": {
            "text": "ERREICHE ES VOR DER RENTE."
        },
        "line.69": {
            "text": "DU MUSST DIE GRENZE ÜBERSCHREITEN."
        }
    }
}