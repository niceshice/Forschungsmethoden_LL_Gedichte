{
    "title": "land-auf-dem-sonntag-1437",
    "gender": "m",
    "author": "Paul Maar",
    "author_birth": "1937",
    "poem": {
        "line.1": {
            "text": "Im Scheinensonn"
        },
        "line.2": {
            "text": "taubt eine Gurr."
        },
        "line.3": {
            "text": "Im Schattenhaus"
        },
        "line.4": {
            "text": "katzt eine Schnurr."
        },
        "line.5": {
            "text": "Es hummelt ein Brumm"
        },
        "line.6": {
            "text": "wie ein Wagenlast."
        },
        "line.7": {
            "text": "Sanft schweint ein Grunz"
        },
        "line.8": {
            "text": "vor der Wirtschaftsgast."
        },
        "line.9": {
            "text": "Im Weiherdorf"
        },
        "line.10": {
            "text": "froscht tief der Tauch."
        },
        "line.11": {
            "text": "Oben am Dachhaus"
        },
        "line.12": {
            "text": "schlotet der Rauch."
        },
        "line.13": {
            "text": "Ein Pinkel, der hundet"
        },
        "line.14": {
            "text": "auf Blumenmohn."
        },
        "line.15": {
            "text": "Der Schimpf vatert laut"
        },
        "line.16": {
            "text": "im Zimmerwohn."
        },
        "line.17": {
            "text": "Ein Fahrerssonntag"
        },
        "line.18": {
            "text": "wagent den Wende."
        },
        "line.19": {
            "text": "Das dauert zu lange,"
        },
        "line.20": {
            "text": "drum gedichtet das Ende."
        }
    }
}