{
    "title": "tabula-rasa-15605",
    "gender": "m",
    "author": "Steffen  Mensching",
    "author_birth": "1958",
    "poem": {
        "line.1": {
            "text": "Alles gleich. Gultig alles."
        },
        "line.2": {
            "text": "Auschwitz, Wandlitz, Austerlitz."
        },
        "line.3": {
            "text": "Ein Abwasch, ein Knopfdruck,"
        },
        "line.4": {
            "text": "Hokuspokus. Ab in den Lokus."
        },
        "line.5": {
            "text": "Loscht die Festplatten."
        },
        "line.6": {
            "text": "Amnesie, Amnestie, Baum und Borke,"
        },
        "line.7": {
            "text": "Vergasen, Vergessen, tertium"
        },
        "line.8": {
            "text": "Non datar. Wer sich zum Fenster"
        },
        "line.9": {
            "text": "Hinauslehnt, Jan Hus z. B.,"
        },
        "line.10": {
            "text": "Muß sich nicht wundern, wenn ihm"
        },
        "line.11": {
            "text": "Der Kopf etcetera, Haretiker,"
        },
        "line.12": {
            "text": "Theoretiker, selber schuld,"
        },
        "line.13": {
            "text": "Pech gehabt."
        },
        "line.14": {
            "text": "Man nehme Dr. Oetker."
        },
        "line.15": {
            "text": "Warum schlagt niemand Dr. Oetker"
        },
        "line.16": {
            "text": "Zum Nobelpreis vor?"
        },
        "line.17": {
            "text": "Kein Taktiker, ein Praktiker."
        },
        "line.18": {
            "text": "Appetitlich, friedlich"
        },
        "line.19": {
            "text": "Haben sich seine Rezepte"
        },
        "line.20": {
            "text": "Zumindest bewahrt. Eigner Herd"
        },
        "line.21": {
            "text": "Und Nahrwert, nix da"
        },
        "line.22": {
            "text": "Mit Mehrwert."
        },
        "line.23": {
            "text": "Der Fetischcharakter der Ware,"
        },
        "line.24": {
            "text": "Ausgelegt von Beate Uhse,"
        },
        "line.25": {
            "text": "Ausgezeichnet. Proletarier"
        },
        "line.26": {
            "text": "Aller Lander? Schwamm druber."
        },
        "line.27": {
            "text": "Schlamm druber. Planierraupen"
        },
        "line.28": {
            "text": "Schieben die Graber zusammen,"
        },
        "line.29": {
            "text": "Vereinigt in der Entsorgung."
        },
        "line.30": {
            "text": "Ab in den Orkus. Glucklich ist,"
        },
        "line.31": {
            "text": "Wer vergißt. Das Jahrtausend"
        },
        "line.32": {
            "text": "Geht sauber uber die Rampe."
        },
        "line.33": {
            "text": "Keiner kommt ungeschoren davon."
        },
        "line.34": {
            "text": "Heil Alzheimer. Dreimal Nullnull."
        },
        "line.35": {
            "text": "In Bad und WC alles okay."
        },
        "line.36": {
            "text": "Demnachst im Kino."
        },
        "line.37": {
            "text": "Sean Connery als Thomas von Aquino."
        }
    }
}