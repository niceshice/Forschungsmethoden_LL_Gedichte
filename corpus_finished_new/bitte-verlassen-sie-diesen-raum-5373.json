{
    "title": "bitte-verlassen-sie-diesen-raum-5373",
    "gender": "m",
    "author": "Nicolai Kobus",
    "author_birth": "1968",
    "poem": {
        "line.1": {
            "text": "bitte verlassen sie diesen raum"
        },
        "line.2": {
            "text": "so wie sie ihn vorfinden möchten"
        },
        "line.3": {
            "text": "danke möchten sie diesen raum"
        },
        "line.4": {
            "text": "vorfinden wie sie ihn verlassen"
        },
        "line.5": {
            "text": "haben bitte räumen sie alles so"
        },
        "line.6": {
            "text": "vorgefundene als wären sie"
        },
        "line.7": {
            "text": "verlassen worden danke sie"
        },
        "line.8": {
            "text": "möchten doch nicht daß man"
        },
        "line.9": {
            "text": "sie so verlassen im raum vor"
        },
        "line.10": {
            "text": "findet bitte seien sie für einen so"
        },
        "line.11": {
            "text": "verlassen vorgefundenen raum"
        },
        "line.12": {
            "text": "dankbar bitte räumen sie diese"
        },
        "line.13": {
            "text": "verlassenheit als hätten sie etwas"
        },
        "line.14": {
            "text": "vorgefunden danke finden sie"
        },
        "line.15": {
            "text": "einen raum der so verlassen"
        },
        "line.16": {
            "text": "ist wie sie möchten bitte mögen"
        },
        "line.17": {
            "text": "sie derart verlassene räume danke"
        },
        "line.18": {
            "text": "so finden sie raum in der verlassen"
        },
        "line.19": {
            "text": "heit bitte versuchen sie nicht sich"
        },
        "line.20": {
            "text": "in verlassenen räumen zu finden"
        },
        "line.21": {
            "text": "danke das vorfinden verlassener"
        },
        "line.22": {
            "text": "räume möchten sie bitte unter"
        },
        "line.23": {
            "text": "lassen danke alles vorgefundene"
        },
        "line.24": {
            "text": "ist wie sie in der verlassenheit"
        },
        "line.25": {
            "text": "des raums bitte finden sie diesen"
        },
        "line.26": {
            "text": "raum so wie sie ihn verlassen haben"
        },
        "line.27": {
            "text": "danke unterlassen sie räumliches"
        },
        "line.28": {
            "text": "vorfinden bitte räumen sie diese"
        },
        "line.29": {
            "text": "so vorgefundene verlassenheit"
        },
        "line.30": {
            "text": "danke wie möchten sie in diesen"
        },
        "line.31": {
            "text": "vorgefundenen räumen verlassen"
        },
        "line.32": {
            "text": "werden bitte verlassen sie sich"
        },
        "line.33": {
            "text": "auf die stattfindende räumung"
        },
        "line.34": {
            "text": "danke möchten sie so vorgefunden"
        },
        "line.35": {
            "text": "den raum noch verlassen bitte"
        },
        "line.36": {
            "text": "verlassen sie diesen raum so wie"
        },
        "line.37": {
            "text": "sie ihn vorfinden möchten danke"
        },
        "line.38": {
            "text": "für Raphael Urweider"
        }
    }
}