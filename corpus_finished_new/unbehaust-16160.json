{
    "title": "unbehaust-16160",
    "gender": "w",
    "author": "Sandra Hubinger",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "Ohne Bleibe die Blicke"
        },
        "line.2": {
            "text": "schweifend über Felder"
        },
        "line.3": {
            "text": "deren Ränder abflachen"
        },
        "line.4": {
            "text": "Im Körper ohne Bleibe"
        },
        "line.5": {
            "text": "dem Sein kein Fleisch kein Bein"
        },
        "line.6": {
            "text": "Ohne Bleibe der Gedanke, hart"
        },
        "line.7": {
            "text": "wie Hacke auf Stein"
        },
        "line.8": {
            "text": "zum Eisen laufend die Enden stumpf"
        },
        "line.9": {
            "text": "In den Anfängen ohne Bleibe"
        },
        "line.10": {
            "text": "das Glänzende dunkelt nach"
        },
        "line.11": {
            "text": "Ohne Bleibe im zueinander Stolpern"
        },
        "line.12": {
            "text": "im Verzahnen"
        },
        "line.13": {
            "text": "ans Herz Gelegtes ohne Bleibe"
        }
    }
}