{
    "title": "mein-engel-5376",
    "gender": "m",
    "author": "Richard Pietraß",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Mein Engel ist elend"
        },
        "line.2": {
            "text": "Und ich steh in Schuld."
        },
        "line.3": {
            "text": "Seine Flügel sind lahm."
        },
        "line.4": {
            "text": "Ich hab dran gerissen."
        },
        "line.5": {
            "text": "Seine Stirn ist kraus."
        },
        "line.6": {
            "text": "Ich hab sie gefurcht."
        },
        "line.7": {
            "text": "Sein Herz war ein Quell."
        },
        "line.8": {
            "text": "Ich hab ihn getrübt."
        },
        "line.9": {
            "text": "Nun schleppt er sich"
        },
        "line.10": {
            "text": "Nach Engelland."
        },
        "line.11": {
            "text": "Nun schirmt ihn"
        },
        "line.12": {
            "text": "Meine schwache Hand."
        },
        "line.13": {
            "text": "(1993)"
        }
    }
}