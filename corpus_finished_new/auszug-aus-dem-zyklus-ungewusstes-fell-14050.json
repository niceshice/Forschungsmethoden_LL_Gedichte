{
    "title": "auszug-aus-dem-zyklus-ungewusstes-fell-14050",
    "gender": "w",
    "author": "Anja Bayer",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "Ich sah, und siehe"
        },
        "line.2": {
            "text": "das Wiedergeschenk"
        },
        "line.3": {
            "text": "beim Profit ausgelöst mit der Hand"
        },
        "line.4": {
            "text": "handweise rette ich mir"
        },
        "line.5": {
            "text": "die Gesichter damit"
        },
        "line.6": {
            "text": "was ich verschwende"
        },
        "line.7": {
            "text": "ist"
        },
        "line.8": {
            "text": "arglos und"
        },
        "line.9": {
            "text": "unbekleidet, aufgeknöpft und verknüpft"
        },
        "line.10": {
            "text": "wenn ich an den Bildstock gerate"
        },
        "line.11": {
            "text": "lassen die Läufe"
        },
        "line.12": {
            "text": "mir den Eifer nach"
        },
        "line.13": {
            "text": "Greif"
        },
        "line.14": {
            "text": "hinters Heiligste"
        },
        "line.15": {
            "text": "unter den Haaren"
        },
        "line.16": {
            "text": "liegt das Herz"
        },
        "line.17": {
            "text": "übe meine Schnauze"
        },
        "line.18": {
            "text": "am Rund der Vollen, sehne"
        },
        "line.19": {
            "text": "blankschwitze ich mich"
        },
        "line.20": {
            "text": "durch deine"
        },
        "line.21": {
            "text": "Falten hin zu"
        },
        "line.22": {
            "text": "der Unruhe"
        },
        "line.23": {
            "text": "die mich gestern befiel"
        },
        "line.24": {
            "text": "habe die Mundwinkel"
        },
        "line.25": {
            "text": "durcheinandergebracht"
        },
        "line.26": {
            "text": "du sagst mir"
        },
        "line.27": {
            "text": "ungewusstes Fell"
        }
    }
}