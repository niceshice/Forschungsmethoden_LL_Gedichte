{
    "title": "ins-innere-14658",
    "gender": "w",
    "author": "Lisa Elsässer",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "gelangt das licht wieder"
        },
        "line.2": {
            "text": "all die zeichen vergangner"
        },
        "line.3": {
            "text": "zeit meerfunken laubgesang"
        },
        "line.4": {
            "text": "in kieselslang versunken wir"
        },
        "line.5": {
            "text": "ins rege regen kenterte an uns"
        },
        "line.6": {
            "text": "zwei einsame sterne im ruf der"
        },
        "line.7": {
            "text": "klaren nacht einander gefährten"
        },
        "line.8": {
            "text": "durchs unterkühlte all"
        }
    }
}