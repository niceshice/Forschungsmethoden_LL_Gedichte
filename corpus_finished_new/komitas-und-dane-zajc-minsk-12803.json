{
    "title": "komitas-und-dane-zajc-minsk-12803",
    "gender": "w",
    "author": "Anne Seidel",
    "author_birth": "1988",
    "poem": {
        "line.1": {
            "text": "komitas                                              schwarzblaue amsel"
        },
        "line.2": {
            "text": "auf einem blauen zaun                        dein auge"
        },
        "line.3": {
            "text": "komita"
        },
        "line.4": {
            "text": "unbewegt"
        },
        "line.5": {
            "text": "eine kleine hand                      in aengstlichkeit zu dir"
        },
        "line.6": {
            "text": "komit"
        },
        "line.7": {
            "text": "as"
        },
        "line.8": {
            "text": "beschienen vom"
        },
        "line.9": {
            "text": "sonnenuntergang"
        },
        "line.10": {
            "text": "in minsk                                               april"
        },
        "line.11": {
            "text": "(dein april)                                       (dein minsk)"
        },
        "line.12": {
            "text": "kom i"
        },
        "line.13": {
            "text": "ta"
        },
        "line.14": {
            "text": "wie ein sturm"
        },
        "line.15": {
            "text": "ein schwarm"
        },
        "line.16": {
            "text": "vorbeiziehender"
        },
        "line.17": {
            "text": "schwalben"
        },
        "line.18": {
            "text": "so leis hallt dein stille gewordenes schweigen"
        },
        "line.19": {
            "text": "ass ass ass"
        },
        "line.20": {
            "text": "die scherben des wassers              auf einem dunkel werdenden feld"
        }
    }
}