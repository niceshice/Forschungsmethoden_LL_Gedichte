{
    "title": "feiernder-abend-talweg-2627",
    "gender": "m",
    "author": "Michael Donhauser",
    "author_birth": "1956",
    "poem": {
        "line.1": {
            "text": "Feiernder Abend, Talweg und"
        },
        "line.2": {
            "text": "Gewalt, aufgebrochen die"
        },
        "line.3": {
            "text": "Augen, Knospen, wir sagten"
        },
        "line.4": {
            "text": "sieh, und glühend war der"
        },
        "line.5": {
            "text": "Himmel, tönten die Berge"
        },
        "line.6": {
            "text": "Flußherwärts der Verkehr, es"
        },
        "line.7": {
            "text": "strömte das Wasser, waren"
        },
        "line.8": {
            "text": "Steine, ti penso und immer"
        },
        "line.9": {
            "text": "habe ich mit Kieseln, Reisig"
        },
        "line.10": {
            "text": "Laub und an dich gedacht"
        },
        "line.11": {
            "text": "Wärmende Böschung, der"
        },
        "line.12": {
            "text": "Flattich, du weintest, deine"
        },
        "line.13": {
            "text": "Hände sanken, müde, und"
        },
        "line.14": {
            "text": "Schatten, kahle, streiften den"
        },
        "line.15": {
            "text": "Asphalt, flüchtig mit Lippen"
        },
        "line.16": {
            "text": "Schnatternd flog auf, rief eine"
        },
        "line.17": {
            "text": "Amsel und flötete, schlug"
        },
        "line.18": {
            "text": "jubelnd mit Pausen, es naht"
        },
        "line.19": {
            "text": "hieß ein Versiegen, wehende"
        },
        "line.20": {
            "text": "Bänder, Drähte, der Weg"
        }
    }
}