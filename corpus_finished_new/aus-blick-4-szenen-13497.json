{
    "title": "aus-blick-4-szenen-13497",
    "gender": "m",
    "author": "José F. A. Oliver",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "I"
        },
        "line.2": {
            "text": "sitzt"
        },
        "line.3": {
            "text": "sitzt die nachbarin"
        },
        "line.4": {
            "text": "mit strohhut unter einer palme"
        },
        "line.5": {
            "text": "sitzt in den tag"
        },
        "line.6": {
            "text": "sitzt in den tag die nachbarin"
        },
        "line.7": {
            "text": "strickt"
        },
        "line.8": {
            "text": "strickt mit den augen"
        },
        "line.9": {
            "text": "strickt mit den augen an"
        },
        "line.10": {
            "text": "ihrer biographie"
        },
        "line.11": {
            "text": "(himmelsgarn, erdhand)"
        },
        "line.12": {
            "text": "stopft"
        },
        "line.13": {
            "text": "stopft wollfäden"
        },
        "line.14": {
            "text": "stopft wollfäden ins gehör"
        },
        "line.15": {
            "text": "stampft auf den ohren davon"
        },
        "line.16": {
            "text": "bärbeißig vor flüchtigkeit"
        },
        "line.17": {
            "text": "II"
        },
        "line.18": {
            "text": "bringt"
        },
        "line.19": {
            "text": "bringt ihr gatte"
        },
        "line.20": {
            "text": "frierwestchen an und cordbehost"
        },
        "line.21": {
            "text": "bringt einen teller"
        },
        "line.22": {
            "text": "bringt einen teller suppe"
        },
        "line.23": {
            "text": "schweigt"
        },
        "line.24": {
            "text": "schweigt einen löffel voll"
        },
        "line.25": {
            "text": "schweigt einen löffel voll nacht"
        },
        "line.26": {
            "text": "in ihr gesicht"
        },
        "line.27": {
            "text": "(wundvergabelt, messerblick)"
        },
        "line.28": {
            "text": "schneidet"
        },
        "line.29": {
            "text": "schneidet ein"
        },
        "line.30": {
            "text": "schneidet ein den plattenboden"
        },
        "line.31": {
            "text": "stampft auf den ohren davon"
        },
        "line.32": {
            "text": "bärbeißig vor flüchtigkeit"
        },
        "line.33": {
            "text": "III"
        },
        "line.34": {
            "text": "torkelt"
        },
        "line.35": {
            "text": "torkelt der sohn"
        },
        "line.36": {
            "text": "auf seiner wodkahaut"
        },
        "line.37": {
            "text": "torkelt das glas"
        },
        "line.38": {
            "text": "torkelt das wodkaglas der sohn"
        },
        "line.39": {
            "text": "blinzelt"
        },
        "line.40": {
            "text": "blinzelt aufs fettauge"
        },
        "line.41": {
            "text": "blinzelt aufs fettauge der suppe"
        },
        "line.42": {
            "text": "kotzt"
        },
        "line.43": {
            "text": "(mundbefäultes, lippenfransig)"
        },
        "line.44": {
            "text": "lächelt"
        },
        "line.45": {
            "text": "lächelt aufgeräumt"
        },
        "line.46": {
            "text": "lächelt das wodkaglas an"
        },
        "line.47": {
            "text": "stampft auf den ohren davon"
        },
        "line.48": {
            "text": "bärbeißig vor flüchtigkeit"
        },
        "line.49": {
            "text": "IV"
        },
        "line.50": {
            "text": "wedelt"
        },
        "line.51": {
            "text": "wedelt der hund"
        },
        "line.52": {
            "text": "wedelt der hund schwanzfamiliär"
        },
        "line.53": {
            "text": "wedelt"
        },
        "line.54": {
            "text": "wedelt"
        },
        "line.55": {
            "text": "(Vorhang fällt)"
        }
    }
}