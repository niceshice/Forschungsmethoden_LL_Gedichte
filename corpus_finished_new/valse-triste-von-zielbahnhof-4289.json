{
    "title": "valse-triste-von-zielbahnhof-4289",
    "gender": "w",
    "author": "Brigitte Struzyk",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Nichts ist so einsam als das"
        },
        "line.2": {
            "text": "Dass die S-Bahn heranjagt"
        },
        "line.3": {
            "text": "dass dieser Sog da ist"
        },
        "line.4": {
            "text": "dass du stolpernd ihm folgst"
        },
        "line.5": {
            "text": "dass der Schuh gr√ºn ist"
        },
        "line.6": {
            "text": "dass der Mann keine Socken hat"
        },
        "line.7": {
            "text": "dass du selbst im Fenster dich siehst"
        },
        "line.8": {
            "text": "dass du frierst an der Heizung"
        },
        "line.9": {
            "text": "dass sich Lippen bewegen"
        },
        "line.10": {
            "text": "dass ein Wort dich erschreckt"
        },
        "line.11": {
            "text": "dass du fremden schlaf siehst"
        },
        "line.12": {
            "text": "dass du doch noch ankommst"
        },
        "line.13": {
            "text": "dass der Falsche wartet"
        }
    }
}