{
    "title": "eine-kuppel-ein-schnitt-1343",
    "gender": "m",
    "author": "Nico Bleutge",
    "author_birth": "1972",
    "poem": {
        "line.1": {
            "text": "grauer plan ein halbkreis"
        },
        "line.2": {
            "text": "unter kuppeln schräg gewicht"
        },
        "line.3": {
            "text": "eines bogens höhe die sich"
        },
        "line.4": {
            "text": "raum ausmißt"
        },
        "line.5": {
            "text": "trägt sich der bau bedeckt"
        },
        "line.6": {
            "text": "in färbung: mildes blau ein rot"
        },
        "line.7": {
            "text": "ton kleiner kammerzweig"
        },
        "line.8": {
            "text": "verästelt ins feinste bis"
        },
        "line.9": {
            "text": "stränge nach zarter drehung"
        },
        "line.10": {
            "text": "mehrere querschnitte zwischen"
        },
        "line.11": {
            "text": "den schenkeln das maß"
        },
        "line.12": {
            "text": "ziffern und richtungen deuten"
        },
        "line.13": {
            "text": "fluß an von winzigem durch"
        },
        "line.14": {
            "text": "kanäle gedrückt ein windungs"
        },
        "line.15": {
            "text": "zugriff nurmehr: verhäutetes nähte"
        },
        "line.16": {
            "text": "bündig den saum läuft ein auge"
        },
        "line.17": {
            "text": "einzeln entlang"
        }
    }
}