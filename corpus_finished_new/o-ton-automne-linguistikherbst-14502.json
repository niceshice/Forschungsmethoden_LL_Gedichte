{
    "title": "o-ton-automne-linguistikherbst-14502",
    "gender": "m",
    "author": "Oskar Pastior",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "O-Ton \"Automne\"  Linguistikherbst"
        },
        "line.2": {
            "text": "Stick Harwest / Osenj / Toamna / Stick"
        },
        "line.3": {
            "text": "Stick Lippstick Nota Bene  heu"
        },
        "line.4": {
            "text": "was da abwest im Dümpel-Sermon:"
        },
        "line.5": {
            "text": "Zero-Phonem"
        },
        "line.6": {
            "text": "Der Kürbis wächst"
        },
        "line.7": {
            "text": "In Eros-Hemden sensen"
        },
        "line.8": {
            "text": "Tristia"
        },
        "line.9": {
            "text": "Trestia"
        },
        "line.10": {
            "text": "Deltageflecht"
        },
        "line.11": {
            "text": "Da ist (\"Kusnejtschik / Zinziwer\") Synopsis"
        },
        "line.12": {
            "text": "von Kolchis her ergangen:"
        },
        "line.13": {
            "text": "Seerosensee / Seerosenbucht"
        },
        "line.14": {
            "text": "Ost-West-Phantom"
        },
        "line.15": {
            "text": "Ovids Metamorphosen"
        },
        "line.16": {
            "text": "am Bösendorfer Luch"
        },
        "line.17": {
            "text": "Die Semaphoren morsen:"
        },
        "line.18": {
            "text": "\"noch steht es zahn / um haaresbrei"
        },
        "line.19": {
            "text": "an topf und hasen / geht es wald"
        },
        "line.20": {
            "text": "das jahr es jährt / sich horn und hin\""
        },
        "line.21": {
            "text": "O Zero Osero  der See"
        },
        "line.22": {
            "text": "Rien ne va plus  O Zero Stick"
        },
        "line.23": {
            "text": "O Lambda Entengrütze Haarnest Fälfä"
        },
        "line.24": {
            "text": "hilf Schilf"
        },
        "line.25": {
            "text": "heu Schelf"
        },
        "line.26": {
            "text": "O-Ton"
        },
        "line.27": {
            "text": "Automne"
        },
        "line.28": {
            "text": "mir ist so rosident phantom"
        },
        "line.29": {
            "text": "Semiramis / Sorbonne / Sa-Um-Weh"
        }
    }
}