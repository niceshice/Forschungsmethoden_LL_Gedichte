{
    "title": "oeh-mau-16234",
    "gender": "w",
    "author": "Cornelia  Hülmbauer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "die anstalt ist in mauern"
        },
        "line.2": {
            "text": "die anstalt ist vor ort"
        },
        "line.3": {
            "text": "die anstalt ist in mänteln"
        },
        "line.4": {
            "text": "die anstalt näht dich ein"
        },
        "line.5": {
            "text": "häkle deckung"
        },
        "line.6": {
            "text": "schürze strümpfe"
        },
        "line.7": {
            "text": "halte habacht"
        },
        "line.8": {
            "text": "fix & foxi"
        },
        "line.9": {
            "text": "& für unfälle ein schwamm"
        },
        "line.10": {
            "text": "der vater ist ein apfelbaum"
        },
        "line.11": {
            "text": "der vater ist ein stumpf"
        },
        "line.12": {
            "text": "der vater ließ die früchte fallen"
        },
        "line.13": {
            "text": "vom vater bleibt nur mus"
        },
        "line.14": {
            "text": "es passiert"
        },
        "line.15": {
            "text": "es grassiert"
        },
        "line.16": {
            "text": "unter der bandage"
        },
        "line.17": {
            "text": "in dieser bagage"
        },
        "line.18": {
            "text": "geht es um"
        },
        "line.19": {
            "text": "das mädchen hat zwei augen"
        },
        "line.20": {
            "text": "das mädchen hat kein fett"
        },
        "line.21": {
            "text": "das mädchen schwimmt in strömen"
        },
        "line.22": {
            "text": "das mädchen tropft ins bett"
        },
        "line.23": {
            "text": "wo speck ist da sind maden"
        },
        "line.24": {
            "text": "wo brot ist da ist speck"
        },
        "line.25": {
            "text": "stirbst du oder frisst du"
        },
        "line.26": {
            "text": "dem onkel alles weg"
        }
    }
}