{
    "title": "duenngewebt-1803",
    "gender": "w",
    "author": "Isabeella Beumer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "in jener nacht"
        },
        "line.2": {
            "text": "das innen von herz"
        },
        "line.3": {
            "text": "in schalen"
        },
        "line.4": {
            "text": "mit einem löffel"
        },
        "line.5": {
            "text": "glänzende stäbchen"
        },
        "line.6": {
            "text": "verrostetes blut"
        },
        "line.7": {
            "text": "UNTER SCHWARZER SEIDE"
        },
        "line.8": {
            "text": "ROT"
        },
        "line.9": {
            "text": "atemnähe"
        },
        "line.10": {
            "text": "da unter deinen häuten"
        },
        "line.11": {
            "text": "lebendig auf des dolches grad"
        },
        "line.12": {
            "text": "über dem rücken"
        },
        "line.13": {
            "text": "ausgestreckter nacht"
        },
        "line.14": {
            "text": "lichtvertrauter"
        },
        "line.15": {
            "text": "der zeit zahlenzerstörer"
        },
        "line.16": {
            "text": "oderder nächste tag"
        },
        "line.17": {
            "text": "zwischen scheren"
        },
        "line.18": {
            "text": "und alles auf unsichtbaren stein"
        },
        "line.19": {
            "text": "getropft"
        },
        "line.20": {
            "text": "mitgebrachte alphabete"
        },
        "line.21": {
            "text": "himmlischer ostmond"
        }
    }
}