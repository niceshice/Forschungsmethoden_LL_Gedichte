{
    "title": "es-wird-licht-es-wird-ostern-1525",
    "gender": "w",
    "author": "Elfriede Gerstl",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "für renald deppe"
        },
        "line.2": {
            "text": "im graubraun dieses winters"
        },
        "line.3": {
            "text": "denke ich ganz fest"
        },
        "line.4": {
            "text": "an wärmende morgensonne"
        },
        "line.5": {
            "text": "die roten holländischen tulpen"
        },
        "line.6": {
            "text": "die ich kaufen werde"
        },
        "line.7": {
            "text": "bunte kinderkleider"
        },
        "line.8": {
            "text": "auf zögerlich spriessenden wiesen"
        },
        "line.9": {
            "text": "vielleicht sagt jemand"
        },
        "line.10": {
            "text": "ES WERDE MÄRZ"
        },
        "line.11": {
            "text": "und vielleicht kommt er aus gewohnheit"
        },
        "line.12": {
            "text": "von alleine"
        },
        "line.13": {
            "text": "ich glaube fest daran"
        },
        "line.14": {
            "text": "dass es auch april wird"
        },
        "line.15": {
            "text": "auch wenn ich nicht mehr da bin"
        },
        "line.16": {
            "text": "auch wenn wir nicht mehr da sind"
        },
        "line.17": {
            "text": "und das soll nun wieder ein trost sein"
        },
        "line.18": {
            "text": "manchmal  wenn ich es gerade so will"
        },
        "line.19": {
            "text": "4. jänner 2003"
        }
    }
}