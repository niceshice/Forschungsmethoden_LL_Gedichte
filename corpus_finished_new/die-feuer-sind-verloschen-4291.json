{
    "title": "die-feuer-sind-verloschen-4291",
    "gender": "w",
    "author": "Brigitte Struzyk",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Die Feuer sind verloschen"
        },
        "line.2": {
            "text": "Ich ziehe durch die Nacht"
        },
        "line.3": {
            "text": "Trag Asche in der Tasche"
        },
        "line.4": {
            "text": "Ich stochere in der Asche"
        },
        "line.5": {
            "text": "Ich habe durchgemacht"
        },
        "line.6": {
            "text": "Die Schatten sind gegessen"
        },
        "line.7": {
            "text": "Die Steine sind geschluckt"
        },
        "line.8": {
            "text": "Die Strecken sind vermessen"
        },
        "line.9": {
            "text": "Die Messen sind gedruckt"
        }
    }
}