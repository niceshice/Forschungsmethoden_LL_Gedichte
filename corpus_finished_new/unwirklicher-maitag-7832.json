{
    "title": "unwirklicher-maitag-7832",
    "gender": "m",
    "author": "Reiner Kunze",
    "author_birth": "1933",
    "poem": {
        "line.1": {
            "text": "So sehr blühten die kirsch-und mostbirnbäume,"
        },
        "line.2": {
            "text": "daß sie sich verwandelten"
        },
        "line.3": {
            "text": "in weißes gewölk"
        },
        "line.4": {
            "text": "Das dorf, eingeblüht,"
        },
        "line.5": {
            "text": "schwebte davon"
        },
        "line.6": {
            "text": "Mit unserem weißen haar"
        },
        "line.7": {
            "text": "täuschten wir vor dazuzugehören"
        },
        "line.8": {
            "text": "und wurden schwerelos"
        }
    }
}