{
    "title": "versionen-11544",
    "gender": "w",
    "author": "Nora Gomringer",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "und"
        },
        "line.2": {
            "text": "ein Boot legt an"
        },
        "line.3": {
            "text": "Böcklin malt ein Boot, das anlegt,"
        },
        "line.4": {
            "text": "umschattet,"
        },
        "line.5": {
            "text": "soghaft."
        },
        "line.6": {
            "text": "Ein Bootsmann, namenlos,"
        },
        "line.7": {
            "text": "allzu willig, sich preiszugeben."
        },
        "line.8": {
            "text": "Hitler besaß eine Version,"
        },
        "line.9": {
            "text": "Utoya wurde eine"
        },
        "line.10": {
            "text": "Insel"
        },
        "line.11": {
            "text": "umschattet,"
        },
        "line.12": {
            "text": "soghaft."
        },
        "line.13": {
            "text": "Ein Boot legt an,"
        },
        "line.14": {
            "text": "an Bord ein Tod"
        },
        "line.15": {
            "text": "ein Übergangsadvokat"
        },
        "line.16": {
            "text": "Böcklin malt ein Boot, das anlegt."
        },
        "line.17": {
            "text": "Ein Bootsmann namenlos,"
        },
        "line.18": {
            "text": "Versionen von Breivik."
        },
        "line.19": {
            "text": "An Bord ein Tod,"
        },
        "line.20": {
            "text": "friedlos,"
        },
        "line.21": {
            "text": "umsogen,"
        },
        "line.22": {
            "text": "schattenhaft,"
        },
        "line.23": {
            "text": "schemenlos,"
        },
        "line.24": {
            "text": "eine Insel"
        },
        "line.25": {
            "text": "und"
        }
    }
}