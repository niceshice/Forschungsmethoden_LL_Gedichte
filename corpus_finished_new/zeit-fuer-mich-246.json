{
    "title": "zeit-fuer-mich-246",
    "gender": "m",
    "author": "Jörg Schieke",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "vorne das haus und dahinter"
        },
        "line.2": {
            "text": "die von der wippe"
        },
        "line.3": {
            "text": "wandernden täler. zurück"
        },
        "line.4": {
            "text": "ein stück film, die farbe"
        },
        "line.5": {
            "text": "der augen und die beschreibung"
        },
        "line.6": {
            "text": "jener farbe"
        },
        "line.7": {
            "text": "in noch anderen farben. ich lieb dich"
        },
        "line.8": {
            "text": "ja auch, kann es nur nicht"
        },
        "line.9": {
            "text": "so zeigen, wie du, mit dem"
        },
        "line.10": {
            "text": "korallenvornamen, korall li, korall"
        },
        "line.11": {
            "text": "la, als ich ging, von rom"
        },
        "line.12": {
            "text": "nach venedig und weiter"
        },
        "line.13": {
            "text": "bis china, die gleise"
        },
        "line.14": {
            "text": "zu siezen und mit der brücke, es mag"
        },
        "line.15": {
            "text": "auch nur ein brett"
        },
        "line.16": {
            "text": "gewesen sein, per du."
        }
    }
}