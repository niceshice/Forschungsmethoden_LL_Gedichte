{
    "title": "grabende-13886",
    "gender": "w",
    "author": "Kerstin Becker",
    "author_birth": "1969",
    "poem": {
        "line.1": {
            "text": "sie werfen das Innere auf wir"
        },
        "line.2": {
            "text": "graben uns ihnen mit weichen Fingern"
        },
        "line.3": {
            "text": "auf der Viehweide mit den verdrehten"
        },
        "line.4": {
            "text": "Grashalmen entgegen"
        },
        "line.5": {
            "text": "die hinterm Haus zum Nonnenwald hin"
        },
        "line.6": {
            "text": "in Büscheln schweigen"
        },
        "line.7": {
            "text": "wir schnüffeln die Krume"
        },
        "line.8": {
            "text": "aus sehen Mulle"
        },
        "line.9": {
            "text": "in der zweihörnigen Gebärmutter keimen"
        },
        "line.10": {
            "text": "Käferlarven reifen Engerlinge"
        },
        "line.11": {
            "text": "bersten Würmer"
        },
        "line.12": {
            "text": "anschwillt das Pochen winziger Herzen"
        },
        "line.13": {
            "text": "und Zellen heißes hungriges Schlingen"
        },
        "line.14": {
            "text": "wir ticken wir schlingen ja selber!"
        },
        "line.15": {
            "text": "Wurzeln Fasern Organe"
        },
        "line.16": {
            "text": "fremde gesottene Zungen"
        }
    }
}