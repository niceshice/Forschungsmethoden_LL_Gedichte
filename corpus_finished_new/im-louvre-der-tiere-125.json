{
    "title": "im-louvre-der-tiere-125",
    "gender": "m",
    "author": "Lutz Seiler",
    "author_birth": "1963",
    "poem": {
        "line.1": {
            "text": "...espenlaub gab es, trocken brot, ka-"
        },
        "line.2": {
            "text": "priziös verteilt um das fleisch der tiere, war es"
        },
        "line.3": {
            "text": "ihre schnellebigkeit, die uns vor die füße sank, so sprachen"
        },
        "line.4": {
            "text": "wir nichts, fast um zu sagen, beständig, ruinös, im geschirr"
        },
        "line.5": {
            "text": "ihres blutes, innen hervor: den kessel zu heizen"
        },
        "line.6": {
            "text": "die därme zu spülen, die nachricht"
        },
        "line.7": {
            "text": "des wellfleischs"
        },
        "line.8": {
            "text": "ins dorf zu tragen, das"
        },
        "line.9": {
            "text": "war die chaussee des fleischbeschauers"
        },
        "line.10": {
            "text": "und dort"
        },
        "line.11": {
            "text": "lag sein werkzeug, die schabeglocken"
        },
        "line.12": {
            "text": "der hilfsmotor... nachlaß"
        },
        "line.13": {
            "text": "mühsam gerittener korrespondenzen"
        },
        "line.14": {
            "text": "vor und zurück der zeitregale, wie"
        },
        "line.15": {
            "text": "man einen stein begreift, betonen wir jetzt"
        },
        "line.16": {
            "text": "seine abwesenheit"
        },
        "line.17": {
            "text": "sein glückloses zittern, keinen satz"
        },
        "line.18": {
            "text": "(zu ende gesprochen)"
        },
        "line.19": {
            "text": "von kindsbeinen sau mit stotterndem scheitel"
        },
        "line.20": {
            "text": "mit stockendem sattel"
        },
        "line.21": {
            "text": "bis dahinaus (wer"
        },
        "line.22": {
            "text": "so glücklos zittert und weggeht)"
        },
        "line.23": {
            "text": "hat das zeug zum roman"
        },
        "line.24": {
            "text": "hat die leere"
        },
        "line.25": {
            "text": "von gemäßigtem licht, allein"
        },
        "line.26": {
            "text": "sind wir nichts, frau koberski"
        },
        "line.27": {
            "text": "und allein frau koberski"
        },
        "line.28": {
            "text": "vermochte"
        },
        "line.29": {
            "text": "den schulterschluß mit sehnsucht, das"
        },
        "line.30": {
            "text": "schulterzucken nach"
        },
        "line.31": {
            "text": "alten melodien (jetzt hätte sie"
        },
        "line.32": {
            "text": "gern alles gesagt"
        },
        "line.33": {
            "text": "über das leben des fleischbeschauers) jetzt"
        },
        "line.34": {
            "text": "zeigte sie"
        },
        "line.35": {
            "text": "photographien; ein angeborenes strandhotel..."
        },
        "line.36": {
            "text": "die fußstapfen uns fremder vögel..."
        },
        "line.37": {
            "text": "es müssen mehrere gewesen sein..."
        },
        "line.38": {
            "text": "wir wissen das nicht, wir selbst"
        },
        "line.39": {
            "text": "fühlen uns"
        },
        "line.40": {
            "text": "vom datum verstoßen, vernichtet, schlecht bestrumpft"
        },
        "line.41": {
            "text": "unglaublich sind unsere zweifel"
        },
        "line.42": {
            "text": "unglaublich ist seine blässe... ein feines blinken"
        },
        "line.43": {
            "text": "im kern seines gehens"
        },
        "line.44": {
            "text": "hätte verkündet... allein"
        },
        "line.45": {
            "text": "er gehe allein, doch"
        },
        "line.46": {
            "text": "nicht wirklich allein"
        },
        "line.47": {
            "text": "im geräusch einer fliege"
        },
        "line.48": {
            "text": "die sich entfernt"
        },
        "line.49": {
            "text": "gleich gehöften"
        },
        "line.50": {
            "text": "im rauschen der chaussee"
        },
        "line.51": {
            "text": "quer durch das rauschen der gehöfte"
        },
        "line.52": {
            "text": "wir wissen, wir sagten;"
        },
        "line.53": {
            "text": "sei er auch"
        },
        "line.54": {
            "text": "nicht wirklich krank gewesen"
        },
        "line.55": {
            "text": "wirklich traurig sei er gewesen"
        },
        "line.56": {
            "text": "nach allem, was geschehen ist"
        },
        "line.57": {
            "text": "krank und traurig"
        },
        "line.58": {
            "text": "wie das fleisch auf dem tisch"
        },
        "line.59": {
            "text": "wie das fleisch"
        },
        "line.60": {
            "text": "der letzten großen probetiere"
        },
        "line.61": {
            "text": "unter den microskopen kreist, sei er"
        },
        "line.62": {
            "text": "noch einmal aufgebrochen"
        },
        "line.63": {
            "text": "aus der dauer seines jahrhundert-"
        },
        "line.64": {
            "text": "und jahrhunderte währenden flugs"
        },
        "line.65": {
            "text": "vom boden seines handwerks"
        },
        "line.66": {
            "text": "ins innere des specks"
        }
    }
}