{
    "title": "ich-bin-so-voll-erregung-4084",
    "gender": "m",
    "author": "Norbert C. Kaser",
    "author_birth": "1947",
    "poem": {
        "line.1": {
            "text": "ich bin so voll erregung"
        },
        "line.2": {
            "text": "daß ich zu zerspringen"
        },
        "line.3": {
            "text": "angst habe"
        },
        "line.4": {
            "text": "wie ein wasserglas"
        },
        "line.5": {
            "text": "& auszulaufen zu"
        },
        "line.6": {
            "text": "versickern in den klaffenden"
        },
        "line.7": {
            "text": "spalten des bodens"
        },
        "line.8": {
            "text": "der frisch"
        },
        "line.9": {
            "text": "gescheuert ist"
        },
        "line.10": {
            "text": "mit sand & asche"
        },
        "line.11": {
            "text": "& wieder ergreift erregung"
        },
        "line.12": {
            "text": "mich"
        },
        "line.13": {
            "text": "daß etwas geschieht"
        },
        "line.14": {
            "text": "zwischen eulen"
        },
        "line.15": {
            "text": "& spritzendem lack"
        },
        "line.16": {
            "text": "daß mir die schuhe"
        },
        "line.17": {
            "text": "zerfallen"
        },
        "line.18": {
            "text": "in rinder"
        },
        "line.19": {
            "text": "03-09-70"
        }
    }
}