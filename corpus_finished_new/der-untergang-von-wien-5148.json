{
    "title": "der-untergang-von-wien-5148",
    "gender": "m",
    "author": "Franzobel",
    "author_birth": "1967",
    "poem": {
        "line.1": {
            "text": "100.000 Wiener lassen es sich länger nicht mehr bieten,"
        },
        "line.2": {
            "text": "wollen endlich etwas tun,"
        },
        "line.3": {
            "text": "100.000 Wiener protestieren öffentlich,"
        },
        "line.4": {
            "text": "und rufen ihre Freunde an,"
        },
        "line.5": {
            "text": "sagen, dass es eine Schande, eine Frechheit ist."
        },
        "line.6": {
            "text": "100.000 Wiener wollen nun den Krieg,"
        },
        "line.7": {
            "text": "ihrer Feinde vollständige Zertrümmerung."
        },
        "line.8": {
            "text": "100.000 Wiener haben sich um und kundgetan"
        },
        "line.9": {
            "text": "wie unterschrieben:"
        },
        "line.10": {
            "text": "Die große Hundstrümmerl-Petition!"
        },
        "line.11": {
            "text": "100.000 Wiener fordern der Verkotung Wiens durch Hunde,"
        },
        "line.12": {
            "text": "der fortgeschrittenen Versinkung und Verstinkung"
        },
        "line.13": {
            "text": "Wiens in und durch und mit Hundescheiße"
        },
        "line.14": {
            "text": "sowie der gesamten beschissenen hündischen"
        },
        "line.15": {
            "text": "Verscheißerung und Verhundstrümmelung"
        },
        "line.16": {
            "text": "dringendst Einhalt zu gebieten,"
        },
        "line.17": {
            "text": "weil es selbstverständlich ist in Wien,"
        },
        "line.18": {
            "text": "dass Hunde überall, wo sie gerade müssen,"
        },
        "line.19": {
            "text": "dürfen, überall man arme Kreaturen"
        },
        "line.20": {
            "text": "auch wenn man gar nicht hinsehen will,"
        },
        "line.21": {
            "text": "Schläuche aus den Därmen pressen sehen muss,"
        },
        "line.22": {
            "text": "während um Verordnungen und Sackerl"
        },
        "line.23": {
            "text": "sich in Wien nie nicht jemand kümmert,"
        },
        "line.24": {
            "text": "was die Stadt als solche zwar sympathisch macht,"
        },
        "line.25": {
            "text": "die 100.000 Wiener aber so erbost,"
        },
        "line.26": {
            "text": "dass sie dafür auf die Strasse gehen würden,"
        },
        "line.27": {
            "text": "wenn die nicht zugeschissen wäre"
        },
        "line.28": {
            "text": "vollständig"
        },
        "line.29": {
            "text": "mit dem von ihnen so verhassten Hundekot."
        },
        "line.30": {
            "text": "Sonst aber geht es ihnen gut."
        }
    }
}