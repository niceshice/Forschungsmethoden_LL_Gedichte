{
    "title": "d-zug-muenchen-frankfurt-3811",
    "gender": "m",
    "author": "Günter Eich",
    "author_birth": "1907",
    "poem": {
        "line.1": {
            "text": "Die Donaubrücke von Ingolstadt,"
        },
        "line.2": {
            "text": "Das Altmühltal, Schiefer bei Solnhofen,"
        },
        "line.3": {
            "text": "in Treuchtlingen Anschlußzüge"
        },
        "line.4": {
            "text": "Dazwischen"
        },
        "line.5": {
            "text": "Wälder, worin der Herbst verbrannt wird,"
        },
        "line.6": {
            "text": "Landstraßen in den Schmerz,"
        },
        "line.7": {
            "text": "Gewölk, das an Gespräche erinnert,"
        },
        "line.8": {
            "text": "flüchtige Dörfer, von meinem Wunsch erbaut,"
        },
        "line.9": {
            "text": "in der Nähe deiner Stimme zu altern."
        },
        "line.10": {
            "text": "Zwischen den Ziffern der Abfahrtszeiten"
        },
        "line.11": {
            "text": "breiten sich die Besitztümer unserer Liebe aus."
        },
        "line.12": {
            "text": "Ungetrennt"
        },
        "line.13": {
            "text": "bleiben darin die Orte der Welt,"
        },
        "line.14": {
            "text": "nicht vermessen und unauffindbar."
        },
        "line.15": {
            "text": "Der Zug aber"
        },
        "line.16": {
            "text": "treibt an Gunzenhausen und Ansbach"
        },
        "line.17": {
            "text": "und an Mondlandschaften der Erinnerung"
        },
        "line.18": {
            "text": "- der sommerlich gewesene Gesang"
        },
        "line.19": {
            "text": "der Frösche von Ornbau -"
        },
        "line.20": {
            "text": "vorbei."
        }
    }
}