{
    "title": "taubenflug-1800",
    "gender": "w",
    "author": "Isabeella Beumer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "taubenflug"
        },
        "line.2": {
            "text": "schnitte ins logbuch gehorcht"
        },
        "line.3": {
            "text": "OHREN AMBOSZ"
        },
        "line.4": {
            "text": "formen ausgestanzter"
        },
        "line.5": {
            "text": "glutnester"
        },
        "line.6": {
            "text": "und sehe wie er mich anschaut"
        },
        "line.7": {
            "text": "die innenseite freigegeben"
        },
        "line.8": {
            "text": "messernotiz"
        },
        "line.9": {
            "text": "die naht der blattadern"
        },
        "line.10": {
            "text": "im SCHLAGSCHATTEN deines körpers"
        },
        "line.11": {
            "text": "liegen"
        },
        "line.12": {
            "text": "andeuten"
        },
        "line.13": {
            "text": "darüberlecken"
        },
        "line.14": {
            "text": "die haut zu atem tragen"
        },
        "line.15": {
            "text": "muskelstück"
        },
        "line.16": {
            "text": "umkreisung"
        },
        "line.17": {
            "text": "flattern"
        },
        "line.18": {
            "text": "ich hebe es auf"
        },
        "line.19": {
            "text": "eine hand voll herzrhythmen"
        },
        "line.20": {
            "text": "zunge - mund - finger und sprache"
        },
        "line.21": {
            "text": "in blutzarte häutchen"
        },
        "line.22": {
            "text": "weiszer flügelsaft"
        },
        "line.23": {
            "text": "abgepackte wunder"
        },
        "line.24": {
            "text": "eine sekunde als tag"
        },
        "line.25": {
            "text": "mehrsprachiges schweigen"
        },
        "line.26": {
            "text": "hautlasuren"
        },
        "line.27": {
            "text": "von speichel und bewegung"
        },
        "line.28": {
            "text": "abgerissene plakatreste"
        },
        "line.29": {
            "text": "aus gestern"
        }
    }
}