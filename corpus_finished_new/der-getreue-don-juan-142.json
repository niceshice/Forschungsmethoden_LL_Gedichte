{
    "title": "der-getreue-don-juan-142",
    "gender": "m",
    "author": "Peter Rühmkorf",
    "author_birth": "1929",
    "poem": {
        "line.1": {
            "text": "Keinen Grund gefunden habend,"
        },
        "line.2": {
            "text": "kein"
        },
        "line.3": {
            "text": "Warum"
        },
        "line.4": {
            "text": "? Und kein"
        },
        "line.5": {
            "text": "Voran"
        },
        "line.6": {
            "text": "!"
        },
        "line.7": {
            "text": "Aber so und so heut abend"
        },
        "line.8": {
            "text": "kommen wir und sehn uns an."
        },
        "line.9": {
            "text": "In den letzten Kunstlachsröten"
        },
        "line.10": {
            "text": "dreimal ums Karree gelatscht;"
        },
        "line.11": {
            "text": "keinen Lebensbund zu löten,"
        },
        "line.12": {
            "text": "nichts! das Lied ist ausgebratscht."
        },
        "line.13": {
            "text": "Gib die Königinpastetchen,"
        },
        "line.14": {
            "text": "nimm mein schräges Flageolett. . ."
        },
        "line.15": {
            "text": "Ewigkeitsmusik, mein Mädchen,"
        },
        "line.16": {
            "text": "macht den Glauben auch nicht fett."
        },
        "line.17": {
            "text": "Brandy-Cola nachgetrichtert,"
        },
        "line.18": {
            "text": "schön, bis Faß und Korken tanzt!"
        },
        "line.19": {
            "text": "Aufgeklärt und ausgenüchtert"
        },
        "line.20": {
            "text": "kuckt die Welt aus Blei gestanzt."
        },
        "line.21": {
            "text": "Komm, die Füße auf den Ecktisch,"
        },
        "line.22": {
            "text": "häng dich aus ins Einerlei,"
        },
        "line.23": {
            "text": "unfreiwillig dialektisch"
        },
        "line.24": {
            "text": "schwabbelnd zwischen Form und Brei."
        },
        "line.25": {
            "text": "Einfach nur mal so heut abend,"
        },
        "line.26": {
            "text": "kein"
        },
        "line.27": {
            "text": "Woher"
        },
        "line.28": {
            "text": "? Und kein"
        },
        "line.29": {
            "text": "Voran"
        },
        "line.30": {
            "text": "!"
        },
        "line.31": {
            "text": "Ausflucht suchend sich vergrabend:"
        },
        "line.32": {
            "text": "Dein  getreuer  Don Juan."
        }
    }
}