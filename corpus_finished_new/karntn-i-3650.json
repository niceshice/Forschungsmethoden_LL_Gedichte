{
    "title": "karntn-i-3650",
    "gender": "m",
    "author": "Axel  Karner",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "i bin a karntna"
        },
        "line.2": {
            "text": "hob i gedocht"
        },
        "line.3": {
            "text": "i war a karntna"
        },
        "line.4": {
            "text": "homs gsogt"
        },
        "line.5": {
            "text": "lei diafat i nit"
        },
        "line.6": {
            "text": "auf mei hamat schimpfn"
        },
        "line.7": {
            "text": "nit mit de redn"
        },
        "line.8": {
            "text": "dera sproch i eh nit vasteah"
        },
        "line.9": {
            "text": "nit olleweil"
        },
        "line.10": {
            "text": "des maul aufreißn"
        },
        "line.11": {
            "text": "fia eppes"
        },
        "line.12": {
            "text": "wos mi nix ongeaht"
        },
        "line.13": {
            "text": "weil"
        },
        "line.14": {
            "text": "schließlich war i a karntna"
        },
        "line.15": {
            "text": "wohl duat gebuan"
        },
        "line.16": {
            "text": "sunst oba nix"
        },
        "line.17": {
            "text": "- - - - -"
        },
        "line.18": {
            "text": "kärnten I"
        },
        "line.19": {
            "text": "ich bin ein kärntner"
        },
        "line.20": {
            "text": "habe ich gedacht"
        },
        "line.21": {
            "text": "ich wäre ein kärntner"
        },
        "line.22": {
            "text": "haben sie gesagt"
        },
        "line.23": {
            "text": "nur dürfte ich nicht"
        },
        "line.24": {
            "text": "auf meine heimat schimpfen"
        },
        "line.25": {
            "text": "nicht mit jenen reden"
        },
        "line.26": {
            "text": "deren sprache ich nicht verstehe"
        },
        "line.27": {
            "text": "nicht immer"
        },
        "line.28": {
            "text": "das maul aufreißen"
        },
        "line.29": {
            "text": "für etwas"
        },
        "line.30": {
            "text": "das mich nichts angeht"
        },
        "line.31": {
            "text": "weil"
        },
        "line.32": {
            "text": "schließlich wäre ich ein kärntner"
        },
        "line.33": {
            "text": "wohl dort geboren"
        },
        "line.34": {
            "text": "sonst aber nichts"
        }
    }
}