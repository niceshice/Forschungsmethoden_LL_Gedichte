{
    "title": "am-geirangerfjord-8408",
    "gender": "w",
    "author": "Doris Runge",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "sieben schwestern"
        },
        "line.2": {
            "text": "ein freier"
        },
        "line.3": {
            "text": "ein brautschleier"
        },
        "line.4": {
            "text": "was soll das werden"
        },
        "line.5": {
            "text": "eine liebe"
        },
        "line.6": {
            "text": "aus schwindel"
        },
        "line.7": {
            "text": "erregender höhe"
        },
        "line.8": {
            "text": "in schluchzende"
        },
        "line.9": {
            "text": "tiefe stürzen"
        },
        "line.10": {
            "text": "die schwestern"
        },
        "line.11": {
            "text": "in den eiskalten"
        },
        "line.12": {
            "text": "den schönsten"
        },
        "line.13": {
            "text": "aller wasserarme"
        },
        "line.14": {
            "text": "was sind das"
        },
        "line.15": {
            "text": "für wilde wasser"
        },
        "line.16": {
            "text": "spiele wer spielt"
        },
        "line.17": {
            "text": "mit wem wer wird"
        },
        "line.18": {
            "text": "erhört wer trägt"
        },
        "line.19": {
            "text": "den schleier"
        },
        "line.20": {
            "text": "er teilte den felsen"
        },
        "line.21": {
            "text": "mit großer kraft"
        },
        "line.22": {
            "text": "er sammelt sehnsucht"
        },
        "line.23": {
            "text": "nach liebe für sein echo"
        },
        "line.24": {
            "text": "er wäscht den schönen"
        },
        "line.25": {
            "text": "das haar er trägt die"
        },
        "line.26": {
            "text": "sieben wie schaum"
        },
        "line.27": {
            "text": "auf den lippen"
        },
        "line.28": {
            "text": "er nimmt"
        },
        "line.29": {
            "text": "ihre herzen mit"
        },
        "line.30": {
            "text": "ins offene meer"
        },
        "line.31": {
            "text": "so singt"
        },
        "line.32": {
            "text": "die jüngste"
        },
        "line.33": {
            "text": "so singt"
        },
        "line.34": {
            "text": "die unerhörte"
        },
        "line.35": {
            "text": "liebe"
        },
        "line.36": {
            "text": "ein felsen"
        },
        "line.37": {
            "text": "hoffnung"
        },
        "line.38": {
            "text": "dem himmel"
        },
        "line.39": {
            "text": "so nah und"
        },
        "line.40": {
            "text": "dem tiefsten"
        },
        "line.41": {
            "text": "grund"
        },
        "line.42": {
            "text": "sie singt"
        },
        "line.43": {
            "text": "für den liebsten"
        },
        "line.44": {
            "text": "kämmt sie"
        },
        "line.45": {
            "text": "das haar"
        },
        "line.46": {
            "text": "binde mich"
        },
        "line.47": {
            "text": "binde dich"
        },
        "line.48": {
            "text": "mich bindet nichts"
        },
        "line.49": {
            "text": "sie sinkt"
        },
        "line.50": {
            "text": "und singt"
        },
        "line.51": {
            "text": "ich habe"
        },
        "line.52": {
            "text": "sechs schwestern"
        },
        "line.53": {
            "text": "die haben"
        },
        "line.54": {
            "text": "ein herz"
        },
        "line.55": {
            "text": "aus eis"
        },
        "line.56": {
            "text": "und wer"
        },
        "line.57": {
            "text": "es schmelzt"
        },
        "line.58": {
            "text": "den reißt es"
        },
        "line.59": {
            "text": "in die tiefe"
        },
        "line.60": {
            "text": "und ihre"
        },
        "line.61": {
            "text": "brautschleier"
        },
        "line.62": {
            "text": "fliegen"
        }
    }
}