{
    "title": "markt-2634",
    "gender": "w",
    "author": "Kerstin Hensel",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "Ach wie man die weltgereisten überseebunten"
        },
        "line.2": {
            "text": "Früchte anpreist: mit gewachster Haut trotzen sie"
        },
        "line.3": {
            "text": "Dem Verderb. Schneewittchen"
        },
        "line.4": {
            "text": "Schluckt was man ihm zuwirft"
        },
        "line.5": {
            "text": "Süß saftlos und nichts"
        },
        "line.6": {
            "text": "bleibt stecken im Hals. Wie gemalt"
        },
        "line.7": {
            "text": "Glänzt das Obst und kann sich nicht riechen. Nur ich"
        },
        "line.8": {
            "text": "Lieg angeschlagen unter dem Tisch"
        },
        "line.9": {
            "text": "Des Händlers"
        },
        "line.10": {
            "text": "Und stink ihm."
        }
    }
}