    "poem": {
        "line.1": {
            "text": "Ging wochenlang"
        },
        "line.2": {
            "text": "im Kreis. Immer"
        },
        "line.3": {
            "text": "nachmittags."
        },
        "line.4": {
            "text": "Kam gestern ans Tor"
        },
        "line.5": {
            "text": "sagte, dass er sich wieder"
        },
        "line.6": {
            "text": "vorstellen k√∂nne:"
        },
        "line.7": {
            "text": "Einen Menschen"
        },
        "line.8": {
            "text": "aus Staub geformt."
        }
    }
}