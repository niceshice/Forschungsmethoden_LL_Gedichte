{
    "title": "ein-gedicht-entsteht-85",
    "gender": "w",
    "author": "Ursula Krechel",
    "author_birth": "1947",
    "poem": {
        "line.1": {
            "text": "Verschwunden war jeglicher Begriff von Arbeit"
        },
        "line.2": {
            "text": "wörterbücherwarm stand eine Sonne hoch"
        },
        "line.3": {
            "text": "hebelte den Schatten aus."
        },
        "line.4": {
            "text": "In dünnen Linien fielen Zaunlatten"
        },
        "line.5": {
            "text": "musterhaft nie ins Gewicht. Kein Wort fiel"
        },
        "line.6": {
            "text": "war fallengelassen worden. Glück war, blieb"
        },
        "line.7": {
            "text": "Honig saugen, blanke Augen."
        }
    }
}