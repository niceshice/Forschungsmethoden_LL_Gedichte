{
    "title": "was-brauchst-du-zu-wachsen-11529",
    "gender": "w",
    "author": "Ulrike Draesner",
    "author_birth": "1962",
    "poem": {
        "line.1": {
            "text": "wenn du mich in den dichteren wald"
        },
        "line.2": {
            "text": "schiebst"
        },
        "line.3": {
            "text": "wird alles biberhaft genagt aber"
        },
        "line.4": {
            "text": "sicher"
        },
        "line.5": {
            "text": "und ich spreche in die kupferne schale"
        },
        "line.6": {
            "text": "um zu hören wer wir sind"
        },
        "line.7": {
            "text": "saturn schickt seine ringe"
        },
        "line.8": {
            "text": "bloß"
        },
        "line.9": {
            "text": "der normale mensch ist jetzt betäubt"
        },
        "line.10": {
            "text": "ich aber falle in die abendstunde"
        },
        "line.11": {
            "text": "und werde dein späterer geist"
        },
        "line.12": {
            "text": "denn auch wir hören vögel die es sichtbar"
        },
        "line.13": {
            "text": "nicht gibt"
        },
        "line.14": {
            "text": "und haben überall öffnungen für zeiten und"
        },
        "line.15": {
            "text": "übungen der trance"
        },
        "line.16": {
            "text": "die ein anderer dem alternden körper"
        },
        "line.17": {
            "text": "schenkt"
        },
        "line.18": {
            "text": "ganz beziehungsorgan wie man immer"
        },
        "line.19": {
            "text": "dem kind hielt die hand bei"
        },
        "line.20": {
            "text": "eingriffen"
        },
        "line.21": {
            "text": "egal wer die verlorenen die aus den augen"
        },
        "line.22": {
            "text": "die nicht gekannten nur"
        },
        "line.23": {
            "text": "gestreiften menschen waren der biberwald"
        },
        "line.24": {
            "text": "der see und wie eingeht und aus"
        },
        "line.25": {
            "text": "die abhängigkeit"
        },
        "line.26": {
            "text": "der betäubung vom kopf"
        },
        "line.27": {
            "text": "das alleinsein"
        },
        "line.28": {
            "text": "in dem immer wieder atmete"
        },
        "line.29": {
            "text": "schwankend ungehalten"
        },
        "line.30": {
            "text": "gemischt und in mir"
        },
        "line.31": {
            "text": "der pechvogel"
        },
        "line.32": {
            "text": "(auch nager, biber"
        },
        "line.33": {
            "text": "am knochen)"
        },
        "line.34": {
            "text": "als die"
        },
        "line.35": {
            "text": "gebrechen"
        },
        "line.36": {
            "text": "begannen"
        },
        "line.37": {
            "text": "die erinnerung an konfetti und scherz"
        },
        "line.38": {
            "text": "und immer wieder die tröstlichen häher"
        },
        "line.39": {
            "text": "abnäher der sprache: offen im rücken"
        },
        "line.40": {
            "text": "das krankenhaushemd die sorgfalt des druckes"
        },
        "line.41": {
            "text": "auf den stoff (menschenstoff): sorgfalt"
        },
        "line.42": {
            "text": "und ohr"
        },
        "line.43": {
            "text": "(leises biberkauen) sie stechen"
        },
        "line.44": {
            "text": "und suchen"
        },
        "line.45": {
            "text": "überall"
        },
        "line.46": {
            "text": "wenn sie mich in den dichten dunkleren"
        },
        "line.47": {
            "text": "forst schieben die kleinere schrift"
        },
        "line.48": {
            "text": "(nicht zaghaft doch jagt und jammert"
        },
        "line.49": {
            "text": "der puls) fallen mich"
        },
        "line.50": {
            "text": "an den erzeugten (nicht"
        },
        "line.51": {
            "text": "gerissenen)"
        },
        "line.52": {
            "text": "hörenden sorgenden stellen"
        },
        "line.53": {
            "text": "suchen und finden als"
        },
        "line.54": {
            "text": "wäre gesundheit der entlaufene (fröhlich"
        },
        "line.55": {
            "text": "springende hund)"
        },
        "line.56": {
            "text": "als er noch nicht diesen saum hatte das alter"
        },
        "line.57": {
            "text": "(die leber)  und ich"
        },
        "line.58": {
            "text": "mich"
        },
        "line.59": {
            "text": "weder ein noch aus als fragment sah"
        },
        "line.60": {
            "text": "(und das mit posaune) - schau:"
        },
        "line.61": {
            "text": "ich schreib"
        },
        "line.62": {
            "text": "langsam und halte das ohr (von unten"
        },
        "line.63": {
            "text": "vom außen des sees) in die immer wieder"
        },
        "line.64": {
            "text": "tauchende liebe"
        },
        "line.65": {
            "text": "(sich färbende?, ungehaltene?"
        },
        "line.66": {
            "text": "steine am ufer)"
        },
        "line.67": {
            "text": "und weiß"
        },
        "line.68": {
            "text": "was ich denke weiß nicht"
        },
        "line.69": {
            "text": "ich. morgen im wald, morgen im tau"
        },
        "line.70": {
            "text": "dehnen backsteine gleise sich"
        },
        "line.71": {
            "text": "werden andere hälse betäubt und ein kind"
        },
        "line.72": {
            "text": "das meines ist sagt, bist nicht untergegluckert"
        },
        "line.73": {
            "text": "bist. ob (ich) ihn ziehe"
        },
        "line.74": {
            "text": "den wandel, den wald? morgen"
        },
        "line.75": {
            "text": "lichter morgen ein anker"
        },
        "line.76": {
            "text": "altes geräusch"
        },
        "line.77": {
            "text": "was brauchst du um"
        },
        "line.78": {
            "text": "mit 50 noch einmal"
        },
        "line.79": {
            "text": "zu wachsen?"
        },
        "line.80": {
            "text": "zuwendung (wendung innere wände"
        },
        "line.81": {
            "text": "zu beruhigen"
        },
        "line.82": {
            "text": "die kappen)"
        },
        "line.83": {
            "text": "obwohl wörter wieder erscheinen das mehrdeutige"
        },
        "line.84": {
            "text": "verlust"
        },
        "line.85": {
            "text": "der ragende fühler ärger"
        },
        "line.86": {
            "text": "das bild einer ziege mit hörnern"
        },
        "line.87": {
            "text": "in grün umbraut von einem bart"
        },
        "line.88": {
            "text": "gehütet von dem kind das du warst um dich"
        },
        "line.89": {
            "text": "zu entwanden"
        },
        "line.90": {
            "text": "(dort, wo du klein bliebst:"
        },
        "line.91": {
            "text": "alt"
        },
        "line.92": {
            "text": "rosa"
        },
        "line.93": {
            "text": "gefleckt) was brauchst du"
        },
        "line.94": {
            "text": "um zuzulegen"
        },
        "line.95": {
            "text": "hölzchen zu türmen"
        },
        "line.96": {
            "text": "und zu nehmen um hölzer zu ziehen"
        },
        "line.97": {
            "text": "mit all"
        },
        "line.98": {
            "text": "deinem"
        },
        "line.99": {
            "text": "geschick"
        },
        "line.100": {
            "text": "auf dass abhanden komme"
        },
        "line.101": {
            "text": "auf dass aus den händen falle"
        },
        "line.102": {
            "text": "das auf dich zukommende"
        },
        "line.103": {
            "text": "nocturne (wie willst du  und an einer kirche"
        },
        "line.104": {
            "text": "unter einem baum) in asche?"
        },
        "line.105": {
            "text": "in einem"
        },
        "line.106": {
            "text": "gefäß?"
        },
        "line.107": {
            "text": "und den knappen siehst die ziege"
        },
        "line.108": {
            "text": "führen die gesenkten gesichts"
        },
        "line.109": {
            "text": "am hanfseil trabt"
        },
        "line.110": {
            "text": "ihren hanffarbenen körper einer wolke"
        },
        "line.111": {
            "text": "gleicht nur"
        },
        "line.112": {
            "text": "das kupferne auge"
        },
        "line.113": {
            "text": "über den glatten"
        },
        "line.114": {
            "text": "sich wölbenden steinen"
        },
        "line.115": {
            "text": "blinkend"
        },
        "line.116": {
            "text": "des grats"
        },
        "line.117": {
            "text": "geplünderter"
        },
        "line.118": {
            "text": "chopin, gespielt mit der hopsenden"
        },
        "line.119": {
            "text": "furchtsamen hand"
        },
        "line.120": {
            "text": "was brauchst du um zu wachsen"
        },
        "line.121": {
            "text": "dir selbst ans herz"
        },
        "line.122": {
            "text": "gewachsen"
        },
        "line.123": {
            "text": "allmählich zieglich"
        },
        "line.124": {
            "text": "zufrieden"
        },
        "line.125": {
            "text": "(bei dir) zu sein"
        },
        "line.126": {
            "text": "brauchst du während noch immer"
        },
        "line.127": {
            "text": "draußen (die betäubung sich"
        },
        "line.128": {
            "text": "hebt aus dem kopf) alles fliegt"
        },
        "line.129": {
            "text": "leichte verbindungen"
        },
        "line.130": {
            "text": "clouds über dir"
        },
        "line.131": {
            "text": "(ast)"
        },
        "line.132": {
            "text": "faserchen"
        },
        "line.133": {
            "text": "geduld der luft"
        },
        "line.134": {
            "text": "angel der tür"
        },
        "line.135": {
            "text": "und später, viel später: chopins"
        },
        "line.136": {
            "text": "kleine hand"
        },
        "line.137": {
            "text": "nocturne"
        }
    }
}