{
    "title": "memoriam-paul-celan-549",
    "gender": "w",
    "author": "Rose Ausländer",
    "author_birth": "1901",
    "poem": {
        "line.1": {
            "text": "Meine blonde Mutter"
        },
        "line.2": {
            "text": "kam nicht heim"
        },
        "line.3": {
            "text": "Paul Celan"
        },
        "line.4": {
            "text": "Kam nicht heim"
        },
        "line.5": {
            "text": "die Mutter"
        },
        "line.6": {
            "text": "nie aufgegeben"
        },
        "line.7": {
            "text": "den Tod"
        },
        "line.8": {
            "text": "vom Sohn genährt"
        },
        "line.9": {
            "text": "mit Schwarzmilch"
        },
        "line.10": {
            "text": "die hielt ihn am Leben"
        },
        "line.11": {
            "text": "das ertrank"
        },
        "line.12": {
            "text": "im Tintenblut"
        },
        "line.13": {
            "text": "Zwischen verschwiegenen Zeilen"
        },
        "line.14": {
            "text": "das Nichtwort"
        },
        "line.15": {
            "text": "im Leerraum"
        },
        "line.16": {
            "text": "leuchtend"
        }
    }
}