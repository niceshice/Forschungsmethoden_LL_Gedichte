{
    "title": "wanderlust-14766",
    "gender": "w",
    "author": "Sabine  Schiffner",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "als du dich aus mir rausziehst ertönt"
        },
        "line.2": {
            "text": "das wandern ist des müllers"
        },
        "line.3": {
            "text": "lust doch ich kann gar nicht so"
        },
        "line.4": {
            "text": "lustig gucken wie du mit"
        },
        "line.5": {
            "text": "deinen schönen augen im moment"
        },
        "line.6": {
            "text": "dann schnürst du schon dein"
        },
        "line.7": {
            "text": "bündel siehst nach vorn nach hinten"
        },
        "line.8": {
            "text": "sagst ich muss jetzt ins büro"
        },
        "line.9": {
            "text": "in wirklichkeit als du das sagst"
        },
        "line.10": {
            "text": "bist du schon fort"
        },
        "line.11": {
            "text": "du schreibst mir einen brief"
        },
        "line.12": {
            "text": "auf dem computer und adressierst ihn an"
        },
        "line.13": {
            "text": "die müllerin was du nicht weißt"
        },
        "line.14": {
            "text": "auch müllerinnen haben ihren stolz"
        },
        "line.15": {
            "text": "und ihren preis"
        },
        "line.16": {
            "text": "sie warten bis das mehl"
        },
        "line.17": {
            "text": "gemahlen ist und keinen augenblick"
        },
        "line.18": {
            "text": "mehr länger auch wenn das telefon"
        },
        "line.19": {
            "text": "verbunden ist bis ins büro"
        },
        "line.20": {
            "text": "auch wenn nun briefe tröpfeln regnen"
        },
        "line.21": {
            "text": "was hält mich hier ich gehe fort"
        },
        "line.22": {
            "text": "das wäre doch gelacht"
        },
        "line.23": {
            "text": "ich bin die müllerin verträge gibt"
        },
        "line.24": {
            "text": "es nicht die liebe liebt"
        },
        "line.25": {
            "text": "das wandern gott hat sie so gemacht"
        }
    }
}