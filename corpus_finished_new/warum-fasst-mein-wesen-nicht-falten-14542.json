{
    "title": "warum-fasst-mein-wesen-nicht-falten-14542",
    "gender": "w",
    "author": "Ruth Wolf-Rehfeldt",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "Warum faßt mein Wesen nicht in Falten"
        },
        "line.2": {
            "text": "Was mir zur Vollendung fehlt"
        },
        "line.3": {
            "text": "Warum kann ich nicht im Blut behalten"
        },
        "line.4": {
            "text": "Was sich nahtlos mir vermählt"
        }
    }
}