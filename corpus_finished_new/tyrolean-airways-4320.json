{
    "title": "tyrolean-airways-4320",
    "gender": "w",
    "author": "Dagmar Leupold",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "Sounds of Silence"
        },
        "line.2": {
            "text": "steht unter den Propellern"
        },
        "line.3": {
            "text": "ein Versprecher"
        },
        "line.4": {
            "text": "kein Versprechen und"
        },
        "line.5": {
            "text": "alle Passagiere lächeln"
        },
        "line.6": {
            "text": "in die aufgerissenen"
        },
        "line.7": {
            "text": "Zeitungen hinein"
        },
        "line.8": {
            "text": "die Männer in die Börse"
        },
        "line.9": {
            "text": "die Frauen in Vermischtes"
        },
        "line.10": {
            "text": "in den Gängen"
        },
        "line.11": {
            "text": "Schwaden zu früh"
        },
        "line.12": {
            "text": "zerstreuter Träume"
        },
        "line.13": {
            "text": "Dort unten"
        },
        "line.14": {
            "text": "Wien, seine Schlösser, die Gärten"
        },
        "line.15": {
            "text": "in bestechender Geometrie"
        },
        "line.16": {
            "text": "der Friedhof voller Helden"
        }
    }
}