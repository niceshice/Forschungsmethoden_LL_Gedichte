{
    "title": "r-l-7813",
    "gender": "m",
    "author": "Dieter M. Gräf",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "werft keine roten Nelken mehr"
        },
        "line.2": {
            "text": "aufs Grab von Rosa Luxemburg, werft"
        },
        "line.3": {
            "text": "alles über Thälmann, Ulbricht, Pieck."
        },
        "line.4": {
            "text": "Verschwindet, so, in Pracht."
        },
        "line.5": {
            "text": "Den Kadern des Pergolenwegs"
        },
        "line.6": {
            "text": "die wilden Katzen Roms; werft"
        },
        "line.7": {
            "text": "keine roten Nelken mehr aufs Grab"
        },
        "line.8": {
            "text": "von Rosa Luxemburg, löscht"
        },
        "line.9": {
            "text": "ihren Namen aus der falschen Platte."
        },
        "line.10": {
            "text": "Wär nun"
        },
        "line.11": {
            "text": "Zwi-zwi"
        },
        "line.12": {
            "text": ", für immer,"
        },
        "line.13": {
            "text": "hat sichs doch so gewünscht. Sie,"
        },
        "line.14": {
            "text": "die Genossen hatte und die Vögel liebte,"
        },
        "line.15": {
            "text": "auch Transparente, ihre"
        },
        "line.16": {
            "text": "Schmetterlinge, damals schon zu sehn"
        }
    }
}