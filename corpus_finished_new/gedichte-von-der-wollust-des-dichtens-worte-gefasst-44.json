{
    "title": "gedichte-von-der-wollust-des-dichtens-worte-gefasst-44",
    "gender": "m",
    "author": "H. C. Artmann",
    "author_birth": "1921",
    "poem": {
        "line.1": {
            "text": "gedichte von der wollust"
        },
        "line.2": {
            "text": "des dichtens"
        },
        "line.3": {
            "text": "in worte gefaßt"
        },
        "line.4": {
            "text": "cagliari:"
        },
        "line.5": {
            "text": "die ameise deiner adern"
        },
        "line.6": {
            "text": "greift meine sehnsüchte"
        },
        "line.7": {
            "text": "wenn ich das beschreibe"
        },
        "line.8": {
            "text": "was ein mittag verschweigt:"
        },
        "line.9": {
            "text": "singt dich selbst caruso"
        },
        "line.10": {
            "text": "meine zeile wird besser"
        },
        "line.11": {
            "text": "im stein deines gesichts"
        },
        "line.12": {
            "text": "die natur ist sinnloser"
        },
        "line.13": {
            "text": "ein sehr blaues wasser"
        },
        "line.14": {
            "text": "setzt dir bögen cagliari"
        },
        "line.15": {
            "text": "carnac:"
        },
        "line.16": {
            "text": "viele verbrannte lebensläufe"
        },
        "line.17": {
            "text": "druiden der aschenbecher euch"
        },
        "line.18": {
            "text": "ich aber ach bin noch student"
        },
        "line.19": {
            "text": "sperling der stadt der türme"
        },
        "line.20": {
            "text": "auf einem vogelbein hocke ich"
        },
        "line.21": {
            "text": "ohne audiovisuelle erfahrung"
        },
        "line.22": {
            "text": "durch den rockärmel der magie"
        },
        "line.23": {
            "text": "spähe ich nach uralten echos"
        },
        "line.24": {
            "text": "irgendwo:"
        },
        "line.25": {
            "text": "frag mich um meinen namen"
        },
        "line.26": {
            "text": "ich will ihn dir beichten"
        },
        "line.27": {
            "text": "kein ort ist mir ähnlich:"
        },
        "line.28": {
            "text": "bin honig unter kanditen"
        },
        "line.29": {
            "text": "bin vergoldeter zuckerhut"
        },
        "line.30": {
            "text": "eigentlich bin ich biene"
        },
        "line.31": {
            "text": "wabengebilde immenhäusle"
        },
        "line.32": {
            "text": "eigentlich bin ich brief"
        },
        "line.33": {
            "text": "adresse postporto stempel"
        },
        "line.34": {
            "text": "ich antworte dir wie noah"
        },
        "line.35": {
            "text": "ein rebstock ein zauberer"
        },
        "line.36": {
            "text": "sieh ich verändere mich:"
        },
        "line.37": {
            "text": "caruso besingt mich als"
        },
        "line.38": {
            "text": "colibri turteltaube aar"
        },
        "line.39": {
            "text": "robinson schilt mich als"
        },
        "line.40": {
            "text": "natter skarabäus mikrobe"
        },
        "line.41": {
            "text": "bin eiland im weltenall"
        },
        "line.42": {
            "text": "bin kraft echter aktien"
        },
        "line.43": {
            "text": "des altai nashorn das gnu"
        },
        "line.44": {
            "text": "des ural der gobi makaki"
        },
        "line.45": {
            "text": "kein spiegel wirft mich"
        },
        "line.46": {
            "text": "kein film zeigt mich auf"
        },
        "line.47": {
            "text": "ich bin der ort nirgendwo"
        },
        "line.48": {
            "text": "jerusalem:"
        },
        "line.49": {
            "text": "du rotweinspur auf"
        },
        "line.50": {
            "text": "einem liebesbrief"
        },
        "line.51": {
            "text": "jerusalem"
        },
        "line.52": {
            "text": "jahwe behüte"
        },
        "line.53": {
            "text": "hast keine lackstiefel nötig"
        },
        "line.54": {
            "text": "um allahs zehen"
        },
        "line.55": {
            "text": "noch jesu überbein"
        },
        "line.56": {
            "text": "zu verbergensag an:"
        },
        "line.57": {
            "text": "wie gibst du dich"
        },
        "line.58": {
            "text": "in deinem abendlicht"
        },
        "line.59": {
            "text": "nachdem du den tag"
        },
        "line.60": {
            "text": "mit mandelseifen"
        },
        "line.61": {
            "text": "weitgehend entfernt hast?"
        },
        "line.62": {
            "text": "jerusalem"
        },
        "line.63": {
            "text": "du rotweinspur auf"
        },
        "line.64": {
            "text": "einem liebesbrief"
        },
        "line.65": {
            "text": "deine taxis und busse"
        },
        "line.66": {
            "text": "sind klarerweise"
        },
        "line.67": {
            "text": "mit dem namen gottes"
        },
        "line.68": {
            "text": "dreier weltreligionen"
        },
        "line.69": {
            "text": "durchnumeriert"
        },
        "line.70": {
            "text": "wer würde dich"
        },
        "line.71": {
            "text": "der dich nie gesehen"
        },
        "line.72": {
            "text": "nicht doch vermissen und"
        },
        "line.73": {
            "text": "deine winkel gäßchen gassen"
        },
        "line.74": {
            "text": "über dem stadtplan"
        },
        "line.75": {
            "text": "nachweinen?"
        },
        "line.76": {
            "text": "lisdoonvarna:"
        },
        "line.77": {
            "text": "die oboe ist der weißdorn"
        },
        "line.78": {
            "text": "unter den instrumenten"
        },
        "line.79": {
            "text": "die menschliche poesie aber"
        },
        "line.80": {
            "text": "der meister der tagebücher"
        },
        "line.81": {
            "text": "wenn der westwind streift"
        },
        "line.82": {
            "text": "im sommer der gräser geäst"
        },
        "line.83": {
            "text": "weht er musik mir her"
        },
        "line.84": {
            "text": "darauf die schrift steht"
        },
        "line.85": {
            "text": "beobachteter handflächen"
        },
        "line.86": {
            "text": "taprobane:"
        },
        "line.87": {
            "text": "wenn die kindheit fertig wird"
        },
        "line.88": {
            "text": "nimmt sie die tischtücher"
        },
        "line.89": {
            "text": "und faltet sie nett zusammen:"
        },
        "line.90": {
            "text": "maritime karten für kommende"
        },
        "line.91": {
            "text": "fort-reisen in meinen eigenen"
        },
        "line.92": {
            "text": "augen die nicht anderer leute"
        },
        "line.93": {
            "text": "fixsterne oder fenster sind"
        },
        "line.94": {
            "text": "sondern flugbares im wind"
        },
        "line.95": {
            "text": "bewegliches zwischen"
        },
        "line.96": {
            "text": "den beiden starren polen"
        },
        "line.97": {
            "text": "die mich einfassen wie ein"
        },
        "line.98": {
            "text": "links und rechts"
        },
        "line.99": {
            "text": "zielort:"
        },
        "line.100": {
            "text": "aus einem völligen durcheinander"
        },
        "line.101": {
            "text": "erscheint mir dein sauberes bild:"
        },
        "line.102": {
            "text": "ovales blättchen oder milchobjekt"
        },
        "line.103": {
            "text": "ein mittelpunkteine pille wohl"
        },
        "line.104": {
            "text": "ein augeninneres könnte es sein"
        },
        "line.105": {
            "text": "wie ich es eben drehees beruht"
        },
        "line.106": {
            "text": "noch ehe berührung zustande kommt"
        },
        "line.107": {
            "text": "flimmert bereits die ankunftszeit"
        },
        "line.108": {
            "text": "meines betastetseinsin ziffern"
        },
        "line.109": {
            "text": "in der tiefe seines bauches"
        },
        "line.110": {
            "text": "nach wörteralgen taucht der dichter"
        },
        "line.111": {
            "text": "am weißen strand des papieres"
        },
        "line.112": {
            "text": "spreitet er sie zum trocknen aus"
        },
        "line.113": {
            "text": "es wird gebeten das seegras nicht"
        },
        "line.114": {
            "text": "vor seiner zeit zu wendendanke"
        }
    }
}