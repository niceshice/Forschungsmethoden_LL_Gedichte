{
    "title": "kein-fehler-im-system-3-2916",
    "gender": "m",
    "author": "Eugen  Gomringer",
    "author_birth": "1925",
    "poem": {
        "line.1": {
            "text": "kein system im fehler"
        },
        "line.2": {
            "text": "kein system mir fehle"
        },
        "line.3": {
            "text": "keiner fehl im system"
        },
        "line.4": {
            "text": "keim in systemfehler"
        },
        "line.5": {
            "text": "sein kystem im fehler"
        },
        "line.6": {
            "text": "ein fehkler im system"
        },
        "line.7": {
            "text": "seine kehl im fyrsten"
        },
        "line.8": {
            "text": "ein symfehler im sekt"
        },
        "line.9": {
            "text": "kein symmet is fehler"
        },
        "line.10": {
            "text": "sey festh kleinr mime"
        }
    }
}