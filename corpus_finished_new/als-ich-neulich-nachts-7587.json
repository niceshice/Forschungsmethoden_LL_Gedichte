{
    "title": "als-ich-neulich-nachts-7587",
    "gender": "m",
    "author": "Christian  Filips",
    "author_birth": "1981",
    "poem": {
        "line.1": {
            "text": "an die Wand gedrückt, wieder"
        },
        "line.2": {
            "text": "(Ich-Taste) wie damals, mit sechs"
        },
        "line.3": {
            "text": "oder sieben, die Eltern (im Traum"
        },
        "line.4": {
            "text": "sind im Krieg)"
        },
        "line.5": {
            "text": "kalt weiß massiv"
        },
        "line.6": {
            "text": "(im Scheidungskrieg) geblieben"
        },
        "line.7": {
            "text": "der Eindruck, den die Wand"
        },
        "line.8": {
            "text": "ihm  wem?"
        },
        "line.9": {
            "text": "wieder gut macht:"
        },
        "line.10": {
            "text": "Zugewandter"
        },
        "line.11": {
            "text": "wendet die Wand sich"
        },
        "line.12": {
            "text": "zu.*"
        }
    }
}