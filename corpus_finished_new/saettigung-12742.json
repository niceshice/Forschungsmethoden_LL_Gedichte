{
    "title": "saettigung-12742",
    "gender": "w",
    "author": "Charlotte Warsen",
    "author_birth": "1984",
    "poem": {
        "line.1": {
            "text": "Chardin noch musste  wie wir von Diderot wissen  verzweifelt auf einen Hasen warten, der seine Ansprüche auf die geforderte Farbigkeit erfüllte."
        },
        "line.2": {
            "text": "Max Imdahl"
        },
        "line.3": {
            "text": "sättigung"
        },
        "line.4": {
            "text": "ca. mitterschwarz"
        },
        "line.5": {
            "text": "verpestet pärchen wie ein"
        },
        "line.6": {
            "text": "rufen durch die fremde farbe"
        },
        "line.7": {
            "text": "krapp wie"
        },
        "line.8": {
            "text": "abgefangen, niedrigflieder durch"
        },
        "line.9": {
            "text": "gefieder-"
        },
        "line.10": {
            "text": "farbe, fallend"
        },
        "line.11": {
            "text": "(im flug verdorbene"
        },
        "line.12": {
            "text": "vögel)"
        },
        "line.13": {
            "text": "halb am stein und fast am"
        },
        "line.14": {
            "text": "stadtmatt nein wie schattig nein wie"
        },
        "line.15": {
            "text": "nichtschatz?"
        },
        "line.16": {
            "text": "als ein"
        },
        "line.17": {
            "text": "vorgang der sich in uns selbst ereignet"
        },
        "line.18": {
            "text": "angespannt wie"
        },
        "line.19": {
            "text": "lollo rosso"
        },
        "line.20": {
            "text": "trollolo wie hallo abgekalbt"
        },
        "line.21": {
            "text": "schon lang die helle die wir flach entfachen wollten und"
        },
        "line.22": {
            "text": "verramschen wie orange auf untergang"
        },
        "line.23": {
            "text": "ein hellunter wie ein"
        },
        "line.24": {
            "text": "augenlick (vom fass)"
        },
        "line.25": {
            "text": "wie dein körperglanz wenn ich"
        },
        "line.26": {
            "text": "den raum verlasse"
        },
        "line.27": {
            "text": "hello nordisch grelle schimmerschwere yellow hellgeschnitten wie"
        },
        "line.28": {
            "text": "ein zögern?"
        },
        "line.29": {
            "text": "devotes rot"
        },
        "line.30": {
            "text": "(verbindlich siechendes)"
        },
        "line.31": {
            "text": "schleppbeige, orthopädisch"
        },
        "line.32": {
            "text": "teiglicht?"
        },
        "line.33": {
            "text": "getrieben wie hiebe ins zwiepink"
        },
        "line.34": {
            "text": "mit goldrute, geil"
        }
    }
}