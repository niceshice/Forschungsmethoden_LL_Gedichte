{
    "title": "ich-sah-ein-huhn-258",
    "gender": "m",
    "author": "Walther Petri",
    "author_birth": "1940",
    "poem": {
        "line.1": {
            "text": "0 0 0 0 0 0 0 0"
        },
        "line.2": {
            "text": "0 0 0 0 0 0 0 0"
        },
        "line.3": {
            "text": "0 0 0 0 0 0 0 0"
        },
        "line.4": {
            "text": "0 0 0 0 0 0 0 0"
        },
        "line.5": {
            "text": "ich sah ein Huhn"
        },
        "line.6": {
            "text": "das unentwegt"
        },
        "line.7": {
            "text": "anstatt Eier"
        },
        "line.8": {
            "text": "Nullen legt"
        },
        "line.9": {
            "text": "es scharrt im Staub"
        },
        "line.10": {
            "text": "pickt in das Grau"
        },
        "line.11": {
            "text": "stumm hinter"
        },
        "line.12": {
            "text": "einem Drahtverhau"
        },
        "line.13": {
            "text": "und scharrt im Staub"
        },
        "line.14": {
            "text": "es sah mich an"
        },
        "line.15": {
            "text": "daß ich sein Leben"
        },
        "line.16": {
            "text": "ahnen kann"
        },
        "line.17": {
            "text": "nach jahrelang"
        },
        "line.18": {
            "text": "erfüllter Norm"
        },
        "line.19": {
            "text": "legt es des Eies"
        },
        "line.20": {
            "text": "leere Form"
        },
        "line.21": {
            "text": "und scharrt im Staub"
        },
        "line.22": {
            "text": "es sah mich an"
        }
    }
}