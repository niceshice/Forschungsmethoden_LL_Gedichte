{
    "title": "entgegen-zu-stehen-vi-br-befallen-sich-kreuzen-1351",
    "gender": "w",
    "author": "Anja Utler",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "letztlich: gefallen sein, ja, wie"
        },
        "line.2": {
            "text": "bestanden besiedelt die: mundhöhle"
        },
        "line.3": {
            "text": "achsel vom: riedgras der luft über-"
        },
        "line.4": {
            "text": "frostet, ja, starren  weiß: poche noch"
        },
        "line.5": {
            "text": "schon: scharen sporen sich im"
        },
        "line.6": {
            "text": "geweiteten blick, und bloß: hingespreizt,"
        },
        "line.7": {
            "text": "klaffe, die firnigen glieder beringt ja: vom reif"
        },
        "line.8": {
            "text": "von der böschung gehöre schon, gleichsam,"
        },
        "line.9": {
            "text": "verliere  nur poren mehr  und lieg mir fern"
        }
    }
}