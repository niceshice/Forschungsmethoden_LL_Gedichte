{
    "title": "muehsam-14530",
    "gender": "w",
    "author": "Ruth Wolf-Rehfeldt",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "Mühsam"
        },
        "line.2": {
            "text": "wachsen"
        },
        "line.3": {
            "text": "werdende Strukturen"
        },
        "line.4": {
            "text": "Doch in eingefahrne Spuren"
        },
        "line.5": {
            "text": "Gleiten mühlos wir zurück"
        }
    }
}