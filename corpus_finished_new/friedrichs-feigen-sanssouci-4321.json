{
    "title": "friedrichs-feigen-sanssouci-4321",
    "gender": "w",
    "author": "Dagmar Leupold",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "wachsen im Freien"
        },
        "line.2": {
            "text": "hinter Fenstern die zu"
        },
        "line.3": {
            "text": "dem Land sich öffnen"
        },
        "line.4": {
            "text": "auf das man anderseitig"
        },
        "line.5": {
            "text": "schaut: blühende Ruinen"
        },
        "line.6": {
            "text": "reiften unter seinem Blick"
        },
        "line.7": {
            "text": "sei unbesorgt: vom Naschen"
        },
        "line.8": {
            "text": "vergeht kein Appetit - wer"
        },
        "line.9": {
            "text": "hat die Feigen wohl alle gegessen?"
        },
        "line.10": {
            "text": "Die Windspiele, nein, die mögen"
        },
        "line.11": {
            "text": "die Trauben und daraus den Wein"
        },
        "line.12": {
            "text": "und dann begraben sein"
        },
        "line.13": {
            "text": "die Läufe noch beschwipst"
        },
        "line.14": {
            "text": "Wir gingen die Terrassen"
        },
        "line.15": {
            "text": "hinauf und hinab unter"
        },
        "line.16": {
            "text": "preußischem Himmelgrau"
        },
        "line.17": {
            "text": "alles in allem: ein Jahr"
        },
        "line.18": {
            "text": "in dem viel wuchs"
        },
        "line.19": {
            "text": "nicht immer so geschützt"
        },
        "line.20": {
            "text": "wie in Friedrichs Garten"
        },
        "line.21": {
            "text": "die Feigen"
        }
    }
}