{
    "title": "tag-dem-ich-aufzuwachen-vergass-2717",
    "gender": "w",
    "author": "Sabina Naef",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "spazierte durch umbenannte Strassen"
        },
        "line.2": {
            "text": "grüsste Unbekannte, juste pour voir"
        },
        "line.3": {
            "text": "versteckte Kinder, deren Namen die Mütter"
        },
        "line.4": {
            "text": "von den Balkonen fallen liessen"
        },
        "line.5": {
            "text": "versuchte Wächter zum Lächeln zu bringen"
        },
        "line.6": {
            "text": "und zählte abends die Haare"
        },
        "line.7": {
            "text": "auf dem Fussboden eines Friseursalons"
        }
    }
}