{
    "title": "zwei-zeilen-heine-inbegriffen-4242",
    "gender": "m",
    "author": "Christian Steinbacher",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "sohin bemüht ums übliche verfahren"
        },
        "line.2": {
            "text": "schliff sich der schätzwert zu notwendigkeiten"
        },
        "line.3": {
            "text": "mit ausmaß wird da zugerichtet"
        },
        "line.4": {
            "text": "auf praktikables rückverwiesen werden"
        },
        "line.5": {
            "text": "ansonsten dient der unterscheidung alles"
        },
        "line.6": {
            "text": "doch nützt dir keiner dieser gut gehegten"
        },
        "line.7": {
            "text": "und disponiblen schutzanzüge bislang"
        },
        "line.8": {
            "text": "stand dem in aussicht etwaige belohnung"
        },
        "line.9": {
            "text": "schon schuftet irgend so ein trottel weiter"
        },
        "line.10": {
            "text": "da sagt es besser gleich nur nein und gütlich"
        },
        "line.11": {
            "text": "und orientiert die sache wieder ins erwidern"
        },
        "line.12": {
            "text": "wenn sie deinen zucker wittern"
        },
        "line.13": {
            "text": "löst erwartung ein sich in melassen"
        },
        "line.14": {
            "text": "heimlich schienst du zu miauen"
        },
        "line.15": {
            "text": "geballt die einstandstreue streu"
        },
        "line.16": {
            "text": "alsbald den ausstand in den klauen"
        },
        "line.17": {
            "text": "was wird als nächstes unterlassen"
        }
    }
}