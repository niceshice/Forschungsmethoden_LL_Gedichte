{
    "title": "dies-ist-meine-truhe-ruhe-637",
    "gender": "m",
    "author": "Arne  Rautenberg",
    "author_birth": "1967",
    "poem": {
        "line.1": {
            "text": "dies ist meine truhe (ruhe)"
        },
        "line.2": {
            "text": "meine landschaft (gesandschaft) ohne grat (pfad) mein"
        },
        "line.3": {
            "text": "a.t.t.e.n.t.a.t. (p.l.a.g.i.a.t.)"
        },
        "line.4": {
            "text": "meine bekanntschaft (gefangenschaft) ohne fahrt (art) mein"
        },
        "line.5": {
            "text": "r.e.s.u.l.t.a.t. (v.e.r.r.a.t.)"
        },
        "line.6": {
            "text": "dies ist mein wort (ort)"
        }
    }
}