{
    "title": "capa-im-kopf-5559",
    "gender": "m",
    "author": "José F. A. Oliver",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "bild/"
        },
        "line.2": {
            "text": "daß ich mir den tod immer"
        },
        "line.3": {
            "text": "SCHWARZWEISS vorstelle (wurmschwarz/ maden"
        },
        "line.4": {
            "text": "weiß) vors aug geschlämmt und nach innen. Aufgebäumt"
        },
        "line.5": {
            "text": "und gedächtnisunkraut wuchernd wie"
        },
        "line.6": {
            "text": "die graumilchnebel (unsicht) der kraftwerke blau"
        },
        "line.7": {
            "text": "vernachten rauchsatt massenknochen unterm grün die"
        },
        "line.8": {
            "text": "leichenfelder laichfächer der schul"
        },
        "line.9": {
            "text": "gebücherten und den umtäglichen aus"
        },
        "line.10": {
            "text": "GABEN unserer Zeitungen/ hilft"
        },
        "line.11": {
            "text": "auch farbstreu nicht der abgelegten blumen"
        },
        "line.12": {
            "text": "fäulnis auf den gräbern und falberes schauen"
        },
        "line.13": {
            "text": "hier: augpochendes. HIERAUG/ schwarz wie es weiß"
        },
        "line.14": {
            "text": "wird und letztgrau im kranznachruf in die"
        },
        "line.15": {
            "text": "himmelsrichtungen gevierteiltes DUMPFEN im ohr und"
        },
        "line.16": {
            "text": "ÖLSCHWÄRZE der pupillen/ aber"
        },
        "line.17": {
            "text": "die gepreßte agonie die immer neue"
        },
        "line.18": {
            "text": "fotografen sucht und schöpfer"
        }
    }
}