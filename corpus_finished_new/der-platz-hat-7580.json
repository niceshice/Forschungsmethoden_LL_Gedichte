{
    "title": "der-platz-hat-7580",
    "gender": "m",
    "author": "Christian  Filips",
    "author_birth": "1981",
    "poem": {
        "line.1": {
            "text": "einen Turm"
        },
        "line.2": {
            "text": "gar nicht weit von mir,"
        },
        "line.3": {
            "text": "hier."
        },
        "line.4": {
            "text": "(Noch steht er, unbeweglich"
        },
        "line.5": {
            "text": "regungslos da)"
        },
        "line.6": {
            "text": "Der Turm: eine Aussicht"
        },
        "line.7": {
            "text": "die Aussicht"
        },
        "line.8": {
            "text": "hat Zukunft. Diese Zukunft"
        },
        "line.9": {
            "text": "hoch, die!"
        },
        "line.10": {
            "text": "immer h√∂her"
        },
        "line.11": {
            "text": "schwindelnde"
        },
        "line.12": {
            "text": "wendelnde"
        },
        "line.13": {
            "text": "Treppe!"
        },
        "line.14": {
            "text": "hat Zeit."
        }
    }
}