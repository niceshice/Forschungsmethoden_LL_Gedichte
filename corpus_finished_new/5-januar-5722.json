{
    "title": "5-januar-5722",
    "gender": "w",
    "author": "Elfriede Czurda",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "was auf den tisch kommt"
        },
        "line.2": {
            "text": "ist bitter ist sauer"
        },
        "line.3": {
            "text": "ist kalt ist herb"
        },
        "line.4": {
            "text": "ist salzig ist scharf"
        },
        "line.5": {
            "text": "ist flau ist zäh"
        },
        "line.6": {
            "text": "ist schal ist galle"
        },
        "line.7": {
            "text": "ist distel ist tran"
        },
        "line.8": {
            "text": "ist seife ist schimmel"
        },
        "line.9": {
            "text": "ist lunte jauche mist"
        },
        "line.10": {
            "text": "ist nicht stichhaltig"
        },
        "line.11": {
            "text": "ist säuerlich bittersüß"
        },
        "line.12": {
            "text": "ist honigsüß sirup kandiert"
        },
        "line.13": {
            "text": "5. januar; tisch"
        }
    }
}