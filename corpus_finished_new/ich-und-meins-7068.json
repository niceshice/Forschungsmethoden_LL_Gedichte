{
    "title": "ich-und-meins-7068",
    "gender": "m",
    "author": "Bęrt Elsmann-Papenfuß",
    "author_birth": "1956",
    "poem": {
        "line.1": {
            "text": "Nehmen wir an,"
        },
        "line.2": {
            "text": "ich finge mit mir an,"
        },
        "line.3": {
            "text": "was finge ich dann mit mir an,"
        },
        "line.4": {
            "text": "was nicht andere dazugetan ham."
        },
        "line.5": {
            "text": "Nahezu ununterscheidbar im Anfang,"
        },
        "line.6": {
            "text": "nehmen wir Gestalt an, durch Nahrung,"
        },
        "line.7": {
            "text": "sogenannte Erziehung sowie Betreuung"
        },
        "line.8": {
            "text": "werden wir geformt, verformt, umgeformt."
        },
        "line.9": {
            "text": "Ich ist eine Selbstbehauptung"
        },
        "line.10": {
            "text": "angemaßten Autoritäten gegenüber,"
        },
        "line.11": {
            "text": "und täglich neu zu manifestieren;"
        },
        "line.12": {
            "text": "auf Teufel komm raus,"
        },
        "line.13": {
            "text": "Schleppschwanz und Eiertanz."
        },
        "line.14": {
            "text": "Meins ist die soziale Substanz"
        },
        "line.15": {
            "text": "des mir eigens vermittelten"
        },
        "line.16": {
            "text": "gut Durchrüttelten:"
        },
        "line.17": {
            "text": "Meins ist sozusagen deins,"
        },
        "line.18": {
            "text": "und ich, dubidu, du."
        }
    }
}