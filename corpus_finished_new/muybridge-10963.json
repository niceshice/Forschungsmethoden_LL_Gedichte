{
    "title": "muybridge-10963",
    "gender": "m",
    "author": "Hendrik  Jackson",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "am Anfang war das Pferd"
        },
        "line.2": {
            "text": "es riss die gespannten Schnüre"
        },
        "line.3": {
            "text": "und jede hundertstel Sekunde brach den Einfall"
        },
        "line.4": {
            "text": "zur gefrorenen Bewegung"
        },
        "line.5": {
            "text": "die es aufzutauen galt"
        },
        "line.6": {
            "text": "das schwarze sichtverdeckende Tuch"
        },
        "line.7": {
            "text": "um die Augen gelegt"
        },
        "line.8": {
            "text": "es müssen seltsame Geschöpfe gewesen sein"
        },
        "line.9": {
            "text": "die sich über Farben so sehr"
        },
        "line.10": {
            "text": "freuten es müssen pompöse"
        },
        "line.11": {
            "text": "Tage gewesen sein"
        },
        "line.12": {
            "text": "voll Spielereien"
        },
        "line.13": {
            "text": "bunter Trikots und Anfeuerungen"
        },
        "line.14": {
            "text": "im Licht, mitunter"
        },
        "line.15": {
            "text": "abgeblendet"
        },
        "line.16": {
            "text": "Stille breitet sich aus"
        },
        "line.17": {
            "text": "Loch Loch Loch"
        },
        "line.18": {
            "text": "dumpfes Klatschen"
        },
        "line.19": {
            "text": "im Panorama"
        },
        "line.20": {
            "text": "stehen 36 Ansichten"
        },
        "line.21": {
            "text": "jede von ihnen ein Pionier"
        },
        "line.22": {
            "text": "der Menschheit ein Tier"
        },
        "line.23": {
            "text": "der Wächter geht"
        },
        "line.24": {
            "text": "und ruft:"
        },
        "line.25": {
            "text": "es wird jetzt geschossen"
        },
        "line.26": {
            "text": "drehen Sie sich"
        },
        "line.27": {
            "text": "eine letzte Minute lang herum"
        },
        "line.28": {
            "text": "dann reißt der Film"
        }
    }
}