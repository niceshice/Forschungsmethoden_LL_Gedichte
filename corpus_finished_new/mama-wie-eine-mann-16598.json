{
    "title": "mama-wie-eine-mann-16598",
    "gender": "w",
    "author": "Franziska Füchsl",
    "author_birth": "1991",
    "poem": {
        "line.1": {
            "text": "Mama, wie eine Mann-"
        },
        "line.2": {
            "text": "dich faltet"
        },
        "line.3": {
            "text": "mich streift"
        },
        "line.4": {
            "text": "dein Blatt Steinpapier will ich sein"
        },
        "line.5": {
            "text": "aufzeichnen: das Verkalkte kreiden"
        },
        "line.6": {
            "text": "dich kleiden"
        },
        "line.7": {
            "text": "sie reißt an"
        },
        "line.8": {
            "text": "Mama, aber ich ziehe mit"
        },
        "line.9": {
            "text": "dehn, beul, well mich"
        },
        "line.10": {
            "text": "dein Kind dein Faltenwurf"
        },
        "line.11": {
            "text": "mein Atem heißt Kluft"
        },
        "line.12": {
            "text": "ich kliebe sie, kliebe"
        },
        "line.13": {
            "text": "lüft den Rock, Mama"
        }
    }
}