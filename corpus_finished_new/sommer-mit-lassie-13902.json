{
    "title": "sommer-mit-lassie-13902",
    "gender": "m",
    "author": "Max Sessner",
    "author_birth": "1959",
    "poem": {
        "line.1": {
            "text": "Am Ende war immer alles"
        },
        "line.2": {
            "text": "wieder gut man stand in einer"
        },
        "line.3": {
            "text": "Küche und lachte jemand trank"
        },
        "line.4": {
            "text": "ein Glas Milch der Hund sah"
        },
        "line.5": {
            "text": "mich an und ich sah aus dem"
        },
        "line.6": {
            "text": "Fenster es roch nach Flieder"
        },
        "line.7": {
            "text": "und Urin Oma die Außerirdische"
        },
        "line.8": {
            "text": "verweilte noch kurz vor dem"
        },
        "line.9": {
            "text": "Fernseher ehe sie den Stecker zog"
        },
        "line.10": {
            "text": "mit einem Seufzer verschwanden"
        },
        "line.11": {
            "text": "alle die lachende Familie und der"
        },
        "line.12": {
            "text": "lachende Hund den Sommer über"
        },
        "line.13": {
            "text": "log ich wie gedruckt und doch"
        },
        "line.14": {
            "text": "ist es wahr dass ich Abenteuer"
        },
        "line.15": {
            "text": "bestand einmal fiel ich vom Mond"
        },
        "line.16": {
            "text": "und jemand warf mich zurück"
        }
    }
}