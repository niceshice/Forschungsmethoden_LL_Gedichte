{
    "title": "wie-wasser-kocht-7581",
    "gender": "m",
    "author": "Christian  Filips",
    "author_birth": "1981",
    "poem": {
        "line.1": {
            "text": "erwarte ich den Liebsten."
        },
        "line.2": {
            "text": "Der Liebste kocht in mir"
        },
        "line.3": {
            "text": "wie Wasser kocht."
        },
        "line.4": {
            "text": "Bedingungslos der Liebste"
        },
        "line.5": {
            "text": "sprudelt  Ich"
        },
        "line.6": {
            "text": "koche Tee."
        },
        "line.7": {
            "text": "Und sehe  selber Blase"
        },
        "line.8": {
            "text": "Blasen lieben."
        },
        "line.9": {
            "text": "Erst bin ich vier, dann Viertel"
        },
        "line.10": {
            "text": "nach, dann sieben. Erst siede,"
        },
        "line.11": {
            "text": "dann koch ich."
        },
        "line.12": {
            "text": "Dann Rauch."
        }
    }
}