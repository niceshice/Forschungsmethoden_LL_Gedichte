{
    "title": "er-kommt-nicht-wenn-man-ruft-6056",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "wir dachten das wetter"
        },
        "line.2": {
            "text": "hält sich der regen"
        },
        "line.3": {
            "text": "lässt sich tagtäglich"
        },
        "line.4": {
            "text": "in seiner sänfte angetragen"
        },
        "line.5": {
            "text": "der erde zur erde"
        },
        "line.6": {
            "text": "in seiner gänze gnädig herab"
        },
        "line.7": {
            "text": "aber dann gab es"
        },
        "line.8": {
            "text": "schnüre die wie maultrommeln"
        },
        "line.9": {
            "text": "rührten ans eingemachte die"
        },
        "line.10": {
            "text": "verkündeten etwas und was"
        },
        "line.11": {
            "text": "nachts in den fensterritzen"
        },
        "line.12": {
            "text": "spielte fügten wir"
        },
        "line.13": {
            "text": "in die träume ein"
        },
        "line.14": {
            "text": "himmlische kinder"
        },
        "line.15": {
            "text": "er kühlt uns, wenn wir schwitzen"
        }
    }
}