{
    "title": "november-3636",
    "gender": "m",
    "author": "Franz Xaver  Kroetz",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Tröste mich."
        },
        "line.2": {
            "text": "Dir hab ich mich"
        },
        "line.3": {
            "text": "anvertraut."
        },
        "line.4": {
            "text": "Dir hab ich"
        },
        "line.5": {
            "text": "meine Frauen geopfert."
        },
        "line.6": {
            "text": "Und meine Kinder"
        },
        "line.7": {
            "text": "die kennen mich nicht."
        },
        "line.8": {
            "text": "Du hast meine Gesundheit."
        },
        "line.9": {
            "text": "Du hast meine Kraft."
        },
        "line.10": {
            "text": "Du hast meine Freunde."
        },
        "line.11": {
            "text": "Du hast vom Besten"
        },
        "line.12": {
            "text": "von mir"
        },
        "line.13": {
            "text": "alles."
        },
        "line.14": {
            "text": "Tröste mich"
        },
        "line.15": {
            "text": "denn ich hasse dich."
        },
        "line.16": {
            "text": "Literatur."
        }
    }
}