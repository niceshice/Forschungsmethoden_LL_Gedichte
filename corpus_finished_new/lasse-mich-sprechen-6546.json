{
    "title": "lasse-mich-sprechen-6546",
    "gender": "w",
    "author": "Ilana Shmueli",
    "author_birth": "1924",
    "poem": {
        "line.1": {
            "text": "Lasse mich sprechen"
        },
        "line.2": {
            "text": "die Erste werd ich nicht sein"
        },
        "line.3": {
            "text": "und nicht die Letzte"
        },
        "line.4": {
            "text": "doch lass mich"
        },
        "line.5": {
            "text": "lasse mich sprechen"
        },
        "line.6": {
            "text": "ohne Meister"
        }
    }
}