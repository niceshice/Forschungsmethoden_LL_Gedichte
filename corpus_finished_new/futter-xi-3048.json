{
    "title": "futter-xi-3048",
    "gender": "w",
    "author": "Karin  Fellner",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "im kloster streunen sie / um verdeckte beete"
        },
        "line.2": {
            "text": "sammeln sonne, tote / wohnen im mulch"
        },
        "line.3": {
            "text": "langsam setzen sie sich"
        },
        "line.4": {
            "text": "dies sei fasten, sie sagen / hunger sei limitiert"
        },
        "line.5": {
            "text": "im angelusläuten warte / ein dunkler laib"
        },
        "line.6": {
            "text": "innen süß"
        },
        "line.7": {
            "text": "staunen sie ins leere"
        },
        "line.8": {
            "text": "versprechen der mandelblüte."
        }
    }
}