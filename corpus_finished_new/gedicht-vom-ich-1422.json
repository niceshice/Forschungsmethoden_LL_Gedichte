{
    "title": "gedicht-vom-ich-1422",
    "gender": "m",
    "author": "Fredrik Vahle",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "Ich bin ich,"
        },
        "line.2": {
            "text": "na klar, oder nicht?"
        },
        "line.3": {
            "text": "Ich bin ich,"
        },
        "line.4": {
            "text": "kann jeder Mensch sagen"
        },
        "line.5": {
            "text": "Aber wer oder was"
        },
        "line.6": {
            "text": "ist denn nun ein 'Ich'?"
        },
        "line.7": {
            "text": "Schon bin ich mittendrin im Fragen. -"
        },
        "line.8": {
            "text": "Wo fängt ich an?"
        },
        "line.9": {
            "text": "Wo hört ich auf?"
        },
        "line.10": {
            "text": "Ist 'Ich' immer gleich,"
        },
        "line.11": {
            "text": "ob ich sitz oder lauf?"
        },
        "line.12": {
            "text": "Ob ich sieben oder siebzig bin?"
        },
        "line.13": {
            "text": "Ist mein Körper das 'Ich',"
        },
        "line.14": {
            "text": "oder steckts mittendrin?"
        },
        "line.15": {
            "text": "In der Brust, im Herz,"
        },
        "line.16": {
            "text": "oder unten im Zeh,"
        },
        "line.17": {
            "text": "oder in den Füßen,"
        },
        "line.18": {
            "text": "auf denen ich steh'?"
        },
        "line.19": {
            "text": "Und etwas, was ich auch gern wüßt':"
        },
        "line.20": {
            "text": "Wenn jemand vor Glück ganz außer sich ist -"
        },
        "line.21": {
            "text": "'außer sich' heißt doch raus aus dem Ich."
        },
        "line.22": {
            "text": "Manchmal versteh ich mich selber nicht."
        },
        "line.23": {
            "text": "Jemand ist außer sich vor Wut -"
        },
        "line.24": {
            "text": "bleibt das Ich dann bei sich"
        },
        "line.25": {
            "text": "und es geht ihm ganz gut?"
        },
        "line.26": {
            "text": "Vielleicht hat das Ich auch was ausgeheckt,"
        },
        "line.27": {
            "text": "als blaues Männchen sich in mir versteckt,"
        },
        "line.28": {
            "text": "lacht wie's Rumpelstilzchen,"
        },
        "line.29": {
            "text": "Sagt: Such nur, such!"
        },
        "line.30": {
            "text": "Kauf dir ein kluges Ich-findungs-buch.."
        },
        "line.31": {
            "text": "oder auch dreizehn, davon gibts genug,"
        },
        "line.32": {
            "text": "mach dich auf die Suche nach deinem Ich,"
        },
        "line.33": {
            "text": "suche und suche,"
        },
        "line.34": {
            "text": "du findest es nicht."
        },
        "line.35": {
            "text": "Es ist kein Persönchen, hat kein Gesicht,"
        },
        "line.36": {
            "text": "wenn du's finden willst, dann suche es nicht..."
        },
        "line.37": {
            "text": "Du bist mit allem dein Ich,"
        },
        "line.38": {
            "text": "was du tust,"
        },
        "line.39": {
            "text": "wie du gehst, wie du atmest"
        },
        "line.40": {
            "text": "wachst oder ruhst,"
        },
        "line.41": {
            "text": "wie du hörst, wie du siehst,"
        },
        "line.42": {
            "text": "wie du riechst, wie du schmeckst"
        },
        "line.43": {
            "text": "und dir nach dem Essen die Lippen leckst."
        },
        "line.44": {
            "text": "Du brauchst dein Ich nicht"
        },
        "line.45": {
            "text": "gesondert zu suchen."
        },
        "line.46": {
            "text": "Das ist philosophischer Käsekuchen."
        },
        "line.47": {
            "text": "Du bist, was du bist in diesem Leben."
        },
        "line.48": {
            "text": "Dich kann's auf der ganzen weiten Welt"
        },
        "line.49": {
            "text": "so, wie du bist"
        },
        "line.50": {
            "text": "nur einmal geben."
        },
        "line.51": {
            "text": "Es ist, wie es ist,"
        },
        "line.52": {
            "text": "Punkt Komma und Strich:"
        },
        "line.53": {
            "text": "Viele Grüße von meinem -"
        },
        "line.54": {
            "text": "an wen?"
        },
        "line.55": {
            "text": "An dein"
        },
        "line.56": {
            "text": "Ich."
        }
    }
}