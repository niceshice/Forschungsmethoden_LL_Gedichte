{
    "title": "days-wine-and-roses-12349",
    "gender": "m",
    "author": "David  Krause",
    "author_birth": "1988",
    "poem": {
        "line.1": {
            "text": "es war sommer. das radio spielte"
        },
        "line.2": {
            "text": "bill evans days of wine and roses."
        },
        "line.3": {
            "text": "beim beckenschlag des drummers sprangst"
        },
        "line.4": {
            "text": "du in den pool und schwammst"
        },
        "line.5": {
            "text": "im schimmern des junis."
        },
        "line.6": {
            "text": "als du schliefst im gras"
        },
        "line.7": {
            "text": "lag eine strähne über deinem gesicht"
        },
        "line.8": {
            "text": "wie ein riss. aus dem radio strömte"
        },
        "line.9": {
            "text": "rauschen in die hitzige luft"
        }
    }
}