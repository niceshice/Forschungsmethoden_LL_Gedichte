{
    "title": "elf-stumme-zahl-5716",
    "gender": "w",
    "author": "Elfriede Czurda",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "grau, LICHTlose sphäre"
        },
        "line.2": {
            "text": "zehn quanten finsternis"
        },
        "line.3": {
            "text": "ein quantum tod überschreitet"
        },
        "line.4": {
            "text": "das gift stumm seine schranke"
        },
        "line.5": {
            "text": "elfmeter! gefahr! ruft der"
        },
        "line.6": {
            "text": "irrtum, ohne jedes verdienst"
        },
        "line.7": {
            "text": "gesellt sich der vollzähligen"
        },
        "line.8": {
            "text": "gruppe der elftausend ruten der"
        },
        "line.9": {
            "text": "überschüssige zähler zu, der verfolger"
        },
        "line.10": {
            "text": "überbordet das runde, das volle"
        },
        "line.11": {
            "text": "boot, schäumt auf in der primzahl"
        },
        "line.12": {
            "text": "zu viel und zu wenig die drehung"
        }
    }
}