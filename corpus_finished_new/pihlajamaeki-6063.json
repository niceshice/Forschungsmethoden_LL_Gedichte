{
    "title": "pihlajamaeki-6063",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "wachhaltung dämmerung hinter doppel"
        },
        "line.2": {
            "text": "fenstern der hügel und ich"
        },
        "line.3": {
            "text": "gespensterspiegel legierungen weiß"
        },
        "line.4": {
            "text": "in weiß ein hautschnee und lust"
        },
        "line.5": {
            "text": "lose schnitte die ebereschen"
        },
        "line.6": {
            "text": "im linoleum des himmels"
        },
        "line.7": {
            "text": "wie träume schmal"
        },
        "line.8": {
            "text": "bei fallendem deckenlicht ich"
        },
        "line.9": {
            "text": "wünsche nichts als die zukunft"
        },
        "line.10": {
            "text": "der steppdecke deckenlichtdicht"
        },
        "line.11": {
            "text": "komme sie auf mich zu ich"
        },
        "line.12": {
            "text": "warte hier und was nicht noch"
        },
        "line.13": {
            "text": "alles ältlicher husten"
        },
        "line.14": {
            "text": "der an die schlafwand klopft"
        },
        "line.15": {
            "text": "ein ausguss und kiefern tröpfe, arme"
        },
        "line.16": {
            "text": "ich sage"
        },
        "line.17": {
            "text": "anteeksi"
        },
        "line.18": {
            "text": "und stoße rauch vom schwarzen balkon"
        },
        "line.19": {
            "text": "ein stein sucht nicht rascher den grund"
        }
    }
}