{
    "title": "mein-name-ist-mir-168",
    "gender": "m",
    "author": "Kurt  Aebli",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "M"
        },
        "line.2": {
            "text": "ein Name ist an mir"
        },
        "line.3": {
            "text": "gescheitert in dem klaustrophilen"
        },
        "line.4": {
            "text": "Wörterbuch wo Bilder Säugetiere"
        },
        "line.5": {
            "text": "sind in dem Halbdunkel das voller"
        },
        "line.6": {
            "text": "Schädel hängt voller daß ich nicht"
        },
        "line.7": {
            "text": "lache eine vergnügliche Zukunft mit"
        },
        "line.8": {
            "text": "vagen nächtlichen Schenkeln ohne den"
        },
        "line.9": {
            "text": "Geruch der Hoffnung ohne den Geruch"
        },
        "line.10": {
            "text": "des Enthaarungsmittels in gewisser"
        },
        "line.11": {
            "text": "Weise Michelle in gewisser"
        },
        "line.12": {
            "text": "Weise Nicole"
        }
    }
}