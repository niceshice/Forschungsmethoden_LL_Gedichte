{
    "title": "morgenhabitus-einer-moewe-5548",
    "gender": "m",
    "author": "José F. A. Oliver",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "querschwimmend"
        },
        "line.2": {
            "text": "1 damenkopf /"
        },
        "line.3": {
            "text": "möwendame"
        },
        "line.4": {
            "text": ": sich mit dem"
        },
        "line.5": {
            "text": "aufgerauhten wasser frisierend"
        },
        "line.6": {
            "text": "schlankes brustweiß silbert & aug-"
        },
        "line.7": {
            "text": "gepuderter haubenstolz 1 eleganz & flüchtig"
        },
        "line.8": {
            "text": ": hält den roten Schnabel [die schnabellippen"
        },
        "line.9": {
            "text": "augbetont]"
        },
        "line.10": {
            "text": "1"
        },
        "line.11": {
            "text": "wunderfitz"
        },
        "line.12": {
            "text": "1 suchpupille neugier"
        },
        "line.13": {
            "text": "die gehißte schwanzfeder"
        },
        "line.14": {
            "text": "1 findiges luftruder oder"
        },
        "line.15": {
            "text": "1 kajalstift des blickes [möwenstreng]"
        },
        "line.16": {
            "text": "1 fliegender delphinleib"
        },
        "line.17": {
            "text": "en miniature"
        },
        "line.18": {
            "text": "& federfischleicht"
        },
        "line.19": {
            "text": "ganz heimweh nach flug plötzlich"
        },
        "line.20": {
            "text": "sehniger perlmuttcorpus / 1 art fragiler stärke"
        },
        "line.21": {
            "text": ": so könnte 1 tag beginnen"
        }
    }
}