{
    "title": "windrose-13871",
    "gender": "m",
    "author": "Tobias Roth",
    "author_birth": "1985",
    "poem": {
        "line.1": {
            "text": "Sinken die Hochbunker in Nadelwald"
        },
        "line.2": {
            "text": "Bald und aufgezwickt die Zäune"
        },
        "line.3": {
            "text": "Und kaum die Ahnung"
        },
        "line.4": {
            "text": "All der faulenden Bürotürme"
        },
        "line.5": {
            "text": "Der Asphalt und die Nutztiere."
        },
        "line.6": {
            "text": "Mehr und mehr Kohelet."
        },
        "line.7": {
            "text": "Bruchstücke aus Volkssprache."
        },
        "line.8": {
            "text": "Vieläugiger Sandwegsüden."
        },
        "line.9": {
            "text": "Frost wird es auseinanderreißen, wir"
        },
        "line.10": {
            "text": "Müssen Gärtner werden, damit"
        },
        "line.11": {
            "text": "Zwischen den Fugen der Süden sprießen"
        },
        "line.12": {
            "text": "Kann im Norden aus Zweck ein leicht"
        },
        "line.13": {
            "text": "Zu findender Süden"
        },
        "line.14": {
            "text": ","
        },
        "line.15": {
            "text": "mehr als"
        },
        "line.16": {
            "text": "ein staubiger Fetzen"
        },
        "line.17": {
            "text": "."
        },
        "line.18": {
            "text": "Das Ziffernblatt der Windrose, flugs,"
        },
        "line.19": {
            "text": "Flussläufe rückwärts,"
        },
        "line.20": {
            "text": "Süden"
        },
        "line.21": {
            "text": "der"
        },
        "line.22": {
            "text": "Sphingen, die auf den Locken"
        },
        "line.23": {
            "text": "Töpfe voller Rosen und Kentauren"
        },
        "line.24": {
            "text": "Tragen, aus denen große Terzen"
        },
        "line.25": {
            "text": "Auf die Löwen tropfen, hat es vielleicht"
        },
        "line.26": {
            "text": "Gegeben, nur mit Vorzeichnungen bekleidet,"
        },
        "line.27": {
            "text": "Denn"
        },
        "line.28": {
            "text": "sie träumen alle vom Süden"
        },
        "line.29": {
            "text": "."
        },
        "line.30": {
            "text": "Windrose, mit Marmor verblendet."
        }
    }
}