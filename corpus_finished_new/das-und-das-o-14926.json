{
    "title": "das-und-das-o-14926",
    "gender": "m",
    "author": "Marcus Roloff",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "1"
        },
        "line.2": {
            "text": "zerschmolzene klaviatur, o bittre wolke"
        },
        "line.3": {
            "text": "feine deutsche [jugend] bakterien"
        },
        "line.4": {
            "text": "[das a] amorphe algebra, gemölk"
        },
        "line.5": {
            "text": "algorithmen-haufen, logik-jo-jo, yoga-rhythmus"
        },
        "line.6": {
            "text": "(logarithmus-logo | yoyo-mythen"
        },
        "line.7": {
            "text": "mystischer hymen)"
        },
        "line.8": {
            "text": "abgespritzte zahlen, o [gleich null]"
        },
        "line.9": {
            "text": "mütterchens champignon satellite"
        },
        "line.10": {
            "text": "auswärtskiesel"
        },
        "line.11": {
            "text": "staben-wahrscheinlichkeit  die form"
        },
        "line.12": {
            "text": "die sich in stücke seufzte"
        },
        "line.13": {
            "text": "2"
        },
        "line.14": {
            "text": "ausdrucksform am strand"
        },
        "line.15": {
            "text": "orgasmussahne"
        },
        "line.16": {
            "text": "die lava des juweliers"
        },
        "line.17": {
            "text": "das scholastische viech im"
        },
        "line.18": {
            "text": "dissimetrischen moos"
        },
        "line.19": {
            "text": "fruchtet, (be-)fruchtet, befruchtend."
        },
        "line.20": {
            "text": "und die libelle erst"
        },
        "line.21": {
            "text": "farbenpracht & ruheloser knochenkamm"
        },
        "line.22": {
            "text": "im innern der eremitischen muschel"
        },
        "line.23": {
            "text": "macht tanz (-figur); tanzt (macht) figur"
        },
        "line.24": {
            "text": "den schimmernd siedenden sud, den schwarzen akt"
        },
        "line.25": {
            "text": "die erben"
        },
        "line.26": {
            "text": "beziehungsweise tiere"
        },
        "line.27": {
            "text": "überstehns."
        },
        "line.28": {
            "text": "3"
        },
        "line.29": {
            "text": "des denkens metallsche [mäeutische]"
        },
        "line.30": {
            "text": "zündkerzen"
        },
        "line.31": {
            "text": "der seelen prosaischer trotz"
        },
        "line.32": {
            "text": "die schönen riesenflügel der nacht"
        },
        "line.33": {
            "text": "fliegend segelnde"
        },
        "line.34": {
            "text": "beständigkeit, und kopf und ding"
        },
        "line.35": {
            "text": "vergöttlichen pfützen [die immer auch] schamlose"
        },
        "line.36": {
            "text": "kugellauge (veilchen oder kristall)"
        },
        "line.37": {
            "text": "l'estaque schindet salz"
        },
        "line.38": {
            "text": "4"
        },
        "line.39": {
            "text": "spätre bräuche und"
        },
        "line.40": {
            "text": "tod des armen"
        },
        "line.41": {
            "text": "fleischers kryptisches zylinderende"
        },
        "line.42": {
            "text": "[a] schrecken des geliebten lochs"
        },
        "line.43": {
            "text": "springschwanz (-brunnen)."
        },
        "line.44": {
            "text": "die gattung (?) fickt vergangnes."
        },
        "line.45": {
            "text": "torf und untergrund. die ersten jahre"
        },
        "line.46": {
            "text": "vollbringn. [o] algier: pest"
        },
        "line.47": {
            "text": "der ekel (der fremde). tod im facel vega."
        },
        "line.48": {
            "text": "an jeder ecke"
        },
        "line.49": {
            "text": "wollust in gruppen."
        },
        "line.50": {
            "text": "wirft () menschen gern () weg-"
        },
        "line.51": {
            "text": "gehaarten abzählreim (...)"
        },
        "line.52": {
            "text": "5"
        },
        "line.53": {
            "text": "bronzeblick"
        },
        "line.54": {
            "text": "zu pferdeschatten werden die gewichte"
        },
        "line.55": {
            "text": "ackerbau"
        },
        "line.56": {
            "text": "mammutbaum, rhododendron"
        },
        "line.57": {
            "text": "und zunge in heuristischen küchen"
        },
        "line.58": {
            "text": "adler und köder"
        },
        "line.59": {
            "text": "baugerüst des schicksals [bekannt]"
        },
        "line.60": {
            "text": "in klammern bilder"
        },
        "line.61": {
            "text": "6"
        },
        "line.62": {
            "text": "rundumbelichtung [auf portra] durchfixiert."
        },
        "line.63": {
            "text": "laufbahn der kristalle  pfahl [im fleische]"
        },
        "line.64": {
            "text": "schwärenschaber, haspelpresse"
        },
        "line.65": {
            "text": "ohne niemals nicht, frühmensch. welt-"
        },
        "line.66": {
            "text": "weit. athletenrotation, geschlecht und topf"
        },
        "line.67": {
            "text": "und kratzfuß, blinde lose."
        },
        "line.68": {
            "text": "wir [alle]"
        },
        "line.69": {
            "text": "existieren"
        }
    }
}