{
    "title": "mundschluss-360",
    "gender": "w",
    "author": "Anne Duden",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "I"
        },
        "line.2": {
            "text": "Engellose Flügel"
        },
        "line.3": {
            "text": "am Todwort"
        },
        "line.4": {
            "text": "geräderten Horizont."
        },
        "line.5": {
            "text": "Kampfschritt"
        },
        "line.6": {
            "text": "in den beinhart gespannten Unterbauch."
        },
        "line.7": {
            "text": "Es soll sich schon ausbrüten"
        },
        "line.8": {
            "text": "taubeneigroß aber schleunigst"
        },
        "line.9": {
            "text": "in den Fratzenhumus gesetzt an der Front"
        },
        "line.10": {
            "text": "und leichthin übergangen"
        },
        "line.11": {
            "text": "und zertreten"
        },
        "line.12": {
            "text": "beim Abmahl bereits."
        },
        "line.13": {
            "text": "WEHE DU VOGEL TÖTEST MICH"
        },
        "line.14": {
            "text": "zwischen drei Wasserläufen"
        },
        "line.15": {
            "text": "eingepfercht in den Korral einiger Söhne."
        },
        "line.16": {
            "text": "An den Bruch- und Reißrändern"
        },
        "line.17": {
            "text": "verhohlene Klarheit."
        },
        "line.18": {
            "text": "Im Helldunkel der Redeschauer"
        },
        "line.19": {
            "text": "stellen sie die Hähne ab"
        },
        "line.20": {
            "text": "drohen mit der Potenz"
        },
        "line.21": {
            "text": "spitzer Knie und frisch geschnittener Nägel."
        },
        "line.22": {
            "text": "First"
        },
        "line.23": {
            "text": "LICK YOURSELF INTO SHAPE"
        },
        "line.24": {
            "text": "dann schwachstimmig"
        },
        "line.25": {
            "text": "wie Tierlieder gesungen"
        },
        "line.26": {
            "text": "über die Brüstung geneigt"
        },
        "line.27": {
            "text": "die Sarkophagwand"
        },
        "line.28": {
            "text": "und schon die ersten Pfeile in der Nasenschleimhaut"
        },
        "line.29": {
            "text": "und Verwandtengemetzel."
        },
        "line.30": {
            "text": "Auf dem Rasengrund"
        },
        "line.31": {
            "text": "der Stelle"
        },
        "line.32": {
            "text": "ein Wind oder Sturm"
        },
        "line.33": {
            "text": "der die Wüste mitreißt"
        },
        "line.34": {
            "text": "ohne sie zu berühren."
        },
        "line.35": {
            "text": "Seraphimschläge aus der leibhaftigen Eibenschwärze"
        },
        "line.36": {
            "text": "der Halt."
        },
        "line.37": {
            "text": "Landschaftsvektoren"
        },
        "line.38": {
            "text": "und Vorwärtsschluchten"
        },
        "line.39": {
            "text": "in denen alles schläft"
        },
        "line.40": {
            "text": "das Bodenpersonal"
        },
        "line.41": {
            "text": "blind den notwendigen Dienst versieht"
        },
        "line.42": {
            "text": "mit versteiften Gliedern."
        },
        "line.43": {
            "text": "Ein Huflattichflanell"
        },
        "line.44": {
            "text": "wäscht die lebensmüde Haut"
        },
        "line.45": {
            "text": "neben den Gleisen"
        },
        "line.46": {
            "text": "während auf dem Picknickschirm"
        },
        "line.47": {
            "text": "immer dieselben"
        },
        "line.48": {
            "text": "Skelette auf- und verglühen."
        },
        "line.49": {
            "text": "II"
        },
        "line.50": {
            "text": "Flugschwärme höhen die krampfenden Orte"
        },
        "line.51": {
            "text": "eilen rauschhaft verstummt"
        },
        "line.52": {
            "text": "die Zugstrecken entlang"
        },
        "line.53": {
            "text": "eigenwillig"
        },
        "line.54": {
            "text": "arglos"
        },
        "line.55": {
            "text": "auf Fernziele zu."
        },
        "line.56": {
            "text": "Wind-"
        },
        "line.57": {
            "text": "triebverwobene Luftrochen"
        },
        "line.58": {
            "text": "hin und zurück"
        },
        "line.59": {
            "text": "gerissen"
        },
        "line.60": {
            "text": "geschlitzt"
        },
        "line.61": {
            "text": "aus dem Blau geschlagen"
        },
        "line.62": {
            "text": "des Tages der Nacht"
        },
        "line.63": {
            "text": "entleibt"
        },
        "line.64": {
            "text": "zerlegt"
        },
        "line.65": {
            "text": "und glasig fettenden Auges"
        },
        "line.66": {
            "text": "geruhsam verdaut."
        },
        "line.67": {
            "text": "Aber Millionen Mal."
        },
        "line.68": {
            "text": "Nachtintelligenz"
        },
        "line.69": {
            "text": "hinterlegt die angebrochenen Brücken"
        },
        "line.70": {
            "text": "beweint trocken und kurz"
        },
        "line.71": {
            "text": "die kleine Tote im Hof"
        },
        "line.72": {
            "text": "bäumt sich auf"
        },
        "line.73": {
            "text": "unter vier schweren Decken"
        },
        "line.74": {
            "text": "stürzt sich zurück"
        },
        "line.75": {
            "text": "bis hinter die Barbiturate"
        },
        "line.76": {
            "text": "den Anschwall"
        },
        "line.77": {
            "text": "die Verkettung der Besatzer"
        },
        "line.78": {
            "text": "das Gellen der Telecombörse."
        },
        "line.79": {
            "text": "Rasant breiten sich weiter"
        },
        "line.80": {
            "text": "die Geschlechtsboten aus"
        },
        "line.81": {
            "text": "reißen die öffentlichen Verkehrsmittel an sich"
        },
        "line.82": {
            "text": "und Schmatzhorden"
        },
        "line.83": {
            "text": "verheben sich an den Trüffeln"
        },
        "line.84": {
            "text": "inmitten all der Massaker"
        },
        "line.85": {
            "text": "ununterschieden."
        },
        "line.86": {
            "text": "An Preßluftbohrer gefesselt"
        },
        "line.87": {
            "text": "treiben sich"
        },
        "line.88": {
            "text": "bis unter den Straßenbelag andere."
        },
        "line.89": {
            "text": "Passanten atmen ruhig"
        },
        "line.90": {
            "text": "beim Abtasten der Attacken."
        },
        "line.91": {
            "text": "III"
        },
        "line.92": {
            "text": "Entsichert"
        },
        "line.93": {
            "text": "bereit für die Querung"
        },
        "line.94": {
            "text": "lösen sich aus dem Salaamkrampf"
        },
        "line.95": {
            "text": "Worte"
        },
        "line.96": {
            "text": "enthoben dem Bellwerk"
        },
        "line.97": {
            "text": "der Aufseher und Abwürger"
        },
        "line.98": {
            "text": "entwichen den Fettzapfern"
        },
        "line.99": {
            "text": "Fallmeistern von heute und morgen"
        },
        "line.100": {
            "text": "umgehen"
        },
        "line.101": {
            "text": "fern nebenan"
        },
        "line.102": {
            "text": "die Durchtrennungen"
        },
        "line.103": {
            "text": "Botswanas Zäune"
        },
        "line.104": {
            "text": "kreuz und quer"
        },
        "line.105": {
            "text": "in Drahtverhaue"
        },
        "line.106": {
            "text": "sich einfolternde Tiere"
        },
        "line.107": {
            "text": "die unverbesserlich lauf- und trinkbesessenen"
        },
        "line.108": {
            "text": "Verwesungshöfe"
        },
        "line.109": {
            "text": "in seicht lakenden Wassern"
        },
        "line.110": {
            "text": "zum Schlußatem auf die Seite gelegte"
        },
        "line.111": {
            "text": "Populationen"
        },
        "line.112": {
            "text": "Abländer zum Verrecken hingestreckt"
        },
        "line.113": {
            "text": "und aufgetriebene"
        },
        "line.114": {
            "text": "schäumend kollabierende Ozeane"
        },
        "line.115": {
            "text": "mit unbeirrbar"
        },
        "line.116": {
            "text": "gläubigen Weißleibern."
        },
        "line.117": {
            "text": "IV"
        },
        "line.118": {
            "text": "Schrieb"
        },
        "line.119": {
            "text": "den Kopf auf der Schüssel"
        },
        "line.120": {
            "text": "angerichtet"
        },
        "line.121": {
            "text": "wie im Schlaf"
        },
        "line.122": {
            "text": "sprach"
        },
        "line.123": {
            "text": "mit eingeklemmten Organen"
        },
        "line.124": {
            "text": "schickte schwimmende Steine aus"
        },
        "line.125": {
            "text": "befispelte"
        },
        "line.126": {
            "text": "Mannlöcher Gullis"
        },
        "line.127": {
            "text": "kratzte Scharten Kuhlen"
        },
        "line.128": {
            "text": "in den Boden"
        },
        "line.129": {
            "text": "mit zersplissenen Nägeln"
        },
        "line.130": {
            "text": "und las abgemurmelt"
        },
        "line.131": {
            "text": "DO NOT TALK OUTSIDE THIS AREA"
        },
        "line.132": {
            "text": "hob das Erbrochene auf"
        },
        "line.133": {
            "text": "und etliche asphaltierte Schlüssel"
        },
        "line.134": {
            "text": "in St. Albans."
        },
        "line.135": {
            "text": "Silberblicks"
        },
        "line.136": {
            "text": "ins Blaue verrückt"
        },
        "line.137": {
            "text": "doch noch"
        },
        "line.138": {
            "text": "mit angehobenem Gesicht"
        },
        "line.139": {
            "text": "zwischen Reißzähnen"
        },
        "line.140": {
            "text": "weißer Taube"
        },
        "line.141": {
            "text": "und Sterbesenke."
        },
        "line.142": {
            "text": "Schwer klumpt das Gedächtnispack"
        },
        "line.143": {
            "text": "verknappt die Vision aufs Allernotwendigste"
        },
        "line.144": {
            "text": "kappt die kleinste Verklärung ab."
        },
        "line.145": {
            "text": "NOLI ME TANGERE"
        },
        "line.146": {
            "text": "an einem Unterort ausharrend"
        },
        "line.147": {
            "text": "bäuchlings"
        },
        "line.148": {
            "text": "verkrallt in Bims"
        },
        "line.149": {
            "text": "die Flügel angelegt"
        },
        "line.150": {
            "text": "aber zum Schirm gebreitet"
        },
        "line.151": {
            "text": "gegen die schlank Aufsteigenden"
        },
        "line.152": {
            "text": "verbissen in den unverdaulichen Saum"
        },
        "line.153": {
            "text": "die Takelage der Dame."
        },
        "line.154": {
            "text": "Liest mit den Zähnen"
        },
        "line.155": {
            "text": "der Zunge"
        },
        "line.156": {
            "text": "drückt Silben an den Gaumen"
        },
        "line.157": {
            "text": "spült sie über den Maulboden"
        },
        "line.158": {
            "text": "speichelt sie ein zu verschlüsselten Noten"
        },
        "line.159": {
            "text": "und behält alles bei sich"
        },
        "line.160": {
            "text": "eingebettet in weite Entfernung."
        },
        "line.161": {
            "text": "Am Einfallstor"
        },
        "line.162": {
            "text": "zum Ende"
        },
        "line.163": {
            "text": "der Verklappung"
        },
        "line.164": {
            "text": "an Kanal Schacht Gang"
        },
        "line.165": {
            "text": "tönen den Raum"
        },
        "line.166": {
            "text": "irrlichternd auf"
        },
        "line.167": {
            "text": "die Kehlchen."
        },
        "line.168": {
            "text": "Nur der Mauerrand brennt"
        },
        "line.169": {
            "text": "lodert schwarzgrün"
        },
        "line.170": {
            "text": "von Samenmänteln glühend befleckt."
        },
        "line.171": {
            "text": "In die Nachtverschalung Tagetes"
        },
        "line.172": {
            "text": "gegen die SEUCHE"
        },
        "line.173": {
            "text": "die anpäßliche Schreckensherrschaft."
        },
        "line.174": {
            "text": "Mit Wolkenrotten gehts auf"
        },
        "line.175": {
            "text": "bliebe der Blick"
        },
        "line.176": {
            "text": "WO DIE AUGEN ZUGEDECKT."
        }
    }
}