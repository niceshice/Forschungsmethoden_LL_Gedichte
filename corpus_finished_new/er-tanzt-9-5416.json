{
    "title": "er-tanzt-9-5416",
    "gender": "m",
    "author": "Hans Raimund",
    "author_birth": "1945",
    "poem": {
        "line.1": {
            "text": "ELIS DREHT SICH"
        },
        "line.2": {
            "text": "Dreht sich"
        },
        "line.3": {
            "text": "Um sich selber"
        },
        "line.4": {
            "text": "Regt"
        },
        "line.5": {
            "text": "Die Arme schöner"
        },
        "line.6": {
            "text": "Knie"
        },
        "line.7": {
            "text": "Peitschen"
        },
        "line.8": {
            "text": "Schwindel"
        },
        "line.9": {
            "text": "Winden sich"
        },
        "line.10": {
            "text": "In Muscheln"
        },
        "line.11": {
            "text": "Bögen"
        },
        "line.12": {
            "text": "Kreischen abwärts"
        },
        "line.13": {
            "text": "Saitenquer"
        },
        "line.14": {
            "text": "Elis dreht sich"
        },
        "line.15": {
            "text": "Dreht sich"
        },
        "line.16": {
            "text": "Auf der Zehen Spitzen"
        },
        "line.17": {
            "text": "Regt"
        },
        "line.18": {
            "text": "Die Arme schöner"
        },
        "line.19": {
            "text": "Alles Sichtbare"
        },
        "line.20": {
            "text": "Fällt ab"
        },
        "line.21": {
            "text": "Das seit je"
        },
        "line.22": {
            "text": "Verbundene"
        },
        "line.23": {
            "text": "Zerfällt"
        },
        "line.24": {
            "text": "Ruhe"
        },
        "line.25": {
            "text": "Reglos in der Mitte"
        },
        "line.26": {
            "text": "Der Bewegung"
        },
        "line.27": {
            "text": "Elis dreht sich"
        },
        "line.28": {
            "text": "Dreht sich"
        },
        "line.29": {
            "text": "Um sich selber"
        },
        "line.30": {
            "text": "Regt"
        },
        "line.31": {
            "text": "Die Arme schöner"
        },
        "line.32": {
            "text": "Drehen biegt"
        },
        "line.33": {
            "text": "Die Gerte"
        },
        "line.34": {
            "text": "Bricht"
        },
        "line.35": {
            "text": "Blut"
        },
        "line.36": {
            "text": "Quillt aus"
        },
        "line.37": {
            "text": "Den Schäften"
        }
    }
}