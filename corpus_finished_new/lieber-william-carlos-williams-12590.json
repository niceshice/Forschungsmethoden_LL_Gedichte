{
    "title": "lieber-william-carlos-williams-12590",
    "gender": "m",
    "author": "Ulrich Zieger",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "Lieber William Carlos Williams,"
        },
        "line.2": {
            "text": "Heute am dritten oktober hat hier"
        },
        "line.3": {
            "text": "der herbst eingesetzt"
        },
        "line.4": {
            "text": "der himmel war dramatisch und als"
        },
        "line.5": {
            "text": "gegen abend die sonne noch einmal"
        },
        "line.6": {
            "text": "kurz durchbrach"
        },
        "line.7": {
            "text": "legten sich ihre strahlen über die ebene"
        },
        "line.8": {
            "text": "wie bei Altdorfer"
        },
        "line.9": {
            "text": "wenn ich an den letzten herbst zurückdenke"
        },
        "line.10": {
            "text": "dann hatte ich angst"
        },
        "line.11": {
            "text": "angst den winter nicht zu überstehen"
        },
        "line.12": {
            "text": "in diesem jahr bin ich auf eine"
        },
        "line.13": {
            "text": "leicht depressive weise"
        },
        "line.14": {
            "text": "gelassen  was mich beunruhigt"
        },
        "line.15": {
            "text": "freunde brachten mir gestern"
        },
        "line.16": {
            "text": "aus ihrem landhaus"
        },
        "line.17": {
            "text": "eine kühltasche mit"
        },
        "line.18": {
            "text": "die ich irgendwann im vergangenen jahr"
        },
        "line.19": {
            "text": "dort vergessen hatte"
        },
        "line.20": {
            "text": "(ich hatte an zwei söhne und ein zelt zu denken)"
        },
        "line.21": {
            "text": "darin fanden sich: eine taschenlampe"
        },
        "line.22": {
            "text": "Edgar Allan Poes Le Scarabe dor"
        },
        "line.23": {
            "text": "in der übertragung von Charles Baudelaire"
        },
        "line.24": {
            "text": "Andr Schwarz-Bart Die Mulattin Solitüde"
        },
        "line.25": {
            "text": "Charles Ferdinand Ramuz Die Trennung der Rassen"
        },
        "line.26": {
            "text": "und Ihre gedichte"
        },
        "line.27": {
            "text": "von denen ich als erstes Morning las"
        },
        "line.28": {
            "text": "ich weiss dass Sie in Leipzig waren"
        },
        "line.29": {
            "text": "ich war diesen sommer in Magdeburg"
        },
        "line.30": {
            "text": "und besuchte die figur des Hl. Mauritius"
        },
        "line.31": {
            "text": "die um 1250 entstanden ist"
        },
        "line.32": {
            "text": "ich kenne aus dieser zeit keine so realistische"
        },
        "line.33": {
            "text": "darstellung eines afrikaners"
        },
        "line.34": {
            "text": "noch Dürers Rhinoceron ist nach beschreibungen"
        },
        "line.35": {
            "text": "entstanden"
        },
        "line.36": {
            "text": "ganz zu schweigen von elefanten und einhörnern"
        },
        "line.37": {
            "text": "und Barlach 1929"
        },
        "line.38": {
            "text": "nach meinem spaziergang"
        },
        "line.39": {
            "text": "unter dramatischen wolken"
        },
        "line.40": {
            "text": "las ich: Young Woman at a Window"
        },
        "line.41": {
            "text": "Sie sitzt mit"
        },
        "line.42": {
            "text": "tränen auf"
        },
        "line.43": {
            "text": "ihrer wange"
        },
        "line.44": {
            "text": "ihre wange auf"
        },
        "line.45": {
            "text": "ihrer hand"
        },
        "line.46": {
            "text": "das kind"
        },
        "line.47": {
            "text": "in ihrem schoss"
        },
        "line.48": {
            "text": "seine nase"
        },
        "line.49": {
            "text": "gepresst"
        },
        "line.50": {
            "text": "an die scheibe"
        },
        "line.51": {
            "text": "es ist herbst geworden"
        }
    }
}