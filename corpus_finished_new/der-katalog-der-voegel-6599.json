{
    "title": "der-katalog-der-voegel-6599",
    "gender": "m",
    "author": "Matthias  Göritz",
    "author_birth": "1969",
    "poem": {
        "line.1": {
            "text": "Wie aber (und wohin)"
        },
        "line.2": {
            "text": "sind Vögel übersetzbar"
        },
        "line.3": {
            "text": "Noten in einer"
        },
        "line.4": {
            "text": "Sprache, die aufscheint"
        },
        "line.5": {
            "text": "morgens"
        },
        "line.6": {
            "text": "das kehlige Klopfen der Blaumeise"
        },
        "line.7": {
            "text": "Schreie des Silberkopfreihers in"
        },
        "line.8": {
            "text": "fliegender Fahrt: Stimmimitate, ein"
        },
        "line.9": {
            "text": "schnelles Seeimplantat: Schilfrohr und Teich"
        },
        "line.10": {
            "text": "hinterm Moosvorhang, eh dann der Sonnenauf- (später"
        },
        "line.11": {
            "text": "Untergang), etwas Morgenwind zeigt"
        },
        "line.12": {
            "text": "(zwei schnelle Achtel)"
        },
        "line.13": {
            "text": "jenes Glück, daß jetzt"
        },
        "line.14": {
            "text": "wieder Tag ward"
        },
        "line.15": {
            "text": "Vögel"
        },
        "line.16": {
            "text": "tauchen auf"
        },
        "line.17": {
            "text": "wie ein unberührbares"
        },
        "line.18": {
            "text": "Transportunternehmen von"
        },
        "line.19": {
            "text": "Gott oder"
        },
        "line.20": {
            "text": "Mutter Natur"
        },
        "line.21": {
            "text": "Etwas spürbar"
        },
        "line.22": {
            "text": "Aufnehmbares"
        },
        "line.23": {
            "text": "(die Zeit)"
        },
        "line.24": {
            "text": "singt"
        },
        "line.25": {
            "text": "wenn die Feder fällt (die Hand"
        },
        "line.26": {
            "text": "jene Schwinge"
        },
        "line.27": {
            "text": "des Komponisten)"
        },
        "line.28": {
            "text": "Dieser Ast  zitternd  ist leer"
        },
        "line.29": {
            "text": "Zaudernd zeichnen deine"
        },
        "line.30": {
            "text": "und meine Finger"
        },
        "line.31": {
            "text": "mehr"
        },
        "line.32": {
            "text": "Himmel, Aufschwung und"
        },
        "line.33": {
            "text": "Luft als ein Mann"
        },
        "line.34": {
            "text": "jemals atmen kann"
        },
        "line.35": {
            "text": "rein wie der blaue Teppich der Luft"
        },
        "line.36": {
            "text": "Landschaft"
        },
        "line.37": {
            "text": "die schweigende Feier"
        },
        "line.38": {
            "text": "er setzte seinen rechten Fuß aufs Meer"
        },
        "line.39": {
            "text": "Vögel, zum Zeichen"
        },
        "line.40": {
            "text": "hinfort wird keine Zeit mehr sein"
        },
        "line.41": {
            "text": "rein schlägt ein"
        },
        "line.42": {
            "text": "letzter Ton, Engel der"
        },
        "line.43": {
            "text": "Apokalypse"
        }
    }
}