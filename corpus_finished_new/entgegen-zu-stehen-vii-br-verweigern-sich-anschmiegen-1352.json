{
    "title": "entgegen-zu-stehen-vii-br-verweigern-sich-anschmiegen-1352",
    "gender": "w",
    "author": "Anja Utler",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "verharren noch und: will vergessen zu"
        },
        "line.2": {
            "text": "sehen zu hören nur: murmeln mehr"
        },
        "line.3": {
            "text": "vielmals am kiefer der wurzelstock,"
        },
        "line.4": {
            "text": "fühlbar, der: schlamm in den poren"
        },
        "line.5": {
            "text": "sie: schuppen sich schorfen  beginnendes"
        },
        "line.6": {
            "text": "nesselfieber  die nässenden flerne sie"
        },
        "line.7": {
            "text": "wollen gerinnen zu: rinde zu borke"
        },
        "line.8": {
            "text": "im wind aus: nordost richtung murmle ich"
        },
        "line.9": {
            "text": "landmasse tundra  versteppende haut"
        },
        "line.10": {
            "text": "areale  sie: spreche ich speichle ich"
        },
        "line.11": {
            "text": "mir in die schürfstellen, wunden, ziel:"
        },
        "line.12": {
            "text": "lungenflechte statt haar"
        }
    }
}