{
    "title": "ein-ueberaus-schoenes-und-blaues-manoever-lilien-auf-die-brust-gemalt-fuer-thomas-kling-56",
    "gender": "w",
    "author": "Friederike  Mayröcker",
    "author_birth": "1924",
    "poem": {
        "line.1": {
            "text": "in den Haaren die Lindenbaumfächer"
        },
        "line.2": {
            "text": "nordafrikanischer Knötchenfrucht"
        },
        "line.3": {
            "text": "springen im funkelnden Wind nämlich"
        },
        "line.4": {
            "text": "zu Boden geschüttelt vom zaubrischen"
        },
        "line.5": {
            "text": "Schopf oder Duft oder Hölderlins Jugendlocke"
        },
        "line.6": {
            "text": "oder es steigt ein Hündchen schwammig"
        },
        "line.7": {
            "text": "ins herbeigerufene Taxi"
        },
        "line.8": {
            "text": "oder es stehen weiße Tennisschuhe zum Trocknen"
        },
        "line.9": {
            "text": "in der Sonne am offenen Fenster"
        },
        "line.10": {
            "text": "oder man liegt ausgestreckt mit wächsernen"
        },
        "line.11": {
            "text": "Ohren auf einer Bank im Halbschatten des Baumes"
        },
        "line.12": {
            "text": "welcher die Herzschläge zählt"
        },
        "line.13": {
            "text": "/ einer heiligen Caterina von Siena"
        },
        "line.14": {
            "text": "mit dem Lilienstab vor den weißen, vor den"
        },
        "line.15": {
            "text": "halbgeöffneten Augen"
        }
    }
}