{
    "title": "zuerich-innsbruckbri-aufzaehlung-i-landschaftsapril-5560",
    "gender": "m",
    "author": "José F. A. Oliver",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "die dreibeinige kuh im milchglas gefunden"
        },
        "line.2": {
            "text": ": 1 blau seilt sich ab aus stein & idyll"
        },
        "line.3": {
            "text": "poliert sich gebirg & gewässer"
        },
        "line.4": {
            "text": "[metapher für Osterreich sagt Fuad & doch keine"
        },
        "line.5": {
            "text": "lösung] 1 kopfknoten bücher"
        },
        "line.6": {
            "text": "wenn die landschaft sich altkämmt"
        },
        "line.7": {
            "text": "das rückwärtsgerollte das wort im verzug"
        },
        "line.8": {
            "text": "in fahrtrichtung gäste, die speisen."
        },
        "line.9": {
            "text": "1 schartiger schatten im brotlaib"
        },
        "line.10": {
            "text": "im"
        },
        "line.11": {
            "text": "krizzeiche"
        },
        "line.12": {
            "text": "schneidet die feier der messer"
        },
        "line.13": {
            "text": "1 altmahl in form & geschliffen"
        },
        "line.14": {
            "text": "1 kindswissen hunger & meertaugeschichten"
        },
        "line.15": {
            "text": "die fließen vom grün"
        },
        "line.16": {
            "text": "die fließen vom blau"
        },
        "line.17": {
            "text": "die fressen im wachschlaf aus gelbkapriolen"
        },
        "line.18": {
            "text": "das impressum aus harz war gefunden"
        },
        "line.19": {
            "text": "in die bücher geschlagene zeit"
        },
        "line.20": {
            "text": "e"
        },
        "line.21": {
            "text": "säsle"
        },
        "line.22": {
            "text": "die axt & gehortete sensen"
        },
        "line.23": {
            "text": "in Beirut St. Anton New York usw."
        },
        "line.24": {
            "text": "in häuser gebrütet. Vergessen, Paris aufzuzählen"
        },
        "line.25": {
            "text": "& Todtnau & Wien & reisende städter"
        },
        "line.26": {
            "text": "1 dichter"
        },
        "line.27": {
            "text": "der neben mir wasser betrachtet"
        },
        "line.28": {
            "text": "die schläfrige landschaft als bett in den augen"
        },
        "line.29": {
            "text": "ihr wächtergeruch aus verlottertem schnee"
        },
        "line.30": {
            "text": "& psalmen im nacken"
        },
        "line.31": {
            "text": "die tunnel verschriftet"
        },
        "line.32": {
            "text": "& lampen wie welten"
        },
        "line.33": {
            "text": "& welten wie lampen"
        },
        "line.34": {
            "text": "das nicht zu besprechende licht im visier"
        },
        "line.35": {
            "text": "1 lustalphabet ins erwarten getrieben & offen"
        },
        "line.36": {
            "text": "die kürzel der bäume"
        },
        "line.37": {
            "text": "in die himmel gestreckt"
        },
        "line.38": {
            "text": "die felsengebete aus stein & verlangen"
        },
        "line.39": {
            "text": "& wieder nur fragen"
        },
        "line.40": {
            "text": "die niemand befragen"
        },
        "line.41": {
            "text": "ist ein fluß übersetzer? verdichter? entrümpler?"
        },
        "line.42": {
            "text": "& schweigt er metaphern"
        },
        "line.43": {
            "text": "wie tote ins meer?"
        },
        "line.44": {
            "text": "Kein wort will ich glauben"
        },
        "line.45": {
            "text": "für Fuad Rifka"
        }
    }
}