{
    "title": "territorium-13893",
    "gender": "w",
    "author": "Kerstin Becker",
    "author_birth": "1969",
    "poem": {
        "line.1": {
            "text": "die Panzerstraße zwischen den Wäldern"
        },
        "line.2": {
            "text": "in denen wir Sommer für Sommer"
        },
        "line.3": {
            "text": "mit Sprossenschmerzen"
        },
        "line.4": {
            "text": "mit dicken Grinden"
        },
        "line.5": {
            "text": "mit schwarzen Fußsohlen"
        },
        "line.6": {
            "text": "aufgehen ist für die Ewigkeit"
        },
        "line.7": {
            "text": "Staubschleier grauen wir pesen"
        },
        "line.8": {
            "text": "davon eingehüllt mit dem Ostwind aus"
        },
        "line.9": {
            "text": "gediente Dachpappefetzen fliegen"
        },
        "line.10": {
            "text": "im süßlichen Simsongestank"
        },
        "line.11": {
            "text": "Rohre für lange Leitungen liegen"
        },
        "line.12": {
            "text": "brach wo die weißen Birken"
        },
        "line.13": {
            "text": "an Wegerichpisten schießen wir"
        },
        "line.14": {
            "text": "fummeln wir suchen"
        },
        "line.15": {
            "text": "immer heftiger knutschend"
        },
        "line.16": {
            "text": "unsern Zusammenhang"
        }
    }
}