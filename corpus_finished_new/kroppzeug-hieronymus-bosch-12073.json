{
    "title": "kroppzeug-hieronymus-bosch-12073",
    "gender": "m",
    "author": "Hendrik  Jackson",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "kleine kohorten tragen durch die tage flaggen eifrig bergan, kroppzeug"
        },
        "line.2": {
            "text": "mit der doppelklaue der raffsucht; warte ich derweil auf die ein oder"
        },
        "line.3": {
            "text": "andere abzweigung (..............................................................."
        },
        "line.4": {
            "text": "...........) zuweilen auch flattern der planen und _"
        },
        "line.5": {
            "text": "leere spiele ohne emblematik und _"
        },
        "line.6": {
            "text": "sonne an der schulter"
        }
    }
}