{
    "title": "cafe-hennerdreck-4322",
    "gender": "w",
    "author": "Dagmar Leupold",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "Ganz anders dieses Abseits:"
        },
        "line.2": {
            "text": "Wespen heißen hier"
        },
        "line.3": {
            "text": "Wepsen und die Hähne"
        },
        "line.4": {
            "text": "benehmen sich wie andernorts"
        },
        "line.5": {
            "text": "ausgestorbene Gockel"
        },
        "line.6": {
            "text": "der alte Apfelbaum der"
        },
        "line.7": {
            "text": "uns beschattet wird nichts"
        },
        "line.8": {
            "text": "verraten, nichts von den"
        },
        "line.9": {
            "text": "großen Portionen und nichts"
        },
        "line.10": {
            "text": "von dem großen Hunger"
        },
        "line.11": {
            "text": "den wir unter seinen Ästen"
        },
        "line.12": {
            "text": "stillen und wecken -"
        },
        "line.13": {
            "text": "amen möchte man sagen"
        },
        "line.14": {
            "text": "wenn die Suppe kommt"
        },
        "line.15": {
            "text": "behütet von sehr viel Busen:"
        },
        "line.16": {
            "text": "zugeknöpft! Aus der Stadt"
        },
        "line.17": {
            "text": "kommen Sie? Ja dann"
        },
        "line.18": {
            "text": "kein Wunder, daß Sie uns"
        },
        "line.19": {
            "text": "immer wieder suchen müssen"
        },
        "line.20": {
            "text": "so gehts allen mit dem Glück"
        }
    }
}