{
    "title": "about-horst-janssen-weisst-du-weisch-13682",
    "gender": "w",
    "author": "Claudia Gabler",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "Weißt du  weisch"
        },
        "line.2": {
            "text": "was da unter uns"
        },
        "line.3": {
            "text": "so schnurrt."
        },
        "line.4": {
            "text": ""
        },
        "line.5": {
            "text": "Das sind 186 heiße Rappen,"
        },
        "line.6": {
            "text": "flüsterte er  auch wenn er wusste,"
        },
        "line.7": {
            "text": "dass wieder jemand sagen würde: DU NARR, DU"
        },
        "line.8": {
            "text": ""
        },
        "line.9": {
            "text": "MIT DEINEN TALENTEN!"
        },
        "line.10": {
            "text": "MIT DEINER TÜCKISCHEN TAKTIK!"
        },
        "line.11": {
            "text": "Doch wie tumm, tieses Tenken"
        },
        "line.12": {
            "text": ""
        },
        "line.13": {
            "text": "Von den Talenten. Weisch,"
        },
        "line.14": {
            "text": "das ist einfach zu groß, einige Meere zu groß"
        },
        "line.15": {
            "text": "für die durchschnittlichen Leut."
        },
        "line.16": {
            "text": ""
        },
        "line.17": {
            "text": "Der Wein ist zum Leben"
        },
        "line.18": {
            "text": "und nicht zum Käse da und da hängt"
        },
        "line.19": {
            "text": "Onkelchen Bilder auf, unter denen steht groß:"
        },
        "line.20": {
            "text": ""
        },
        "line.21": {
            "text": "Kunscht"
        },
        "line.22": {
            "text": "oder Knuscht"
        },
        "line.23": {
            "text": "oder Knutsch"
        },
        "line.24": {
            "text": ""
        },
        "line.25": {
            "text": "Mir doch den A* weg"
        },
        "line.26": {
            "text": "Weg"
        },
        "line.27": {
            "text": "bietet grad keine Taste an."
        }
    }
}