{
    "title": "o-ton-schonendes-gedicht-11647",
    "gender": "w",
    "author": "Odile Kennel",
    "author_birth": "1967",
    "poem": {
        "line.1": {
            "text": "ich will ja ich WILL wieder"
        },
        "line.2": {
            "text": "in die allmähliche gemächliche"
        },
        "line.3": {
            "text": "Wendung fallen der Wörter"
        },
        "line.4": {
            "text": "ins Drehen faul Herumstehen"
        },
        "line.5": {
            "text": "einfallen lassen über das flüchtige"
        },
        "line.6": {
            "text": "Gesuchte fluchen flüstern"
        },
        "line.7": {
            "text": "wie das knistert wie das flirrt"
        },
        "line.8": {
            "text": "beim Silben zwirbeln will Silber"
        },
        "line.9": {
            "text": "verticken Taktik die mir"
        },
        "line.10": {
            "text": "liegt mir Taktfühlung gibt"
        },
        "line.11": {
            "text": "genieße frühe Früchtchen"
        },
        "line.12": {
            "text": "führ Süßkirschengeklimper"
        },
        "line.13": {
            "text": "zur Lippe die Wimper bestimme"
        },
        "line.14": {
            "text": "mit Stimmungsbüchern sieh wie"
        },
        "line.15": {
            "text": "sie zwinkert blinzelt im Sinn-"
        },
        "line.16": {
            "text": "gewitter trittsicher Zwirn entzweit"
        },
        "line.17": {
            "text": "von Wolle: schon stolpert O-Ton"
        },
        "line.18": {
            "text": "ohne Not in den Worthort,"
        },
        "line.19": {
            "text": "soit!:"
        },
        "line.20": {
            "text": "WILL zottlige Wortzossen wildern"
        },
        "line.21": {
            "text": "den U-Wert des Gedichts vertuscheln"
        },
        "line.22": {
            "text": "um Kruscht buhlend fuchteln (so"
        },
        "line.23": {
            "text": "könnte das lange weitergehen)"
        }
    }
}