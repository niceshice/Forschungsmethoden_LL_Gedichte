{
    "title": "was-ist-nur-mit-mir-828",
    "gender": "m",
    "author": "Gerhard Rühm",
    "author_birth": "1930",
    "poem": {
        "line.1": {
            "text": "was ist nur mit mir"
        },
        "line.2": {
            "text": "ich bin so verwirr"
        },
        "line.3": {
            "text": "ein hund bellt mich an"
        },
        "line.4": {
            "text": "es stupst mich ein mann"
        },
        "line.5": {
            "text": "meine hände sind grau"
        },
        "line.6": {
            "text": "es glotzt eine frau"
        },
        "line.7": {
            "text": "was ist nur mit mir"
        },
        "line.8": {
            "text": "ich bin so verwirr"
        },
        "line.9": {
            "text": "es trifft mich ein blatt"
        },
        "line.10": {
            "text": "und der regen hat"
        },
        "line.11": {
            "text": "mir den herbst beschert"
        },
        "line.12": {
            "text": "mein hut sitzt verkehrt"
        },
        "line.13": {
            "text": "was ist nur mit mir"
        },
        "line.14": {
            "text": "ich bin so verwirr"
        },
        "line.15": {
            "text": "vor wildfremdem haus"
        },
        "line.16": {
            "text": "gleite ich aus"
        },
        "line.17": {
            "text": "und fall in den dreck"
        },
        "line.18": {
            "text": "ein finger ist weg"
        },
        "line.19": {
            "text": "was ist nur mit mir"
        },
        "line.20": {
            "text": "ich bin so verwirr"
        },
        "line.21": {
            "text": "ich zähl mit bedacht"
        },
        "line.22": {
            "text": "es sind nur noch acht"
        },
        "line.23": {
            "text": "ich spucke blut"
        },
        "line.24": {
            "text": "mir ist gar nicht gut"
        },
        "line.25": {
            "text": "was ist nur mit mir"
        },
        "line.26": {
            "text": "ich bin ganz verwirr"
        },
        "line.27": {
            "text": "da kommt eine welle"
        },
        "line.28": {
            "text": "spült mich von der stelle"
        },
        "line.29": {
            "text": "drückt mich auf die bank"
        },
        "line.30": {
            "text": "und rauscht ich sei krank"
        },
        "line.31": {
            "text": "was ist nur mit mir"
        },
        "line.32": {
            "text": "ich bin so verwirr"
        },
        "line.33": {
            "text": "ich hocke da"
        },
        "line.34": {
            "text": "weiss nicht was geschah"
        },
        "line.35": {
            "text": "weiss nicht wo ich bin"
        },
        "line.36": {
            "text": "versteh nicht den sinn"
        },
        "line.37": {
            "text": "was ist nur mit mir"
        },
        "line.38": {
            "text": "ich bin ganz verwirr"
        },
        "line.39": {
            "text": "vor mir steht ein paar"
        },
        "line.40": {
            "text": "eine frau und ihr haar"
        },
        "line.41": {
            "text": "und daneben glatt"
        },
        "line.42": {
            "text": "der regen mit blatt"
        },
        "line.43": {
            "text": "um blatt um blatt"
        },
        "line.44": {
            "text": "dahinter das haus"
        },
        "line.45": {
            "text": "ich schwanke heraus"
        },
        "line.46": {
            "text": "wildfremd"
        },
        "line.47": {
            "text": "im hemd"
        },
        "line.48": {
            "text": "die finger im munde"
        },
        "line.49": {
            "text": "da bellen die hunde"
        },
        "line.50": {
            "text": "und quellen die haare"
        },
        "line.51": {
            "text": "und jahre vergehen"
        },
        "line.52": {
            "text": "jahre um jahre"
        }
    }
}