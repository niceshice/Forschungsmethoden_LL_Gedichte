{
    "title": "nicht-gesagt-1278",
    "gender": "w",
    "author": "Marie Luise Kaschnitz",
    "author_birth": "1901",
    "poem": {
        "line.1": {
            "text": "Nicht gesagt"
        },
        "line.2": {
            "text": "Was von der Sonne zu sagen gewesen wäre"
        },
        "line.3": {
            "text": "Und vom Blitz nicht das einzige Richtige"
        },
        "line.4": {
            "text": "Geschweige denn von der Liebe."
        },
        "line.5": {
            "text": "Versuche. Gesuche. Mißlungen"
        },
        "line.6": {
            "text": "Ungenaue Beschreibung"
        },
        "line.7": {
            "text": "Weggelassen das Morgenrot"
        },
        "line.8": {
            "text": "Nicht gesprochen vom Sämann"
        },
        "line.9": {
            "text": "Und nur am Rande vermerkt"
        },
        "line.10": {
            "text": "Den Hahnenfuß und das Veilchen."
        },
        "line.11": {
            "text": "Euch nicht den Rücken gestärkt"
        },
        "line.12": {
            "text": "Mit ewiger Seligkeit"
        },
        "line.13": {
            "text": "Den Verfall nicht geleugnet"
        },
        "line.14": {
            "text": "Und nicht die Verzweiflung"
        },
        "line.15": {
            "text": "Den Teufel nicht an die Wand"
        },
        "line.16": {
            "text": "Weil ich nicht an ihn glaube"
        },
        "line.17": {
            "text": "Gott nicht gelobt"
        },
        "line.18": {
            "text": "Aber wer bin ich daß"
        }
    }
}