{
    "title": "anders-jedoch-682",
    "gender": "m",
    "author": "Jürg Schubiger",
    "author_birth": "1936",
    "poem": {
        "line.1": {
            "text": "Ach, das meiste"
        },
        "line.2": {
            "text": "ist doch hundsgewöhnlich."
        },
        "line.3": {
            "text": "Dieser Hund zum Beispiel,"
        },
        "line.4": {
            "text": "oder dass die Vögel fliegen,"
        },
        "line.5": {
            "text": "dass die Flüsse fliessen"
        },
        "line.6": {
            "text": "und die Ufer bleiben."
        },
        "line.7": {
            "text": "Anderes jedoch"
        },
        "line.8": {
            "text": "ist höchst erstaunlich."
        },
        "line.9": {
            "text": "Dieser Hund zum Beispiel,"
        },
        "line.10": {
            "text": "oder dass die Vögel fliegen,"
        },
        "line.11": {
            "text": "dass die Flüsse fliessen"
        },
        "line.12": {
            "text": "und die Ufer bleiben."
        },
        "line.13": {
            "text": "Dass uns solche Dinge"
        },
        "line.14": {
            "text": "durch die Köpfe gehn."
        }
    }
}