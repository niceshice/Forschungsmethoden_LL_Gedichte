{
    "title": "als-ich-ging-191",
    "gender": "m",
    "author": "Peter Waterhouse",
    "author_birth": "1956",
    "poem": {
        "line.1": {
            "text": "Als ich ging"
        },
        "line.2": {
            "text": "in einem Wäldchen"
        },
        "line.3": {
            "text": "standen Blumen"
        },
        "line.4": {
            "text": "gelbe Blumen"
        },
        "line.5": {
            "text": "gelbes Licht"
        },
        "line.6": {
            "text": "Aber dunkel"
        },
        "line.7": {
            "text": "dunkel dunkel"
        },
        "line.8": {
            "text": "voll von Duft"
        },
        "line.9": {
            "text": "als ich ging"
        },
        "line.10": {
            "text": "auf Sand und Moos"
        },
        "line.11": {
            "text": "Mich berührte"
        },
        "line.12": {
            "text": "eine Hand"
        },
        "line.13": {
            "text": "viele Finger"
        },
        "line.14": {
            "text": "Münder Münder"
        },
        "line.15": {
            "text": "voller Duft"
        },
        "line.16": {
            "text": "Kleider"
        },
        "line.17": {
            "text": "schnelle Beine"
        },
        "line.18": {
            "text": "weiße Füße"
        },
        "line.19": {
            "text": "weiße Haut"
        },
        "line.20": {
            "text": "als ich ging"
        },
        "line.21": {
            "text": "Du"
        },
        "line.22": {
            "text": "wer bist du"
        },
        "line.23": {
            "text": "dunkel dunkel"
        },
        "line.24": {
            "text": "Licht und Licht"
        },
        "line.25": {
            "text": "auf süßem Moos"
        },
        "line.26": {
            "text": "Harz"
        },
        "line.27": {
            "text": "Harz ist gelb"
        },
        "line.28": {
            "text": "Harz ist klebrig"
        },
        "line.29": {
            "text": "rinnt hinab"
        },
        "line.30": {
            "text": "über meine Augen"
        },
        "line.31": {
            "text": "Blumen"
        },
        "line.32": {
            "text": "halb geöffnet"
        },
        "line.33": {
            "text": "halb versunken"
        },
        "line.34": {
            "text": "unter Gras"
        },
        "line.35": {
            "text": "und Harz"
        },
        "line.36": {
            "text": "Was bist du dort"
        },
        "line.37": {
            "text": "Ein Apfelbaum"
        },
        "line.38": {
            "text": "Eine Wolke"
        },
        "line.39": {
            "text": "Schaum"
        },
        "line.40": {
            "text": "in einem Wäldchen"
        },
        "line.41": {
            "text": "Zweige"
        },
        "line.42": {
            "text": "Schaum"
        },
        "line.43": {
            "text": "Zwischenraum"
        },
        "line.44": {
            "text": "in einem Wäldchen"
        },
        "line.45": {
            "text": "halb geöffnet"
        },
        "line.46": {
            "text": "Ein Stachel blitzt"
        },
        "line.47": {
            "text": "Honig blitzt"
        },
        "line.48": {
            "text": "ein Mond blitzt"
        },
        "line.49": {
            "text": "Erdbeer"
        },
        "line.50": {
            "text": "und Klee"
        },
        "line.51": {
            "text": "Tot"
        },
        "line.52": {
            "text": "tot und lebendig"
        },
        "line.53": {
            "text": "Lebendig"
        },
        "line.54": {
            "text": "lebendig und tot"
        },
        "line.55": {
            "text": "Weit gekommen"
        },
        "line.56": {
            "text": "Nein, von nah"
        },
        "line.57": {
            "text": "Von nah gekommen"
        },
        "line.58": {
            "text": "Nein, von weit"
        },
        "line.59": {
            "text": "Sonnestrahl"
        },
        "line.60": {
            "text": "die Blätter zittern"
        },
        "line.61": {
            "text": "auf meiner Brust"
        },
        "line.62": {
            "text": "zittern die Schatten"
        },
        "line.63": {
            "text": "Gesicht"
        },
        "line.64": {
            "text": "Gewendet"
        },
        "line.65": {
            "text": "Gereift"
        },
        "line.66": {
            "text": "In einem Mäntelchen ich"
        },
        "line.67": {
            "text": "Als ich ging"
        },
        "line.68": {
            "text": "in einem Wäldchen"
        },
        "line.69": {
            "text": "stellte alles"
        },
        "line.70": {
            "text": "im Geist sich dar"
        }
    }
}