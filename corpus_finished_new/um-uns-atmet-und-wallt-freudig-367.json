{
    "title": "um-uns-atmet-und-wallt-freudig-367",
    "gender": "m",
    "author": "Paul Wühr",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "UM UNS ATMET und wallt freudig"
        },
        "line.2": {
            "text": "und jedem hold um uns jedem"
        },
        "line.3": {
            "text": "trauernd"
        },
        "line.4": {
            "text": "wallt freudig gerne wo Lebendes"
        },
        "line.5": {
            "text": "uns atmet habe ich meinen"
        },
        "line.6": {
            "text": "Dienst aufgesagt"
        },
        "line.7": {
            "text": "sind denn mir nicht verwandt"
        },
        "line.8": {
            "text": "alle Lebendigen weiter fort"
        },
        "line.9": {
            "text": "aus dem Tod"
        },
        "line.10": {
            "text": "rede ich mich und fürchte mich"
        },
        "line.11": {
            "text": "sehr nämlich in schweigender"
        },
        "line.12": {
            "text": "Wassertiefe"
        },
        "line.13": {
            "text": "der leichte Schwimmer wandelt"
        },
        "line.14": {
            "text": "wohin die Woge den Blöden"
        },
        "line.15": {
            "text": "treulich"
        },
        "line.16": {
            "text": "schmeichelnd hinuntergezogen"
        },
        "line.17": {
            "text": "wirklich ist das wahrlich"
        },
        "line.18": {
            "text": "einmal"
        },
        "line.19": {
            "text": "schon ertrunken worden seitdem"
        },
        "line.20": {
            "text": "diese Rede ihren Frieden"
        },
        "line.21": {
            "text": "gebrochen hat"
        },
        "line.22": {
            "text": "wie es die Weise zu sein hat als"
        },
        "line.23": {
            "text": "jedem sein Gott verschwiegen"
        },
        "line.24": {
            "text": "wird oder"
        },
        "line.25": {
            "text": "ist mein Mund in der Luft hat er"
        },
        "line.26": {
            "text": "die Linien in die Freude und"
        },
        "line.27": {
            "text": "bin ich"
        },
        "line.28": {
            "text": "grüner geworden bis zum Hals"
        },
        "line.29": {
            "text": "hinauf als ich blöder wurde"
        },
        "line.30": {
            "text": "in meinem schlimmen Gedächtnis"
        },
        "line.31": {
            "text": "so abhanden gekommen ist mir mein"
        },
        "line.32": {
            "text": "ganzes Gesicht als es geschlossen"
        },
        "line.33": {
            "text": "die Augen"
        },
        "line.34": {
            "text": "den Mund aufmacht zu dir war es"
        },
        "line.35": {
            "text": "nie zerrissener im Gedenken an"
        },
        "line.36": {
            "text": "dein Gesicht"
        },
        "line.37": {
            "text": "zum Reden gekommen wer weiß was"
        },
        "line.38": {
            "text": "die Zunge in meinem Kopf noch"
        },
        "line.39": {
            "text": "sagen wird oben"
        },
        "line.40": {
            "text": "jedem hold jedem trauernd wo sich"
        },
        "line.41": {
            "text": "vieles gesellt freudig und jedem"
        },
        "line.42": {
            "text": "offen"
        },
        "line.43": {
            "text": "sind denn dir nicht verwandt alle"
        },
        "line.44": {
            "text": "weinen wo der Bruder uns verließ"
        },
        "line.45": {
            "text": "denke ich manches"
        },
        "line.46": {
            "text": "wohl an warnender Stelle und wenn"
        },
        "line.47": {
            "text": "ich rede wie schweigt seine Stimme"
        },
        "line.48": {
            "text": "in blauender Halle"
        },
        "line.49": {
            "text": "vor einer langen Gerechtigkeit"
        }
    }
}