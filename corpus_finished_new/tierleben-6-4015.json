        "line.1": {
            "text": "6"
        },
        "line.2": {
            "text": "Ist die zweite Natur"
        },
        "line.3": {
            "text": "zwischen Ruhe und Swing"
        },
        "line.4": {
            "text": "die der Welle. In allen"
        },
        "line.5": {
            "text": "Dingen schwingt"
        },
        "line.6": {
            "text": "schon das Werk und"
        },
        "line.7": {
            "text": "drängt die Tat. Wie das Jucken"
        },
        "line.8": {
            "text": "unterm Schulterblatt wo"
        },
        "line.9": {
            "text": "das Begehren wächst. Wo"
        },
        "line.10": {
            "text": "Flügel spriessen ist"
        },
        "line.11": {
            "text": "die Lust nicht weit. (Wer"
        },
        "line.12": {
            "text": "träume wen:) Die Luft"
        },
        "line.13": {
            "text": "zu zweit zu reiten. Worauf"
        },
        "line.14": {
            "text": "schon bald  kein Tier"
        },
        "line.15": {
            "text": "dazwischen!"
        },
        "line.16": {
            "text": "der Dreckeffekt der"
        },
        "line.17": {
            "text": "Jetztzeit folgt."
        },
        "line.18": {
            "text": "(\"Zwischen Ruhe und"
        },
        "line.19": {
            "text": "Swing die Wellennatur. In"
        },
        "line.20": {
            "text": "allen Dingen schwingt"
        },
        "line.21": {
            "text": "die Tat. Wie ein Jucken wo"
        },
        "line.22": {
            "text": "die Lust  die Luft"
        },
        "line.23": {
            "text": "zu zweit zu reiten  wächst:"
        },
        "line.24": {
            "text": "Am linken Schulterblatt"
        },
        "line.25": {
            "text": "der rechte Flügel."
        },
        "line.26": {
            "text": "Worauf schon bald die"
        },
        "line.27": {
            "text": "Jetztzeit folgt.\")"
        }
    }
