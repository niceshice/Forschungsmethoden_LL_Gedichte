{
    "title": "minsk-11917",
    "gender": "w",
    "author": "Anja Kampmann",
    "author_birth": "1983",
    "poem": {
        "line.1": {
            "text": "Ich habe nie so viel Schnee gesehen"
        },
        "line.2": {
            "text": "nicht so viele Landstriche so dick überfroren"
        },
        "line.3": {
            "text": "kalt und eisstill wie das Land das sich abkehrt von uns"
        },
        "line.4": {
            "text": "in ein inneres Gespräch senkt sich"
        },
        "line.5": {
            "text": "die Weite mit der Weite"
        },
        "line.6": {
            "text": "einige rennen darauf sterben verrecken tausende"
        },
        "line.7": {
            "text": "graben und ziehen Linien aus Draht in die"
        },
        "line.8": {
            "text": "vereiste Luft in. Die Wälder sind tief"
        },
        "line.9": {
            "text": "keiner geht darin"
        },
        "line.10": {
            "text": "ohne Grund"
        },
        "line.11": {
            "text": "nur stufenweise Geschichte aufgeblättert"
        },
        "line.12": {
            "text": "die Propeller der Maschinen"
        },
        "line.13": {
            "text": "unaufhörliche Wiederholungen der"
        },
        "line.14": {
            "text": "Kälte"
        },
        "line.15": {
            "text": "nicht Wille nicht Erdknochen nur"
        },
        "line.16": {
            "text": "wie weit kann man laufen"
        },
        "line.17": {
            "text": "wenn der Himmel sich einwärts biegt Eisregen fasst"
        },
        "line.18": {
            "text": "die Statik des Himmels der Sterne"
        },
        "line.19": {
            "text": "Schiffsladungen die Docker fremder Häfen"
        },
        "line.20": {
            "text": "Kohle Knochen Hoffen auf etwas"
        },
        "line.21": {
            "text": "was fern liegt"
        },
        "line.22": {
            "text": "angerührt vom Innersten her"
        },
        "line.23": {
            "text": "junge Buchen"
        },
        "line.24": {
            "text": "hell oder neu oder Wind."
        }
    }
}