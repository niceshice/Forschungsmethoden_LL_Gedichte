{
    "title": "woat-i-3653",
    "gender": "m",
    "author": "Axel  Karner",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "wos du aa heast"
        },
        "line.2": {
            "text": "im haus is es stüll"
        },
        "line.3": {
            "text": "de leit rednd nit"
        },
        "line.4": {
            "text": "fia dei woat"
        },
        "line.5": {
            "text": "muaßt ongst hobn"
        },
        "line.6": {
            "text": "oba hinta da wond"
        },
        "line.7": {
            "text": "leise weata"
        },
        "line.8": {
            "text": "deine briada"
        },
        "line.9": {
            "text": "in da aufgrissenen tia"
        },
        "line.10": {
            "text": "ka woat wead do gredet"
        },
        "line.11": {
            "text": "- - - - -"
        },
        "line.12": {
            "text": "wort I"
        },
        "line.13": {
            "text": "was du auch hörst"
        },
        "line.14": {
            "text": "im haus ist es still"
        },
        "line.15": {
            "text": "die leute reden nicht"
        },
        "line.16": {
            "text": "für dein wort"
        },
        "line.17": {
            "text": "musst du angst haben"
        },
        "line.18": {
            "text": "aber hinter der wand"
        },
        "line.19": {
            "text": "leise wörter"
        },
        "line.20": {
            "text": "deine brüder"
        },
        "line.21": {
            "text": "in der aufgerissenen tür"
        },
        "line.22": {
            "text": "kein wort wird da geredet"
        }
    }
}