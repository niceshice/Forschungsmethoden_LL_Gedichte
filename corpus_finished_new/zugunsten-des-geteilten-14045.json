{
    "title": "zugunsten-des-geteilten-14045",
    "gender": "w",
    "author": "Anja Bayer",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "zugunsten des geteilten"
        },
        "line.2": {
            "text": "die bildervergiftung"
        },
        "line.3": {
            "text": "zurückhalten oder aus"
        },
        "line.4": {
            "text": "den zellen spülen"
        },
        "line.5": {
            "text": "ohne dritte"
        },
        "line.6": {
            "text": "zu schaden zu bringen"
        },
        "line.7": {
            "text": "ohne umgebungsluft wird"
        },
        "line.8": {
            "text": "der ausatem tödlich"
        },
        "line.9": {
            "text": "so hockendes aus"
        },
        "line.10": {
            "text": "tauschen ohne zu"
        },
        "line.11": {
            "text": "tat nicht umsonst"
        },
        "line.12": {
            "text": "will das dritte"
        },
        "line.13": {
            "text": "das geistreichende"
        },
        "line.14": {
            "text": "sein statt anruf"
        },
        "line.15": {
            "text": "der anruf"
        },
        "line.16": {
            "text": "ung statt"
        },
        "line.17": {
            "text": "geben"
        }
    }
}