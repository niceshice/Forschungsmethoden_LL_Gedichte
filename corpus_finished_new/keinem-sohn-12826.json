    "author": "Rainer René Mueller",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "... im Juli :"
        },
        "line.2": {
            "text": "wir hätten das abgeklopft,"
        },
        "line.3": {
            "text": "was"
        },
        "line.4": {
            "text": ": Namen heißt / Ding-"
        },
        "line.5": {
            "text": "-'en, zu sein ..."
        },
        "line.6": {
            "text": "Da : die Quitte"
        },
        "line.7": {
            "text": "setzt (,wenn es glückt,)"
        },
        "line.8": {
            "text": "dieses und"
        },
        "line.9": {
            "text": "/ und ihr Gelb"
        },
        "line.10": {
            "text": "an; so,"
        },
        "line.11": {
            "text": "als gälte das"
        },
        "line.12": {
            "text": ",wie sich, z.B. die Stein-"
        },
        "line.13": {
            "text": "Nelke"
        },
        "line.14": {
            "text": "durch den Zaun quetscht -"
        },
        "line.15": {
            "text": ": dass es ein"
        },
        "line.16": {
            "text": "ver- /"
        },
        "line.17": {
            "text": "sei, ein -gangenes :"
        },
        "line.18": {
            "text": "dieses Zusehn"
        }
    }
}