{
    "title": "6-januar-5723",
    "gender": "w",
    "author": "Elfriede Czurda",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "ist auch der tisch"
        },
        "line.2": {
            "text": "gedeckt so fehlt doch"
        },
        "line.3": {
            "text": "oft das salz als korn"
        },
        "line.4": {
            "text": "es fehlt die mühle"
        },
        "line.5": {
            "text": "die den pfeffer mahlt"
        },
        "line.6": {
            "text": "es fehlt der löffel"
        },
        "line.7": {
            "text": "der die suppe löffelt"
        },
        "line.8": {
            "text": "und es fehlt der mut"
        },
        "line.9": {
            "text": "die ganz versalzne suppe"
        },
        "line.10": {
            "text": "dem koch zurückzupfeffern"
        },
        "line.11": {
            "text": "auf daß er nicht verliebt"
        },
        "line.12": {
            "text": "das salz als korn verkennt"
        },
        "line.13": {
            "text": "6. januar; tisch"
        }
    }
}