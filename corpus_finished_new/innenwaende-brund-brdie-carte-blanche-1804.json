{
    "title": "innenwaende-brund-brdie-carte-blanche-1804",
    "gender": "w",
    "author": "Isabeella Beumer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "letztgereicht"
        },
        "line.2": {
            "text": "dem korrespondenten in die phantasie"
        },
        "line.3": {
            "text": "blicksprung"
        },
        "line.4": {
            "text": "trittrauschen"
        },
        "line.5": {
            "text": "und"
        },
        "line.6": {
            "text": "der netzthaut"
        },
        "line.7": {
            "text": "einander das selbe boot"
        },
        "line.8": {
            "text": "schatten verspiegelt"
        },
        "line.9": {
            "text": "du wirst dir selbst die akkorde verspielen"
        },
        "line.10": {
            "text": "sondern jede möglichkeit"
        },
        "line.11": {
            "text": "einer roten beere"
        },
        "line.12": {
            "text": "in den bruchteil"
        },
        "line.13": {
            "text": "der eisdecke"
        },
        "line.14": {
            "text": "soll ich dir die wahrscheinlichkeit häuten"
        },
        "line.15": {
            "text": "obwohl"
        },
        "line.16": {
            "text": "kenntlich gemachte"
        },
        "line.17": {
            "text": "fenster"
        },
        "line.18": {
            "text": "baumkronen"
        },
        "line.19": {
            "text": "ornamente"
        },
        "line.20": {
            "text": "beginn und"
        },
        "line.21": {
            "text": "einzig voll moos"
        },
        "line.22": {
            "text": "federnd dem einbein"
        },
        "line.23": {
            "text": "die hand"
        },
        "line.24": {
            "text": "das souterrain"
        },
        "line.25": {
            "text": "ins auge zitiert"
        },
        "line.26": {
            "text": "zwei farben"
        },
        "line.27": {
            "text": "einer nacht"
        },
        "line.28": {
            "text": "manchmal blutet eine kleine wunde"
        },
        "line.29": {
            "text": "wieder"
        },
        "line.30": {
            "text": "schweigen"
        },
        "line.31": {
            "text": "zu traumasche"
        },
        "line.32": {
            "text": "herzkerben"
        },
        "line.33": {
            "text": "kalendersperrung"
        },
        "line.34": {
            "text": "vertropft kristalle"
        },
        "line.35": {
            "text": "an dir die finger"
        },
        "line.36": {
            "text": "durchschwiegen"
        },
        "line.37": {
            "text": "eine abgewandte hälfte"
        },
        "line.38": {
            "text": "das licht als tag"
        },
        "line.39": {
            "text": "wieder aufbricht"
        },
        "line.40": {
            "text": "oder der vogel schlafender"
        },
        "line.41": {
            "text": "wolken zieht"
        },
        "line.42": {
            "text": "mein los aus"
        },
        "line.43": {
            "text": "dem himmelsbaum"
        },
        "line.44": {
            "text": "der nacht"
        },
        "line.45": {
            "text": "verbirg dich darin"
        },
        "line.46": {
            "text": "im liebkosen der gegenwart"
        },
        "line.47": {
            "text": "konturen einer lautlosen lawine"
        },
        "line.48": {
            "text": "streich das masz"
        },
        "line.49": {
            "text": "verfall in die unhörbarkeit"
        },
        "line.50": {
            "text": "der hände"
        },
        "line.51": {
            "text": "all deiner zungen"
        },
        "line.52": {
            "text": "als berührung"
        },
        "line.53": {
            "text": "nachgeahnt"
        },
        "line.54": {
            "text": "der winter tropft kreise"
        },
        "line.55": {
            "text": "verspielt"
        },
        "line.56": {
            "text": "windakkorde im knopfloch"
        },
        "line.57": {
            "text": "der park noch blutleer"
        },
        "line.58": {
            "text": "lasz uns die wunden vernähen"
        },
        "line.59": {
            "text": "mit weiszem faden"
        },
        "line.60": {
            "text": "federnhaut und"
        },
        "line.61": {
            "text": "magnolienblüten"
        },
        "line.62": {
            "text": "ein noch leerer schuh"
        },
        "line.63": {
            "text": "lebender schwalben"
        },
        "line.64": {
            "text": "ankunftsorte"
        },
        "line.65": {
            "text": "in die schale des himmels"
        },
        "line.66": {
            "text": "aufgehoben"
        },
        "line.67": {
            "text": "über die flugbahn"
        },
        "line.68": {
            "text": "ein lidschlag"
        },
        "line.69": {
            "text": "auf das löschpapier"
        },
        "line.70": {
            "text": "sondern immer"
        },
        "line.71": {
            "text": "unmittelbar"
        }
    }
}