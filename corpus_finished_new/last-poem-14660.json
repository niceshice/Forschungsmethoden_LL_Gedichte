{
    "title": "last-poem-14660",
    "gender": "w",
    "author": "Lisa Elsässer",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "deine tage dort"
        },
        "line.2": {
            "text": "kannst du jetzt zählen"
        },
        "line.3": {
            "text": "lies deine gedichte"
        },
        "line.4": {
            "text": "noch einmal unter"
        },
        "line.5": {
            "text": "der tür im flur"
        },
        "line.6": {
            "text": "am grossen fenster"
        },
        "line.7": {
            "text": "zum stillen hof"
        },
        "line.8": {
            "text": "geh hinein in die worte"
        },
        "line.9": {
            "text": "als blieben sie raum"
        },
        "line.10": {
            "text": "dir für immer einziger"
        },
        "line.11": {
            "text": "ort und geheimnis"
        },
        "line.12": {
            "text": "alle strassen leuchten"
        },
        "line.13": {
            "text": "nun wieder im regen"
        },
        "line.14": {
            "text": "lesen erinnern fluss"
        },
        "line.15": {
            "text": "die nacht und du geh"
        },
        "line.16": {
            "text": "in ihr zurück in die"
        },
        "line.17": {
            "text": "sommertage war es"
        },
        "line.18": {
            "text": "mittag oder abend"
        },
        "line.19": {
            "text": "als nebel aufzog und"
        },
        "line.20": {
            "text": "durch unsere sätze"
        },
        "line.21": {
            "text": "waberte nichts blieb"
        },
        "line.22": {
            "text": "den worten erspart"
        },
        "line.23": {
            "text": "nichts uns und den"
        },
        "line.24": {
            "text": "blätter ich sehe sie"
        },
        "line.25": {
            "text": "und dich diese zeit"
        },
        "line.26": {
            "text": "nur kurz durchschwirrt"
        },
        "line.27": {
            "text": "die amsel noch einmal"
        },
        "line.28": {
            "text": "das fenster und scheucht"
        },
        "line.29": {
            "text": "dich aus den zeilen du"
        },
        "line.30": {
            "text": "schliess nun diese türe"
        },
        "line.31": {
            "text": "hinter dir der schlag"
        },
        "line.32": {
            "text": "schlag des herzens"
        },
        "line.33": {
            "text": "verkleidet"
        }
    }
}