{
    "title": "ich-heiss-frank-471",
    "gender": "m",
    "author": "Lutz Rathenow",
    "author_birth": "1952",
    "poem": {
        "line.1": {
            "text": "Ich heiß Frank,"
        },
        "line.2": {
            "text": "sitz im Schrank"
        },
        "line.3": {
            "text": "auf einer Bank."
        },
        "line.4": {
            "text": "Gott sei Dank,"
        },
        "line.5": {
            "text": "meint Frank,"
        },
        "line.6": {
            "text": "bin ich im Schrank"
        },
        "line.7": {
            "text": "nicht ohne Bank."
        },
        "line.8": {
            "text": "Und kein Gestank"
        },
        "line.9": {
            "text": "in diesem Schrank"
        },
        "line.10": {
            "text": "auf meiner Bank,"
        },
        "line.11": {
            "text": "denkt Frank."
        },
        "line.12": {
            "text": "FrankimSchrank,"
        },
        "line.13": {
            "text": "der auf der Bank."
        },
        "line.14": {
            "text": "Ohne Bank"
        },
        "line.15": {
            "text": "im Schrank"
        },
        "line.16": {
            "text": "wär ich krank."
        },
        "line.17": {
            "text": "Weiß Frank"
        },
        "line.18": {
            "text": "auf seiner Bank."
        },
        "line.19": {
            "text": "\"Frank im Schrank\""
        },
        "line.20": {
            "text": "denkt die Bank,"
        },
        "line.21": {
            "text": "in dem Schrank"
        },
        "line.22": {
            "text": "über Frank."
        },
        "line.23": {
            "text": "So schläft er ein,"
        },
        "line.24": {
            "text": "im Schrank allein."
        }
    }
}