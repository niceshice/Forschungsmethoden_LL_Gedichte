{
    "title": "urspuengliche-akkumulation-29",
    "gender": "w",
    "author": "Elke Erb",
    "author_birth": "1938",
    "poem": {
        "line.1": {
            "text": "Ameisenstaat: Unterbringung. People-Verkehr."
        },
        "line.2": {
            "text": "Unds stiefelt. Verfrachtet. Nachtdunkle, nachtleere"
        },
        "line.3": {
            "text": "Arbeiterviertel. Abträglichkeit, schartig, in allem."
        },
        "line.4": {
            "text": "Augen: Mary vom Lande"
        },
        "line.5": {
            "text": "weiß durch Jahrhunderte nicht,"
        },
        "line.6": {
            "text": "daß sie vom Land ist."
        },
        "line.7": {
            "text": "Kein Auf-Bau, kein Über-."
        },
        "line.8": {
            "text": "Das Heimchen die Geige."
        },
        "line.9": {
            "text": "Wolke, wohin du gewolkt bist."
        },
        "line.10": {
            "text": "Ein herrlicher Maitag - mir im Gemüte."
        },
        "line.11": {
            "text": "Augen: Robin von der Plantage"
        },
        "line.12": {
            "text": "ist nicht mehr Unter-, doch auch kein Einbau:"
        },
        "line.13": {
            "text": "Wies so geht macht sichs."
        },
        "line.14": {
            "text": "Worte Architekturinfektion sieche Strukturelemente."
        },
        "line.15": {
            "text": "Zivilisation blank - das Wesen des Gartens."
        },
        "line.16": {
            "text": "Die Absicht, den Moloch zu modeln"
        },
        "line.17": {
            "text": "(Problemannahme und -knete)"
        },
        "line.18": {
            "text": "verloren. Disteln geköpft."
        },
        "line.19": {
            "text": "18.11.1995"
        }
    }
}