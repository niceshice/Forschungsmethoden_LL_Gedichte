{
    "title": "wie-das-trudelnde-trudelnde-linden-1011",
    "gender": "w",
    "author": "Brigitte Oleschinski",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "blatt, mitten im zärtlichsten Blutbad, berührt"
        },
        "line.2": {
            "text": "deine Hand mich, viel zu blond"
        },
        "line.3": {
            "text": "flammt"
        },
        "line.4": {
            "text": "aus der Schulter jetzt"
        },
        "line.5": {
            "text": "dieser steile"
        },
        "line.6": {
            "text": "Achilles-"
        },
        "line.7": {
            "text": "flügel"
        }
    }
}