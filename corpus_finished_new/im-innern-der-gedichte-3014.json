{
    "title": "im-innern-der-gedichte-3014",
    "gender": "m",
    "author": "Nicolas  Born",
    "author_birth": "1937",
    "poem": {
        "line.1": {
            "text": "Du kannst nicht davon leben"
        },
        "line.2": {
            "text": "mit der Wirklichkeit zu konkurrieren"
        },
        "line.3": {
            "text": "noch kannst du von der Wirklichkeit leben"
        },
        "line.4": {
            "text": "aber du kannst einen Eingriff überleben"
        },
        "line.5": {
            "text": "und alles zurück kriegen"
        },
        "line.6": {
            "text": "und durch Das Leben gehen"
        },
        "line.7": {
            "text": "durch schnell verfallende Bilder"
        },
        "line.8": {
            "text": "das warst du"
        },
        "line.9": {
            "text": "du und Das Werdende Leben"
        },
        "line.10": {
            "text": "Personen keuchend unter ihren Grabsteinen"
        },
        "line.11": {
            "text": "Mit einer ungeheuren Anstrengung"
        },
        "line.12": {
            "text": "von dir und allen Vorfahren"
        },
        "line.13": {
            "text": "blendest du dich aus"
        },
        "line.14": {
            "text": "Land und Wasser sind geblieben"
        },
        "line.15": {
            "text": "der Himmel ist geblieben"
        },
        "line.16": {
            "text": "und du bist geblieben"
        },
        "line.17": {
            "text": "du hast dich auf nichts einzurichten"
        },
        "line.18": {
            "text": "kleine Sonnen erleuchten deine Demokratie Und"
        },
        "line.19": {
            "text": "du wählst das Leben und den Tod"
        },
        "line.20": {
            "text": "du hast viele Schöne Stimmen"
        },
        "line.21": {
            "text": "du bist Viele"
        },
        "line.22": {
            "text": "deine Haut ist deine Haut Und endlich"
        },
        "line.23": {
            "text": "nichts als Haut"
        },
        "line.24": {
            "text": "du bist der Unternehmer des Lebens"
        },
        "line.25": {
            "text": "der Veranstalter weißer Erscheinungen"
        },
        "line.26": {
            "text": "du bist der RaumMensch im Freien"
        },
        "line.27": {
            "text": "der Autor des Laufs der Geschichte"
        },
        "line.28": {
            "text": "du bist imstande Zeit zu drucken wie Bücher"
        },
        "line.29": {
            "text": "du wiegst und siebst und liebst Und im Wind"
        },
        "line.30": {
            "text": "wehen die Ruinen der Diktatmaschinen"
        },
        "line.31": {
            "text": "die Unvernunft steht in voller Blüte"
        },
        "line.32": {
            "text": "du bist die Blüte und die Unvernunft"
        },
        "line.33": {
            "text": "du bist Tag und Nacht bei Tag und Nacht"
        },
        "line.34": {
            "text": "du bist der Mörder"
        },
        "line.35": {
            "text": "kreisend in der eigenen Blutbahn"
        },
        "line.36": {
            "text": "du bist Vater und Sohn"
        },
        "line.37": {
            "text": "du bist der ausgeschlachtete Indianer"
        },
        "line.38": {
            "text": "und der registrierte Indianer"
        },
        "line.39": {
            "text": "du bist alle Farben und Rassen"
        },
        "line.40": {
            "text": "du bist die Witwen und Waisen"
        },
        "line.41": {
            "text": "du bist die Rebellion der Gefangenen"
        },
        "line.42": {
            "text": "du bist Geheul ohne Aufenthalt"
        },
        "line.43": {
            "text": "Messerwürfe Schüsse"
        },
        "line.44": {
            "text": "du bist der phantastische Sportler der TraumMeilen"
        },
        "line.45": {
            "text": "der Bildersturm im Haupt der Demokratie"
        },
        "line.46": {
            "text": "du bist der Sprengmeister aller Ketten"
        },
        "line.47": {
            "text": "du bist die geheim leuchtende Parole"
        },
        "line.48": {
            "text": "die Banderole"
        },
        "line.49": {
            "text": "die Avantgarde der FreiKüchen"
        },
        "line.50": {
            "text": "du bist Mensch Und"
        },
        "line.51": {
            "text": "Tier wenn es den Tod fühlt"
        },
        "line.52": {
            "text": "du bist allein und du bist Alle"
        },
        "line.53": {
            "text": "du bist dein Tod und du bist der Große Wunsch"
        },
        "line.54": {
            "text": "du bist der Plan den du ausbreitest Und"
        },
        "line.55": {
            "text": "du bist dein Tod"
        }
    }
}