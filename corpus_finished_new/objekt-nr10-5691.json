{
    "title": "objekt-nr10-5691",
    "gender": "m",
    "author": "Thien Tran",
    "author_birth": "1979",
    "poem": {
        "line.1": {
            "text": "im Luftzwischenraum"
        },
        "line.2": {
            "text": "bewegen sich die Sinne abstrahierend voran"
        },
        "line.3": {
            "text": "weiter und immer weiter. und zurück"
        },
        "line.4": {
            "text": "bis die Person verschwindet. oder meinetwegen"
        },
        "line.5": {
            "text": "bis das Objekt verschwindet"
        },
        "line.6": {
            "text": ". Maschinen-"
        },
        "line.7": {
            "text": "konstruktionen in Braun- und Beigetönen gehalten"
        },
        "line.8": {
            "text": "und schwer zu identifizieren"
        },
        "line.9": {
            "text": "ein gerade abkühlendes Heliumherzaggregat"
        },
        "line.10": {
            "text": "den Männern entgegen"
        },
        "line.11": {
            "text": "dass der pazifische Ozean fortreibt"
        },
        "line.12": {
            "text": "und nicht zurückkehrt. als gäbe es keine Rückkehr"
        },
        "line.13": {
            "text": "für dieses Mädchen ist wahr. wahr sind auch"
        },
        "line.14": {
            "text": "diese ineinander- und auseinander-"
        },
        "line.15": {
            "text": "strebenden Emotionspartikel. bunte Kügelchen"
        },
        "line.16": {
            "text": "durch einen Bindfaden verbunden"
        },
        "line.17": {
            "text": "der vorher durch einen dünnen Benzinfilm"
        },
        "line.18": {
            "text": "gezogen wurde"
        },
        "line.19": {
            "text": "im Optimalfall sondern Bindfäden"
        },
        "line.20": {
            "text": "Blütenaromen aus. so dass diese Kügelchen anfangen"
        },
        "line.21": {
            "text": "zu vibrieren. sobald auch das Begehren wahr ist"
        },
        "line.22": {
            "text": "und der Optimalfall ist jetzt: Blüten-"
        },
        "line.23": {
            "text": "aromen und bunte Kügelchen. die an einem Bindfaden"
        },
        "line.24": {
            "text": "hinabgleiten. Personen deren Gegenstände"
        },
        "line.25": {
            "text": "brüchig geworden sind. deren Gegen-"
        },
        "line.26": {
            "text": "stände durchscheinen."
        }
    }
}