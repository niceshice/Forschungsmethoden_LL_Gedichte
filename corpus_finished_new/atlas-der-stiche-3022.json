{
    "title": "atlas-der-stiche-3022",
    "gender": "m",
    "author": "Ron Winkler",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "über die Jahre hattest du deine Mückenstiche"
        },
        "line.2": {
            "text": "auf einer Schaufensterpuppe eingetragen."
        },
        "line.3": {
            "text": "denn nichts könne"
        },
        "line.4": {
            "text": "nur"
        },
        "line.5": {
            "text": "Zufall sein,"
        },
        "line.6": {
            "text": "alles habe System, oder nichtmechanistisch"
        },
        "line.7": {
            "text": "gesprochen: einen Gott im Gepäck."
        },
        "line.8": {
            "text": "und jenem System nach sei dein Atlas der Stiche"
        },
        "line.9": {
            "text": "längst eine nicht mehr zu leugnende eigene Welt."
        }
    }
}