{
    "title": "cyberschlafstoerung-12367",
    "gender": "m",
    "author": "Martin  Piekar",
    "author_birth": "1990",
    "poem": {
        "line.1": {
            "text": "Lass den Ghost in der Shell"
        },
        "line.2": {
            "text": "Er muss brüten"
        },
        "line.3": {
            "text": "Vom Mond im Jupiter im Livestream"
        },
        "line.4": {
            "text": "Eines Nichtschlafs,"
        },
        "line.5": {
            "text": "Denn"
        },
        "line.6": {
            "text": "mich flieht der Schlaf"
        },
        "line.7": {
            "text": "Durch die Nacht"
        },
        "line.8": {
            "text": "Chat ich mich"
        },
        "line.9": {
            "text": "Wenn du eine Revolution willst"
        },
        "line.10": {
            "text": "Bestell sie über Amazon Prime"
        },
        "line.11": {
            "text": "Gesicherter Versandt verringert Risiken"
        },
        "line.12": {
            "text": "In gestörter Einsamkeit"
        },
        "line.13": {
            "text": "Zieh ich meinen besten Schlafanzug an"
        },
        "line.14": {
            "text": "Dustern gezwungen zu wachen"
        },
        "line.15": {
            "text": "Ich halte Dunkel nicht träumend aus"
        },
        "line.16": {
            "text": "Share me, share me, share me with you applephone"
        },
        "line.17": {
            "text": "Und lass uns Doppelgänger tauschen"
        },
        "line.18": {
            "text": "Per Zufall sind wie Foetalisten"
        },
        "line.19": {
            "text": "Und hüpfen von USB"
        },
        "line.20": {
            "text": "Zu USB-Port zu wälzen hilft"
        },
        "line.21": {
            "text": "Nicht einer Ruhe beizuwohnen"
        },
        "line.22": {
            "text": "Ich habe den Anschluss an Schlaf verloren"
        },
        "line.23": {
            "text": "Und Versuche durch alte Tags"
        },
        "line.24": {
            "text": "Wer die Langeweile sucht"
        },
        "line.25": {
            "text": "Bleibt ungefunden"
        },
        "line.26": {
            "text": "Du kannst nicht einfach"
        },
        "line.27": {
            "text": "Deinen Beziehungsstatus ändern ohne"
        },
        "line.28": {
            "text": "Dich zu ändern"
        },
        "line.29": {
            "text": "Ich hab ne Buchempfehlung für dich"
        },
        "line.30": {
            "text": "Schreib eins"
        },
        "line.31": {
            "text": "Trommle ein paar Server ab und"
        },
        "line.32": {
            "text": "Finde mein Leiden immer wieder"
        },
        "line.33": {
            "text": "Scheißreziprozität des Netzes"
        },
        "line.34": {
            "text": "Ich brauche einen Kollaps"
        },
        "line.35": {
            "text": "Ich ghoste keinen SleepStream"
        },
        "line.36": {
            "text": "Ich puste die WifiVerbindung aus"
        },
        "line.37": {
            "text": "Und wünsch mir was"
        }
    }
}