{
    "title": "dnipro-15199",
    "gender": "w",
    "author": "Anja Kampmann",
    "author_birth": "1983",
    "poem": {
        "line.1": {
            "text": "in der abendglühenden luft"
        },
        "line.2": {
            "text": "die irgendwie matt ist, unklar"
        },
        "line.3": {
            "text": "mit dem deutlichen geruch von industrie"
        },
        "line.4": {
            "text": "anlagen, mit den großen blauen zügen"
        },
        "line.5": {
            "text": "loks, langsam, fast lautlos"
        },
        "line.6": {
            "text": "im untergehenden licht"
        },
        "line.7": {
            "text": "vereinzelt"
        },
        "line.8": {
            "text": "soldaten"
        },
        "line.9": {
            "text": "mit freiem oberkörper"
        },
        "line.10": {
            "text": "eine frau mit gebäck"
        },
        "line.11": {
            "text": "geschwister, ein junge und ein kleines mädchen"
        },
        "line.12": {
            "text": "halten sich an den händen"
        },
        "line.13": {
            "text": "vor einer säule die silbrig glänzt"
        },
        "line.14": {
            "text": "wie alle säulen hier"
        },
        "line.15": {
            "text": "und das licht das alles"
        },
        "line.16": {
            "text": "verlangsamt"
        },
        "line.17": {
            "text": "mit den geräuschen"
        },
        "line.18": {
            "text": "und in die geräusche hinein"
        },
        "line.19": {
            "text": "einsteigen"
        },
        "line.20": {
            "text": "dämmerung, die flussunruhe"
        },
        "line.21": {
            "text": "und der geruch der keinen von uns"
        },
        "line.22": {
            "text": "entlässt"
        },
        "line.23": {
            "text": "die nervosität ist breitbeinig"
        },
        "line.24": {
            "text": "sie hat müde freundinnen"
        },
        "line.25": {
            "text": "in kurzen jeans"
        },
        "line.26": {
            "text": "sie glaubt nicht alles was du sagst"
        },
        "line.27": {
            "text": "oder sagen möchtest"
        },
        "line.28": {
            "text": "übers wiederkommen."
        }
    }
}