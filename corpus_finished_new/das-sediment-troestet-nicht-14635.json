{
    "title": "das-sediment-troestet-nicht-14635",
    "gender": "w",
    "author": "Carolin Callies",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "ich kündig dem *"
        },
        "line.2": {
            "text": "& ist trauter *,"
        },
        "line.3": {
            "text": "ism holz nicht würdig gewesen."
        },
        "line.4": {
            "text": "ich schneid mir die buttrigen blätter & bretter,"
        },
        "line.5": {
            "text": "die glocken läuten im *."
        },
        "line.6": {
            "text": "das lüpfen von hüten & karten,"
        },
        "line.7": {
            "text": "darunter: ich hab den * so lieb."
        },
        "line.8": {
            "text": "der morgen ist maulwurf."
        },
        "line.9": {
            "text": "dem tier ist zu hell,"
        },
        "line.10": {
            "text": "'n topf dampfendes unter dem *"
        },
        "line.11": {
            "text": "*boden"
        }
    }
}