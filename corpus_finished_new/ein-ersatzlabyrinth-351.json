{
    "title": "ein-ersatzlabyrinth-351",
    "gender": "m",
    "author": "Kurt  Aebli",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "E"
        },
        "line.2": {
            "text": "in Ersatzlabyrinth und das"
        },
        "line.3": {
            "text": "Gefühl festen Boden unter"
        },
        "line.4": {
            "text": "den Füßen zu vergeuden von etwas"
        },
        "line.5": {
            "text": "nicht piep Sagendem durchbohrt"
        },
        "line.6": {
            "text": "überseeische Dinge ihm in die"
        },
        "line.7": {
            "text": "Hand gedrückt hier geht's lang"
        },
        "line.8": {
            "text": "zum Nebel undeutliches Gemurmel"
        },
        "line.9": {
            "text": "ihm vorgesetzt am Tag an dem ich"
        },
        "line.10": {
            "text": "von meinem hohen Markenzeichen"
        },
        "line.11": {
            "text": "heruntersteige dachte er"
        }
    }
}