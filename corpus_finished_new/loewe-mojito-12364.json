{
    "title": "loewe-mojito-12364",
    "gender": "m",
    "author": "Martin  Piekar",
    "author_birth": "1990",
    "poem": {
        "line.1": {
            "text": "Ich grins Limettenzähne"
        },
        "line.2": {
            "text": "Auf deinen Augenauftakt. vergiss nicht,"
        },
        "line.3": {
            "text": "Wie Barhocker sich anstellen."
        },
        "line.4": {
            "text": "Im Dunkel"
        },
        "line.5": {
            "text": "Der Minze leg ich mich"
        },
        "line.6": {
            "text": "Dir auf die Lippen. wie Strohhalme"
        },
        "line.7": {
            "text": "Greifen unsere Zungen und"
        },
        "line.8": {
            "text": "Wir treiben Rum"
        },
        "line.9": {
            "text": "Durch die Sprachplätze. zwischen deinem"
        },
        "line.10": {
            "text": "Mund, meinem Ohr, deinem Ohr"
        },
        "line.11": {
            "text": "Meinem Mund."
        },
        "line.12": {
            "text": "Eine Geisterfahrt gegen die Musik. hörst du"
        },
        "line.13": {
            "text": "Viel zu viele Zucker röhren"
        },
        "line.14": {
            "text": "Zwischen unseren Zähnen. Süße."
        },
        "line.15": {
            "text": "Schwapp hin und her. doch ich"
        },
        "line.16": {
            "text": "Werde fort sein"
        },
        "line.17": {
            "text": "Bevor das Eis schmilzt."
        },
        "line.18": {
            "text": "Rette unser Eis."
        },
        "line.19": {
            "text": "Zutaten:"
        },
        "line.20": {
            "text": "Final Destination Club, Frankfurt am Main, stürmischer Weg zur Tanzfläche"
        }
    }
}