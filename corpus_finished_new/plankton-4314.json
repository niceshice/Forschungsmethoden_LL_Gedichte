    "poem": {
        "line.1": {
            "text": "Tage verstreichen"
        },
        "line.2": {
            "text": "weit"
        },
        "line.3": {
            "text": "aufgerissenen Mauls"
        },
        "line.4": {
            "text": "besinnungslos Verschlucktes"
        },
        "line.5": {
            "text": "zerdichten."
        },
        "line.6": {
            "text": "Es sind unser"
        },
        "line.7": {
            "text": "aller Geschichten:"
        },
        "line.8": {
            "text": "Mit geringer Eigenbewegung"
        },
        "line.9": {
            "text": "Umherirrendes"
        },
        "line.10": {
            "text": "wie im Kehlsack"
        },
        "line.11": {
            "text": "des Pelikans"
        },
        "line.12": {
            "text": "gehortet."
        },
        "line.13": {
            "text": "Die Zunge ist ein"
        },
        "line.14": {
            "text": "guter Polizist."
        },
        "line.15": {
            "text": "FÃ¼r jede Korruption"
        },
        "line.16": {
            "text": "zu haben."
        }
    }
}