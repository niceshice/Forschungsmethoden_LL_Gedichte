{
    "title": "der-zettel-11681",
    "gender": "m",
    "author": "Franz Hohler",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "Oha"
        },
        "line.2": {
            "text": "dachte ich"
        },
        "line.3": {
            "text": "als ich heimkam"
        },
        "line.4": {
            "text": "nachts"
        },
        "line.5": {
            "text": "und am Boden"
        },
        "line.6": {
            "text": "den hellen Flecken sah"
        },
        "line.7": {
            "text": "da liegt"
        },
        "line.8": {
            "text": "eine Nachricht für mich"
        },
        "line.9": {
            "text": "auf der Treppe."
        },
        "line.10": {
            "text": "Doch sieh da"
        },
        "line.11": {
            "text": "es war nur"
        },
        "line.12": {
            "text": "das Mondlicht."
        },
        "line.13": {
            "text": "Und"
        },
        "line.14": {
            "text": "fiel mir später ein"
        },
        "line.15": {
            "text": "ist das etwa"
        },
        "line.16": {
            "text": "keine Nachricht?"
        }
    }
}