{
    "title": "mit-langem-bogen-streicht-635",
    "gender": "m",
    "author": "Arne  Rautenberg",
    "author_birth": "1967",
    "poem": {
        "line.1": {
            "text": "mit langem bogen streicht"
        },
        "line.2": {
            "text": "der schöpfergeiger an"
        },
        "line.3": {
            "text": "der jahreszeitenvioline"
        },
        "line.4": {
            "text": "herr vitaler zeiger wann"
        },
        "line.5": {
            "text": "fällst du den guillotinen"
        },
        "line.6": {
            "text": "schnitt: langen wogen gleich"
        }
    }
}