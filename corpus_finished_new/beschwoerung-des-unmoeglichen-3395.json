{
    "title": "beschwoerung-des-unmoeglichen-3395",
    "gender": "m",
    "author": "Arno  Reinfrank",
    "author_birth": "1934",
    "poem": {
        "line.1": {
            "text": "Ich tanze,"
        },
        "line.2": {
            "text": "ein sibirischer Schamane,"
        },
        "line.3": {
            "text": "zu einer Rassel"
        },
        "line.4": {
            "text": "harter Bärentatzen"
        },
        "line.5": {
            "text": "in einem lichtgrünen Gewölbe"
        },
        "line.6": {
            "text": "aus Waldgesträuch:"
        },
        "line.7": {
            "text": "Ich kann euch heilen."
        },
        "line.8": {
            "text": "Aus Brettchen"
        },
        "line.9": {
            "text": "schnitzte ich Konturen"
        },
        "line.10": {
            "text": "des Schwans,"
        },
        "line.11": {
            "text": "des himmlischsten der Vögel,"
        },
        "line.12": {
            "text": "die baumeln in den Fetischzweigen."
        },
        "line.13": {
            "text": "Ich kann,"
        },
        "line.14": {
            "text": "ich kann euch heilen."
        },
        "line.15": {
            "text": "Mein Mund ist eine Sprichwortkelter,"
        },
        "line.16": {
            "text": "die spuckt euch"
        },
        "line.17": {
            "text": "kleinzerkaut Kwass in den Krug."
        },
        "line.18": {
            "text": "Ich esse gern"
        },
        "line.19": {
            "text": "gebratene Fische."
        },
        "line.20": {
            "text": "Lauscht dem Gesang:"
        },
        "line.21": {
            "text": "Ich kann euch heilen."
        },
        "line.22": {
            "text": "Rutscht"
        },
        "line.23": {
            "text": "auf geknicktem Knie zu mir,"
        },
        "line.24": {
            "text": "werft OpfergeJd"
        },
        "line.25": {
            "text": "in meine Holzschüsse]"
        },
        "line.26": {
            "text": "und seht: Für euch"
        },
        "line.27": {
            "text": "tanzt meine Rentierhaut."
        },
        "line.28": {
            "text": "Ich kann euch heilen."
        },
        "line.29": {
            "text": "Glaubt an die Kräfte"
        },
        "line.30": {
            "text": "meiner Rabenfedern"
        },
        "line.31": {
            "text": "beim Namen aller Väter meines Vaters."
        },
        "line.32": {
            "text": "Legt nur den Kopf"
        },
        "line.33": {
            "text": "vor meinen Fuß und glaubt,"
        },
        "line.34": {
            "text": "ich könnte,"
        },
        "line.35": {
            "text": "was kein Sanatorium kann:"
        },
        "line.36": {
            "text": "Ich könnte euere Übel heilen."
        }
    }
}