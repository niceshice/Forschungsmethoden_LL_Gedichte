{
    "title": "gestrandeter-pottwal-13545",
    "gender": "w",
    "author": "Sabine Scho",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "\"something like the tack-tack-tack"
        },
        "line.2": {
            "text": "of a few dozen typewriters\""
        },
        "line.3": {
            "text": "wenn die finger fehlen"
        },
        "line.4": {
            "text": "nervös auf die tischplatte"
        },
        "line.5": {
            "text": "zu trommeln und der tisch"
        },
        "line.6": {
            "text": "fehlt dazu"
        },
        "line.7": {
            "text": "wenn das medium nicht"
        },
        "line.8": {
            "text": "die message, aber"
        },
        "line.9": {
            "text": "das medium spendiert"
        },
        "line.10": {
            "text": "das den schall schneller"
        },
        "line.11": {
            "text": "transportiert"
        },
        "line.12": {
            "text": "sonic youth"
        },
        "line.13": {
            "text": "is the loudest"
        },
        "line.14": {
            "text": "animal on earth"
        },
        "line.15": {
            "text": "wenn das walkalb an land"
        },
        "line.16": {
            "text": "allein deshalb verloren ist"
        },
        "line.17": {
            "text": "weil es niemand mehr hört"
        },
        "line.18": {
            "text": "selbst das click verstärkende"
        },
        "line.19": {
            "text": "meer, fortgezogen, seinen"
        },
        "line.20": {
            "text": "größten spross nicht vermisst"
        },
        "line.21": {
            "text": "wenn zahnwale mit dem kiefer"
        },
        "line.22": {
            "text": "knirschen, im schlaf, im sand"
        },
        "line.23": {
            "text": "im traum, ozeanlos geweckt"
        },
        "line.24": {
            "text": "vom eigenen gewicht"
        },
        "line.25": {
            "text": "wenn sich das erhabene"
        },
        "line.26": {
            "text": "am gestade bläht"
        },
        "line.27": {
            "text": "gedärme den blubber"
        },
        "line.28": {
            "text": "wie stollen stützen"
        },
        "line.29": {
            "text": "wenn das gerippe noch"
        },
        "line.30": {
            "text": "steht, ein serviettenring"
        },
        "line.31": {
            "text": "dem keiner ein"
        },
        "line.32": {
            "text": "leichentuch näht"
        }
    }
}