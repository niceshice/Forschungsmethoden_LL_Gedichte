{
    "title": "about-horst-janssen-gegen-verfluchtes-chaos-geheiligte-ordnung-13681",
    "gender": "w",
    "author": "Claudia Gabler",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "Gegen verfluchtes Chaos geheiligte Ordnung"
        },
        "line.2": {
            "text": "geile Gesundheit  geht alles anders."
        },
        "line.3": {
            "text": "Geht eigentlich gar nicht"
        },
        "line.4": {
            "text": ""
        },
        "line.5": {
            "text": "bei soviel Dante / bei soviel Tante"
        },
        "line.6": {
            "text": "(das versteht jetzt wieder keiner)"
        },
        "line.7": {
            "text": "aber nicht jeder hat Eltern"
        },
        "line.8": {
            "text": ""
        },
        "line.9": {
            "text": "Die einen vom Saufen abhalten."
        },
        "line.10": {
            "text": "Von der Krankheit Kunst, von der man in jungen Jahren"
        },
        "line.11": {
            "text": "evtl. noch hätte geheilt werden können."
        },
        "line.12": {
            "text": ""
        },
        "line.13": {
            "text": "Doch wenn sich die Elite solo"
        },
        "line.14": {
            "text": "vorm Spiegel bläht"
        },
        "line.15": {
            "text": "verspricht sich nur der Küsster"
        },
        "line.16": {
            "text": ""
        },
        "line.17": {
            "text": "Sicherheit vom Marschhalten"
        },
        "line.18": {
            "text": "und ein blaues Bild"
        },
        "line.19": {
            "text": "von vermuteten Himmelreichen."
        },
        "line.20": {
            "text": ""
        },
        "line.21": {
            "text": "Der Neffe dagegen ertränkt seine tückische Intelligenz"
        },
        "line.22": {
            "text": "und sein zufälliges Talent"
        },
        "line.23": {
            "text": "in 300 Dosen billig Bier."
        }
    }
}