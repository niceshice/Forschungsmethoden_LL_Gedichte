{
    "title": "himbeerranken-3817",
    "gender": "m",
    "author": "Günter Eich",
    "author_birth": "1907",
    "poem": {
        "line.1": {
            "text": "Der Wald hinter den Gedanken,"
        },
        "line.2": {
            "text": "die Regentropfen an ihnen"
        },
        "line.3": {
            "text": "und der Herbst, der sie vergilben läßt"
        },
        "line.4": {
            "text": "ach, Himbeerranken aussprechen,"
        },
        "line.5": {
            "text": "dir Beeren ins Ohr flüstern,"
        },
        "line.6": {
            "text": "die roten, die ins Moos fielen."
        },
        "line.7": {
            "text": "Dein Ohr versteht sie nicht,"
        },
        "line.8": {
            "text": "mein Mund spricht sie nicht aus,"
        },
        "line.9": {
            "text": "Worte halten ihren Verfall nicht auf."
        },
        "line.10": {
            "text": "Hand in Hand zwischen undenkbaren Gedanken."
        },
        "line.11": {
            "text": "Im Dickicht verliert sich die Spur."
        },
        "line.12": {
            "text": "Der Mond schlägt sein Auge auf,"
        },
        "line.13": {
            "text": "gelb und für immer."
        }
    }
}