{
    "title": "goldrand-mit-schweigen-12828",
    "gender": "m",
    "author": "Rainer René Mueller",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "Du"
        },
        "line.2": {
            "text": "hast mich schweigen hören\","
        },
        "line.3": {
            "text": "sagst"
        },
        "line.4": {
            "text": "Du am Pfingstmorgen"
        },
        "line.5": {
            "text": "auf dem goldheiß überdampften"
        },
        "line.6": {
            "text": "Balkon, beim Taubenruf:"
        },
        "line.7": {
            "text": "batsch du de bue, batsch du de bue\""
        },
        "line.8": {
            "text": "... indes du dir die"
        },
        "line.9": {
            "text": "Wanderschuhe schnürst"
        },
        "line.10": {
            "text": "Haken für Haken den Schnürsenkel"
        },
        "line.11": {
            "text": "hochziehst"
        },
        "line.12": {
            "text": "&"
        },
        "line.13": {
            "text": "den Doppelknoten machst - :"
        },
        "line.14": {
            "text": "... gepackt für'n Weg, für'n"
        },
        "line.15": {
            "text": "uns, nicht in ..."
        },
        "line.16": {
            "text": "was ach, was ach\" über'n"
        },
        "line.17": {
            "text": "Col de Bon Homme"
        },
        "line.18": {
            "text": ": Maien-Lust:"
        },
        "line.19": {
            "text": "gehen, gehen, gehen\" : diese"
        },
        "line.20": {
            "text": "Litanei, wie beim :"
        },
        "line.21": {
            "text": "reiten, reiten, reiten\""
        },
        "line.22": {
            "text": "und dann der Abgrund unter"
        },
        "line.23": {
            "text": "den Vögeln hoch über Dir :"
        },
        "line.24": {
            "text": "der Oberlin-Ruf:"
        },
        "line.25": {
            "text": "... sondern hat Lust zum Gesetz des Herrn ...\" :"
        },
        "line.26": {
            "text": "Du"
        }
    }
}