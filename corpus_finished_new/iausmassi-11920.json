{
    "title": "iausmassi-11920",
    "gender": "w",
    "author": "Anja Kampmann",
    "author_birth": "1983",
    "poem": {
        "line.1": {
            "text": "der sorge in worten von denen"
        },
        "line.2": {
            "text": "wir die bienen fernhalten auf dass sie"
        },
        "line.3": {
            "text": "sie nicht tragen in die hintersten winkel"
        },
        "line.4": {
            "text": "der"
        },
        "line.5": {
            "text": "karten"
        },
        "line.6": {
            "text": "die träume sind schon erreicht"
        },
        "line.7": {
            "text": "gebiete in denen die angst sich nicht ausspricht"
        },
        "line.8": {
            "text": "aber anwesend ist wir"
        },
        "line.9": {
            "text": "richten"
        },
        "line.10": {
            "text": "den blick auf blankes"
        },
        "line.11": {
            "text": "land ränder"
        },
        "line.12": {
            "text": "rauschschwarz"
        },
        "line.13": {
            "text": "sind die tage als meldung"
        },
        "line.14": {
            "text": "lassen die welt unlesbar zurück eine karte am anfang"
        },
        "line.15": {
            "text": "steht kein wort der abermals traurige gaul lässt sich nicht"
        },
        "line.16": {
            "text": "lenken nur sein silber im blick und wie er dich"
        },
        "line.17": {
            "text": "verlässt"
        },
        "line.18": {
            "text": "es sind keine geschichten wenn ein land"
        },
        "line.19": {
            "text": "verblasst"
        },
        "line.20": {
            "text": "wie du hierher eilst wen wir nicht mehr finden"
        },
        "line.21": {
            "text": "nirgends und wo in diesem land wohnte"
        },
        "line.22": {
            "text": "nicht bitternis"
        },
        "line.23": {
            "text": "still jetzt."
        }
    }
}