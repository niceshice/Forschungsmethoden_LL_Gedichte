{
    "title": "es-ist-ein-sterben-angebrochen-3359",
    "gender": "w",
    "author": "Dorothea  Grünzweig",
    "author_birth": "1952",
    "poem": {
        "line.1": {
            "text": "Es ist ein Sterben angebrochen"
        },
        "line.2": {
            "text": "der Erdball unter uns ist"
        },
        "line.3": {
            "text": "klein geworden"
        },
        "line.4": {
            "text": "kommt es vom Druck der"
        },
        "line.5": {
            "text": "Menschenschwere von Atemnot"
        },
        "line.6": {
            "text": "von einer Traurigkeit die zehrt"
        },
        "line.7": {
            "text": "der Erdball klein"
        },
        "line.8": {
            "text": "er"
        },
        "line.9": {
            "text": "hat sich in sich selbst verkrochen"
        },
        "line.10": {
            "text": "leis weint hinein in sich"
        },
        "line.11": {
            "text": "er ist nach innen eingezogen als ob"
        },
        "line.12": {
            "text": "aus einer Not er lehnen wolle"
        },
        "line.13": {
            "text": "Es ist ein Sterben angebrochen"
        },
        "line.14": {
            "text": "einSchrumpfen unter unseren Sohlen Ziehn"
        },
        "line.15": {
            "text": "Unruhe die schlaflos macht"
        },
        "line.16": {
            "text": "der Erdball klein weint weint geworden"
        },
        "line.17": {
            "text": "wie Bäume vor dem Fällen"
        },
        "line.18": {
            "text": "wie Sterbende hinein in sich"
        },
        "line.19": {
            "text": "die nicht ans SprechenKlagen glauben"
        },
        "line.20": {
            "text": "Es ist ein Sterben in den auch"
        },
        "line.21": {
            "text": "Träumen angebrochen sahen wir"
        },
        "line.22": {
            "text": "den Erdball Erdbälle als Köpfe"
        },
        "line.23": {
            "text": "von ihnen Haare wie in die Luft"
        },
        "line.24": {
            "text": "gesprengte stürzten Häuser Bäume"
        },
        "line.25": {
            "text": "stürzten unzählig Tote aus aller armen Welt"
        },
        "line.26": {
            "text": "Es ist ein grosses Sterben"
        },
        "line.27": {
            "text": "der kleine Erdball angebrochen"
        },
        "line.28": {
            "text": "weint es rings weint um und viel"
        },
        "line.29": {
            "text": "Verlassenheit von guten Geistern"
        }
    }
}