{
    "title": "voegel-im-mai-1594",
    "gender": "w",
    "author": "Erika Burkart",
    "author_birth": "1922",
    "poem": {
        "line.1": {
            "text": "Vom Dachfirst aus Blütenkronen"
        },
        "line.2": {
            "text": "fliegen sie in Sicht vor den Fenstern,"
        },
        "line.3": {
            "text": "Schatten und Blitze,"
        },
        "line.4": {
            "text": "und ich weiss nicht:"
        },
        "line.5": {
            "text": "Sind es Dämonen"
        },
        "line.6": {
            "text": "oder verzauberte Raben,"
        },
        "line.7": {
            "text": "mit Königsflügeln und Nornenruf"
        },
        "line.8": {
            "text": "Schweigen gebietend, fordernd"
        },
        "line.9": {
            "text": "das Nesselwerk der Erlösung."
        }
    }
}