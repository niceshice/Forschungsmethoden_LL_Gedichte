{
    "title": "der-mann-der-grachten-223",
    "gender": "m",
    "author": "Detlev Meyer",
    "author_birth": "1948",
    "poem": {
        "line.1": {
            "text": "Stehen Männer an den Grachten,"
        },
        "line.2": {
            "text": "die auf Grachten-Männer achten,"
        },
        "line.3": {
            "text": "die die Nacht im Bett verbrachten,"
        },
        "line.4": {
            "text": "daß die Betten nur so krachten."
        },
        "line.5": {
            "text": "Stehen Männer an den Grachten,"
        },
        "line.6": {
            "text": "die nach Grachten-Männern trachten,"
        },
        "line.7": {
            "text": "die die Einsamkeitverachten,"
        },
        "line.8": {
            "text": "ihre Betten gern verpachten."
        },
        "line.9": {
            "text": "Stehen Männer an den Grachten,"
        },
        "line.10": {
            "text": "die nach Grachten-Männern schmachten,"
        },
        "line.11": {
            "text": "nach den wilden, nach den sachten,"
        },
        "line.12": {
            "text": "die im Bette alles machten,"
        },
        "line.13": {
            "text": "die im Bette alles tun,"
        },
        "line.14": {
            "text": "wenn die Grachten-Wasser ruhen."
        },
        "line.15": {
            "text": "Die bedenken und bedachten"
        },
        "line.16": {
            "text": "liebevoll den Mann der Grachten."
        }
    }
}