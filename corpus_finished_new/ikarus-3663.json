{
    "title": "ikarus-3663",
    "gender": "m",
    "author": "Günter  Kunert",
    "author_birth": "1929",
    "poem": {
        "line.1": {
            "text": "Es sind ein paar Federn geblieben"
        },
        "line.2": {
            "text": "nach seinem tiefen Sturz."
        },
        "line.3": {
            "text": "Höhenflüge dauern eben kurz"
        },
        "line.4": {
            "text": "und werden zur Warnung beschrieben"
        },
        "line.5": {
            "text": "gegen Übermütigkeiten. Doch die Geschichte"
        },
        "line.6": {
            "text": "behält Gestalten des Scheiterns im Sinn."
        },
        "line.7": {
            "text": "In Clios Hauptbuch verzeichnet Gewinn"
        },
        "line.8": {
            "text": "die dürftigsten Berichte."
        },
        "line.9": {
            "text": "Der Untergang als letzter Akt"
        },
        "line.10": {
            "text": "ist dem Vergessen entzogen."
        },
        "line.11": {
            "text": "Und steigt aus des Alltags Katarakt"
        },
        "line.12": {
            "text": "wie ein seltsamer Regenbogen."
        }
    }
}