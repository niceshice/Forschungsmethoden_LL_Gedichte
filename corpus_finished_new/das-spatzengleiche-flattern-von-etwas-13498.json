{
    "title": "das-spatzengleiche-flattern-von-etwas-13498",
    "gender": "m",
    "author": "José F. A. Oliver",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "wie hoffnung"
        },
        "line.2": {
            "text": "in den augen / aus"
        },
        "line.3": {
            "text": "den augenbuchten herausgeschält das licht / dämmerlicht"
        },
        "line.4": {
            "text": "das sehen & versehen / die welt & fragen im kopfaug grenz-"
        },
        "line.5": {
            "text": "bündel/ schuppenwörter im haar. Wie schuppen von den augen"
        },
        "line.6": {
            "text": "weht &"
        },
        "line.7": {
            "text": "kein wimpernschlag eher, wie ist, was ist, kein"
        },
        "line.8": {
            "text": "atemholen des blickes danach, wie war, was war, nur schöpfen"
        },
        "line.9": {
            "text": "aus dem inneren schöpfen & umgekehrt wach-"
        },
        "line.10": {
            "text": "bleiben& noch"
        },
        "line.11": {
            "text": "ein bild vom bild"
        },
        "line.12": {
            "text": ":1 auf-"
        },
        "line.13": {
            "text": "fliegen1 schwarm einsamkeit der auffliegt, etwas"
        },
        "line.14": {
            "text": "wie"
        },
        "line.15": {
            "text": "zuflüggen"
        },
        "line.16": {
            "text": "/ zuflüchtig & nestwärme greifend, so"
        },
        "line.17": {
            "text": "luftgreifend, wo"
        },
        "line.18": {
            "text": "hin?"
        },
        "line.19": {
            "text": "Für Ulrike Draesner"
        }
    }
}