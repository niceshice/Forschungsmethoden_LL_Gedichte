{
    "title": "schliefen-die-oefen-ii-glauchau-3734",
    "gender": "w",
    "author": "Uljana Wolf",
    "author_birth": "1979",
    "poem": {
        "line.1": {
            "text": "glauchau"
        },
        "line.2": {
            "text": "als wir krank waren von ruß"
        },
        "line.3": {
            "text": "und schüttgut aus archiven"
        },
        "line.4": {
            "text": "zogen wir mit den großvätern"
        },
        "line.5": {
            "text": "ins geschlossne stellwerk ein"
        },
        "line.6": {
            "text": "sahn die alten gleisbewacher"
        },
        "line.7": {
            "text": "ihre hände an die hebel legen"
        },
        "line.8": {
            "text": "durch die tote weichenleitung"
        },
        "line.9": {
            "text": "ging ein zittern wie auf reisen"
        }
    }
}