{
    "title": "ich-sitze-da-wo-du-mich-sitzen-laesst-7586",
    "gender": "m",
    "author": "Christian  Filips",
    "author_birth": "1981",
    "poem": {
        "line.1": {
            "text": "auf deines Vaters Mauer, hoch dort oben"
        },
        "line.2": {
            "text": "(Landschaft mit Kranich, Schilf"
        },
        "line.3": {
            "text": "und Brise, weiter hinten)"
        },
        "line.4": {
            "text": "höre ich dich"
        },
        "line.5": {
            "text": "an dir vorbei ..."
        },
        "line.6": {
            "text": "und höre"
        },
        "line.7": {
            "text": "einen Flussgott, jung"
        },
        "line.8": {
            "text": "der an die hohe, hehre"
        },
        "line.9": {
            "text": "strullt (Mauer in der"
        },
        "line.10": {
            "text": "Abenddämmerung)"
        }
    }
}