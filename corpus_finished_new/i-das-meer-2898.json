{
    "title": "i-das-meer-2898",
    "gender": "m",
    "author": "Gregor  Laschen",
    "author_birth": "1941",
    "poem": {
        "line.1": {
            "text": "Auch hier, reine Figur, das"
        },
        "line.2": {
            "text": "rhythmische Ein-, das Ausatmen"
        },
        "line.3": {
            "text": "einer großen Form: leg dich"
        },
        "line.4": {
            "text": "zu ihr, in ihre dunkle"
        },
        "line.5": {
            "text": "Gültigkeit, die dich freispricht"
        },
        "line.6": {
            "text": "von deines Unglücks Strophen."
        }
    }
}