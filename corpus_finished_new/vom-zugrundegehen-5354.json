{
    "title": "vom-zugrundegehen-5354",
    "gender": "m",
    "author": "Tom Schulz",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "zu den See"
        },
        "line.2": {
            "text": "Anemonen, den Muschel"
        },
        "line.3": {
            "text": "Bänken, bei denen es nichts"
        },
        "line.4": {
            "text": "zu holen gibt"
        },
        "line.5": {
            "text": "außer Lichtgewinn"
        },
        "line.6": {
            "text": "aus der Tiefe, die ins Laszive zieht"
        },
        "line.7": {
            "text": "mit den Zugehfrauen, den Wund"
        },
        "line.8": {
            "text": "Verschlüssen, hinab"
        },
        "line.9": {
            "text": "als unvollendete Sinfonie"
        },
        "line.10": {
            "text": "mit zerbrochenem Notenschlüssel"
        },
        "line.11": {
            "text": "als Kaufhausspezies mit Mensch"
        },
        "line.12": {
            "text": "Jacke, genetischer Strichcode"
        },
        "line.13": {
            "text": "zu den Bachblüten, den zweimal"
        },
        "line.14": {
            "text": "aufgegossenen Melissen der Aus"
        },
        "line.15": {
            "text": "Linderung, frenetischer Zungenabdruck"
        },
        "line.16": {
            "text": "in die Mehrzahl von Liebe, zu jener"
        },
        "line.17": {
            "text": "Schmerzensmutter, den Opium"
        },
        "line.18": {
            "text": "Tropfen am Grunde des Eimers"
        },
        "line.19": {
            "text": "mit der Schneeharfe, der Hunger"
        },
        "line.20": {
            "text": "Harke, zur zellularen Spiel"
        },
        "line.21": {
            "text": "Bank,"
        },
        "line.22": {
            "text": "where no time is money"
        }
    }
}