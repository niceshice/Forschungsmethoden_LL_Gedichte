{
    "title": "der-brocken-2707",
    "gender": "m",
    "author": "Karl Mickel",
    "author_birth": "1935",
    "poem": {
        "line.1": {
            "text": "Die wir gern in Lüften leben"
        },
        "line.2": {
            "text": "Ziehn durch Erdes Eingeweid"
        },
        "line.3": {
            "text": "Bleiben in den Schlüften kleben"
        },
        "line.4": {
            "text": "Alte klebt in neuer Zeit"
        },
        "line.5": {
            "text": "Waren wir auch unbeständig"
        },
        "line.6": {
            "text": "Warn wir ungebärdig auch"
        },
        "line.7": {
            "text": "Unsre Enkelin verständig"
        },
        "line.8": {
            "text": "Fordert Recht auf ihren Bauch"
        },
        "line.9": {
            "text": "Ausgedörrt auf Scheiterhaufen"
        },
        "line.10": {
            "text": "Lüstern aus der Welt gebrannt"
        },
        "line.11": {
            "text": "Reichen euch die Schwarzgetauften"
        },
        "line.12": {
            "text": "Schwesterlich die Knochenhand"
        },
        "line.13": {
            "text": "Ach die Tänze, die ihr neulich"
        },
        "line.14": {
            "text": "Wieder in die Welt gesetzt"
        },
        "line.15": {
            "text": "Sind uns ganz und gar erfreulich"
        },
        "line.16": {
            "text": "So hat uns St. Veit gehetzt"
        },
        "line.17": {
            "text": "Und die langgehaarten Knaben"
        },
        "line.18": {
            "text": "Sind uns nicht so unvertraut"
        },
        "line.19": {
            "text": "Möchte Den und Jenen haben"
        },
        "line.20": {
            "text": "Stäke gern in eurer Haut"
        }
    }
}