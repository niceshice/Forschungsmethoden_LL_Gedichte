{
    "title": "zwischen-dem-jetzt-und-dem-jetzt-6554",
    "gender": "w",
    "author": "Ilana Shmueli",
    "author_birth": "1924",
    "poem": {
        "line.1": {
            "text": "Zwischen dem Jetzt und dem Jetzt"
        },
        "line.2": {
            "text": "unüberbrückbar"
        },
        "line.3": {
            "text": "im Nu ists vorbei"
        },
        "line.4": {
            "text": "nie wieder wie früher"
        },
        "line.5": {
            "text": "nie wieder wie jetzt"
        },
        "line.6": {
            "text": "der Zeiger verrückt"
        },
        "line.7": {
            "text": "an die Rädchen geklammert"
        },
        "line.8": {
            "text": "synchronisiert"
        },
        "line.9": {
            "text": "die Schließmundschnecke"
        },
        "line.10": {
            "text": "kleine Klausilie"
        }
    }
}