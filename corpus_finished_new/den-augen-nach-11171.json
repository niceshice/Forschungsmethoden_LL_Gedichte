{
    "title": "den-augen-nach-11171",
    "gender": "m",
    "author": "Norbert Lange",
    "author_birth": "1978",
    "poem": {
        "line.1": {
            "text": "Ließ die Augen, als flögen sie"
        },
        "line.2": {
            "text": "defekte Röhren, in die Luft"
        },
        "line.3": {
            "text": "und flögen"
        },
        "line.4": {
            "text": "ans Himmelzelt, gegen"
        },
        "line.5": {
            "text": "die knackende Schale gekickt"
        },
        "line.6": {
            "text": "wuchtig, in weitem Bogen"
        },
        "line.7": {
            "text": "über der Straße, an der ich ging"
        },
        "line.8": {
            "text": "ein Schwalbenpaar, den Himmel"
        },
        "line.9": {
            "text": "schwirrend zerschneiden"
        },
        "line.10": {
            "text": "sah ihn von ihnen geschnitten"
        },
        "line.11": {
            "text": "wie Pappe, von Flügeln"
        },
        "line.12": {
            "text": "geschliffen, den lose im Wind"
        },
        "line.13": {
            "text": "trudelnden Fetzen"
        },
        "line.14": {
            "text": "sich vom Firmament lösen"
        },
        "line.15": {
            "text": "zwischen den Sphären"
        },
        "line.16": {
            "text": "heruntertropfen bleiern"
        },
        "line.17": {
            "text": "bis es plump zu Boden sank"
        },
        "line.18": {
            "text": "eine Dal-Uhr den Augen nach"
        },
        "line.19": {
            "text": "und als ich dagegentrat"
        },
        "line.20": {
            "text": "eine scheppernde Dose"
        },
        "line.21": {
            "text": "in hohem Bogen"
        },
        "line.22": {
            "text": "die Schale durchschlagen"
        },
        "line.23": {
            "text": "sah sich weitende Sprünge"
        },
        "line.24": {
            "text": "über der Stadt, am Horizont"
        },
        "line.25": {
            "text": "den Himmel spalten"
        },
        "line.26": {
            "text": "vor den Augen die Kuppel"
        },
        "line.27": {
            "text": "gesprengt, ein Sternbild am blanken"
        },
        "line.28": {
            "text": "flackernden Himmel, zwei Sicheln"
        },
        "line.29": {
            "text": "aus Nichts in dem Gesicht"
        },
        "line.30": {
            "text": "das dort hing; war genauso"
        },
        "line.31": {
            "text": "wie der Himmel, unter dem ich ging"
        }
    }
}