            "text": "saläbaum"
        },
        "line.8": {
            "text": "wo denn das segelkieseln"
        },
        "line.9": {
            "text": "unsere wauselilasen raubt"
        },
        "line.10": {
            "text": "wer in segelkistl größer"
        },
        "line.11": {
            "text": "gebebelalammier bachänann"
        },
        "line.12": {
            "text": "glühsanwachagaumiegelas"
        },
