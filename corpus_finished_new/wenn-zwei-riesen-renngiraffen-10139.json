{
    "title": "wenn-zwei-riesen-renngiraffen-10139",
    "gender": "m",
    "author": "Arne  Rautenberg",
    "author_birth": "1967",
    "poem": {
        "line.1": {
            "text": "wenn zwei"
        },
        "line.2": {
            "text": "riesen renngiraffen"
        },
        "line.3": {
            "text": "gerne mal"
        },
        "line.4": {
            "text": "nach hinten gaffen"
        },
        "line.5": {
            "text": "während sie"
        },
        "line.6": {
            "text": "durch steppen rennen"
        },
        "line.7": {
            "text": "dabei manchmal"
        },
        "line.8": {
            "text": "auch noch pennen"
        },
        "line.9": {
            "text": "wenn sie dann"
        },
        "line.10": {
            "text": "von beiden seiten"
        },
        "line.11": {
            "text": "sich auch noch"
        },
        "line.12": {
            "text": "entgegenreiten"
        },
        "line.13": {
            "text": "denkt man nur"
        },
        "line.14": {
            "text": "was wäre wenn"
        },
        "line.15": {
            "text": "jetzt die langen"
        },
        "line.16": {
            "text": "hälse - - denn"
        },
        "line.17": {
            "text": "nah und näher"
        },
        "line.18": {
            "text": "kommn sie sich"
        },
        "line.19": {
            "text": "gleich wirds rappeln"
        },
        "line.20": {
            "text": "fürchterlich!"
        },
        "line.21": {
            "text": "niemand ruft:"
        },
        "line.22": {
            "text": "halt! stop! verboten!"
        },
        "line.23": {
            "text": "um gottes willen!!"
        },
        "line.24": {
            "text": "das gibt nen knoten!!!"
        }
    }
}