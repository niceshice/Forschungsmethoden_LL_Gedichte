{
    "title": "sieben-geliebte-158",
    "gender": "m",
    "author": "Gerhard Falkner",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "meine erste geliebte"
        },
        "line.2": {
            "text": "heißt nicht laura und nicht liliana"
        },
        "line.3": {
            "text": "sie tanzt nicht und schläft nicht und lebt nicht"
        },
        "line.4": {
            "text": "gleicht sie nicht gott"
        },
        "line.5": {
            "text": "meine zweite geliebte"
        },
        "line.6": {
            "text": "öffnet die fenster zum jungen april"
        },
        "line.7": {
            "text": "ich seh die jubelnden lerchen steigen"
        },
        "line.8": {
            "text": "wie schön muß sie sein"
        },
        "line.9": {
            "text": "meine dritte geliebte"
        },
        "line.10": {
            "text": "ist reich bis über die schultern"
        },
        "line.11": {
            "text": "sie schenkt mir ruhe und lust, ganz wie sie will"
        },
        "line.12": {
            "text": "ihre bekanntschaft bleibt sie mir schuldig"
        },
        "line.13": {
            "text": "meine vierte geliebte"
        },
        "line.14": {
            "text": "lahmt. als ich mich umblickte"
        },
        "line.15": {
            "text": "da wir die gestrafte stadt verließen"
        },
        "line.16": {
            "text": "streifte mein blick ihr bein"
        },
        "line.17": {
            "text": "meine fünfte geliebte"
        },
        "line.18": {
            "text": "ist mir beim küssen"
        },
        "line.19": {
            "text": "ins weinglas gefallen. sie ist dahin"
        },
        "line.20": {
            "text": "und kommt nicht mehr woher"
        },
        "line.21": {
            "text": "meine siebte geliebte"
        },
        "line.22": {
            "text": "ist dieselbe wie die sechste"
        },
        "line.23": {
            "text": "ich habe sie doppelt. immer müssen"
        },
        "line.24": {
            "text": "sie wegen sich weinen"
        }
    }
}