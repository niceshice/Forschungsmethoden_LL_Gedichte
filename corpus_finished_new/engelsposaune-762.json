{
    "title": "engelsposaune-762",
    "gender": "m",
    "author": "Maik Lippert",
    "author_birth": "1966",
    "poem": {
        "line.1": {
            "text": "jeden morgen der kelch"
        },
        "line.2": {
            "text": "das gelb von gummihandschuhen"
        },
        "line.3": {
            "text": "ein fingerzeig gottes"
        },
        "line.4": {
            "text": "liegt auf dir"
        },
        "line.5": {
            "text": "beim vorbeigehen am vorgarten"
        },
        "line.6": {
            "text": "zur haltestelle"
        },
        "line.7": {
            "text": "lukas stöbert"
        },
        "line.8": {
            "text": "in den abfallkörben"
        },
        "line.9": {
            "text": "den krumen nach"
        },
        "line.10": {
            "text": "wandert am packpapier"
        },
        "line.11": {
            "text": "die geäderte stierzunge"
        },
        "line.12": {
            "text": "wie ein blatt"
        },
        "line.13": {
            "text": "vom nachtschatten"
        },
        "line.14": {
            "text": "noch bleich die geliebte"
        },
        "line.15": {
            "text": "in der hand"
        },
        "line.16": {
            "text": "das buch mit blauem einband"
        },
        "line.17": {
            "text": "monologe über engelarten"
        },
        "line.18": {
            "text": "und du denkst"
        },
        "line.19": {
            "text": "an zierliche schulterblätter"
        }
    }
}