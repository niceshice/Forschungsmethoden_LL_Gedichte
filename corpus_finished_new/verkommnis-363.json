{
    "title": "verkommnis-363",
    "gender": "w",
    "author": "Anne Duden",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "I"
        },
        "line.2": {
            "text": "Den Tamariskenkillern heute"
        },
        "line.3": {
            "text": "den Ent- und Erledigern"
        },
        "line.4": {
            "text": "Abhackern und Stampfern"
        },
        "line.5": {
            "text": "kochblütig"
        },
        "line.6": {
            "text": "Krebs ins Essen geschickt"
        },
        "line.7": {
            "text": "und in die Saucen gebrannt"
        },
        "line.8": {
            "text": "unbekommen nachbarlich."
        },
        "line.9": {
            "text": "Give me a strength."
        },
        "line.10": {
            "text": "Give me a break."
        },
        "line.11": {
            "text": "Den Normalschwadronen"
        },
        "line.12": {
            "text": "und Mordfabrikanten immer schon"
        },
        "line.13": {
            "text": "und nichts als weißglühende Schlußworte"
        },
        "line.14": {
            "text": "hinterher oder entgegen"
        },
        "line.15": {
            "text": "geschüttet oder gezischt."
        },
        "line.16": {
            "text": "Alles andere"
        },
        "line.17": {
            "text": "nur noch schräg anzusehen gewagt"
        },
        "line.18": {
            "text": "und keinerlei Sitzgelegenheit im Freien."
        },
        "line.19": {
            "text": "II"
        },
        "line.20": {
            "text": "Nackte Quartiere"
        },
        "line.21": {
            "text": "erwarten kernlos das Abgemachte"
        },
        "line.22": {
            "text": "und die neue Bebrütung."
        },
        "line.23": {
            "text": "Anrainer lange entlaubt."
        },
        "line.24": {
            "text": "Niemand ist aufgerufen"
        },
        "line.25": {
            "text": "windige Strick-Kinder bloß"
        },
        "line.26": {
            "text": "am Forellenteich"
        },
        "line.27": {
            "text": "und einzig den Gepfählten entgeht nichts."
        },
        "line.28": {
            "text": "Bitte, das Haus brennt"
        },
        "line.29": {
            "text": "ich sagte es schon."
        },
        "line.30": {
            "text": "Die Kontinente wachsen"
        },
        "line.31": {
            "text": "ruckhaft zusammen"
        },
        "line.32": {
            "text": "erbrechen Flüchtende ineinander."
        },
        "line.33": {
            "text": "III"
        },
        "line.34": {
            "text": "Beachten Sie Nimmerwiedergesehenes"
        },
        "line.35": {
            "text": "all die Krusten und Schalen"
        },
        "line.36": {
            "text": "und mondsüchtige Schädel"
        },
        "line.37": {
            "text": "junger Vögel auf dem Gilbrasen."
        },
        "line.38": {
            "text": "Es rauscht, aber lauthals"
        },
        "line.39": {
            "text": "vor Sonne."
        },
        "line.40": {
            "text": "IV"
        },
        "line.41": {
            "text": "Ausdrücklich gelangweilt"
        },
        "line.42": {
            "text": "mit dauernd breiter werdenden Magnetzehen"
        },
        "line.43": {
            "text": "werden die Geräte betätigt."
        },
        "line.44": {
            "text": "Wer."
        },
        "line.45": {
            "text": "Niemand wächst wirklich nach."
        },
        "line.46": {
            "text": "Kein Würgeengel fällt in den Blick"
        },
        "line.47": {
            "text": "kein Schweiß- oder Schwachpatron"
        },
        "line.48": {
            "text": "kein Kosemeister gegen das Bluten"
        },
        "line.49": {
            "text": "keines Nachts."
        },
        "line.50": {
            "text": "Nur Katzenzimmer"
        },
        "line.51": {
            "text": "und die Auf- und Abtretenden"
        },
        "line.52": {
            "text": "und der die Worte entherzt und verreißt aus Not"
        },
        "line.53": {
            "text": "der schlagende Sturmvogel"
        },
        "line.54": {
            "text": "im Rippenkäfig."
        }
    }
}