{
    "title": "boese-gewichte-3792",
    "gender": "w",
    "author": "Sylvia  Geist",
    "author_birth": "1963",
    "poem": {
        "line.1": {
            "text": "Zimmer mit Aussicht auf Zimmer"
        },
        "line.2": {
            "text": "mit Aussicht. Luftlinien"
        },
        "line.3": {
            "text": "graben am Fluss. Hinüber"
        },
        "line.4": {
            "text": "zweigleicht, zweighell"
        },
        "line.5": {
            "text": "laufen Hunde mit"
        },
        "line.6": {
            "text": "ihrem Laut unter sich"
        },
        "line.7": {
            "text": "über Steine, schrittdicht"
        },
        "line.8": {
            "text": "in die Ruhe gesetzt. Feierabende"
        },
        "line.9": {
            "text": "laden Lichtirre von den Balkonen"
        },
        "line.10": {
            "text": "ein unter einander"
        },
        "line.11": {
            "text": "zu frieren. Zwischen uns"
        },
        "line.12": {
            "text": "die Körper heben"
        },
        "line.13": {
            "text": "kein Gedenken auf, auch das Paar"
        },
        "line.14": {
            "text": "ertrunken am Tag der Hochzeit"
        },
        "line.15": {
            "text": "erinnert an nichts sonst. In Ufernähe"
        },
        "line.16": {
            "text": "nur warten Verwandte seit langem"
        },
        "line.17": {
            "text": "nicht schwerer als Röhricht"
        },
        "line.18": {
            "text": "auf ihre Rückkehr hierher."
        }
    }
}