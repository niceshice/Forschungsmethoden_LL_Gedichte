{
    "title": "da-ist-es-12827",
    "gender": "m",
    "author": "Rainer René Mueller",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": ": zerschlissen, dieses /"
        },
        "line.2": {
            "text": "... was"
        },
        "line.3": {
            "text": "zählst du; du / ich :"
        },
        "line.4": {
            "text": "um vier Uhr früh. / Früh wird's"
        },
        "line.5": {
            "text": "abgetschilpt. Es überdauert"
        },
        "line.6": {
            "text": "dich,"
        },
        "line.7": {
            "text": "was vom Grün pfeift, von den"
        },
        "line.8": {
            "text": "Firsten : Antennen"
        },
        "line.9": {
            "text": "Firstendach\" : Worten /"
        },
        "line.10": {
            "text": "tön ..., so"
        },
        "line.11": {
            "text": "oben ein Schönes ist,"
        },
        "line.12": {
            "text": "ist's Tönen, so."
        },
        "line.13": {
            "text": "Haben zu sehen, so"
        },
        "line.14": {
            "text": "zu hören. So, zu tönen."
        },
        "line.15": {
            "text": "Es ist hör'n wie einer /"
        },
        "line.16": {
            "text": "denkt's : Stille"
        }
    }
}