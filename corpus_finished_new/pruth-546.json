{
    "title": "pruth-546",
    "gender": "w",
    "author": "Rose Ausländer",
    "author_birth": "1901",
    "poem": {
        "line.1": {
            "text": "Da zirpten die Kiesel im Pruth"
        },
        "line.2": {
            "text": "ritzten flüchtige Muster in"
        },
        "line.3": {
            "text": "unsre Sohlen"
        },
        "line.4": {
            "text": "Narzisse wir lagen im Wasserspiegel"
        },
        "line.5": {
            "text": "hielten uns selbst im Arm"
        },
        "line.6": {
            "text": "Nachts vom Wind bedeckt"
        },
        "line.7": {
            "text": "Bett mit Fischen gefüllt"
        },
        "line.8": {
            "text": "Goldfisch der Mond"
        },
        "line.9": {
            "text": "Schläfenlockengeflüster:"
        },
        "line.10": {
            "text": "der Rabbi in Kaftan und Stramel"
        },
        "line.11": {
            "text": "von glückäugigen Chassidim umringt"
        },
        "line.12": {
            "text": "Vögel  wir kennen nicht"
        },
        "line.13": {
            "text": "ihre Namen ihr Schrei"
        },
        "line.14": {
            "text": "lockt und erschreckt"
        },
        "line.15": {
            "text": "Auch unser Gefieder ist fertig"
        },
        "line.16": {
            "text": "wir folgen euch"
        },
        "line.17": {
            "text": "über Kukuruzfelder"
        },
        "line.18": {
            "text": "schaukelnde Synagogen"
        },
        "line.19": {
            "text": "Immer zurück zum Pruth"
        },
        "line.20": {
            "text": "Flöße"
        },
        "line.21": {
            "text": "(aus Holz oder Johannisbrot?)"
        },
        "line.22": {
            "text": "pruthab"
        },
        "line.23": {
            "text": "Wohin ihr Eilenden"
        },
        "line.24": {
            "text": "und wir hier allein"
        },
        "line.25": {
            "text": "mit den Steinen?"
        }
    }
}