{
    "title": "vorm-absterben-3-4523",
    "gender": "m",
    "author": "Robert Schindel",
    "author_birth": "1944",
    "poem": {
        "line.1": {
            "text": "So nahe vom Unabänderlichen"
        },
        "line.2": {
            "text": "In geschickter Nachbarschaft"
        },
        "line.3": {
            "text": "Zum Erstarrten wirbelt"
        },
        "line.4": {
            "text": "Mein Nachlassleben durch"
        },
        "line.5": {
            "text": "Die Vorläufigkeiten"
        },
        "line.6": {
            "text": "Schlafkristalle schließen"
        },
        "line.7": {
            "text": "Die Tagträume weg"
        }
    }
}