{
    "title": "wolken-darueber-ich-weiss-nur-das-eine-94",
    "gender": "w",
    "author": "Elke Erb",
    "author_birth": "1938",
    "poem": {
        "line.1": {
            "text": "Ich gehe neben dem Rad."
        },
        "line.2": {
            "text": "Der Wagen ist höher als ich."
        },
        "line.3": {
            "text": "Er fährt eine hohe Fracht."
        },
        "line.4": {
            "text": "Dahinter die Giebel stehn"
        },
        "line.5": {
            "text": "zart mit der Luft."
        },
        "line.6": {
            "text": "Sie schließen die Dächer."
        },
        "line.7": {
            "text": "So steigt es rechts von mir weiter."
        },
        "line.8": {
            "text": "Dann endet es, aber rechts vorn"
        },
        "line.9": {
            "text": "ragt der Kastanienbaum."
        },
        "line.10": {
            "text": "Ein Landweg, im Dorf, eine"
        },
        "line.11": {
            "text": "Dorfstraße, unsere."
        },
        "line.12": {
            "text": "Die Fuhre fährt hinaus."
        },
        "line.13": {
            "text": "Der Fuhrmann auf seinem Kutschbock?"
        },
        "line.14": {
            "text": "Blickt lustig. Die Ochsen blicken wie Ochsen."
        },
        "line.15": {
            "text": "Ich blicke ernst."
        },
        "line.16": {
            "text": "Wer entgegen kommt, sieht es."
        },
        "line.17": {
            "text": "Ich bin eine Achtjährige."
        },
        "line.18": {
            "text": "Der Fuhrmann - im Alter des Fuhrmanns."
        },
        "line.19": {
            "text": "Die Ochsen sind ihr Teil Ochsen."
        },
        "line.20": {
            "text": "Neben meiner Schläfe rechts"
        },
        "line.21": {
            "text": "das Trapez der hölzernen Wagenwand."
        },
        "line.22": {
            "text": "Links - nichts, der Garten; entfernt,"
        },
        "line.23": {
            "text": "wo er endet, das Elternhaus."
        },
        "line.24": {
            "text": "Ich gehe mit der Fracht."
        },
        "line.25": {
            "text": "Der Fuhrmann blickt verschmitzt."
        },
        "line.26": {
            "text": "Unter dem Mützenschirm"
        },
        "line.27": {
            "text": "die ewigen Lachfältchen."
        },
        "line.28": {
            "text": "Ich bleibe neben dem Rad,"
        },
        "line.29": {
            "text": "als sei ich es, die ab und zu"
        },
        "line.30": {
            "text": "achtsam die Zügel bewegt."
        },
        "line.31": {
            "text": "Unter den Schwingen, was ist."
        },
        "line.32": {
            "text": "Ein sachter Flug."
        },
        "line.33": {
            "text": "30.11.1997 / 23.1.1998"
        }
    }
}