{
    "title": "aus-duengerkind-ii-15422",
    "gender": "w",
    "author": "Sonja vom Brocke",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "Fühler, die aus Vessel ragen"
        },
        "line.2": {
            "text": "kosen Bröselbrot und Bauch"
        },
        "line.3": {
            "text": "von der Motte."
        },
        "line.4": {
            "text": "Und ein Mottenkuss, ihr Rausch"
        },
        "line.5": {
            "text": "schlagen Wellen"
        },
        "line.6": {
            "text": "weit und fleckig"
        },
        "line.7": {
            "text": "brodeln zierlos unbehaust  zur Revolte-"
        },
        "line.8": {
            "text": "Vibration"
        },
        "line.9": {
            "text": "wie im Fiebertraum verkehren"
        },
        "line.10": {
            "text": "Dimensionen und Reflexe"
        },
        "line.11": {
            "text": "und die Quälersekte schrumpft"
        },
        "line.12": {
            "text": "zur Rosinenkolonie"
        },
        "line.13": {
            "text": "für ein kolossales Müsli"
        },
        "line.14": {
            "text": "denn auch Motten mögen Müsli"
        },
        "line.15": {
            "text": "von der Seide mit Rosine"
        },
        "line.16": {
            "text": "frisst die Schwester, schlürft die Fliege"
        },
        "line.17": {
            "text": "kaut die Ratte, pickt der Hahn"
        },
        "line.18": {
            "text": "ein feuchtfröhliches Schlamassel"
        },
        "line.19": {
            "text": "Neuer Tag"
        }
    }
}