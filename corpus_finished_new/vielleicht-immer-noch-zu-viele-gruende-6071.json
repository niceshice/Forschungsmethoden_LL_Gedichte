{
    "title": "vielleicht-immer-noch-zu-viele-gruende-6071",
    "gender": "m",
    "author": "Christoph W. Bauer",
    "author_birth": "1968",
    "poem": {
        "line.1": {
            "text": "vielleicht immer noch zu viele gründe"
        },
        "line.2": {
            "text": "die koffer auszupacken und die gedanken"
        },
        "line.3": {
            "text": "an ein exil wieder ins sprechen anderer"
        },
        "line.4": {
            "text": "zu entlassen mit jeder erwartung"
        },
        "line.5": {
            "text": "entrümpelter augen und routiniertem"
        },
        "line.6": {
            "text": "träumeaufsagen ist ja nur schnee der"
        },
        "line.7": {
            "text": "fällt so nüchtern und mechanisch"
        },
        "line.8": {
            "text": "wie ein blick aus dem fenster"
        },
        "line.9": {
            "text": "auf den ort meiner wahl"
        }
    }
}