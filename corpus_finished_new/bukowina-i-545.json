{
    "title": "bukowina-i-545",
    "gender": "w",
    "author": "Rose Ausländer",
    "author_birth": "1901",
    "poem": {
        "line.1": {
            "text": "Grüne Mutter"
        },
        "line.2": {
            "text": "Bukowina"
        },
        "line.3": {
            "text": "Schmetterlinge im Haar"
        },
        "line.4": {
            "text": "Trink"
        },
        "line.5": {
            "text": "sagt die Sonne"
        },
        "line.6": {
            "text": "rote Melonenmilch"
        },
        "line.7": {
            "text": "weiße Kukuruzmilch"
        },
        "line.8": {
            "text": "ich machte sie süß"
        },
        "line.9": {
            "text": "Violette Föhrenzapfen"
        },
        "line.10": {
            "text": "Luftflügel Vögel und Laub"
        },
        "line.11": {
            "text": "Der Karpatenrücken"
        },
        "line.12": {
            "text": "väterlich"
        },
        "line.13": {
            "text": "lädt dich ein"
        },
        "line.14": {
            "text": "dich zu tragen"
        },
        "line.15": {
            "text": "Vier Sprachen"
        },
        "line.16": {
            "text": "Viersprachenlieder"
        },
        "line.17": {
            "text": "Menschen"
        },
        "line.18": {
            "text": "die sich verstehn"
        }
    }
}