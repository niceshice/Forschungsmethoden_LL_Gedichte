{
    "title": "ankara-slum-5671",
    "gender": "m",
    "author": "Manfred Peter Hein",
    "author_birth": "1931",
    "poem": {
        "line.1": {
            "text": "Wie der Blick umschweift"
        },
        "line.2": {
            "text": "frei unfrei frei entgegen"
        },
        "line.3": {
            "text": "dem Tag Nacht um Nacht"
        },
        "line.4": {
            "text": "Verschlag an Verschlag gebaut"
        },
        "line.5": {
            "text": "zu Füßen der Marmorpracht"
        }
    }
}