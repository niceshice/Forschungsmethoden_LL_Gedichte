{
    "title": "im-fluchtland-eilt-3349",
    "gender": "w",
    "author": "Maja  Haderlap",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "im fluchtland eilt"
        },
        "line.2": {
            "text": "mir gerede voraus. mein kleid"
        },
        "line.3": {
            "text": "raschelt und kracht wie aus militärstoff"
        },
        "line.4": {
            "text": "genäht, aus steifem tuch."
        },
        "line.5": {
            "text": "gespensterfahnen"
        },
        "line.6": {
            "text": "flattern von windigen firsten."
        },
        "line.7": {
            "text": "frauen mit rüde geschnittenem haar"
        },
        "line.8": {
            "text": "schmücken geschnitzte trophäen."
        },
        "line.9": {
            "text": "ihr freundlicher gruß kneift ins gesicht."
        },
        "line.10": {
            "text": "vorher und nachher schlagen hier zu."
        },
        "line.11": {
            "text": "mein und dein ist das wort für verrat."
        },
        "line.12": {
            "text": "ich lebe bedeckt und nähre"
        },
        "line.13": {
            "text": "mich vom gestohlenen licht."
        },
        "line.14": {
            "text": "wachse und wachse davon"
        },
        "line.15": {
            "text": "über mich hinaus, stolpere,"
        },
        "line.16": {
            "text": "falle dann auf die nase. bin wieder"
        },
        "line.17": {
            "text": "klein und mein kleid ist aus haut."
        }
    }
}