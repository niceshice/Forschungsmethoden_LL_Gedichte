{
    "title": "humbug-ist-eine-bahnstation-256",
    "gender": "m",
    "author": "Walther Petri",
    "author_birth": "1940",
    "poem": {
        "line.1": {
            "text": "Humbug ist eine Bahnstation"
        },
        "line.2": {
            "text": "in der verkehrten Welt"
        },
        "line.3": {
            "text": "und wenn der Zug hält"
        },
        "line.4": {
            "text": "siehst du schon"
        },
        "line.5": {
            "text": "daß hier der Hund blüht"
        },
        "line.6": {
            "text": "und die Rose bellt"
        },
        "line.7": {
            "text": "in Humbug leben Hum und Bug"
        },
        "line.8": {
            "text": "das unzertrennliche Pärchen"
        },
        "line.9": {
            "text": "es wohnt in einem Mostekrug"
        },
        "line.10": {
            "text": "und schenkt dir Humbuger Märchen"
        },
        "line.11": {
            "text": "Herr Unfug ist der Bürgermeister"
        },
        "line.12": {
            "text": "über tausend Humbuger Geister"
        },
        "line.13": {
            "text": "gestern ist ihm verliehen worden"
        },
        "line.14": {
            "text": "der große güldene Kinkerlitz  Orden"
        },
        "line.15": {
            "text": "denn Humbug ist eine Bahnstation"
        },
        "line.16": {
            "text": "in der verkehrten Welt"
        },
        "line.17": {
            "text": "wo in der schönen Winterzeit"
        },
        "line.18": {
            "text": "der blanke Quark vom Himmel fällt"
        },
        "line.19": {
            "text": "wenn Unfug nicht gestorben ist"
        },
        "line.20": {
            "text": "verzapft er heut noch Most und Mist"
        },
        "line.21": {
            "text": "und sollt er doch mal sterben"
        },
        "line.22": {
            "text": "dann wirst du Humbug erben"
        },
        "line.23": {
            "text": "falls aber böse Finsternis"
        },
        "line.24": {
            "text": "im schwarzen Mantel"
        },
        "line.25": {
            "text": "aus der Kutsche steigt"
        },
        "line.26": {
            "text": "verblüht der Hund"
        },
        "line.27": {
            "text": "und jede Rose schweigt"
        }
    }
}