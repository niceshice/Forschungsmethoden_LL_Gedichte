{
    "title": "gruen-lesen-8406",
    "gender": "w",
    "author": "Doris Runge",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "dass man ist"
        },
        "line.2": {
            "text": "dass man gewesen ist"
        },
        "line.3": {
            "text": "hier war"
        },
        "line.4": {
            "text": "oder hätte sein können"
        },
        "line.5": {
            "text": "ein grund für etwas"
        },
        "line.6": {
            "text": "eine decke für etwas"
        },
        "line.7": {
            "text": "eine nach der man"
        },
        "line.8": {
            "text": "sich streckt decke"
        },
        "line.9": {
            "text": "die zudeckt eine"
        },
        "line.10": {
            "text": "verdeckt"
        },
        "line.11": {
            "text": "ein fell"
        },
        "line.12": {
            "text": "für alle fälle"
        },
        "line.13": {
            "text": "grün"
        },
        "line.14": {
            "text": "durch das"
        },
        "line.15": {
            "text": "der wind geht"
        },
        "line.16": {
            "text": "und liest"
        }
    }
}