{
    "title": "nichts-spannt-mich-mehr-14536",
    "gender": "w",
    "author": "Ruth Wolf-Rehfeldt",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "Nichts spannt mich, mehr"
        },
        "line.2": {
            "text": "Und hebt mich auf"
        },
        "line.3": {
            "text": "Zieht mich nach vorn"
        },
        "line.4": {
            "text": "Sehnt mich zurück"
        },
        "line.5": {
            "text": "Mein Schwerpunkt ruht"
        },
        "line.6": {
            "text": "Doch fühl ich nicht"
        },
        "line.7": {
            "text": "Das Glück"
        },
        "line.8": {
            "text": "Nur mein Gewicht"
        }
    }
}