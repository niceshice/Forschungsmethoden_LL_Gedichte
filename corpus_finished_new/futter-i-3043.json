{
    "title": "futter-i-3043",
    "gender": "w",
    "author": "Karin  Fellner",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "hüllen sie sich in stimmen / nieseln auf geköpfte"
        },
        "line.2": {
            "text": "platanen über pfützen / geduckte passanten"
        },
        "line.3": {
            "text": "sie sitzen"
        },
        "line.4": {
            "text": "an der theke, pastis / mahlen maschinen francs"
        },
        "line.5": {
            "text": "spannen gerüchte aus / steigen auf pyrenäen"
        },
        "line.6": {
            "text": "schlürfen sie"
        },
        "line.7": {
            "text": "kaffee, sind helden von"
        },
        "line.8": {
            "text": "vagem mut, ziehen los."
        }
    }
}