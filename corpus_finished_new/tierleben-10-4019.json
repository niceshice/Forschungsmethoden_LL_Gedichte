{
    "title": "tierleben-10-4019",
    "gender": "m",
    "author": "Felix Philipp Ingold",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "10"
        },
        "line.2": {
            "text": "Ein Ort noch"
        },
        "line.3": {
            "text": "ungeschieden von Geist"
        },
        "line.4": {
            "text": "und Bewegung. Ruht"
        },
        "line.5": {
            "text": "nah der Natur die das Rätsel"
        },
        "line.6": {
            "text": "der Lösung ist. Das"
        },
        "line.7": {
            "text": "Knirschen und Wummern"
        },
        "line.8": {
            "text": "zwischen Kontinent und Meer."
        },
        "line.9": {
            "text": "Das Wort wo endlich"
        },
        "line.10": {
            "text": "das Blut beginnt. Wo wie im"
        },
        "line.11": {
            "text": "Märch... das erste"
        },
        "line.12": {
            "text": "Ja! klart. Im Spieglein immer"
        },
        "line.13": {
            "text": "schön verschieden"
        },
        "line.14": {
            "text": "der Leib. Wie er tobt im Auge"
        },
        "line.15": {
            "text": "des Andern."
        }
    }
}