{
    "title": "gestern-bekam-ich-3728",
    "gender": "m",
    "author": "Peter Turrini",
    "author_birth": "1944",
    "poem": {
        "line.1": {
            "text": "Gestern bekam ich"
        },
        "line.2": {
            "text": "keinen Steifen."
        },
        "line.3": {
            "text": "Vorgestern"
        },
        "line.4": {
            "text": "auch nicht."
        },
        "line.5": {
            "text": "Und vorvorgestern"
        },
        "line.6": {
            "text": "war er h√∂chstens"
        },
        "line.7": {
            "text": "halb steif."
        },
        "line.8": {
            "text": "Die Liebe ist eben"
        },
        "line.9": {
            "text": "etwas Weiches"
        },
        "line.10": {
            "text": "Zartes."
        }
    }
}