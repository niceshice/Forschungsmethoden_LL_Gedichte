{
    "title": "namen-wird-der-vogel-nie-auf-nachtigall-fliegen-371",
    "gender": "m",
    "author": "Paul Wühr",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "Namen"
        },
        "line.2": {
            "text": "Wird der Vogel nie auf"
        },
        "line.3": {
            "text": "Nachtigall fliegen"
        },
        "line.4": {
            "text": "ihr Name nie singen"
        },
        "line.5": {
            "text": "die Blume nie blühen"
        },
        "line.6": {
            "text": "in Rose"
        },
        "line.7": {
            "text": "ihr Name nie duften"
        },
        "line.8": {
            "text": "der Fisch nie schwimmen"
        },
        "line.9": {
            "text": "im Gold"
        },
        "line.10": {
            "text": "Gold nie schweigen"
        },
        "line.11": {
            "text": "als Fisch"
        },
        "line.12": {
            "text": "Schweigen nie Silber"
        },
        "line.13": {
            "text": "heißen dürfen"
        },
        "line.14": {
            "text": "wo Silber schweigend"
        },
        "line.15": {
            "text": "fliegt in den Vogel"
        },
        "line.16": {
            "text": "der seinen Namen blühen"
        },
        "line.17": {
            "text": "läßt als Blume die"
        },
        "line.18": {
            "text": "im Wasser verschweigt"
        },
        "line.19": {
            "text": "mit ihrem Namen eine"
        },
        "line.20": {
            "text": "Fisch"
        },
        "line.21": {
            "text": "der so heißen wird"
        },
        "line.22": {
            "text": "nie"
        }
    }
}