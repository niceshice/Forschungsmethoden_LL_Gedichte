{
    "title": "die-roten-schuhe-157",
    "gender": "m",
    "author": "Gerhard Falkner",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "fremd bin ich aufgewacht und früh"
        },
        "line.2": {
            "text": "der stecker steckte noch"
        },
        "line.3": {
            "text": "eine frau, kleiner als ein pferd"
        },
        "line.4": {
            "text": "reichte mir einen apfel auf englisch:"
        },
        "line.5": {
            "text": "apple, she said"
        },
        "line.6": {
            "text": "willst du nicht beißen"
        },
        "line.7": {
            "text": "doch wer sind die roten schuhe"
        },
        "line.8": {
            "text": "the red shoes"
        },
        "line.9": {
            "text": "dort auf seiner saueren seite"
        },
        "line.10": {
            "text": "blutig steigen sie den apfel herab"
        },
        "line.11": {
            "text": "ach ich muß sterben und habe"
        },
        "line.12": {
            "text": "noch gar nicht gefrühstückt"
        }
    }
}