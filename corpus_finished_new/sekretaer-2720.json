{
    "title": "sekretaer-2720",
    "gender": "w",
    "author": "Sabina Naef",
    "author_birth": "1974",
    "poem": {
        "line.1": {
            "text": "eine Schublade für gepresste Pusteblumen"
        },
        "line.2": {
            "text": "eine Schreibplatte für die Schallplattenrillensprache"
        },
        "line.3": {
            "text": "eine Schublade für verlorene Absätze"
        },
        "line.4": {
            "text": "ein Geheimfach für den ersten Schnee"
        }
    }
}