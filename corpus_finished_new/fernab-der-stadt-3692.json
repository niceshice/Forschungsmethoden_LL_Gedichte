{
    "title": "fernab-der-stadt-3692",
    "gender": "m",
    "author": "Yaak  Karsunke",
    "author_birth": "1934",
    "poem": {
        "line.1": {
            "text": "auf tierfüßen geht es auf krallen"
        },
        "line.2": {
            "text": "halb verborgen in federn in fell"
        },
        "line.3": {
            "text": "tappt es nachts um das haus"
        },
        "line.4": {
            "text": "schreit vor geschlossenen fenstern"
        },
        "line.5": {
            "text": "verstummt wenn du öffnest"
        },
        "line.6": {
            "text": "nur ein flügelschlag streift dich"
        },
        "line.7": {
            "text": "mit reisigbesen"
        },
        "line.8": {
            "text": "glätteten wir einmal"
        },
        "line.9": {
            "text": "abends den sand auf den wegen"
        },
        "line.10": {
            "text": "in dieser nacht blieb es aus"
        },
        "line.11": {
            "text": "& die stille"
        },
        "line.12": {
            "text": "verschlug uns den schlaf"
        }
    }
}