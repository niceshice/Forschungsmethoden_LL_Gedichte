{
    "title": "verwundert-548",
    "gender": "w",
    "author": "Rose Ausländer",
    "author_birth": "1901",
    "poem": {
        "line.1": {
            "text": "Wenn der Tisch nach Brot duftet"
        },
        "line.2": {
            "text": "Erdbeeren der Wein Kristall"
        },
        "line.3": {
            "text": "denkt an den Raum aus Rauch"
        },
        "line.4": {
            "text": "Rauch ohne Gestalt"
        },
        "line.5": {
            "text": "Noch nicht abgestreift"
        },
        "line.6": {
            "text": "das Ghettokleid"
        },
        "line.7": {
            "text": "sitzen wir um den duftenden Tisch"
        },
        "line.8": {
            "text": "verwundert"
        },
        "line.9": {
            "text": "daß wir hier sitzen"
        }
    }
}