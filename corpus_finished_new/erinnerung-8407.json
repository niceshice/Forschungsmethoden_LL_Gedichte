{
    "title": "erinnerung-8407",
    "gender": "w",
    "author": "Doris Runge",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "gut daß sie"
        },
        "line.2": {
            "text": "vergessen kennt"
        },
        "line.3": {
            "text": "und trennt"
        },
        "line.4": {
            "text": "was war und nicht"
        },
        "line.5": {
            "text": "mehr weiß"
        },
        "line.6": {
            "text": "was wahr"
        },
        "line.7": {
            "text": "und moose"
        },
        "line.8": {
            "text": "wachsen lässt"
        }
    }
}