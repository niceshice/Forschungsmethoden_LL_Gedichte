{
    "title": "aus-der-wirtschaft-12766",
    "gender": "m",
    "author": "F. W. Bernstein",
    "author_birth": "1938",
    "poem": {
        "line.1": {
            "text": "Der Täter tut,"
        },
        "line.2": {
            "text": "die Mutter muht,"
        },
        "line.3": {
            "text": "der Hüter hütet seinen Hut."
        },
        "line.4": {
            "text": "Was das bedeutet - wer weiß es? Ich!"
        },
        "line.5": {
            "text": "Die Zeichen für Konjunkturschwäche mehren sich."
        }
    }
}