{
    "title": "einer-kennt-alles-beim-namen-813",
    "gender": "m",
    "author": "Christian Loidl",
    "author_birth": "1957",
    "poem": {
        "line.1": {
            "text": "einer kennt alles beim namen."
        },
        "line.2": {
            "text": "wo der ist, ist alles still."
        },
        "line.3": {
            "text": "eine welt aus altem schlittenholz."
        },
        "line.4": {
            "text": "eine nacht schlafen, und"
        },
        "line.5": {
            "text": "das haus steht nicht mehr."
        },
        "line.6": {
            "text": "der himmel und die sterne sind"
        },
        "line.7": {
            "text": "ein tierfell, und auf dem fell"
        },
        "line.8": {
            "text": "liegst du, geboren"
        },
        "line.9": {
            "text": "zur sprache."
        }
    }
}