{
    "title": "ich-wuensche-dir-erinnerung-16239",
    "gender": "w",
    "author": "Cornelia  Hülmbauer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "ich wünsche dir"
        },
        "line.2": {
            "text": "Erinnerung"
        },
        "line.3": {
            "text": "u. damit"
        },
        "line.4": {
            "text": "mir"
        },
        "line.5": {
            "text": "ich"
        },
        "line.6": {
            "text": "\"schüttle\""
        },
        "line.7": {
            "text": "wach"
        },
        "line.8": {
            "text": "mich"
        },
        "line.9": {
            "text": "so wie ich dich"
        },
        "line.10": {
            "text": "vergesse"
        },
        "line.11": {
            "text": "jetzt schüttelst vielleicht du"
        },
        "line.12": {
            "text": "weißt du"
        },
        "line.13": {
            "text": "Wir"
        },
        "line.14": {
            "text": "wirst sehen"
        },
        "line.15": {
            "text": "nun lass"
        }
    }
}