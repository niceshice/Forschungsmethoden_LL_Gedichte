    "poem": {
        "line.1": {
            "text": "Those are pearls that were his eyes\""
        },
        "line.2": {
            "text": "kyrie-heh ..."
        },
        "line.3": {
            "text": "eintreten Hirnen. Eins-"
        },
        "line.4": {
            "text": "zweidrei"
        },
        "line.5": {
            "text": "Liegen in der Mitten\" :"
        },
        "line.6": {
            "text": "Lampenschein und Haut"
        },
        "line.7": {
            "text": "draufschreiben : Oh"
        },
        "line.8": {
            "text": "Staunen"
        },
        "line.9": {
            "text": ": einer hängt! Schlagen-"
        },
        "line.10": {
            "text": "drauf, Schlager-le"
        },
        "line.11": {
            "text": "'emblem-, -atik; ticken :"
        },
        "line.12": {
            "text": "-tockte,"
        },
        "line.13": {
            "text": "-pannte :"
        },
        "line.14": {
            "text": "s,s,"
        },
        "line.15": {
            "text": ": cri"
        }
    }
}