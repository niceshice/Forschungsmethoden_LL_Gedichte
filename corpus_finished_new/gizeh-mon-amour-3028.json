{
    "title": "gizeh-mon-amour-3028",
    "gender": "m",
    "author": "Ron Winkler",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "für Christiane Wohlrab"
        },
        "line.2": {
            "text": "die Pyramiden scheinen"
        },
        "line.3": {
            "text": "einem Lehrbuch über Ästhetik entnommen."
        },
        "line.4": {
            "text": "fliegende Händler bieten Begriffe"
        },
        "line.5": {
            "text": "zur Bewältigung ihrer Schönheit an."
        },
        "line.6": {
            "text": "die Hieroglyphe für Stoizismus"
        },
        "line.7": {
            "text": "muss ein Kamel sein."
        },
        "line.8": {
            "text": "zwangsläufig erliegt man nach kurzer Zeit"
        },
        "line.9": {
            "text": "der Heliopunktur der Sonne."
        },
        "line.10": {
            "text": "für andere alte Götter ist Entree"
        },
        "line.11": {
            "text": "zu entrichten. die Sphinx"
        },
        "line.12": {
            "text": "vermittelt möglicherweise"
        },
        "line.13": {
            "text": "ein ganz falsches Bild."
        }
    }
}