{
    "title": "wie-denn-auch-14657",
    "gender": "w",
    "author": "Lisa Elsässer",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "wo immer noch die kälte"
        },
        "line.2": {
            "text": "in den bleichen tannen wohnt"
        },
        "line.3": {
            "text": "abschied nehmen"
        },
        "line.4": {
            "text": "ein langer langer winter"
        },
        "line.5": {
            "text": "berge voller schnee"
        },
        "line.6": {
            "text": "keine keine vorgestapfte"
        },
        "line.7": {
            "text": "spur im weiten feld"
        },
        "line.8": {
            "text": "wie gehen"
        },
        "line.9": {
            "text": "mit dem frostvernagten herz"
        },
        "line.10": {
            "text": "dem vereisten harz im kopf"
        }
    }
}