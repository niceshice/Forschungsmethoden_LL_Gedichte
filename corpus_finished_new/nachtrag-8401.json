{
    "title": "nachtrag-8401",
    "gender": "w",
    "author": "Doris Runge",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "dieser handlauf z. b."
        },
        "line.2": {
            "text": "weiß nichts"
        },
        "line.3": {
            "text": "von schwung und abfedern"
        },
        "line.4": {
            "text": "und wie man"
        },
        "line.5": {
            "text": "auf die füße kam"
        },
        "line.6": {
            "text": "wieder und wieder"
        },
        "line.7": {
            "text": "die stufen"
        },
        "line.8": {
            "text": "nichts"
        },
        "line.9": {
            "text": "vom auf und ab"
        },
        "line.10": {
            "text": "nicht einmal die fundstücke"
        },
        "line.11": {
            "text": "erinnern sich"
        },
        "line.12": {
            "text": "dieser hut vergaß"
        },
        "line.13": {
            "text": "den jäger der gamsbart"
        },
        "line.14": {
            "text": "die gams"
        },
        "line.15": {
            "text": "der mantel"
        },
        "line.16": {
            "text": "den träger"
        },
        "line.17": {
            "text": "der haken weiß nichts"
        },
        "line.18": {
            "text": "vom erhängten"
        },
        "line.19": {
            "text": "im gerätehaus"
        },
        "line.20": {
            "text": "finde ich spaten"
        },
        "line.21": {
            "text": "und besen"
        }
    }
}