{
    "title": "vor-dem-verschwinden-loben-4304",
    "gender": "w",
    "author": "Andrea Heuser",
    "author_birth": "1971",
    "poem": {
        "line.1": {
            "text": "LOBEN und gelobt sein werden"
        },
        "line.2": {
            "text": "die löwenzahnwiesen, der flieder"
        },
        "line.3": {
            "text": "baum, ginster, gleise, der rotdorn"
        },
        "line.4": {
            "text": "brombeeren und birken, buchen"
        },
        "line.5": {
            "text": "bucheckern und farn, die gartenschaukel"
        },
        "line.6": {
            "text": "die hintertür, gras, das gras, das"
        },
        "line.7": {
            "text": "und altweiberfäden spinnen die kindheit ein"
        },
        "line.8": {
            "text": "das fahrrad, rostig jetzt bienensommer, wetterstille, das"
        },
        "line.9": {
            "text": "was allen gehört, gehörte, was"
        },
        "line.10": {
            "text": "weiter, tiefer wächst"
        }
    }
}