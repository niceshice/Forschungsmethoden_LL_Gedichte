{
    "title": "entgegen-zu-stehen-ii-br-vernehmen-zerfurchen-1347",
    "gender": "w",
    "author": "Anja Utler",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "nur an den schürfstellen: ausgelöst"
        },
        "line.2": {
            "text": "stehn, spüren, drifte schon: schlingre dir zu"
        },
        "line.3": {
            "text": "durchs: zerklüftete höre  du sprichst mir vom"
        },
        "line.4": {
            "text": "abraum, den halden vom: aufpflanzen,"
        },
        "line.5": {
            "text": "windrose, -rad vom: rotieren sprichst, glänzendes"
        },
        "line.6": {
            "text": "rotorblatt sagst du  es: schlingert, ja, schlingre, wie"
        },
        "line.7": {
            "text": "arm, flügel pflügt: sich den rücken, ja, furcht"
        },
        "line.8": {
            "text": "durch die erdrauch- die taubnesselfelder"
        },
        "line.9": {
            "text": "und stumpft: erst am schulterblatt ab"
        }
    }
}