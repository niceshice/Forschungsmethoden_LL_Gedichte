{
    "title": "mosaikstein-11189",
    "gender": "m",
    "author": "Tobias Roth",
    "author_birth": "1985",
    "poem": {
        "line.1": {
            "text": "occasione | disinvoltura"
        },
        "line.2": {
            "text": "Wir versuchen, die Welt"
        },
        "line.3": {
            "text": "Zu streicheln mit den Lemniskaten unserer Tage,"
        },
        "line.4": {
            "text": "Und manchmal gelingt es."
        },
        "line.5": {
            "text": "Manchmal kommen wir gleichzeitig. Manchmal"
        },
        "line.6": {
            "text": "Sind die Münder nicht von Tulpen und Tragopanen zu unterscheiden."
        },
        "line.7": {
            "text": "Blumen in den Gärten und abwegige Vögel."
        },
        "line.8": {
            "text": "Tiefer Flieder, Pfauen, Lächeln."
        },
        "line.9": {
            "text": "Alles bleibt in Umlaufbahnen."
        },
        "line.10": {
            "text": "Nichts gilt mir unmittelbar."
        }
    }
}