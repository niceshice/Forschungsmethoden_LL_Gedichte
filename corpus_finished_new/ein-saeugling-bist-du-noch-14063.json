{
    "title": "ein-saeugling-bist-du-noch-14063",
    "gender": "w",
    "author": "Karin  Fellner",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "ein Säugling bist du noch"
        },
        "line.2": {
            "text": "immer verworfen von Winden"
        },
        "line.3": {
            "text": "nackte Nervenenden"
        },
        "line.4": {
            "text": "parasympathische Schlingen"
        },
        "line.5": {
            "text": "Opfer greller Fantasmen"
        },
        "line.6": {
            "text": "im täglichen Gedränge"
        },
        "line.7": {
            "text": "heller und dunkler Dinge"
        },
        "line.8": {
            "text": "ein Wirbel Gemengelage"
        },
        "line.9": {
            "text": "dann schlägt eine Vokabel"
        },
        "line.10": {
            "text": "auf in der Fontanelle"
        },
        "line.11": {
            "text": "realm alma"
        },
        "line.12": {
            "text": "verströmt"
        },
        "line.13": {
            "text": "Hochebene und Kühle"
        }
    }
}