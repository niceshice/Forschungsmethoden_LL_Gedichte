{
    "title": "wo-wir-wohnen-7831",
    "gender": "m",
    "author": "Reiner Kunze",
    "author_birth": "1933",
    "poem": {
        "line.1": {
            "text": "Für Felix, den enkel"
        },
        "line.2": {
            "text": "Dort wo am morgen der hahnenschrei"
        },
        "line.3": {
            "text": "die Autos im tal"
        },
        "line.4": {
            "text": "um ein winziges übertönt"
        },
        "line.5": {
            "text": "Um ein winziges"
        },
        "line.6": {
            "text": "Komm, dem hahn zu helfen"
        }
    }
}