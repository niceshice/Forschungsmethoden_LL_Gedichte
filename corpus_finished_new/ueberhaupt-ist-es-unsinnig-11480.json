{
    "title": "ueberhaupt-ist-es-unsinnig-11480",
    "gender": "m",
    "author": "Franz Dodel",
    "author_birth": "1949",
    "poem": {
        "line.1": {
            "text": "überhaupt ist es"
        },
        "line.2": {
            "text": "unsinnig sich auf festen"
        },
        "line.3": {
            "text": "Grund zu verlassen"
        },
        "line.4": {
            "text": "auf eine unbewegte"
        },
        "line.5": {
            "text": "Wirklichkeit die wer"
        },
        "line.6": {
            "text": "weiß wo sich festhält während"
        },
        "line.7": {
            "text": "alles in Aufruhr"
        },
        "line.8": {
            "text": "sich regt die Steine deren"
        },
        "line.9": {
            "text": "langsames Atmen"
        },
        "line.10": {
            "text": "sich nicht feststellen lässt im"
        },
        "line.11": {
            "text": "Gegensatz zu dem"
        },
        "line.12": {
            "text": "sirrenden Flügelschlagen"
        },
        "line.13": {
            "text": "dieses Insekts das"
        },
        "line.14": {
            "text": "vor einer Blüte schwebt schwankend"
        },
        "line.15": {
            "text": "irritiert vielleicht"
        },
        "line.16": {
            "text": "von der Kreiselbewegung"
        },
        "line.17": {
            "text": "der Erdachse und"
        },
        "line.18": {
            "text": "wohl auch von den Zuckungen"
        },
        "line.19": {
            "text": "des Lebens wenn es"
        },
        "line.20": {
            "text": "sich mit der Liebe abgibt"
        },
        "line.21": {
            "text": "um sein Beginnen"
        },
        "line.22": {
            "text": "fortzusetzen in einer"
        },
        "line.23": {
            "text": "Bewegung deren"
        },
        "line.24": {
            "text": "Scheitern absehbar ist doch"
        },
        "line.25": {
            "text": "nicht ohne dass sie"
        },
        "line.26": {
            "text": "sich auch noch beim Abnehmen"
        },
        "line.27": {
            "text": "auflädt mit einer"
        },
        "line.28": {
            "text": "dem Unverfügbaren schnell"
        },
        "line.29": {
            "text": "entrissenen Kraft"
        }
    }
}