    "author": "Kerstin Hensel",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "In meinem Schreibtisch finde ich"
        },
        "line.2": {
            "text": "Mehrere alte Brillen. Seit einundsechzig"
        },
        "line.3": {
            "text": "Mache ich Fortschritte"
        },
        "line.4": {
            "text": "In Kurzsichtigkeit. In einem anderen Fach"
        },
        "line.5": {
            "text": "Gilbe Programme nach denen"
        },
        "line.6": {
            "text": "Die Revolution stattfand"
        },
        "line.7": {
            "text": "Auf dem Theater."
        },
        "line.8": {
            "text": "Drei Brillen setz ich"
        },
        "line.9": {
            "text": "Ãœbereinander damit ich erkenne: mein Name"
        },
        "line.10": {
            "text": "Steht unter den Spielern."
        }
    }
}