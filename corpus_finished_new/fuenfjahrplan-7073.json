{
    "title": "fuenfjahrplan-7073",
    "gender": "m",
    "author": "Bęrt Elsmann-Papenfuß",
    "author_birth": "1956",
    "poem": {
        "line.1": {
            "text": "1. Angelernte"
        },
        "line.2": {
            "text": "Sans soleil der Einäugige"
        },
        "line.3": {
            "text": "am Born der Lubrikation."
        },
        "line.4": {
            "text": "Auf Schlupf folgt Schwall,"
        },
        "line.5": {
            "text": "das Ende allen Wassers ."
        },
        "line.6": {
            "text": "Die Wölfin der Gischt,"
        },
        "line.7": {
            "text": "als sie Grund gefaßt,"
        },
        "line.8": {
            "text": "trug den Dengler des Rohrs"
        },
        "line.9": {
            "text": "ins Dorthinaus des Moors."
        },
        "line.10": {
            "text": "Erst eingebildet, dann ausgebildet,"
        },
        "line.11": {
            "text": "lernen wir weiter und ernten Fagin."
        },
        "line.12": {
            "text": "Aber da sind noch Haken"
        },
        "line.13": {
            "text": "im feuchten Laken drin."
        },
        "line.14": {
            "text": "2. Lustration"
        },
        "line.15": {
            "text": "Das ungestreifte Rohr sich abzustreifen!"
        },
        "line.16": {
            "text": "Das volle Rohr! Die totale Lubrikation!"
        },
        "line.17": {
            "text": "Hintenrum, Lysistrata,"
        },
        "line.18": {
            "text": "stagniert die Tschistka ."
        },
        "line.19": {
            "text": "Lehre und Sache ficken einander,"
        },
        "line.20": {
            "text": "werden aneinander gerecht:"
        },
        "line.21": {
            "text": "Jaucht, gauckt und saugt,"
        },
        "line.22": {
            "text": "geußt, seufzt und sauft."
        },
        "line.23": {
            "text": "Nicht mehr heißt auch nicht weniger,"
        },
        "line.24": {
            "text": "die Lehre ist die Sache nicht, zuinnerst"
        },
        "line.25": {
            "text": "verheerend; womit der Schwanz"
        },
        "line.26": {
            "text": "selbst in den Schwanz sich bisse."
        },
        "line.27": {
            "text": "3. Brisingamen"
        },
        "line.28": {
            "text": "Niedergeschlagene Heerscharen,"
        },
        "line.29": {
            "text": "von verzweifelten Frauen getrennt,"
        },
        "line.30": {
            "text": "spreizen ihre Flügel"
        },
        "line.31": {
            "text": "unter dem Herbstlaub ,"
        },
        "line.32": {
            "text": "ritzen den Blutaar, nehmen sich wahr"
        },
        "line.33": {
            "text": "und Rache an sich selbst für nichts."
        },
        "line.34": {
            "text": "Steuern die erste Gondel"
        },
        "line.35": {
            "text": "in den Hauptgürtel rein."
        },
        "line.36": {
            "text": "Was ineinander fällt, bricht auf in die Welt,"
        },
        "line.37": {
            "text": "in den Raum, die Zeit und die letzte Biege."
        },
        "line.38": {
            "text": "Mit Votzenschaum jeschmiert un Fickpisse"
        },
        "line.39": {
            "text": "jespült  watn Jefühl mitten in Jewühl."
        }
    }
}