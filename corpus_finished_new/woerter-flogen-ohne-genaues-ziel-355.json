{
    "title": "woerter-flogen-ohne-genaues-ziel-355",
    "gender": "m",
    "author": "Kurt  Aebli",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "W"
        },
        "line.2": {
            "text": "örter flogen ohne genaues Ziel"
        },
        "line.3": {
            "text": "durch die Gegend neurotische"
        },
        "line.4": {
            "text": "Tomaten sagte er jemand lässt"
        },
        "line.5": {
            "text": "mysteriöse Beinamen in die"
        },
        "line.6": {
            "text": "ansonsten nicht mehr benutzte"
        },
        "line.7": {
            "text": "Luft steigen hinzu kam viel"
        },
        "line.8": {
            "text": "Blut das unter Gelächter von"
        },
        "line.9": {
            "text": "uns Besitz nahm jetzt können"
        },
        "line.10": {
            "text": "Sie winken sagte er winken"
        },
        "line.11": {
            "text": "heißt zum Beispiel leben alles"
        },
        "line.12": {
            "text": "eine Frage des Alphabets"
        }
    }
}