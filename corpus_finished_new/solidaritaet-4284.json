{
    "title": "solidaritaet-4284",
    "gender": "w",
    "author": "Brigitte Struzyk",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Die Kopfweiden stehen immer noch"
        },
        "line.2": {
            "text": "So offen da"
        },
        "line.3": {
            "text": "Dort wo ich mich verkroch,"
        },
        "line.4": {
            "text": "das Loch ist immer noch"
        },
        "line.5": {
            "text": "so gut versteckt"
        },
        "line.6": {
            "text": "Die Hänge fallen immer noch"
        },
        "line.7": {
            "text": "So schusslig ab"
        },
        "line.8": {
            "text": "Die Apfelbäume meiner Kindheit"
        },
        "line.9": {
            "text": "Haben aufgehört zu wachsen"
        },
        "line.10": {
            "text": "Ich habe noch immer"
        },
        "line.11": {
            "text": "Steine und Gänseblümchen"
        },
        "line.12": {
            "text": "In meiner Manteltasche"
        }
    }
}