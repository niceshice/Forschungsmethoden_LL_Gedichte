{
    "title": "sonanz-16582",
    "gender": "m",
    "author": "Norbert Lange",
    "author_birth": "1978",
    "poem": {
        "line.1": {
            "text": "Die Funktion der Wellen"
        },
        "line.2": {
            "text": "zwischen den Blöcken im Sand,"
        },
        "line.3": {
            "text": "ein Festlandknebel, rote Grenze"
        },
        "line.4": {
            "text": "gezogen mit dem Zollstock,"
        },
        "line.5": {
            "text": "korrodiert. Die Lippen, dumpf"
        },
        "line.6": {
            "text": "aufeinandergepresst, reiben"
        },
        "line.7": {
            "text": "History, aus Boxen: Hysterie."
        },
        "line.8": {
            "text": "Noch Touristen sagen Beach,"
        },
        "line.9": {
            "text": "Störung konkurrierender"
        },
        "line.10": {
            "text": "Sender, Vibration im Bereich"
        },
        "line.11": {
            "text": "der Teilchen; oder Tunnelgräber, Helme"
        },
        "line.12": {
            "text": "auf, als würde angetreten"
        },
        "line.13": {
            "text": "gegen den Beton, den Winde"
        },
        "line.14": {
            "text": "verhören, Brandungen quälen."
        },
        "line.15": {
            "text": "Blechzungen-Equipment"
        },
        "line.16": {
            "text": "ohne Marke, schon gekrümmte"
        },
        "line.17": {
            "text": "Winkelkreuze, Stars & Stripes,"
        },
        "line.18": {
            "text": "die nach Wahrheit hinter Dünen"
        },
        "line.19": {
            "text": "stehen. Achsen, achso. Ersoffene"
        },
        "line.20": {
            "text": "Mündungen. Langwelle. Sprachrohre."
        },
        "line.21": {
            "text": "Ahnungslosigkeit."
        },
        "line.22": {
            "text": "KVA"
        }
    }
}