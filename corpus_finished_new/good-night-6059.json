{
    "title": "good-night-6059",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "und schnappen"
        },
        "line.2": {
            "text": "zu selbiger stunde da"
        },
        "line.3": {
            "text": "die flimmerkisten das kammerflimmern"
        },
        "line.4": {
            "text": "abzugleichen beginnen mimikrymi"
        },
        "line.5": {
            "text": "moraste mal wahrheit pro sekunde"
        },
        "line.6": {
            "text": "einfällt die felder die nacht bestellt"
        },
        "line.7": {
            "text": "mit blauen ins kraut schießenden"
        },
        "line.8": {
            "text": "bohnen ein bisschen"
        },
        "line.9": {
            "text": "luft"
        },
        "line.10": {
            "text": "vertreten"
        },
        "line.11": {
            "text": "die beine"
        },
        "line.12": {
            "text": "uns auf den stolperplatten inmitten"
        },
        "line.13": {
            "text": "von abschied und raps und die pappeln"
        },
        "line.14": {
            "text": "haben den besten sound"
        },
        "line.15": {
            "text": "und lauter allerleirauhe tiere auch reiher"
        },
        "line.16": {
            "text": "wie fuchs und hase natürlich"
        },
        "line.17": {
            "text": "gezückt die läufe zum scheuen"
        },
        "line.18": {
            "text": "sleep tight"
        }
    }
}