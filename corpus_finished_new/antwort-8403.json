{
    "title": "antwort-8403",
    "gender": "w",
    "author": "Doris Runge",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "aber ich breche dir"
        },
        "line.2": {
            "text": "wörter wie brot"
        },
        "line.3": {
            "text": "streue krumen"
        },
        "line.4": {
            "text": "auf weiße wege"
        },
        "line.5": {
            "text": "elstern folgen"
        },
        "line.6": {
            "text": "füchse nehmen"
        },
        "line.7": {
            "text": "witterung"
        },
        "line.8": {
            "text": "du aber fürchtest dich"
        },
        "line.9": {
            "text": "vor dem schnee"
        },
        "line.10": {
            "text": "vor der stille"
        },
        "line.11": {
            "text": "vor dem gläsernen"
        },
        "line.12": {
            "text": "gesicht"
        },
        "line.13": {
            "text": "vor dem augenblick"
        },
        "line.14": {
            "text": "wo der vogel"
        },
        "line.15": {
            "text": "seinen schatten erkennt"
        },
        "line.16": {
            "text": "die erde berührt"
        }
    }
}