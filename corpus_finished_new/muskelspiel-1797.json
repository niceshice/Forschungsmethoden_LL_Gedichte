{
    "title": "muskelspiel-1797",
    "gender": "w",
    "author": "Isabeella Beumer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "aufgeknöpfte lippen"
        },
        "line.2": {
            "text": "benachbarter blätter"
        },
        "line.3": {
            "text": "auch wenn der"
        },
        "line.4": {
            "text": "gassenfüszler fragloses sammelt"
        },
        "line.5": {
            "text": "farben reihen sich an schwarz"
        },
        "line.6": {
            "text": "bis in die steine"
        },
        "line.7": {
            "text": "getrocknetes harren"
        },
        "line.8": {
            "text": "in knospenlicht"
        },
        "line.9": {
            "text": "nur noch herzstaub gehungert"
        },
        "line.10": {
            "text": "schriftbläue"
        },
        "line.11": {
            "text": "gemäsz unter glas"
        },
        "line.12": {
            "text": "umlacht"
        },
        "line.13": {
            "text": "durch taschen getragen"
        },
        "line.14": {
            "text": "trinkt tanzend schatullen"
        },
        "line.15": {
            "text": "in tropfenden wind"
        },
        "line.16": {
            "text": "bis ins drehen"
        },
        "line.17": {
            "text": "angezupfte zungen"
        },
        "line.18": {
            "text": "wie verriebener halbschlaf"
        },
        "line.19": {
            "text": "luftkoloriertes der folge"
        },
        "line.20": {
            "text": "als geschmolzener schnee"
        },
        "line.21": {
            "text": "durch zäune gestreut"
        },
        "line.22": {
            "text": "ebenso lichtfäden"
        },
        "line.23": {
            "text": "in getauschte ufer"
        },
        "line.24": {
            "text": "gleichzeitige spuren"
        },
        "line.25": {
            "text": "einer flügeltür"
        },
        "line.26": {
            "text": "schallgeleckt"
        },
        "line.27": {
            "text": "als gebrauch von sommer"
        },
        "line.28": {
            "text": "kein dunkel"
        },
        "line.29": {
            "text": "entgegenfallender nacht"
        },
        "line.30": {
            "text": "auch wenn zwielicht flaschen"
        },
        "line.31": {
            "text": "durchschwemmt"
        },
        "line.32": {
            "text": "das stroh ist geflochten"
        },
        "line.33": {
            "text": "koordinaten von nischenhaut"
        },
        "line.34": {
            "text": "bis unter aufgeschlagene schatten"
        },
        "line.35": {
            "text": "schildkrötkristalle"
        },
        "line.36": {
            "text": "in der saat von weisz"
        },
        "line.37": {
            "text": "ziffernverhangen"
        },
        "line.38": {
            "text": "spannt sich klang in fäden"
        },
        "line.39": {
            "text": "blattlos gekämmt"
        },
        "line.40": {
            "text": "hängt schrei wie frühling am zweig"
        },
        "line.41": {
            "text": "ohne salztau"
        },
        "line.42": {
            "text": "im gebet kammerndes flirren"
        },
        "line.43": {
            "text": "nachtfalter schaben am tor"
        }
    }
}