{
    "title": "vogel-sequenza-12821",
    "gender": "m",
    "author": "Rainer René Mueller",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "Wie ein Vogel über der Ebene schreit"
        },
        "line.2": {
            "text": "schreit ein Vogel, wie"
        },
        "line.3": {
            "text": "er, er"
        },
        "line.4": {
            "text": "schrier, schrier"
        },
        "line.5": {
            "text": "(Bilder, gewest)"
        },
        "line.6": {
            "text": "es ist Schnee"
        },
        "line.7": {
            "text": "(-weiß)"
        },
        "line.8": {
            "text": "im Ohr. Es schneit"
        },
        "line.9": {
            "text": "die Stimme von weit."
        },
        "line.10": {
            "text": "Her und hin :"
        },
        "line.11": {
            "text": "Pendel : Auge  Ohr."
        },
        "line.12": {
            "text": "... lassen es zu."
        },
        "line.13": {
            "text": "Wie ein Vogel über der Ebene schreit"
        },
        "line.14": {
            "text": "schreit ein Vogel, wie"
        },
        "line.15": {
            "text": "er, er"
        },
        "line.16": {
            "text": "Ich erschlag dich"
        },
        "line.17": {
            "text": "wenn du schreist"
        },
        "line.18": {
            "text": "sagt mein Vater."
        },
        "line.19": {
            "text": "Ich schrei -"
        },
        "line.20": {
            "text": "-be, -be"
        },
        "line.21": {
            "text": "Bilder"
        },
        "line.22": {
            "text": "Schreit ein Vogel"
        },
        "line.23": {
            "text": "wie er : Liebstengruß"
        }
    }
}