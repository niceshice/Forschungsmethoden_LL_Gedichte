{
    "title": "im-theater-der-wien-11479",
    "gender": "m",
    "author": "Franz Dodel",
    "author_birth": "1949",
    "poem": {
        "line.1": {
            "text": "im Theater an der Wien"
        },
        "line.2": {
            "text": "über der Loge"
        },
        "line.3": {
            "text": "oben im zweiten Rang steht"
        },
        "line.4": {
            "text": "die Warnung: Vorsicht"
        },
        "line.5": {
            "text": "geringe Durchgangshöhe"
        },
        "line.6": {
            "text": "hier im Halbdunkel"
        },
        "line.7": {
            "text": "erinnert man uns an die"
        },
        "line.8": {
            "text": "Unbeholfenheit"
        },
        "line.9": {
            "text": "unseres aufrechten Gangs"
        },
        "line.10": {
            "text": "an den Wagemut"
        },
        "line.11": {
            "text": "des Aufrichtens überhaupt"
        },
        "line.12": {
            "text": "der jedoch nur bei"
        },
        "line.13": {
            "text": "Gräsern manchmal zu einer"
        },
        "line.14": {
            "text": "Vollkommenheit führt"
        },
        "line.15": {
            "text": "sich wiegender Eleganz"
        },
        "line.16": {
            "text": "uns unerreichbar"
        },
        "line.17": {
            "text": "wobei sie sich nicht scheuen"
        },
        "line.18": {
            "text": "ihre Bangigkeit"
        },
        "line.19": {
            "text": "vielleicht auch ihre Freude"
        },
        "line.20": {
            "text": "zu zeigen wie das"
        },
        "line.21": {
            "text": "Zittergras dessen Ährchen"
        },
        "line.22": {
            "text": "wie Tropfen an den"
        },
        "line.23": {
            "text": "Rispen hängen und das man"
        },
        "line.24": {
            "text": "an manchen Orten"
        },
        "line.25": {
            "text": "Muttergottestränen nennt"
        },
        "line.26": {
            "text": "es scheint fast als ob"
        },
        "line.27": {
            "text": "diese zitternden Blüten"
        },
        "line.28": {
            "text": "nicht mehr zur Ordnung"
        },
        "line.29": {
            "text": "gehörten der Zeit doch selbst"
        },
        "line.30": {
            "text": "aus reinster Zeit ein"
        },
        "line.31": {
            "text": "kleines Quantum enthielten"
        }
    }
}