{
    "title": "auf-dem-troedelmarkt-2638",
    "gender": "w",
    "author": "Kerstin Hensel",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "AUF DEM TRÖDELMARKT"
        },
        "line.2": {
            "text": "Bietet einer die Liebe an"
        },
        "line.3": {
            "text": "Für kein Geld und keinen Betrug. Die Leute"
        },
        "line.4": {
            "text": "Gehen vorbei. Mein Geliebter"
        },
        "line.5": {
            "text": "Am Nebenstand"
        },
        "line.6": {
            "text": "Kauft einen goldenen Ring."
        }
    }
}