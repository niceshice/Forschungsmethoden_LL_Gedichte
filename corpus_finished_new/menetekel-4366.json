{
    "title": "menetekel-4366",
    "gender": "m",
    "author": "Christian Schloyer",
    "author_birth": "1976",
    "poem": {
        "line.1": {
            "text": "das ist kein sanfter sphären"
        },
        "line.2": {
            "text": "sex  an einem sonntag,"
        },
        "line.3": {
            "text": "dein dreifruchtigkeits-"
        },
        "line.4": {
            "text": "reich komme"
        },
        "line.5": {
            "text": "unser faltiges frucht"
        },
        "line.6": {
            "text": "fleisch  trägst du alltäglich"
        },
        "line.7": {
            "text": "wie heute  dein nachtwächter"
        },
        "line.8": {
            "text": "lächeln auf"
        },
        "line.9": {
            "text": "rouge  wenn ich die lider schließe"
        },
        "line.10": {
            "text": "spielst du"
        },
        "line.11": {
            "text": "wie sonnen in glasfronten  quillt wolken"
        },
        "line.12": {
            "text": "glühen  von fenstern + mauern herab musen"
        },
        "line.13": {
            "text": "kussverschleiert du"
        },
        "line.14": {
            "text": "feenmärchen  fülle mich aus wenn du kannst"
        },
        "line.15": {
            "text": "nostalgie  das weltinterieur in silben + lauten"
        },
        "line.16": {
            "text": "gleicht einer rotte knallbunter raben ein platzregen"
        },
        "line.17": {
            "text": "über mir"
        },
        "line.18": {
            "text": "korah korah"
        },
        "line.19": {
            "text": "!"
        },
        "line.20": {
            "text": "wer verscherbelt das tafelsilben-"
        },
        "line.21": {
            "text": "+ gummigefieder"
        }
    }
}