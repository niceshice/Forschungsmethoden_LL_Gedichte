{
    "title": "wo-ich-doch-stets-da-war-sagte-352",
    "gender": "m",
    "author": "Kurt  Aebli",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "W"
        },
        "line.2": {
            "text": "o ich doch stets da war sagte"
        },
        "line.3": {
            "text": "er in meinem schwarzgekleideten"
        },
        "line.4": {
            "text": "Englisch Satzende das sogleich"
        },
        "line.5": {
            "text": "zerbröselte und auf etwas hindeutete"
        },
        "line.6": {
            "text": "das aussah wie ein Schlüsselbrett"
        },
        "line.7": {
            "text": "an dem fast alle Stunden des Tages"
        },
        "line.8": {
            "text": "hingen"
        }
    }
}