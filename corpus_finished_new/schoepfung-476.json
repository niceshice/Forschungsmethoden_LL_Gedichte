    "poem": {
        "line.1": {
            "text": "Am Anfang war die Nacht"
        },
        "line.2": {
            "text": "Heimlich wuchs Licht"
        },
        "line.3": {
            "text": "Und Tag war"
        },
        "line.4": {
            "text": "bis zur Nacht"
        },
        "line.5": {
            "text": "Dann wieder Tag"
        },
        "line.6": {
            "text": "Nacht"
        },
        "line.7": {
            "text": "Zwei Stehaufm√§nnchen"
        },
        "line.8": {
            "text": "bezwingen einander im Wechsel"
        }
