{
    "title": "liebe-13568",
    "gender": "w",
    "author": "Mara-Daria Cojocaru",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "Liebe"
        },
        "line.2": {
            "text": "hero shrew"
        },
        "line.3": {
            "text": ", ich wäre gern so"
        },
        "line.4": {
            "text": "Stark wie du. Nach jeder Katastrophe"
        },
        "line.5": {
            "text": "Schmerzt nicht nur mein Rückgrat"
        },
        "line.6": {
            "text": "Mich, auch bleib ich immer immer"
        },
        "line.7": {
            "text": "Ich hätt so gern dein Herz"
        },
        "line.8": {
            "text": "Zu springen ins punktierte."
        },
        "line.9": {
            "text": "Gleichgewicht"
        }
    }
}