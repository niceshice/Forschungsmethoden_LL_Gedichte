{
    "title": "braunschweig-hinter-uns-4288",
    "gender": "w",
    "author": "Brigitte Struzyk",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Baukörper an Baukörper"
        },
        "line.2": {
            "text": "Nicht ein Haus dazwischen"
        },
        "line.3": {
            "text": "In Stammelbach"
        },
        "line.4": {
            "text": "Kurz gehalten"
        },
        "line.5": {
            "text": "Über Handy"
        },
        "line.6": {
            "text": "Wo steckst du"
        },
        "line.7": {
            "text": "Wann kommst du"
        },
        "line.8": {
            "text": "Wie Tränen ins Tal"
        },
        "line.9": {
            "text": "Noch jemand zugestiegen?"
        },
        "line.10": {
            "text": "Vite-vite! zischt ein Messer"
        },
        "line.11": {
            "text": "In die Kurve"
        },
        "line.12": {
            "text": "In den Tunnel"
        },
        "line.13": {
            "text": "Über Handy"
        },
        "line.14": {
            "text": "Komm doch einfach"
        },
        "line.15": {
            "text": "(Vorbei"
        },
        "line.16": {
            "text": "bleibt im Netz"
        },
        "line.17": {
            "text": "Wo bei"
        },
        "line.18": {
            "text": "bye bye"
        },
        "line.19": {
            "text": "ausblieb)"
        }
    }
}