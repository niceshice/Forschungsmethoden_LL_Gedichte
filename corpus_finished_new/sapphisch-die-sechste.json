{
    "title": "sapphisch-die-sechste",
    "gender": "m",
    "author": "Urs Allemann",
    "author_birth": "1948",
    "poem": {
        "line.1": {
            "text": "Unsre Lippen fremd ineinander rollen"
        },
        "line.2": {
            "text": "früh Gedächtnistöter in Zauberschläuchen"
        },
        "line.3": {
            "text": "Steine Brutmaschinen ins Leere schlagen"
        },
        "line.4": {
            "text": "rosige Brüller"
        },
        "line.5": {
            "text": "wären Hölzer schraubten die Euterteile"
        },
        "line.6": {
            "text": "um das Rindenfenster im Stamm des Bolzen-"
        },
        "line.7": {
            "text": "spürgeräts betörender Rauch der leuzer-"
        },
        "line.8": {
            "text": "reissenden Buhlen"
        },
        "line.9": {
            "text": "Gliederschwere schlafender Boote Küste"
        },
        "line.10": {
            "text": "Mädchen Böcke fauchende Mäuler keimten"
        },
        "line.11": {
            "text": "Schlummerfisteln ferne der Wanderscholle"
        },
        "line.12": {
            "text": "lyrisches Ätzen"
        }
    }
}