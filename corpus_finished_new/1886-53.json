{
    "title": "1886-53",
    "gender": "m",
    "author": "Gottfried Benn",
    "author_birth": "1886",
    "poem": {
        "line.1": {
            "text": "Ostern am spätesten Termin,"
        },
        "line.2": {
            "text": "an der Elbe blühte schon der Flieder,"
        },
        "line.3": {
            "text": "dafür Anfang Dezember ein so unerhörter Schneefall,"
        },
        "line.4": {
            "text": "dass der gesamte Bahnverkehr"
        },
        "line.5": {
            "text": "in Nord- und Mitteldeutschland"
        },
        "line.6": {
            "text": "für Wochen zum Erliegen kam."
        },
        "line.7": {
            "text": "Paul Heyse veröffentlicht eine einaktige Tragödie,"
        },
        "line.8": {
            "text": "Es ist Hochzeitsabend, die junge Frau entdeckt,"
        },
        "line.9": {
            "text": "dass ihr Mann einmal ihre Mutter geliebt hat,"
        },
        "line.10": {
            "text": "alle längst tot, immerhin"
        },
        "line.11": {
            "text": "von ihrer Tante, die Mutterstelle vertrat,"
        },
        "line.12": {
            "text": "hat sie ein Morphiumfläschchen:"
        },
        "line.13": {
            "text": "störe das sanfte Mittel nicht,"
        },
        "line.14": {
            "text": "sie sinkt zurück, hascht nach seiner Hand,"
        },
        "line.15": {
            "text": "Theodor (düster, aufschreiend):"
        },
        "line.16": {
            "text": "Lydia! Mein Weib! Nimm mich mit Dir!"
        },
        "line.17": {
            "text": "Titel: Zwischen Lipp und Kelchesrand."
        },
        "line.18": {
            "text": "England erobert Mandelai,"
        },
        "line.19": {
            "text": "eröffnet das weite Tal des Irawaday dem Welthandel;"
        },
        "line.20": {
            "text": "Madagaskar kommt an Frankreich;"
        },
        "line.21": {
            "text": "Russland vertreibt den Fürsten Alexander"
        },
        "line.22": {
            "text": "aus Bulgarien."
        },
        "line.23": {
            "text": "Der Deutsche Radfahrbund"
        },
        "line.24": {
            "text": "zählt 1500 Mitglieder."
        },
        "line.25": {
            "text": "Güssfeld besteigt zum ersten Mal"
        },
        "line.26": {
            "text": "den Montblanc"
        },
        "line.27": {
            "text": "über den Grand Mulet."
        },
        "line.28": {
            "text": "Die Barsois aus dem Perchinozwinger"
        },
        "line.29": {
            "text": "im Gouvernement Tula,"
        },
        "line.30": {
            "text": "die mit der besonders tiefbefahnten Brust,"
        },
        "line.31": {
            "text": "die Wolfsjäger,"
        },
        "line.32": {
            "text": "erscheinen auf der Berliner Hundeausstellung,"
        },
        "line.33": {
            "text": "Asmodey erhält die Goldene Medaille."
        },
        "line.34": {
            "text": "Die Registertonne wird einheitlich"
        },
        "line.35": {
            "text": "auf 2,8 cbm Raumgehalt festgesetzt;"
        },
        "line.36": {
            "text": "Übergang des Raddampfers zum Schraubendampfer;"
        },
        "line.37": {
            "text": "Rückgang der Holzschiffe;"
        },
        "line.38": {
            "text": "über das chinesische Kauffahrteiwesen"
        },
        "line.39": {
            "text": "ist statistisch nichts bekannt;"
        },
        "line.40": {
            "text": "Norddeutscher Lloyd: 38 Schiffe, 63000 t,"
        },
        "line.41": {
            "text": "Hamburg  Amerika: 19 Schiffe, 34200 t,"
        },
        "line.42": {
            "text": "Hamburg  Süd: 9 Schiffe, 13500 t."
        },
        "line.43": {
            "text": "Turgenjew in Baden-Baden"
        },
        "line.44": {
            "text": "besucht täglich die Schwestern Viardot,"
        },
        "line.45": {
            "text": "unvergessliche Abende,"
        },
        "line.46": {
            "text": "sein Lieblingslied, das selten gehörte:"
        },
        "line.47": {
            "text": "wenn meine Grillen schwirren"
        },
        "line.48": {
            "text": "(Schubert),"
        },
        "line.49": {
            "text": "oft auch lesen sie Scheffels Ekkehard."
        },
        "line.50": {
            "text": "Es werden entdeckt:"
        },
        "line.51": {
            "text": "der flügellose Vogel Kiwi-kiwi in Neuseeland,"
        },
        "line.52": {
            "text": "der augenlose Molch in der Krainer Tropfsteinklamm,"
        },
        "line.53": {
            "text": "ein blinder Fisch in der Mammuthhöhle von Kentucky."
        },
        "line.54": {
            "text": "Beobachtet werden:"
        },
        "line.55": {
            "text": "Schwinden des Haarkleides (Wale, Delphine),"
        },
        "line.56": {
            "text": "Weisslichwerden der Haut (Schnecken, Köcherfliegen),"
        },
        "line.57": {
            "text": "Panzerrückbildung (Krebse, Insekten)"
        },
        "line.58": {
            "text": "Entwicklungsfragen,"
        },
        "line.59": {
            "text": "Befruchtungsstudien,"
        },
        "line.60": {
            "text": "Naturgeheimnis,"
        },
        "line.61": {
            "text": "nachgestammelt."
        },
        "line.62": {
            "text": "Kampf gegen Fremdwörter,"
        },
        "line.63": {
            "text": "Luna, Cephir, Chrysalide,"
        },
        "line.64": {
            "text": "1088 Wörter aus dem Faust"
        },
        "line.65": {
            "text": "sollen verdeutscht werden."
        },
        "line.66": {
            "text": "Agitation der Handlungsgehilfen"
        },
        "line.67": {
            "text": "für Schliessung der Geschäfte an den"
        },
        "line.68": {
            "text": "Sonntagnachmittagen,"
        },
        "line.69": {
            "text": "sozialdemokratische Stimmen"
        },
        "line.70": {
            "text": "bei der Wahl in Berlin: 68535."
        },
        "line.71": {
            "text": "Das Tiergartenviertel ist freisinnig,"
        },
        "line.72": {
            "text": "Singer hält seine erste"
        },
        "line.73": {
            "text": "Kandidatenrede."
        },
        "line.74": {
            "text": "13. Auflage von Brockhaus"
        },
        "line.75": {
            "text": "Konversationslexikon."
        },
        "line.76": {
            "text": "Die Zeitungen beklagen die Aufführung"
        },
        "line.77": {
            "text": "von Tolstois Macht der Finsternis,"
        },
        "line.78": {
            "text": "dagegen ist Blumenthals Ein Tropfen Gift"
        },
        "line.79": {
            "text": "eines langen Nachklangs von Wohllaut sicher;"
        },
        "line.80": {
            "text": "Über dem Haupt des Grafen Albrecht Vahlberg,"
        },
        "line.81": {
            "text": "der eine geachtete Stellung in der hauptstädtischen"
        },
        "line.82": {
            "text": "Gesellschaft"
        },
        "line.83": {
            "text": "einnimmt,"
        },
        "line.84": {
            "text": "schwebt eine dunkle Wolke,"
        },
        "line.85": {
            "text": "Zola, Ibsen, Hauptmann sind unerfreulich,"
        },
        "line.86": {
            "text": "Salambo verfehlt,"
        },
        "line.87": {
            "text": "Liszt Kosmopolit,"
        },
        "line.88": {
            "text": "und nun kommt die Rubrik"
        },
        "line.89": {
            "text": "Der Leser hat das Wort,"
        },
        "line.90": {
            "text": "er will etwas wissen"
        },
        "line.91": {
            "text": "über Wadenkrämpfe"
        },
        "line.92": {
            "text": "und Fremdkörperentfernung."
        },
        "line.93": {
            "text": "Es taucht auf:"
        },
        "line.94": {
            "text": "Pithekanthropos,"
        },
        "line.95": {
            "text": "Javarudimente, -"
        },
        "line.96": {
            "text": "die Vorstufen."
        },
        "line.97": {
            "text": "Es stirbt aus:"
        },
        "line.98": {
            "text": "der kleine Vogel von Hawai"
        },
        "line.99": {
            "text": "für die königlichen Federmäntel:"
        },
        "line.100": {
            "text": "ein gelber Flaumstreif an jedem Flügel,"
        },
        "line.101": {
            "text": "genannt der Honigsauger."
        },
        "line.102": {
            "text": "1886"
        },
        "line.103": {
            "text": "Geburtsjahr gewisser Expressionisten,"
        },
        "line.104": {
            "text": "ferner von Staatsrat Furtwängler,"
        },
        "line.105": {
            "text": "Emigrant Kokoschka,"
        },
        "line.106": {
            "text": "Generalfeldmarschall v. W. (),"
        },
        "line.107": {
            "text": "Kapitalverdoppelung"
        },
        "line.108": {
            "text": "bei Schneider-Creuzot, Krupp-Stahl, Putiloff."
        }
    }
}