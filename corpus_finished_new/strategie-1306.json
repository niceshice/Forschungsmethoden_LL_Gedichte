{
    "title": "strategie-1306",
    "gender": "m",
    "author": "Klaus Merz",
    "author_birth": "1945",
    "poem": {
        "line.1": {
            "text": "Das Tapetenmuster"
        },
        "line.2": {
            "text": "des Traums, Blattwerk"
        },
        "line.3": {
            "text": "lanzettlich. Und dann"
        },
        "line.4": {
            "text": "der erneute Versuch"
        },
        "line.5": {
            "text": "den Tag mit Gelächter"
        },
        "line.6": {
            "text": "zu bezwingen. Oder"
        },
        "line.7": {
            "text": "sitzend auf den Hinterläufen"
        },
        "line.8": {
            "text": "die Nacht abwarten."
        },
        "line.9": {
            "text": "Wie die Hasen."
        }
    }
}