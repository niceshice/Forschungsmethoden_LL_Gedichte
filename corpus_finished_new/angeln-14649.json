{
    "title": "angeln-14649",
    "gender": "w",
    "author": "Lisa Elsässer",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "es ist nicht mehr weit"
        },
        "line.2": {
            "text": "bis zum teich ungefähr"
        },
        "line.3": {
            "text": "eine durchwachte nacht"
        },
        "line.4": {
            "text": "mit flöhen und anderm ungeziefer"
        },
        "line.5": {
            "text": "in begleitung stummer fische"
        },
        "line.6": {
            "text": "tauchst du in den trüben grund"
        },
        "line.7": {
            "text": "und alles alles wirfst du ihnen"
        },
        "line.8": {
            "text": "in den offenen runden mund"
        }
    }
}