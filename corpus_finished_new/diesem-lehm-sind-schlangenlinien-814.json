{
    "title": "diesem-lehm-sind-schlangenlinien-814",
    "gender": "m",
    "author": "Christian Loidl",
    "author_birth": "1957",
    "poem": {
        "line.1": {
            "text": "in diesem lehm sind schlangenlinien"
        },
        "line.2": {
            "text": "schlangenlinien die du kennst und"
        },
        "line.3": {
            "text": "schlangenlinien die du nicht kennst"
        },
        "line.4": {
            "text": "die schlange selbst verschwindet in ihr muster"
        },
        "line.5": {
            "text": "das muster verschwindet im lehm"
        }
    }
}