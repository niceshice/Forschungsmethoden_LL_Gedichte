{
    "title": "schwarze-kirschen-13891",
    "gender": "w",
    "author": "Kerstin Becker",
    "author_birth": "1969",
    "poem": {
        "line.1": {
            "text": "wir hocken in der alten Krone"
        },
        "line.2": {
            "text": "hinter der Fabrik"
        },
        "line.3": {
            "text": "unsere Bäuche ackern sich"
        },
        "line.4": {
            "text": "an rußbezognen Kirschen ab"
        },
        "line.5": {
            "text": "die Lippen kleistern wir"
        },
        "line.6": {
            "text": "spucken Kerne unsichtbar"
        },
        "line.7": {
            "text": "und unten:"
        },
        "line.8": {
            "text": "Laster Rampen"
        },
        "line.9": {
            "text": "Männer schleppen"
        },
        "line.10": {
            "text": "Flaschen fallen"
        },
        "line.11": {
            "text": "Kästen klirren"
        },
        "line.12": {
            "text": "Scherben schimmern"
        },
        "line.13": {
            "text": "zwischen Lachen"
        },
        "line.14": {
            "text": "Frauen kommen"
        },
        "line.15": {
            "text": "aus der Käserei"
        },
        "line.16": {
            "text": "sie knöpfen"
        },
        "line.17": {
            "text": "ihre Kittel auf"
        },
        "line.18": {
            "text": "sie ziehen übern Hof"
        },
        "line.19": {
            "text": "die Brüste blank"
        },
        "line.20": {
            "text": "und alles johlt"
        },
        "line.21": {
            "text": "wir sehn die Würmer an"
        },
        "line.22": {
            "text": "bevor wir sie mitsamt verzehrn"
        }
    }
}