        "line.2": {
            "text": "Hast du oxidiert"
        },
        "line.3": {
            "text": "Neben mir"
        },
        "line.4": {
            "text": "Hast auf mich reagiert"
        },
        "line.5": {
            "text": "Bist rostig geworden"
        },
        "line.6": {
            "text": "Du sagst"
        },
        "line.7": {
            "text": "Golden"
        },
        "line.8": {
            "text": "Ich lecke an deinem Hals"
        },
        "line.9": {
            "text": "Du schmeckst wie der"
        },
        "line.10": {
            "text": "Wetterhahn"
        }
