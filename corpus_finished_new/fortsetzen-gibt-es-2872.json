{
    "title": "fortsetzen-gibt-es-2872",
    "gender": "w",
    "author": "Elisabeth  Wandeler-Deck",
    "author_birth": "1939",
    "poem": {
        "line.1": {
            "text": "fortsetzen faulen Zauber gibt es es"
        },
        "line.2": {
            "text": "gibt es gibt und zerperlt was da alles"
        },
        "line.3": {
            "text": "sich perlt zerpellt pelzt"
        },
        "line.4": {
            "text": "faulende Traufe schimmern dringen"
        },
        "line.5": {
            "text": "es gibt es gibt sprechen bitte ja doch"
        },
        "line.6": {
            "text": "na nu mit Netzen verhängte Abstürze"
        },
        "line.7": {
            "text": "Felswarzen blaue Felchen was da"
        },
        "line.8": {
            "text": "schimmert dringt prallt was da alles"
        },
        "line.9": {
            "text": "perlt surrt sirrt pocht"
        },
        "line.10": {
            "text": "Artikulation kleinteiliges Flektieren"
        },
        "line.11": {
            "text": "gibt es"
        }
    }
}