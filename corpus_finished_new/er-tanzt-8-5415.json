{
    "title": "er-tanzt-8-5415",
    "gender": "m",
    "author": "Hans Raimund",
    "author_birth": "1945",
    "poem": {
        "line.1": {
            "text": "DREHENKREISELNWIRBELN"
        },
        "line.2": {
            "text": "Das ist der Verlust"
        },
        "line.3": {
            "text": "Jeglichen Zusammenhangs"
        },
        "line.4": {
            "text": "und die Hügel tanzen"
        },
        "line.5": {
            "text": "Text ist"
        },
        "line.6": {
            "text": "Tanz ist"
        },
        "line.7": {
            "text": "Bewegung ist"
        },
        "line.8": {
            "text": "Musik ist"
        },
        "line.9": {
            "text": "Äußerung des Innerstenist"
        },
        "line.10": {
            "text": "Spiel"
        },
        "line.11": {
            "text": "und der Liebende ist Schleier"
        },
        "line.12": {
            "text": "mit Elementen"
        },
        "line.13": {
            "text": "X-beliebiger Wirklichkeiten"
        },
        "line.14": {
            "text": "Silben DrehenWörter KreiselnSätze Wirbeln"
        },
        "line.15": {
            "text": "Schatten"
        },
        "line.16": {
            "text": "Würfe"
        },
        "line.17": {
            "text": "Grenzen"
        },
        "line.18": {
            "text": "Lose"
        },
        "line.19": {
            "text": "gleich der Flöte die in Melodien ausbricht"
        }
    }
}