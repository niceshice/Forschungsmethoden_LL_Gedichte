{
    "title": "so-ist-das-i-16353",
    "gender": "w",
    "author": "Andrea Grill",
    "author_birth": "1975",
    "poem": {
        "line.1": {
            "text": "I."
        },
        "line.2": {
            "text": "muss die Zeit in Stücke geschnitten"
        },
        "line.3": {
            "text": "mit Butter bestrichen"
        },
        "line.4": {
            "text": "essen"
        },
        "line.5": {
            "text": "verlasse mich auf deinen Körper"
        },
        "line.6": {
            "text": "wo ich nur den Stoff"
        },
        "line.7": {
            "text": "kenne glatt sauber genäht"
        },
        "line.8": {
            "text": "lasse Arme an allen"
        },
        "line.9": {
            "text": "Ecken und Enden"
        },
        "line.10": {
            "text": "zurecht gestutzter Pflanzen"
        },
        "line.11": {
            "text": "(wachsen"
        },
        "line.12": {
            "text": "Reste von dir"
        },
        "line.13": {
            "text": "zwischen den Museen)"
        }
    }
}