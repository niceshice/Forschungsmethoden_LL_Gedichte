{
    "title": "nach-hause-6061",
    "gender": "w",
    "author": "Judith Zander",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "es war einmal so spät wir gelangten"
        },
        "line.2": {
            "text": "in eine zwischenzeit und der kontakt"
        },
        "line.3": {
            "text": "brach ab in die pause des raumes"
        },
        "line.4": {
            "text": "kam böhlen im bodennebel das schien"
        },
        "line.5": {
            "text": "ein unerreichbarer herrlich giftiger stern"
        },
        "line.6": {
            "text": "wir ließen uns anziehen"
        },
        "line.7": {
            "text": "durch shuttlefenster lugten wir auf"
        },
        "line.8": {
            "text": "traute antwortsignale ein leuchten"
        },
        "line.9": {
            "text": "in scheiben ein leuchten"
        },
        "line.10": {
            "text": "befeuerte teerschwarze atmosphären"
        },
        "line.11": {
            "text": "bestehend zu gleichen teilen aus"
        },
        "line.12": {
            "text": "pvc neon und dederonschweiß"
        },
        "line.13": {
            "text": "des aktivisten nachtschicht um nachtschicht"
        },
        "line.14": {
            "text": "durchquerten wir unsere innenrauminstrumente"
        },
        "line.15": {
            "text": "spielten verrückt ein zeitmesser zeigte"
        },
        "line.16": {
            "text": "die neigung zur zahl einundachtzig"
        },
        "line.17": {
            "text": "wir wurden kleiner und kleiner und kein"
        },
        "line.18": {
            "text": "radarschirm verlangte nach uns"
        }
    }
}