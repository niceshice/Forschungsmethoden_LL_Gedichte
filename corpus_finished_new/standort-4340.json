{
    "title": "standort-4340",
    "gender": "m",
    "author": "Wulf Kirsten",
    "author_birth": "1934",
    "poem": {
        "line.1": {
            "text": "tritt vor die tür, und schon kannst du"
        },
        "line.2": {
            "text": "hindurchgehn, hinein und wieder hinaus,"
        },
        "line.3": {
            "text": "eine welt für sich sehn als andere"
        },
        "line.4": {
            "text": "schöpfungsgeschichte aus erster hand,"
        },
        "line.5": {
            "text": "das ödland und der ballungsraum haarscharf"
        },
        "line.6": {
            "text": "getrennt, skyline, hintergründig,"
        },
        "line.7": {
            "text": "kalkanzeiger, sternhaarig, im kontrast,"
        },
        "line.8": {
            "text": "augenfällig donnerdistelland, was entlief"
        },
        "line.9": {
            "text": "nicht alles in die wiesen?"
        },
        "line.10": {
            "text": "blühgemeinschaften schnellfüßig die hangkante"
        },
        "line.11": {
            "text": "hinunter, lauter ausgedachte gebilde"
        },
        "line.12": {
            "text": "voller lichthunger würgendes gehänge,"
        },
        "line.13": {
            "text": "mächtig aller drei sprachen des himmels,"
        },
        "line.14": {
            "text": "sich selbst überlassen, wo die natur"
        },
        "line.15": {
            "text": "mit ihrem pfunde wuchert, nur noch"
        },
        "line.16": {
            "text": "zu zeugen aus liebe, was immer sie liebt:"
        },
        "line.17": {
            "text": "das unberechenbare, in parzellierter"
        },
        "line.18": {
            "text": "wildnis aufgelaßner gärten, geflammt"
        },
        "line.19": {
            "text": "und gefleckt, blumen in allen farben,"
        },
        "line.20": {
            "text": "selbstanbieter ohne vorbedacht, wer wohl"
        },
        "line.21": {
            "text": "brachte den speierling auf mit baumeignem"
        },
        "line.22": {
            "text": "naturell und setzte ihn derart lebensecht"
        },
        "line.23": {
            "text": "ins buckelnde gewell, daß er fortwuchs"
        },
        "line.24": {
            "text": "und fort aus einem stück holz,"
        },
        "line.25": {
            "text": "mainoberwärts? der quellhorizont"
        },
        "line.26": {
            "text": "im gedriesch, ringsum erdfälle, strossen,"
        },
        "line.27": {
            "text": "mag alles sein, die berge zerschmelzen"
        },
        "line.28": {
            "text": "wie wachs, der standort Deutschland"
        },
        "line.29": {
            "text": "bewegt sich, wenn der schleichsand"
        },
        "line.30": {
            "text": "es will und der schmirgel im sinngefüge"
        },
        "line.31": {
            "text": "der welt - such den punkt, der dich trägt."
        }
    }
}