{
    "title": "zuweilen-warst-du-nur-4740",
    "gender": "m",
    "author": "SAID",
    "author_birth": "1947",
    "poem": {
        "line.1": {
            "text": "zuweilen warst du nur"
        },
        "line.2": {
            "text": "was wir beschlossen hatten"
        },
        "line.3": {
            "text": "eine königin die ihre münder verschenkt"
        },
        "line.4": {
            "text": "ein nasses tier"
        },
        "line.5": {
            "text": "durstig und blind"
        },
        "line.6": {
            "text": "das nach mehr wasser schreit"
        },
        "line.7": {
            "text": "nach mehr wort"
        }
    }
}