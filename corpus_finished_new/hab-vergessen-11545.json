{
    "title": "hab-vergessen-11545",
    "gender": "w",
    "author": "Nora Gomringer",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "Hab vergessen"
        },
        "line.2": {
            "text": "Zu benennen wie die Straßen"
        },
        "line.3": {
            "text": "Die Dingen auf denen die Tassen"
        },
        "line.4": {
            "text": "Im Regal dort hinten in der Auffahrt"
        },
        "line.5": {
            "text": "Steh ich nackt"
        },
        "line.6": {
            "text": "Die Haare offen trag ich deinen Ring"
        },
        "line.7": {
            "text": "Kommt ein Mann täglich"
        },
        "line.8": {
            "text": "Wie ein wie heißen die"
        },
        "line.9": {
            "text": "Will mich Kindlein wiegen"
        },
        "line.10": {
            "text": "Streichelt über meine Wange denk ich"
        },
        "line.11": {
            "text": "Mörder du Dieb Sie lassen Sie das"
        },
        "line.12": {
            "text": "Bitte weitermachen unablässig"
        },
        "line.13": {
            "text": "Riech ich nach Arnika alte Frau"
        },
        "line.14": {
            "text": "Rufen sie mir zu ich frage sie"
        },
        "line.15": {
            "text": "wen meint ihr damit"
        },
        "line.16": {
            "text": "Steh ich nackt in der Auffahrt"
        },
        "line.17": {
            "text": "Hab vergessen"
        }
    }
}