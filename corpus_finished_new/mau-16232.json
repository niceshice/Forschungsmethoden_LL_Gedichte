{
    "title": "mau-16232",
    "gender": "w",
    "author": "Cornelia  Hülmbauer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "die mutter steigt hinauf aufs dach"
        },
        "line.2": {
            "text": "die mutter kratzt die kretzen runter"
        },
        "line.3": {
            "text": "die mutter zieht das rockerl rauf"
        },
        "line.4": {
            "text": "die mutter will heut tanzen gehen"
        },
        "line.5": {
            "text": "fuchs du hackst die gänschen klein"
        },
        "line.6": {
            "text": "stopf sie lieber aus"
        },
        "line.7": {
            "text": "stopf sie lieber aus"
        },
        "line.8": {
            "text": "tatar vom offnen herzen"
        },
        "line.9": {
            "text": "tamtam die bäume blühen"
        },
        "line.10": {
            "text": "das mädchen wickelt sich in deckchen"
        },
        "line.11": {
            "text": "das mädchen steht im eck"
        },
        "line.12": {
            "text": "das mädchen macht sich dreckig"
        },
        "line.13": {
            "text": "das mädchen spielt versteck"
        },
        "line.14": {
            "text": "i"
        },
        "line.15": {
            "text": "ch in fetzen hingerissen"
        },
        "line.16": {
            "text": "vögelchen wo schwärmst du hin"
        },
        "line.17": {
            "text": "beim knopf annähen"
        },
        "line.18": {
            "text": "verwende kreuzstich"
        },
        "line.19": {
            "text": "beim kopf verlieren"
        },
        "line.20": {
            "text": "nur den strick"
        }
    }
}