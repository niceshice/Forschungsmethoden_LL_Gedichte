{
    "title": "hinausgehen-1798",
    "gender": "w",
    "author": "Isabeella Beumer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "hinausgehen"
        },
        "line.2": {
            "text": "den wind zu jagen"
        },
        "line.3": {
            "text": "rote adern durch den horizont"
        },
        "line.4": {
            "text": "atem auf blut geeist"
        },
        "line.5": {
            "text": "tisch oder taubenflug"
        },
        "line.6": {
            "text": "staubversilberte"
        },
        "line.7": {
            "text": "ränder"
        },
        "line.8": {
            "text": "dazwischen pendelnder fels"
        },
        "line.9": {
            "text": "das messer im brot"
        },
        "line.10": {
            "text": "oder identisches hungern"
        },
        "line.11": {
            "text": "ins tuch geritzt"
        },
        "line.12": {
            "text": "es flieszt kein blut"
        },
        "line.13": {
            "text": "rote adern durch den horizont"
        },
        "line.14": {
            "text": "im bewegenden spiel"
        },
        "line.15": {
            "text": "der ort"
        },
        "line.16": {
            "text": "und ändert sich lebensgrosz."
        }
    }
}