{
    "title": "den-toten-des-surrealismus-4028",
    "gender": "m",
    "author": "Steffen Popp",
    "author_birth": "1978",
    "poem": {
        "line.1": {
            "text": "Wer mit den Dingen zusammenstößt,"
        },
        "line.2": {
            "text": "wird es derselbe sein, der sie harmonisiert?"
        },
        "line.3": {
            "text": "Das ist es wohl, was mich traurig macht."
        },
        "line.4": {
            "text": "Hugo Ball"
        },
        "line.5": {
            "text": "I"
        },
        "line.6": {
            "text": "Draußen ists still, kein Tank von Shell"
        },
        "line.7": {
            "text": "im Resonanzbereich meines Lauschens"
        },
        "line.8": {
            "text": "kein Atom bricht ab"
        },
        "line.9": {
            "text": "fällt in den Schacht, kein deprimiertes Organ"
        },
        "line.10": {
            "text": "probt den Verrat und die Lilie"
        },
        "line.11": {
            "text": "vor mir im träumenden Glas"
        },
        "line.12": {
            "text": "dieses Gewächs meiner Sehnsucht auch"
        },
        "line.13": {
            "text": "schneuzt sein Arom nur semiotisch"
        },
        "line.14": {
            "text": "ohne Geräusch in die entsetzliche Nacht!"
        },
        "line.15": {
            "text": "Draußen ists still, der leere Parkplatz"
        },
        "line.16": {
            "text": "Schubumkehr des Glücks und eine winzige"
        },
        "line.17": {
            "text": "Akademie"
        },
        "line.18": {
            "text": "fern dräut Asien"
        },
        "line.19": {
            "text": "ein Horn des Poseidon, mit Güterzügen"
        },
        "line.20": {
            "text": "So kommt nun die Welt über den Winter!"
        },
        "line.21": {
            "text": "Die Zwiebeln liegen auf dem Tisch"
        },
        "line.22": {
            "text": "die Apparate des Wunders kreisen"
        },
        "line.23": {
            "text": "Aber der Schnee ist nicht mehr gotisch"
        },
        "line.24": {
            "text": "eine wunschlose Erzform, ein hellblau"
        },
        "line.25": {
            "text": "getakteter Geiger"
        },
        "line.26": {
            "text": "nein, er ist grau"
        },
        "line.27": {
            "text": "und labbrig, die Abraumetage des Frühlings"
        },
        "line.28": {
            "text": "ach"
        },
        "line.29": {
            "text": "und nur die untersten Schnee-Engel"
        },
        "line.30": {
            "text": "halten sich noch an den Tankstellen"
        },
        "line.31": {
            "text": "vermummt und marxistisch"
        },
        "line.32": {
            "text": "in ihren winddichten Anoraks."
        },
        "line.33": {
            "text": "II"
        },
        "line.34": {
            "text": "Draußen ists still, es schlafen Berg und Tal"
        },
        "line.35": {
            "text": "reglos die Stadt, das Elend der Sprengwerke"
        },
        "line.36": {
            "text": "ihr langsames Feuer vereinfacht den Raum"
        },
        "line.37": {
            "text": "die Herzensgebrechen der Trainer"
        },
        "line.38": {
            "text": "Balkonpflanzen"
        },
        "line.39": {
            "text": "und in den Meeren der Wal"
        },
        "line.40": {
            "text": "und im Gefrierfach der Aal"
        },
        "line.41": {
            "text": "Delikatessen, am Rand meiner Schwäche"
        },
        "line.42": {
            "text": "ruhet die Liebe auch, ein Ghetto Rosen"
        },
        "line.43": {
            "text": "die großen Betonkörper winkeln das Licht"
        },
        "line.44": {
            "text": "wo meine Hand liegt"
        },
        "line.45": {
            "text": "ein Joch für Nachtfalter"
        },
        "line.46": {
            "text": "und die Gedichte gehn über den Schnee"
        },
        "line.47": {
            "text": "in kleinen Schritten"
        },
        "line.48": {
            "text": "Die toten Surrealisten"
        },
        "line.49": {
            "text": "rumoren unecht im Grundstock der Wälder"
        },
        "line.50": {
            "text": "kauen den Sternklee in diese Nacht"
        },
        "line.51": {
            "text": "trockene Seekoffer, Schneeklima"
        },
        "line.52": {
            "text": "hinter dem Elend der Bäume"
        },
        "line.53": {
            "text": "leuchtet die Heimat"
        },
        "line.54": {
            "text": "die Elemente erfinden sich, meine Geliebten"
        },
        "line.55": {
            "text": "liegen im Streit und zerfallen"
        },
        "line.56": {
            "text": "aufsteht der Mond, von seinem Sitz"
        },
        "line.57": {
            "text": "da sein gelber Mund"
        },
        "line.58": {
            "text": "dort seine Beine, die schleifen."
        }
    }
}