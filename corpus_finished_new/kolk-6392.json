{
    "title": "kolk-6392",
    "gender": "w",
    "author": "Sylvia  Geist",
    "author_birth": "1963",
    "poem": {
        "line.1": {
            "text": "Wie Drachen auf"
        },
        "line.2": {
            "text": "und ab mit dem Luftstrom,"
        },
        "line.3": {
            "text": "ihr Glänzen und Kreisen"
        },
        "line.4": {
            "text": "einander doubelnde dunkle"
        },
        "line.5": {
            "text": "Momente. Einer stößt oder fällt"
        },
        "line.6": {
            "text": "hinab für eine Beere, Beute,"
        },
        "line.7": {
            "text": "Laune, was weiß ich. Wahr,"
        },
        "line.8": {
            "text": "Raben klingen nüchtern,"
        },
        "line.9": {
            "text": "bekannte Tonspur eines Films,"
        },
        "line.10": {
            "text": "die sich vielleicht imitieren ließe,"
        },
        "line.11": {
            "text": "das Echo aber"
        },
        "line.12": {
            "text": "mit nichts zu vergleichen."
        }
    }
}