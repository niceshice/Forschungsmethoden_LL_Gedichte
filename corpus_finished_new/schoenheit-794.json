{
    "title": "schoenheit-794",
    "gender": "m",
    "author": "Michael Lentz",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "nimmt nicht"
        },
        "line.2": {
            "text": "überhand"
        },
        "line.3": {
            "text": "genug"
        },
        "line.4": {
            "text": "der dinge"
        },
        "line.5": {
            "text": "die da sind"
        },
        "line.6": {
            "text": "und zeit und"
        },
        "line.7": {
            "text": "räume nehmen"
        },
        "line.8": {
            "text": "(speisen füllen"
        },
        "line.9": {
            "text": "blasen reste"
        },
        "line.10": {
            "text": "löcher stopfen ...)"
        },
        "line.11": {
            "text": "doch dann"
        },
        "line.12": {
            "text": "dazwischen"
        },
        "line.13": {
            "text": "blaupause"
        },
        "line.14": {
            "text": "DU"
        },
        "line.15": {
            "text": "der mundstock"
        },
        "line.16": {
            "text": "die wirbelschleppe"
        },
        "line.17": {
            "text": "die blaue wolke"
        },
        "line.18": {
            "text": "auf weißem grund"
        },
        "line.19": {
            "text": "du bist ganz live"
        },
        "line.20": {
            "text": "die unverschämte"
        },
        "line.21": {
            "text": "jugend"
        },
        "line.22": {
            "text": "du spannst"
        },
        "line.23": {
            "text": "dein blaues band"
        },
        "line.24": {
            "text": "so im aufenthalt"
        },
        "line.25": {
            "text": "du bist so"
        },
        "line.26": {
            "text": "präzidier"
        },
        "line.27": {
            "text": "so einfach"
        },
        "line.28": {
            "text": "wunderbar"
        },
        "line.29": {
            "text": "ganz"
        },
        "line.30": {
            "text": "durchsicht"
        },
        "line.31": {
            "text": "und empfindung"
        },
        "line.32": {
            "text": "so"
        },
        "line.33": {
            "text": "los"
        },
        "line.34": {
            "text": "so losgelassen"
        },
        "line.35": {
            "text": "so dringend"
        },
        "line.36": {
            "text": "und das"
        },
        "line.37": {
            "text": "was du so"
        },
        "line.38": {
            "text": "ganz"
        },
        "line.39": {
            "text": "für dich alleine"
        },
        "line.40": {
            "text": "bist"
        },
        "line.41": {
            "text": "denn ferner"
        },
        "line.42": {
            "text": "auch"
        },
        "line.43": {
            "text": "ein stück weit"
        },
        "line.44": {
            "text": "mir"
        },
        "line.45": {
            "text": "ach leg deine warme hand"
        },
        "line.46": {
            "text": "auf mein sprachloch"
        },
        "line.47": {
            "text": "\"während du gleichsam"
        },
        "line.48": {
            "text": "die liebe bewegst\""
        },
        "line.49": {
            "text": "eine zeit"
        },
        "line.50": {
            "text": "die fort"
        },
        "line.51": {
            "text": "erzählt"
        },
        "line.52": {
            "text": "die zählt"
        },
        "line.53": {
            "text": "einen ganz"
        },
        "line.54": {
            "text": "so"
        },
        "line.55": {
            "text": "los"
        },
        "line.56": {
            "text": "so losgelassen"
        },
        "line.57": {
            "text": "bist du"
        },
        "line.58": {
            "text": "hungrig"
        },
        "line.59": {
            "text": "komme ich gern"
        }
    }
}