{
    "title": "ich-indes-michel-de-montaigne-375",
    "gender": "m",
    "author": "Paul Wühr",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "ICH"
        },
        "line.2": {
            "text": "Indes"
        },
        "line.3": {
            "text": "Michel de Montaigne hier"
        },
        "line.4": {
            "text": "die leere Stelle"
        },
        "line.5": {
            "text": "in seiner Mitte nicht mit"
        },
        "line.6": {
            "text": "sich selbst zu"
        },
        "line.7": {
            "text": "füllen imstande ist wie sein"
        },
        "line.8": {
            "text": "Maler die Leinwand"
        },
        "line.9": {
            "text": "mit einem Gemälde sondern"
        },
        "line.10": {
            "text": "wie dieser den Rand nur"
        },
        "line.11": {
            "text": "mit Grotesken kommt ihn"
        },
        "line.12": {
            "text": "das Leid an was"
        },
        "line.13": {
            "text": "enthält sagt er zu Etienne"
        },
        "line.14": {
            "text": "de la Botie"
        },
        "line.15": {
            "text": "dieses Buch hier bei Licht"
        },
        "line.16": {
            "text": "besehen anderes"
        },
        "line.17": {
            "text": "als phantastische Leiber"
        },
        "line.18": {
            "text": "die keine bestimmte"
        },
        "line.19": {
            "text": "Gestalt haben und in keine"
        },
        "line.20": {
            "text": "Ordnung gehören es"
        },
        "line.21": {
            "text": "ist so daß ich mich da nicht"
        },
        "line.22": {
            "text": "finde wo ich mich"
        },
        "line.23": {
            "text": "suche"
        },
        "line.24": {
            "text": "Als"
        },
        "line.25": {
            "text": "Montaigne sagt Etienne"
        },
        "line.26": {
            "text": "schau in dich"
        },
        "line.27": {
            "text": "hinein sage mir was du"
        },
        "line.28": {
            "text": "dort siehst sagt"
        },
        "line.29": {
            "text": "der Freund nichts wenn"
        },
        "line.30": {
            "text": "ich mich sehen"
        },
        "line.31": {
            "text": "soll wie du es verlangst"
        },
        "line.32": {
            "text": "von mir alles"
        },
        "line.33": {
            "text": "aber wonach du nicht"
        },
        "line.34": {
            "text": "fragst"
        },
        "line.35": {
            "text": "Also"
        },
        "line.36": {
            "text": "sagt Etienne dort wo"
        },
        "line.37": {
            "text": "ich sein soll bin"
        },
        "line.38": {
            "text": "ich nicht sozusagen ich"
        },
        "line.39": {
            "text": "bin nicht in mir"
        },
        "line.40": {
            "text": "da bist du Michel und"
        },
        "line.41": {
            "text": "die deinen die"
        },
        "line.42": {
            "text": "auch die meinen sind"
        },
        "line.43": {
            "text": "die entfernt werden"
        },
        "line.44": {
            "text": "müssen soll es so leer"
        },
        "line.45": {
            "text": "werden dort in mir"
        },
        "line.46": {
            "text": "wo ich sein soll"
        },
        "line.47": {
            "text": "Ob"
        },
        "line.48": {
            "text": "ich nur diese Leere bin und"
        },
        "line.49": {
            "text": "sonst nichts also"
        },
        "line.50": {
            "text": "gar nicht erwarten muß jemals"
        },
        "line.51": {
            "text": "vernichtet zu werden"
        },
        "line.52": {
            "text": "sagt Etienne denn das bin"
        },
        "line.53": {
            "text": "ich doch schon"
        },
        "line.54": {
            "text": "und wie du weißt sagt"
        },
        "line.55": {
            "text": "Montaigne lebt es"
        },
        "line.56": {
            "text": "sich über einer Leere nicht"
        },
        "line.57": {
            "text": "schlecht"
        },
        "line.58": {
            "text": "Ob"
        },
        "line.59": {
            "text": "die Leere wem sagt"
        },
        "line.60": {
            "text": "das Montaigne"
        },
        "line.61": {
            "text": "schon etwas ist und"
        },
        "line.62": {
            "text": "gar nicht"
        },
        "line.63": {
            "text": "etwas vernichtet und"
        },
        "line.64": {
            "text": "aber je"
        },
        "line.65": {
            "text": "mehr anwesend sein"
        },
        "line.66": {
            "text": "wollen in"
        },
        "line.67": {
            "text": "so einer Leere bis"
        },
        "line.68": {
            "text": "sie selbst"
        },
        "line.69": {
            "text": "nicht mehr anwesend"
        },
        "line.70": {
            "text": "ist desto"
        },
        "line.71": {
            "text": "geringer an sich"
        },
        "line.72": {
            "text": "das Risiko"
        },
        "line.73": {
            "text": "werden dürfte daß"
        },
        "line.74": {
            "text": "sich Vernichtung"
        },
        "line.75": {
            "text": "rentiert"
        },
        "line.76": {
            "text": "Oder"
        },
        "line.77": {
            "text": "war ich fragt Etienne unter"
        },
        "line.78": {
            "text": "denen doch anzutreffen"
        },
        "line.79": {
            "text": "die ich entfernte alles aus"
        },
        "line.80": {
            "text": "mir sollte ich"
        },
        "line.81": {
            "text": "mich übersehen haben in dieser"
        },
        "line.82": {
            "text": "Menge oder"
        },
        "line.83": {
            "text": "in wie vielen von diesen allen"
        },
        "line.84": {
            "text": "bin ich doch"
        },
        "line.85": {
            "text": "unter anderen gewesen oder"
        },
        "line.86": {
            "text": "suchen sich andere"
        },
        "line.87": {
            "text": "nur wieder in sich und deshalb"
        },
        "line.88": {
            "text": "entfernen diese alle"
        },
        "line.89": {
            "text": "unter anderem mich"
        },
        "line.90": {
            "text": "Und"
        },
        "line.91": {
            "text": "sagt Etienne wo wenn ich"
        },
        "line.92": {
            "text": "sterbe ich frage"
        },
        "line.93": {
            "text": "mit wem findet dieses Ende"
        },
        "line.94": {
            "text": "statt stirbt was"
        },
        "line.95": {
            "text": "ich in mir habe aber mich"
        },
        "line.96": {
            "text": "hab ich doch"
        },
        "line.97": {
            "text": "nicht in mir ich kann nicht"
        },
        "line.98": {
            "text": "sterben in mir"
        },
        "line.99": {
            "text": "dort bin ich von Anfang"
        },
        "line.100": {
            "text": "an tot"
        },
        "line.101": {
            "text": "ist dieser Etienne von allen"
        },
        "line.102": {
            "text": "anderen nur"
        },
        "line.103": {
            "text": "das Grab wo überall liege"
        },
        "line.104": {
            "text": "ich dann begraben"
        },
        "line.105": {
            "text": "Also"
        },
        "line.106": {
            "text": "sind wir gar nicht in uns"
        },
        "line.107": {
            "text": "wo"
        },
        "line.108": {
            "text": "anders als immer anderswo"
        },
        "line.109": {
            "text": "muß ein"
        },
        "line.110": {
            "text": "jeder von uns von einem"
        },
        "line.111": {
            "text": "der mit"
        },
        "line.112": {
            "text": "uns allen nur er selber"
        },
        "line.113": {
            "text": "ist also"
        },
        "line.114": {
            "text": "vernichtet werden wenn"
        },
        "line.115": {
            "text": "wir uns"
        },
        "line.116": {
            "text": "ganz aus dem Sinn kommen"
        },
        "line.117": {
            "text": "sollen"
        },
        "line.118": {
            "text": "Ob"
        },
        "line.119": {
            "text": "ein jeder sagt Montaigne"
        },
        "line.120": {
            "text": "zu allen"
        },
        "line.121": {
            "text": "sich nur als einer dazu"
        },
        "line.122": {
            "text": "zählen kann"
        },
        "line.123": {
            "text": "der alle um noch einmal"
        },
        "line.124": {
            "text": "alle"
        },
        "line.125": {
            "text": "vermehrt mit sich selber"
        },
        "line.126": {
            "text": "Etienne"
        },
        "line.127": {
            "text": "Falls"
        },
        "line.128": {
            "text": "ich nur draußen bei dir"
        },
        "line.129": {
            "text": "als Etienne bin"
        },
        "line.130": {
            "text": "sagt Etienne de la Botie"
        },
        "line.131": {
            "text": "und nicht nur"
        },
        "line.132": {
            "text": "in dir spielt sich also"
        },
        "line.133": {
            "text": "meine Person"
        },
        "line.134": {
            "text": "bei euch ab wenn du"
        },
        "line.135": {
            "text": "sehen willst wie"
        },
        "line.136": {
            "text": "sagt Montaigne schau"
        },
        "line.137": {
            "text": "bei mir herein"
        },
        "line.138": {
            "text": "Wenn"
        },
        "line.139": {
            "text": "Montaigne sagt Etienne"
        },
        "line.140": {
            "text": "schau bei mir"
        },
        "line.141": {
            "text": "herein sage mir was du"
        },
        "line.142": {
            "text": "dort nicht siehst sagt"
        },
        "line.143": {
            "text": "dieser dich sehe ich immer"
        },
        "line.144": {
            "text": "in mir aber"
        },
        "line.145": {
            "text": "in dir bin ich einer von"
        },
        "line.146": {
            "text": "vielen nur"
        },
        "line.147": {
            "text": "mit welchen ich Etienne"
        },
        "line.148": {
            "text": "bin der deine"
        },
        "line.149": {
            "text": "mit dem ich in mich"
        },
        "line.150": {
            "text": "hineinschaue"
        }
    }
}