{
    "title": "es-wurde-fruehling-und-ich-ging-12741",
    "gender": "w",
    "author": "Charlotte Warsen",
    "author_birth": "1984",
    "poem": {
        "line.1": {
            "text": "es wurde Frühling und ich ging"
        },
        "line.2": {
            "text": "auf der Via Porta Rossa"
        },
        "line.3": {
            "text": "in Fäulnis über ohne Zögern"
        },
        "line.4": {
            "text": "die Zunge vorgespult"
        },
        "line.5": {
            "text": "der Schädel eingedrückt"
        },
        "line.6": {
            "text": "die Augen reichlich abgedreht"
        },
        "line.7": {
            "text": "kleine, mit Fett gefüllte Steinlampen"
        },
        "line.8": {
            "text": "die Arme Dochte"
        },
        "line.9": {
            "text": "aus Wacholderzweigen"
        }
    }
}