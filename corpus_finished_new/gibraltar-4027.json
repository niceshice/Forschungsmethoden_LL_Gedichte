{
    "title": "gibraltar-4027",
    "gender": "m",
    "author": "Steffen Popp",
    "author_birth": "1978",
    "poem": {
        "line.1": {
            "text": "dahin ("
        },
        "line.2": {
            "text": "Goethe"
        },
        "line.3": {
            "text": ")"
        },
        "line.4": {
            "text": "Kennst du, Geliebter, den Hass"
        },
        "line.5": {
            "text": "Bäume, die spanische Wand"
        },
        "line.6": {
            "text": "kennst du die Inseln, Kreuzfahrer"
        },
        "line.7": {
            "text": "in ihren Ein-Mann-Torpedos"
        },
        "line.8": {
            "text": "Geliebter, verzeih, ich wollte nach Golgatha"
        },
        "line.9": {
            "text": "aber im Wäscheschrank"
        },
        "line.10": {
            "text": "lag nur ein gelber Revolver (so blieb ich)."
        },
        "line.11": {
            "text": "Kennst du die See, Spuren von Kühnheit"
        },
        "line.12": {
            "text": "im Schaum, Geliebter, im Schaum"
        },
        "line.13": {
            "text": "kennst du das Land, seinen verrenkten Tragarm"
        },
        "line.14": {
            "text": "schwer liegt es herum, unter der hungrigen Luft"
        },
        "line.15": {
            "text": "planloser Wind"
        },
        "line.16": {
            "text": "Wollspuren, Wärme und Staub darin."
        },
        "line.17": {
            "text": "Wie gern zög ich hinaus"
        },
        "line.18": {
            "text": "wohnte, Geliebter, mit dir, unter Zitronen:"
        },
        "line.19": {
            "text": "gingen nicht, in meinen tragischen Venen"
        },
        "line.20": {
            "text": "die Elemente Gibraltars"
        },
        "line.21": {
            "text": "schwer um und aller verschollenen Kaps"
        },
        "line.22": {
            "text": "manische Augen, lidlos"
        },
        "line.23": {
            "text": "mein Herz ist eine Gräfin, umstellt von Pflegern"
        },
        "line.24": {
            "text": "am Rand der City, Geliebter"
        },
        "line.25": {
            "text": "blöken die Mietklaviere!"
        }
    }
}