{
    "title": "ich-saite-singe-14539",
    "gender": "w",
    "author": "Ruth Wolf-Rehfeldt",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "Ich"
        },
        "line.2": {
            "text": "Saite"
        },
        "line.3": {
            "text": "singe"
        },
        "line.4": {
            "text": "Von niemand gezupft"
        },
        "line.5": {
            "text": "und springe"
        },
        "line.6": {
            "text": "Von niemand geschlagen"
        },
        "line.7": {
            "text": "Entzwei"
        }
    }
}