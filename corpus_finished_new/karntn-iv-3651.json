{
    "title": "karntn-iv-3651",
    "gender": "m",
    "author": "Axel  Karner",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "ka scheanas lond"
        },
        "line.2": {
            "text": "hot iba nocht"
        },
        "line.3": {
            "text": "so vül hamlichkeitn"
        },
        "line.4": {
            "text": "a toates kind"
        },
        "line.5": {
            "text": "mit an schra ols locha"
        },
        "line.6": {
            "text": "a liab"
        },
        "line.7": {
            "text": "von zwa kolte augn"
        },
        "line.8": {
            "text": "und hoss"
        },
        "line.9": {
            "text": "aufgrissene tian"
        },
        "line.10": {
            "text": "schnell schnell"
        },
        "line.11": {
            "text": "ka aufbegeahn"
        },
        "line.12": {
            "text": "de händ ins kreiz gedraht"
        },
        "line.13": {
            "text": "ka scheanas lond"
        },
        "line.14": {
            "text": "konn"
        },
        "line.15": {
            "text": "so vül stüll sein"
        },
        "line.16": {
            "text": "- - - - -"
        },
        "line.17": {
            "text": "kärnten IV"
        },
        "line.18": {
            "text": "kein schöneres land"
        },
        "line.19": {
            "text": "hat über nacht"
        },
        "line.20": {
            "text": "so viele heimlichkeiten"
        },
        "line.21": {
            "text": "ein totes kind"
        },
        "line.22": {
            "text": "mit einem schrei als lachen"
        },
        "line.23": {
            "text": "eine liebe"
        },
        "line.24": {
            "text": "von zwei kalten augen"
        },
        "line.25": {
            "text": "und hass"
        },
        "line.26": {
            "text": "aufgerissene türen"
        },
        "line.27": {
            "text": "schnell schnell"
        },
        "line.28": {
            "text": "kein aufbegehren"
        },
        "line.29": {
            "text": "die hände ins kreuz gedreht"
        },
        "line.30": {
            "text": "kein schöneres land"
        },
        "line.31": {
            "text": "kann"
        },
        "line.32": {
            "text": "so viel still sein"
        }
    }
}