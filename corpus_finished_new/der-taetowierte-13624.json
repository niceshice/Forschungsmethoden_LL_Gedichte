{
    "title": "der-taetowierte-13624",
    "gender": "w",
    "author": "Silke Scheuermann",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "Wenn er sich bewegte"
        },
        "line.2": {
            "text": "bewegten sich Baume und Vogel"
        },
        "line.3": {
            "text": "Pfauen Adler Schwane Schmetterlinge"
        },
        "line.4": {
            "text": "bewegten sich Flatterten"
        },
        "line.5": {
            "text": "Kolibris winzige Drachen"
        },
        "line.6": {
            "text": "Einzelwesen Landschaften"
        },
        "line.7": {
            "text": "Eva von Schlangen umzingelt"
        },
        "line.8": {
            "text": "bewegte sich auf dem Brustkorb"
        },
        "line.9": {
            "text": "Die Tinte bewegte sich Eine Arche"
        },
        "line.10": {
            "text": "Abraham mit langem Bart"
        },
        "line.11": {
            "text": "bewegte sich"
        },
        "line.12": {
            "text": "Noah zwischen Elefanten"
        },
        "line.13": {
            "text": "Die fetten Russel"
        },
        "line.14": {
            "text": "Gerettete Schildkroten Lurche"
        },
        "line.15": {
            "text": "Die Trauer gerettet eingesperrt"
        },
        "line.16": {
            "text": "zur Ansicht frei gegeben"
        },
        "line.17": {
            "text": "Zebras mit Ringen Schwarzweiße Korper"
        },
        "line.18": {
            "text": "Gesattelte Zebras Baren mit Ringen"
        },
        "line.19": {
            "text": "durch ihre Nasen Tanzbaren"
        },
        "line.20": {
            "text": "Eisbaren Braunbaren"
        },
        "line.21": {
            "text": "Das Holz des Schiffes war dunkel"
        },
        "line.22": {
            "text": "Die Farben bewegten sich"
        },
        "line.23": {
            "text": "Sein Schmerz war vergessen"
        },
        "line.24": {
            "text": "Karmesinrot bewegte sich"
        },
        "line.25": {
            "text": "Nachtblau Azurblau"
        },
        "line.26": {
            "text": "Nebelweiß bewegte sich lebte"
        },
        "line.27": {
            "text": "Eierschalfarbe Leben aus einem Ei"
        },
        "line.28": {
            "text": "heraus geboren bewegte sich"
        },
        "line.29": {
            "text": "Alles war fruchtbar sprach"
        },
        "line.30": {
            "text": "Hier war alles da"
        },
        "line.31": {
            "text": "Der Himmel war"
        },
        "line.32": {
            "text": "hautfarben bewegte sich"
        },
        "line.33": {
            "text": "Ein Buch fiel herunter"
        },
        "line.34": {
            "text": "Das Zimmer bewegte"
        },
        "line.35": {
            "text": "sich in gnadigem Rhythmus"
        },
        "line.36": {
            "text": "Der Nachttisch das Bucherbord"
        },
        "line.37": {
            "text": "Pflanzen Eine Erinnerung Eine Sorge"
        },
        "line.38": {
            "text": "Ein Schrei bewegte sich"
        },
        "line.39": {
            "text": "loste sich auf"
        },
        "line.40": {
            "text": "Die Fenster waren geoffnet"
        },
        "line.41": {
            "text": "Sommerluft bewegte sich"
        },
        "line.42": {
            "text": "Stimmen Gelachter flog herein"
        },
        "line.43": {
            "text": "Kinder lachten einander an"
        },
        "line.44": {
            "text": "ihre Zukunft schwebte noch"
        },
        "line.45": {
            "text": "eine blassblaue Skizze"
        },
        "line.46": {
            "text": "Tinte war die naturliche Sprache deines Gefuhls"
        },
        "line.47": {
            "text": "Symbole von Sieg und Niederlage"
        },
        "line.48": {
            "text": "Herzen von Pfeilen durchstochen"
        },
        "line.49": {
            "text": "bewegten sich Ein Dreizack"
        },
        "line.50": {
            "text": "Wasser lebte bewegte sich"
        },
        "line.51": {
            "text": "Delphine lebten weil wir uns bewegten"
        },
        "line.52": {
            "text": "Gift lebte Diese unnaturliche"
        },
        "line.53": {
            "text": "Buntheit deiner Oberflache"
        },
        "line.54": {
            "text": "Worter bewegten sich"
        },
        "line.55": {
            "text": "Alte Namen"
        },
        "line.56": {
            "text": "Die Frau von diesem"
        },
        "line.57": {
            "text": "die Frau vom letzten Jahr"
        },
        "line.58": {
            "text": "Abschiede bewegten sich"
        },
        "line.59": {
            "text": "Wunsche schliefen lange und blieben"
        },
        "line.60": {
            "text": "ich schlief mit dir umarmte"
        },
        "line.61": {
            "text": "dich die Welt auf deinen Schultern zuckte"
        },
        "line.62": {
            "text": "sie bewegte sich so stark ich musste"
        },
        "line.63": {
            "text": "mir die Augen reiben"
        },
        "line.64": {
            "text": "Es bewegten sich"
        },
        "line.65": {
            "text": "die Madonnen"
        },
        "line.66": {
            "text": "Fische Amphibien Kreuze"
        },
        "line.67": {
            "text": "Berge Zauberer Kreuze"
        },
        "line.68": {
            "text": "Nixen Feen Satan Kreuze"
        },
        "line.69": {
            "text": "Es floss alles Bilder bewegten sich"
        },
        "line.70": {
            "text": "ich bewegte mich auf ihm"
        },
        "line.71": {
            "text": "Das neue Leben bewegte sich"
        },
        "line.72": {
            "text": "Ich glaubte ich"
        },
        "line.73": {
            "text": "konnte es spuren"
        },
        "line.74": {
            "text": "Ein Name klein noch"
        },
        "line.75": {
            "text": "Munder und Zweige bewegten sich"
        },
        "line.76": {
            "text": "Kuriose Schnecken zogen ihre Hauser mit sich"
        },
        "line.77": {
            "text": "Die Hauser bewegten sich"
        },
        "line.78": {
            "text": "wenn du hustetest oder"
        },
        "line.79": {
            "text": "dich umdrehtest"
        },
        "line.80": {
            "text": "du schliefst"
        },
        "line.81": {
            "text": "Ich bewegte mich mit dir"
        },
        "line.82": {
            "text": "Traume bewegten sich"
        },
        "line.83": {
            "text": "Die Augen der Hauser ließen uns eintreten"
        },
        "line.84": {
            "text": "Ich krallte mich in deinem Blick fest"
        },
        "line.85": {
            "text": "Der Blick bewegte sich"
        },
        "line.86": {
            "text": "ich schwebte schrie und"
        },
        "line.87": {
            "text": "musste gehalten werden vom Zimmer"
        },
        "line.88": {
            "text": "Du glaubtest an Tinte"
        },
        "line.89": {
            "text": "Du schriebst Schreie immer auf ubersetztest"
        },
        "line.90": {
            "text": "Dein Korper war weiß und"
        },
        "line.91": {
            "text": "unschuldig doch er bewegte sich"
        },
        "line.92": {
            "text": "es bewegte sich alles"
        },
        "line.93": {
            "text": "Wir bewegten uns"
        },
        "line.94": {
            "text": "wir tatowierten uns"
        },
        "line.95": {
            "text": "gegenseitig"
        },
        "line.96": {
            "text": "jede Nacht"
        },
        "line.97": {
            "text": "Nach jedem Mal war noch"
        },
        "line.98": {
            "text": "ein wenig Farbe auszubluten"
        },
        "line.99": {
            "text": "Wir glaubten an das Blut"
        }
    }
}