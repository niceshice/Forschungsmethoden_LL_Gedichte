{
    "title": "der-nackte-ginsberg-3942",
    "gender": "m",
    "author": "Dieter M. Gräf",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "ist auf das Feuerleiter-Z geklettert,"
        },
        "line.2": {
            "text": "zeigt denen, die unter ihm wohnen:"
        },
        "line.3": {
            "text": "ich habe Gott gesehen! Immer"
        },
        "line.4": {
            "text": "noch dort, auf der Höhe eines"
        },
        "line.5": {
            "text": "niederen Seraphim, ungefähr"
        },
        "line.6": {
            "text": "in der Sphäre der sechs Gesims"
        },
        "line.7": {
            "text": "engel des Bayardbuildings;"
        },
        "line.8": {
            "text": "ihre weit ausgebreiteten Arme,"
        },
        "line.9": {
            "text": "als würden alle gemeinsam"
        },
        "line.10": {
            "text": "herunterspringen. Altmodisch"
        },
        "line.11": {
            "text": "ist die Stadt geworden,"
        },
        "line.12": {
            "text": "spätestens seit die"
        },
        "line.13": {
            "text": "fedayin"
        },
        "line.14": {
            "text": "-Piloten"
        },
        "line.15": {
            "text": "ihre Filme echteten, toppten,"
        },
        "line.16": {
            "text": "seit das 21. Jahrhundert sein Colosseum"
        },
        "line.17": {
            "text": "reinschlug als Grube, Nichts."
        },
        "line.18": {
            "text": "Die Entkommenden sahen"
        },
        "line.19": {
            "text": "aus wie ein Stamm."
        },
        "line.20": {
            "text": "Sonne, Mond und Sterne"
        },
        "line.21": {
            "text": "sind nach wie vor Wolkenkratzer,"
        },
        "line.22": {
            "text": "und auf denen, die den nackten Ginsberg"
        },
        "line.23": {
            "text": "beherbergen, wachsen Bäume."
        },
        "line.24": {
            "text": "Selbst gewaltigere spenden farbiges Licht."
        },
        "line.25": {
            "text": "Solche Gebirge, nebelverhangen,"
        },
        "line.26": {
            "text": "und aus den Gullies der Schluchten"
        },
        "line.27": {
            "text": "steigt Dampf, in die Madison Ave,"
        },
        "line.28": {
            "text": "in die Fifth Avenue. Donald Trump"
        },
        "line.29": {
            "text": "verkündet, die Twin Towers nach"
        },
        "line.30": {
            "text": "bauen zu wollen, höher! Derweil"
        },
        "line.31": {
            "text": "in einem Shabu Shabu Restaurant"
        },
        "line.32": {
            "text": "sitzen, am Haus von Edgar Allen Poe"
        },
        "line.33": {
            "text": "der berittenen Polizei zusehen."
        },
        "line.34": {
            "text": "John Lennon ist tot. Das verstehen"
        },
        "line.35": {
            "text": "wir alle, unter und über den Wassertanks."
        }
    }
}