{
    "title": "etwas-1806",
    "gender": "w",
    "author": "Isabeella Beumer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "etwas"
        },
        "line.2": {
            "text": "das allem"
        },
        "line.3": {
            "text": "und noch niemand war"
        },
        "line.4": {
            "text": "nur überrestbuchstäblich"
        },
        "line.5": {
            "text": "welkes laub"
        },
        "line.6": {
            "text": "entwurzelter baum"
        },
        "line.7": {
            "text": "leerer stuhl"
        },
        "line.8": {
            "text": "erinnertes leben"
        },
        "line.9": {
            "text": "treibgut"
        },
        "line.10": {
            "text": "wie blumen"
        },
        "line.11": {
            "text": "einfach anwesendzärtlichkeit und stille"
        },
        "line.12": {
            "text": "möglicherweise nur ahnen"
        },
        "line.13": {
            "text": "läszt geschehen"
        },
        "line.14": {
            "text": "das erinnern ins Noch-Nicht der sehnsucht"
        },
        "line.15": {
            "text": "aberauchdiesistnureinspiel"
        },
        "line.16": {
            "text": "nichtdasselbe"
        },
        "line.17": {
            "text": "überleben der reste von liebe"
        },
        "line.18": {
            "text": "die umarmung faszt scherben"
        },
        "line.19": {
            "text": "streichelnde hände greifen ins leere"
        },
        "line.20": {
            "text": "stuhl und tisch in perfekter haltung"
        },
        "line.21": {
            "text": "sie finden allein zur tür"
        }
    }
}