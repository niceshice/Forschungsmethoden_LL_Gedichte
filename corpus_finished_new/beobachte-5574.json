{
    "title": "beobachte-5574",
    "gender": "m",
    "author": "José F. A. Oliver",
    "author_birth": "1961",
    "poem": {
        "line.1": {
            "text": "1"
        },
        "line.2": {
            "text": "mädchen mit roten schuhen"
        },
        "line.3": {
            "text": "& entengeräusche 1 entenzunge"
        },
        "line.4": {
            "text": "obsidiangrüne äugen. Beobachte"
        },
        "line.5": {
            "text": "den landschritt"
        },
        "line.6": {
            "text": "geht an land"
        },
        "line.7": {
            "text": "/"
        },
        "line.8": {
            "text": "wird von der mutter aus dem rad-"
        },
        "line.9": {
            "text": "anhänger gehievt"
        },
        "line.10": {
            "text": "/ geht vierbeinig geht"
        },
        "line.11": {
            "text": "mit 1 mutterstütze. Beobachte"
        },
        "line.12": {
            "text": "in die schritte eingewoben / verschnürt"
        },
        "line.13": {
            "text": "atemzöpfe 1 schwere zartheit"
        },
        "line.14": {
            "text": "1 augenfroh"
        },
        "line.15": {
            "text": "1 engelsgestalt der haare. Über allem"
        },
        "line.16": {
            "text": "1 lebensgegend"
        }
    }
}