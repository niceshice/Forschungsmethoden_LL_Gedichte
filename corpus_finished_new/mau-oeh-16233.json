{
    "title": "mau-oeh-16233",
    "gender": "w",
    "author": "Cornelia  Hülmbauer",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "das mädchen schneidet sich ins tischtuch"
        },
        "line.2": {
            "text": "das mädchen will prinzessin sein"
        },
        "line.3": {
            "text": "das mädchen schreit wie aufgespießt"
        },
        "line.4": {
            "text": "das mädchen spürt die spitzen"
        },
        "line.5": {
            "text": "t"
        },
        "line.6": {
            "text": "anz den tanz"
        },
        "line.7": {
            "text": "dreh den rock"
        },
        "line.8": {
            "text": "dreh den rock wie teller"
        },
        "line.9": {
            "text": "mache gute mienen"
        },
        "line.10": {
            "text": "spiele gutes spiel"
        },
        "line.11": {
            "text": "sprenge trockne felder"
        },
        "line.12": {
            "text": "aus dem haus gerät die mutter"
        },
        "line.13": {
            "text": "an den hans gerät sie recht"
        },
        "line.14": {
            "text": "in den seilzug eine schlaufe"
        },
        "line.15": {
            "text": "was dem kopf der kragen wird"
        },
        "line.16": {
            "text": "schweigen ist hochzeit"
        },
        "line.17": {
            "text": "reden ist blech"
        },
        "line.18": {
            "text": "schweigen ist ehern"
        },
        "line.19": {
            "text": "scheiden ist pech"
        },
        "line.20": {
            "text": "der onkel nimmt dich untern arm"
        },
        "line.21": {
            "text": "der onkel will nur spielen"
        },
        "line.22": {
            "text": "der onkel sperrt dich in den kasten"
        },
        "line.23": {
            "text": "der onkel tut nichts tut er wohl"
        },
        "line.24": {
            "text": "ein teil tier"
        },
        "line.25": {
            "text": "& ein teil herzen"
        },
        "line.26": {
            "text": "ein überwurf wie pelz"
        },
        "line.27": {
            "text": "mein gehäuse geht auf stelzen"
        }
    }
}