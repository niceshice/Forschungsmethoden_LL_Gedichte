{
    "title": "die-kinder-des-lichts-12369",
    "gender": "m",
    "author": "Martin  Piekar",
    "author_birth": "1990",
    "poem": {
        "line.1": {
            "text": "Und es macht euch nicht schöner"
        },
        "line.2": {
            "text": "Was ihr mögt"
        },
        "line.3": {
            "text": "Nicht greller, nicht verheißender"
        },
        "line.4": {
            "text": "Ich wollte nie schön, nicht"
        },
        "line.5": {
            "text": "Schön für das TV-Licht sein"
        },
        "line.6": {
            "text": "Die Korruption ist ein Mob"
        },
        "line.7": {
            "text": "Ein Knall"
        },
        "line.8": {
            "text": "Eine Gewalt im Licht"
        },
        "line.9": {
            "text": "Ihr wollt Kredit?"
        },
        "line.10": {
            "text": "Zieht lieber"
        },
        "line.11": {
            "text": "Eure teuersten Schuhe an"
        },
        "line.12": {
            "text": "Denn ihr geht nicht auf die Knie"
        },
        "line.13": {
            "text": "Nicht vor einem Staat"
        },
        "line.14": {
            "text": "Nicht vor zwei"
        },
        "line.15": {
            "text": "Oder vor der"
        },
        "line.16": {
            "text": "Erhabenheit der Kontobewegungen"
        },
        "line.17": {
            "text": "Wer EinwegProtestler ist"
        },
        "line.18": {
            "text": "Bleibt nicht standhaft"
        },
        "line.19": {
            "text": "Wer EinwegProtestler ist"
        },
        "line.20": {
            "text": "Vergreift sich"
        },
        "line.21": {
            "text": "Ein brennendes Auto"
        },
        "line.22": {
            "text": "Hat noch niemanden zu"
        },
        "line.23": {
            "text": "Prometheus gemacht"
        },
        "line.24": {
            "text": "Wann erkennt ihr das?"
        },
        "line.25": {
            "text": "Wann erkennt ihr ihn?"
        },
        "line.26": {
            "text": "Verpiss dich Idol,"
        },
        "line.27": {
            "text": "Du machst alles nur noch schlimmer"
        },
        "line.28": {
            "text": "Wie SplatterPropheten"
        },
        "line.29": {
            "text": "Verblendet TV-Licht"
        },
        "line.30": {
            "text": "Und das wisst ihr"
        },
        "line.31": {
            "text": "Haltet euch die Ohren zu,"
        },
        "line.32": {
            "text": "Ich schlage dem TV in seinen"
        },
        "line.33": {
            "text": "Scheißscreen, weil meiner alt ist"
        },
        "line.34": {
            "text": "Und ich mir keinen neuen leiste"
        },
        "line.35": {
            "text": "Die größte Kapitalismuskritik sollte"
        },
        "line.36": {
            "text": "Ein bisschen Armut sein, ein"
        },
        "line.37": {
            "text": "Bisschen Dumping"
        },
        "line.38": {
            "text": "Auf endzeitlich, ein bisschen Discount"
        },
        "line.39": {
            "text": "Auf Leben"
        },
        "line.40": {
            "text": "Trollende Krawalliere auf Europatournee"
        },
        "line.41": {
            "text": "Eure Penetrationsstrategie ist leck"
        },
        "line.42": {
            "text": "Ich wollte nie schön sein, ich bin"
        },
        "line.43": {
            "text": "Im Schatten zu gern die Stimme"
        },
        "line.44": {
            "text": "Ich ist ein"
        },
        "line.45": {
            "text": "Schwarzer Block"
        },
        "line.46": {
            "text": "Da ist doch keine Vermassung nötig"
        },
        "line.47": {
            "text": "Ich kann mich selbst revoltieren"
        },
        "line.48": {
            "text": "Mich umarmen und weiter"
        },
        "line.49": {
            "text": "Wer sein eigener schwarzer Block ist"
        },
        "line.50": {
            "text": "Weiß um den Rückblick des Lichts"
        },
        "line.51": {
            "text": "Wer Revolution fordert"
        },
        "line.52": {
            "text": "Will vernascht werden"
        },
        "line.53": {
            "text": "Wofür ich demonstriere"
        },
        "line.54": {
            "text": "Weiß keiner"
        },
        "line.55": {
            "text": "Außer mir, komm doch her"
        },
        "line.56": {
            "text": "Wenn du mehr wissen willst"
        },
        "line.57": {
            "text": "Wenn du versprichst"
        },
        "line.58": {
            "text": "Mit leeren Händen"
        },
        "line.59": {
            "text": "Deine Meinung ändern zu können"
        },
        "line.60": {
            "text": "Das Verreckbare will ich feiern"
        },
        "line.61": {
            "text": "Es ist, was uns erhält"
        },
        "line.62": {
            "text": "Mehr noch"
        },
        "line.63": {
            "text": "Was uns belebt, auch wieder-"
        },
        "line.64": {
            "text": "Lass es in Ruhe, es verreckt grade"
        },
        "line.65": {
            "text": "Ihr Kinder des Lichts"
        },
        "line.66": {
            "text": "Lasst euch nicht"
        },
        "line.67": {
            "text": "Zu Kapital schlagen"
        },
        "line.68": {
            "text": "Ihr Kinder des Lichts"
        },
        "line.69": {
            "text": "Zieht euer Schwarz aus"
        },
        "line.70": {
            "text": "Es ist keine Meinung"
        },
        "line.71": {
            "text": "Noch nötig"
        },
        "line.72": {
            "text": "Ihr habt die Verschiedenheit"
        },
        "line.73": {
            "text": "Des Schwarz noch nicht erkannt"
        },
        "line.74": {
            "text": "Folgt mir nicht"
        },
        "line.75": {
            "text": "Es gibt Pfade an Orte"
        },
        "line.76": {
            "text": "An denen ihr nichts zu sehen habt"
        },
        "line.77": {
            "text": "Keine Flamme im Auto oder Auge"
        },
        "line.78": {
            "text": "Keine Banken"
        },
        "line.79": {
            "text": "Fragt Prometheus nach dem Weg"
        },
        "line.80": {
            "text": "Und ihr seid entfacht."
        }
    }
}