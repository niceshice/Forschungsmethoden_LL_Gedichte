{
    "title": "je-est-un-autre-14798",
    "gender": "m",
    "author": "Eberhard Häfner",
    "author_birth": "1941",
    "poem": {
        "line.1": {
            "text": "All die Barmherzigkeiten segnet Gott,"
        },
        "line.2": {
            "text": "und die Welt segnet die Dichter."
        },
        "line.3": {
            "text": "( Arthur Rimbaud )"
        },
        "line.4": {
            "text": "Profile, die mich berühren"
        },
        "line.5": {
            "text": "markante Rillen zerbrochener Scheiben"
        },
        "line.6": {
            "text": "und ich höre die Paternostererbsen kreischen"
        },
        "line.7": {
            "text": "an den Pechsträhnen der Peitsche"
        },
        "line.8": {
            "text": "Kugeln in Nächten aus Blei"
        },
        "line.9": {
            "text": "erinnert mich das Vergessen"
        },
        "line.10": {
            "text": "wie das Geschlecht sich öffnet und schließt"
        },
        "line.11": {
            "text": "das Fleisch eine Frucht ohne Stiel"
        },
        "line.12": {
            "text": "ein profitables Fossil im elysischen Grab"
        },
        "line.13": {
            "text": "nur Bittermandelgeschmack"
        },
        "line.14": {
            "text": "in der Absteige  Zum Tabernakel"
        },
        "line.15": {
            "text": "auf der Hostie ein Lamm ohne Schnörkel"
        },
        "line.16": {
            "text": "sah ich durchs Schlüssellochfenster"
        },
        "line.17": {
            "text": "im narkotischen Qualm die Profile aufsteigen"
        },
        "line.18": {
            "text": "harzige Tränen des Nabatäerbaums"
        },
        "line.19": {
            "text": "Einer, den ich meine zu kennen"
        },
        "line.20": {
            "text": "im Orionnebel auf der Weihrauchstraße"
        }
    }
}