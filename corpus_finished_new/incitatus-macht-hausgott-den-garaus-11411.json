{
    "title": "incitatus-macht-hausgott-den-garaus-11411",
    "gender": "w",
    "author": "Dagmara Kraus",
    "author_birth": "1981",
    "poem": {
        "line.1": {
            "text": "heut hab ich meinen hausgott"
        },
        "line.2": {
            "text": "auf die straße geworfen"
        },
        "line.3": {
            "text": "er hat mir die hufe verprellt"
        },
        "line.4": {
            "text": "wäre er irden vielleicht hätte"
        },
        "line.5": {
            "text": "sein sprung ein herz bekommen"
        },
        "line.6": {
            "text": "oder die toga oder die nase"
        },
        "line.7": {
            "text": "die zweite etage hat er nicht"
        },
        "line.8": {
            "text": "überlebt ist flung da zerschellt"
        },
        "line.9": {
            "text": "vertreib mir so verworfene tage"
        },
        "line.10": {
            "text": "ganz schön verkommen nicht"
        }
    }
}