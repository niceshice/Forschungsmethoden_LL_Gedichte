        },
        "line.2": {
            "text": "ZWISCHNBERICHT"
        },
        "line.3": {
            "text": "gegnüber. eingelassene plattn; pro-"
        },
        "line.4": {
            "text": "tzigste heraldik. weißestn marmors"
        },
        "line.5": {
            "text": "parade: di superfette SPRACH-"
        },
        "line.6": {
            "text": "INSTALLATION."
        },
        "line.7": {
            "text": "(innenansicht außnvor: hat sichsn fürstbi-"
        },
        "line.8": {
            "text": "schof feingemacht, getäfelt, drin. drauf-"
        },
        "line.9": {
            "text": "sicht intarsienspielchn; draufsicht turm-"
        },
        "line.10": {
            "text": "ofn ALLEGORIEN; nix wi mädels"
        },
        "line.11": {
            "text": "mit blankn möpsn auffe reliefkacheln,"
        },
        "line.12": {
            "text": "hübsch glasierte ofnwärme.)"
        },
        "line.13": {
            "text": "und vor-"
        },
        "line.14": {
            "text": "geblendet. kellen, kehrbleche. aus-"
        },
        "line.15": {
            "text": "gräbersound. DIE GESCHICHTE"
        },
        "line.16": {
            "text": "HERBRETTERND AUF SACKKARREN."
        },
        "line.17": {
            "text": "der ganze weggeächzte schutt, durch-"
        },
        "line.18": {
            "text": "gesiebte sprache. dies asservieren auf"
        },
        "line.19": {
            "text": "knien; kratzen geschieht, gekratz, bürstn,"
        },
        "line.20": {
            "text": "abgepinselt. knien, nebnnander, an"
        },
        "line.21": {
            "text": "irgend (kloster)mauer bei rasselndm,"
        },
        "line.22": {
            "text": "heiser schlürfendm INDUSTRIESTAUB-"
        },
        "line.23": {
            "text": "SAUGER. so landn, schürf-schürf, schä-"
        },
        "line.24": {
            "text": "del in obstkistn marke papa clemente;"
        },
        "line.25": {
            "text": "säuberlich schädeldeckn (caput mortuum),"
        },
        "line.26": {
            "text": "sargbrettchn (pestbeständig, siena) in"
        },
        "line.27": {
            "text": "cellophantütn, auf geflattertm, windgezerr-"
        },
        "line.28": {
            "text": "tm zeitungspapier. gotisch und durch-"
        },
        "line.29": {
            "text": "numeriert. durchnumerierter"
        },
        "line.30": {
            "text": "grabungsbericht."
        },
        "line.31": {
            "text": "2"
        },
        "line.32": {
            "text": "MITSCHNITT CALVENSCHLACHT"
        },
        "line.33": {
            "text": "inzwischn: 1499. luftbilder, -spiegel,"
        },
        "line.34": {
            "text": "spiegelungn ausm engadinerkrieg. ein"
        },
        "line.35": {
            "text": "sommerlicher brückenkopp (gesprengt) so"
        },
        "line.36": {
            "text": "nimmt das wasser andre farbe an."
        },
        "line.37": {
            "text": "di gute"
        },
        "line.38": {
            "text": "geschoßverschraubte luft! im luftzug"
        },
        "line.39": {
            "text": "NAPALMHEIDE / das in den fluß"
        },
        "line.40": {
            "text": "zischt, nähte zwischen leib und panzern"
        },
        "line.41": {
            "text": "zieht (da ist ja keine landschaft mehr,"
        },
        "line.42": {
            "text": "TONLOS / hättns sollen fahren"
        },
        "line.43": {
            "text": "lassn). darüber geht di kamera, ho-"
        },
        "line.44": {
            "text": "lpernd; kamerafahrt heiser, angekro-"
        },
        "line.45": {
            "text": "chenes aug. vorm ortler. vor schi-"
        },
        "line.46": {
            "text": "mmerndn 3000ern: demolirte sand-"
        },
        "line.47": {
            "text": "sackberge (...)"
        },
        "line.48": {
            "text": "und sorgnfaltn, kummer-"
        },
        "line.49": {
            "text": "volles redaktörsgesict: ICH HÖRE EBEN!,"
        },
        "line.50": {
            "text": "ICH RUFE WILLIBALD PIRKHEIMER"
        },
        "line.51": {
            "text": "(sonst in Nürnberg stationiert)!, am sattel, AM HU-"
        },
        "line.52": {
            "text": "MANISTISCHEN SATTELLITTNTELEPHON:"
        },
        "line.53": {
            "text": "PIRKHEIMER: ja, hier pirkheimer. humanitär-"
        },
        "line.54": {
            "text": "sanitäre verhältnisse ... mit grawenvollen bildern"
        },
        "line.55": {
            "text": "zu tun ... habn hohe-luste zu zeichnen ... di"
        },
        "line.56": {
            "text": "geiselerschießung von meran ... (FADING),"
        },
        "line.57": {
            "text": "obwohl bevölkerun dagegn ... (RAUSCH-"
        },
        "line.58": {
            "text": "RAUSCH), im -iegesrausch, durchrau-"
        },
        "line.59": {
            "text": "schende flüchtlinge ... strom weg, alles weg ..."
        },
        "line.60": {
            "text": "KAISER MAXIMILIAN SOLL GEWEINT"
        },
        "line.61": {
            "text": "HABN ALS / FADING /DI KINDER ESSENZ /"
        },
        "line.62": {
            "text": "BROTKLEE VON DEN WIESN da br-"
        },
        "line.63": {
            "text": "icht"
        },
        "line.64": {
            "text": "di leitun zamm, damundherrn, in weißm rau-"
        },
        "line.65": {
            "text": "schn, klappt zusamm das bilderwelt in riesen OH-"
        },
        "line.66": {
            "text": "NELÖSCHLÖSCH. romanisch di bandnwerbun"
        },
        "line.67": {
            "text": "Gottes, tonlos di engel vorm himml. jerusalem, wei-"
        },
        "line.68": {
            "text": "ße bänder spruchbänder in händn. ohne text."
        },
        "line.69": {
            "text": "3"
        },
        "line.70": {
            "text": "STREIFNHINTERGRÜNDE"
        },
        "line.71": {
            "text": "als stünde"
        },
        "line.72": {
            "text": "dem schlüsselmann, blitzeblau,"
        },
        "line.73": {
            "text": "di bergfarbe im haar:"
        },
        "line.74": {
            "text": "hochzüng-,"
        },
        "line.75": {
            "text": "höchstzüngelnder lapislazuli (1) in"
        },
        "line.76": {
            "text": "kopfüberducktm tibet. gewölbejoch"
        },
        "line.77": {
            "text": "gewölbefeld, gewölbter scheitel."
        },
        "line.78": {
            "text": "in tagwerkn gemessener farb-"
        },
        "line.79": {
            "text": "auftrag. WIR STREIFEN"
        },
        "line.80": {
            "text": "GERADE MAL DIE HINTER-"
        },
        "line.81": {
            "text": "GRÜNDE / feuchtigkeitsein-"
        },
        "line.82": {
            "text": "brüche; ein liegn auf haupthöhe."
        },
        "line.83": {
            "text": "befallne passi, paßhöhen be-"
        },
        "line.84": {
            "text": "nannt, benommen und FEINPUTZ /"
        },
        "line.85": {
            "text": "ORIGINALPUTZ gesagt, oder"
        },
        "line.86": {
            "text": "meerpalme, zu klammem material,"
        },
        "line.87": {
            "text": "schreibmaterie hingebeugter chro-"
        },
        "line.88": {
            "text": "nist: zeitmenschen in wallun, in wand-"
        },
        "line.89": {
            "text": "lun begriffn (oberstübchens skriptorium)."
        },
        "line.90": {
            "text": "der blick der di wandfelder begattet: der"
        },
        "line.91": {
            "text": "westwand der südtreppe dem ostseite zur"
        },
        "line.92": {
            "text": "nordkappe. lippmlesend dies."
        },
        "line.93": {
            "text": "lippmlesn. strahl."
        },
        "line.94": {
            "text": "(1): aufgeriebener lasurstein mit"
        },
        "line.95": {
            "text": "10 bis 30 m großn körnern."
        }
    }
