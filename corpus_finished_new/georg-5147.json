{
    "title": "georg-5147",
    "gender": "m",
    "author": "Franzobel",
    "author_birth": "1967",
    "poem": {
        "line.1": {
            "text": "Das Blut ist A und O"
        },
        "line.2": {
            "text": "und O und A das Blut"
        },
        "line.3": {
            "text": "vom Anfang bis in die"
        },
        "line.4": {
            "text": "Aorta, bis ins Arschloch"
        },
        "line.5": {
            "text": "alles A und Orsch voll Blut"
        },
        "line.6": {
            "text": "voll Blut vom Schorsch"
        },
        "line.7": {
            "text": "sein A und O ist alles"
        },
        "line.8": {
            "text": "voll mit seinem Blut,"
        },
        "line.9": {
            "text": "das ist genug, zuviel"
        },
        "line.10": {
            "text": "schon A und O und Blut"
        },
        "line.11": {
            "text": "zuviel schon ausgelaufner"
        },
        "line.12": {
            "text": "Schorsch, denkt sich der"
        },
        "line.13": {
            "text": "Drache, geht und wird"
        },
        "line.14": {
            "text": "erstochen. Hinterr√ºcks."
        },
        "line.15": {
            "text": "Vom Schorsch."
        }
    }
}