{
    "title": "september-6041",
    "gender": "w",
    "author": "Ilma Rakusa",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Tumber Sonntag. Die Leitung schweigt."
        },
        "line.2": {
            "text": "Und Schneeluft in den Gärten. Kalt."
        },
        "line.3": {
            "text": "Der neunte Monat altert vor sich hin"
        },
        "line.4": {
            "text": "fatal und ohne Farben. Es ist schon"
        },
        "line.5": {
            "text": "spät. Zu spät vielleicht. Und klamm."
        },
        "line.6": {
            "text": "Am Ebereschenbaum die Beeren"
        },
        "line.7": {
            "text": "wehren keine Vögel ab. Der Stamm"
        },
        "line.8": {
            "text": "der Birke keine Spechte. Flechten"
        },
        "line.9": {
            "text": "überall. Ein Rechen. Die Gartenstühle"
        },
        "line.10": {
            "text": "schlaff und weiss. Verzichtbar ist der"
        },
        "line.11": {
            "text": "Tisch. Die Leere kommt. Geheiss."
        }
    }
}