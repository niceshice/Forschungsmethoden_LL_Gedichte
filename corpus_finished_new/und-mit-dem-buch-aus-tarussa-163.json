{
    "title": "und-mit-dem-buch-aus-tarussa-163",
    "gender": "m",
    "author": "Paul Celan",
    "author_birth": "1920",
    "poem": {
        "line.1": {
            "text": ""
        },
        "line.2": {
            "text": "(Marina Zwetajewa)"
        },
        "line.3": {
            "text": "Vom"
        },
        "line.4": {
            "text": "Sternbild des Hundes, vom"
        },
        "line.5": {
            "text": "Hellstern darin und der Zwerg-"
        },
        "line.6": {
            "text": "leuchte, die mitwebt"
        },
        "line.7": {
            "text": "an erdwärts gespiegelten Wegen,"
        },
        "line.8": {
            "text": "von"
        },
        "line.9": {
            "text": "Pilgerstäben, auch dort, von Südlichem, fremd"
        },
        "line.10": {
            "text": "und nachtfasernah"
        },
        "line.11": {
            "text": "wie unbestattete Worte,"
        },
        "line.12": {
            "text": "streunend"
        },
        "line.13": {
            "text": "im Bannkreis erreichter"
        },
        "line.14": {
            "text": "Ziele und Stelen und Wiegen."
        },
        "line.15": {
            "text": "Von"
        },
        "line.16": {
            "text": "Wahr- und Voraus- und Vorüber-zu-dir-,"
        },
        "line.17": {
            "text": "von"
        },
        "line.18": {
            "text": "Hinaufgesagtem,"
        },
        "line.19": {
            "text": "das dort bereitliegt, einem"
        },
        "line.20": {
            "text": "der eigenen Herzsteine gleich, die man ausspie"
        },
        "line.21": {
            "text": "mitsamt ihren un-"
        },
        "line.22": {
            "text": "verwüstlichen Uhrwerk, hinaus"
        },
        "line.23": {
            "text": "in Unland und Unzeit. Von solchem"
        },
        "line.24": {
            "text": "Ticken und Ticken inmitten"
        },
        "line.25": {
            "text": "der Kies-Kuben mit"
        },
        "line.26": {
            "text": "der auf Hyänenspur rückwärts,"
        },
        "line.27": {
            "text": "aufwärts verfolgbaren"
        },
        "line.28": {
            "text": "Ahnen-"
        },
        "line.29": {
            "text": "reihe Derer-"
        },
        "line.30": {
            "text": "vom-Namen-und-Seiner-"
        },
        "line.31": {
            "text": "Rundschlucht."
        },
        "line.32": {
            "text": "Von"
        },
        "line.33": {
            "text": "einem Baum, von einem."
        },
        "line.34": {
            "text": "Ja, auch von ihm. Und vom Wald um ihn her. Vom Wald"
        },
        "line.35": {
            "text": "Unbetreten, vom"
        },
        "line.36": {
            "text": "Gedanken, dem er entwuchs, als Laut"
        },
        "line.37": {
            "text": "und Halblaut und Ablaut und Auslaut, skythisch"
        },
        "line.38": {
            "text": "zusammengereimt"
        },
        "line.39": {
            "text": "im Takt"
        },
        "line.40": {
            "text": "der Verschlagenen-Schläfe,"
        },
        "line.41": {
            "text": "mit"
        },
        "line.42": {
            "text": "geatmeten Steppen-"
        },
        "line.43": {
            "text": "halmen geschrieben ins Herz"
        },
        "line.44": {
            "text": "der Stundenzäsur  in das Reich,"
        },
        "line.45": {
            "text": "in der Reiche"
        },
        "line.46": {
            "text": "weitestes, in"
        },
        "line.47": {
            "text": "den Großbinnenreim"
        },
        "line.48": {
            "text": "jenseits"
        },
        "line.49": {
            "text": "der Stummvölker-Zone, in dich"
        },
        "line.50": {
            "text": "Sprachwaage, Wortwaage, Heimat-"
        },
        "line.51": {
            "text": "waage Exil."
        },
        "line.52": {
            "text": "Von diesem Baum, diesem Wald."
        },
        "line.53": {
            "text": "Von der Brücken-"
        },
        "line.54": {
            "text": "quader, von der"
        },
        "line.55": {
            "text": "er ins Leben hinüber-"
        },
        "line.56": {
            "text": "prallte, flügge"
        },
        "line.57": {
            "text": "von Wunden, - vom"
        },
        "line.58": {
            "text": "Pont Mirabeau."
        },
        "line.59": {
            "text": "Wo die Oka nicht mitfließt. Et quels"
        },
        "line.60": {
            "text": "amours! (Kyrillisches, Freunde, auch das"
        },
        "line.61": {
            "text": "ritt ich über die Seine,"
        },
        "line.62": {
            "text": "ritts übern Rhein.)"
        },
        "line.63": {
            "text": "Von einem Brief, von ihm."
        },
        "line.64": {
            "text": "Vom Ein-Brief, vom Ost-Brief. Vom harten,"
        },
        "line.65": {
            "text": "winzigen Worthaufen, vom"
        },
        "line.66": {
            "text": "unbewaffneten Auge, das er"
        },
        "line.67": {
            "text": "den drei"
        },
        "line.68": {
            "text": "Gürtelsternen Orions  Jakobs-"
        },
        "line.69": {
            "text": "stab, du,"
        },
        "line.70": {
            "text": "abermals kommst du gegangen!"
        },
        "line.71": {
            "text": "zuführt auf der"
        },
        "line.72": {
            "text": "Himmelskarte, die sich ihm aufschlug."
        },
        "line.73": {
            "text": "Vom Tisch, wo das geschah."
        },
        "line.74": {
            "text": "Von einem Wort, aus dem Haufen,"
        },
        "line.75": {
            "text": "an dem er, der Tisch,"
        },
        "line.76": {
            "text": "zur Ruderbank wurde, vom Oka-Fluß her"
        },
        "line.77": {
            "text": "und den Wassern."
        },
        "line.78": {
            "text": "Vom Nebenwort, das"
        },
        "line.79": {
            "text": "ein Ruderknecht nachknirscht, ins Spätsommerohr"
        },
        "line.80": {
            "text": "seiner hell-"
        },
        "line.81": {
            "text": "hörigen Dolle:"
        },
        "line.82": {
            "text": "Kolchis."
        }
    }
}