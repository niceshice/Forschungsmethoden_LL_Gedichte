{
    "title": "plakatkunst-plakatkunst-plakatkunst-12838",
    "gender": "w",
    "author": "Margret Kreidl",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "PLAKATKUNST"
        },
        "line.2": {
            "text": "Hör auf zu weinen!"
        },
        "line.3": {
            "text": "Lass die Straße laufen!"
        },
        "line.4": {
            "text": "Schreib ein Gedicht für Hasen!"
        },
        "line.5": {
            "text": "Einfache Erklärung: Der Traum ist eine Wunscherfüllung,"
        },
        "line.6": {
            "text": "sagt Sigmund Freud."
        },
        "line.7": {
            "text": "PLAKATKUNST"
        },
        "line.8": {
            "text": "Denk nicht an China!"
        },
        "line.9": {
            "text": "Trink Pfirsichsaft!"
        },
        "line.10": {
            "text": "Tröste die Falschen!"
        },
        "line.11": {
            "text": "Einfache Erklärung: Der Traum ist eine Wunscherfüllung,"
        },
        "line.12": {
            "text": "sagt Sigmund Freud."
        },
        "line.13": {
            "text": "PLAKATKUNST"
        },
        "line.14": {
            "text": "Glaub an deinen Feiertag!"
        },
        "line.15": {
            "text": "Handle schnell!"
        },
        "line.16": {
            "text": "Kümmere dich nicht um die Zahnpasta!"
        },
        "line.17": {
            "text": "Einfache Erklärung: Der Traum ist eine Wunscherfüllung,"
        },
        "line.18": {
            "text": "sagt Sigmund Freud."
        }
    }
}