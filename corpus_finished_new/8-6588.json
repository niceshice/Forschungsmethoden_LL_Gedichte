{
    "title": "8-6588",
    "gender": "m",
    "author": "Matthias  Göritz",
    "author_birth": "1969",
    "poem": {
        "line.1": {
            "text": "Gedichte gehn ins Blau"
        },
        "line.2": {
            "text": "das Monster zeigt"
        },
        "line.3": {
            "text": "noch bin ich nicht"
        },
        "line.4": {
            "text": "im Nichts"
        },
        "line.5": {
            "text": "die schöne Stelle hier"
        },
        "line.6": {
            "text": "im Blau"
        },
        "line.7": {
            "text": "zeigt mir das Monster an"
        },
        "line.8": {
            "text": "das sagt"
        },
        "line.9": {
            "text": "ich bin"
        },
        "line.10": {
            "text": "im Bau"
        },
        "line.11": {
            "text": "an dieser Stelle hier"
        },
        "line.12": {
            "text": "entsteht"
        },
        "line.13": {
            "text": "noch"
        },
        "line.14": {
            "text": "ein Gedicht"
        }
    }
}