{
    "title": "vom-nutzen-der-brombeere-480",
    "gender": "m",
    "author": "Günter Saalmann",
    "author_birth": "1936",
    "poem": {
        "line.1": {
            "text": "Emsig bücken."
        },
        "line.2": {
            "text": "Nur die schwarzen pflücken."
        },
        "line.3": {
            "text": "Möglichst wenig naschen."
        },
        "line.4": {
            "text": "Zu Hause gut waschen."
        },
        "line.5": {
            "text": "Auf kleiner Hitze garen"
        },
        "line.6": {
            "text": "- Strom sparen!"
        },
        "line.7": {
            "text": "DurchTuch oder Sieb drücken,"
        },
        "line.8": {
            "text": "den Saft weiter eindicken."
        },
        "line.9": {
            "text": "Der Mühe Lohn:"
        },
        "line.10": {
            "text": "Brom!"
        },
        "line.11": {
            "text": "Brom füllt man in kleine Dosen."
        },
        "line.12": {
            "text": "In kleinen Dosen"
        },
        "line.13": {
            "text": "hilft Brom Schlaflosen."
        }
    }
}