{
    "title": "vor-einem-hellen-vokal-13802",
    "gender": "w",
    "author": "Yoko Tawada",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "Glei"
        },
        "line.2": {
            "text": "ch"
        },
        "line.3": {
            "text": "werde ich meinen"
        },
        "line.4": {
            "text": "Bau"
        },
        "line.5": {
            "text": "ch"
        },
        "line.6": {
            "text": "zeigen und tanzen an einem"
        },
        "line.7": {
            "text": "Tei"
        },
        "line.8": {
            "text": "ch"
        },
        "line.9": {
            "text": "wo eine deutsche"
        },
        "line.10": {
            "text": "Ei"
        },
        "line.11": {
            "text": "ch"
        },
        "line.12": {
            "text": "e steht. Ein gottloses"
        },
        "line.13": {
            "text": "Bu"
        },
        "line.14": {
            "text": "ch"
        },
        "line.15": {
            "text": "werde ich"
        },
        "line.16": {
            "text": "eu"
        },
        "line.17": {
            "text": "ch"
        },
        "line.18": {
            "text": "schreiben und steige"
        },
        "line.19": {
            "text": "ho"
        },
        "line.20": {
            "text": "ch"
        },
        "line.21": {
            "text": "auf den Galgen. Ich bin ein fliegender"
        },
        "line.22": {
            "text": "Teppi"
        },
        "line.23": {
            "text": "ch"
        },
        "line.24": {
            "text": "mit einem"
        },
        "line.25": {
            "text": "Kopftu"
        },
        "line.26": {
            "text": "ch"
        },
        "line.27": {
            "text": ". So ein"
        },
        "line.28": {
            "text": "Pe"
        },
        "line.29": {
            "text": "ch"
        },
        "line.30": {
            "text": "! Kann ich fliehen? Kennst du das Land"
        },
        "line.31": {
            "text": "CH"
        },
        "line.32": {
            "text": "?"
        },
        "line.33": {
            "text": "Die Lesart der heiligen S-"
        },
        "line.34": {
            "text": "chriftzeichen"
        },
        "line.35": {
            "text": "c"
        },
        "line.36": {
            "text": "und"
        },
        "line.37": {
            "text": "h"
        },
        "line.38": {
            "text": "bleibt weiter offen"
        }
    }
}