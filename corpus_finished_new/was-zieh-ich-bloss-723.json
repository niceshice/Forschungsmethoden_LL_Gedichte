{
    "title": "was-zieh-ich-bloss-723",
    "gender": "w",
    "author": "Sabine Scho",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "Genügt Strassenanzug"
        },
        "line.2": {
            "text": "beim"
        },
        "line.3": {
            "text": "djeuner sur lherbe"
        },
        "line.4": {
            "text": "aus süßem Hefeteig, mitviel Butter"
        },
        "line.5": {
            "text": "Brioche im Grünen, Eigenschaften"
        },
        "line.6": {
            "text": "gut verbacken verletzen das"
        },
        "line.7": {
            "text": "öffentliche Schamgefühl"
        },
        "line.8": {
            "text": "Kaisers Kleiderordnung steht, kommt"
        },
        "line.9": {
            "text": "zum zweiten Male hoch: Brumaire"
        },
        "line.10": {
            "text": "das zergeht, Zungenzergang"
        },
        "line.11": {
            "text": "Evas Konsumtion, ihr ungeschnürtes"
        },
        "line.12": {
            "text": "Zünglein ~ unverschämt nah dran"
        },
        "line.13": {
            "text": "mittelfetter Vogel der Gattung"
        },
        "line.14": {
            "text": "turdus"
        },
        "line.15": {
            "text": "lüftet seine Lungen (schweres Zwerch-"
        },
        "line.16": {
            "text": "fellatmen) und brummt (Dolby-A-Sound)"
        },
        "line.17": {
            "text": "keine Resonanz, Schwarzdrossel, du rauschst"
        },
        "line.18": {
            "text": "bloß"
        },
        "line.19": {
            "text": "was zieh ich an?"
        }
    }
}