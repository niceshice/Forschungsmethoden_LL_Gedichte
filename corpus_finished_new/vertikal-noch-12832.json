{
    "title": "vertikal-noch-12832",
    "gender": "m",
    "author": "Rainer René Mueller",
    "author_birth": "0",
    "poem": {
        "line.1": {
            "text": "... noch niedriger, zum Knien, zum,"
        },
        "line.2": {
            "text": "zum hinunter Beugen, zum Hinknien"
        },
        "line.3": {
            "text": ": tiefer, noch, noch"
        },
        "line.4": {
            "text": "mehr, das Riechen dessen, was"
        },
        "line.5": {
            "text": "da hochkommt: wächst, sich"
        },
        "line.6": {
            "text": "aus Eins und Nichts, aus Licht"
        },
        "line.7": {
            "text": "ins Grün schiebt, blüht :"
        },
        "line.8": {
            "text": "..."
        },
        "line.9": {
            "text": "und seine frucht bringt zu seiner zeit\""
        },
        "line.10": {
            "text": ":"
        },
        "line.11": {
            "text": "dieses Geringste z.b. der Mohn, oder:"
        },
        "line.12": {
            "text": "dieses von Dreck überschüttete Stück-"
        },
        "line.13": {
            "text": "chen Boden, Brache,"
        },
        "line.14": {
            "text": "Ö"
        },
        "line.15": {
            "text": "dnis"
        },
        "line.16": {
            "text": "genannt:"
        },
        "line.17": {
            "text": "da raus, drüber, beugt sich, hängt"
        },
        "line.18": {
            "text": "hinunter, bis zur Berührung :"
        },
        "line.19": {
            "text": "Basaltschotter: Schmetterlingsflieder, nicht"
        },
        "line.20": {
            "text": "Strelitzie,... Blütchen für Blütchen,"
        },
        "line.21": {
            "text": "heliotrop;"
        },
        "line.22": {
            "text": "auch das Ausblühn, nach"
        },
        "line.23": {
            "text": "oben, wenn's schließlich zur Ver -,"
        },
        "line.24": {
            "text": "-kommt-, zur Verrottung: vertikal..."
        },
        "line.25": {
            "text": "... nicht getilgt: das Staunen,"
        },
        "line.26": {
            "text": "aristotelisch"
        }
    }
}