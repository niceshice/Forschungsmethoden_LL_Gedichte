{
    "title": "futur-ii-279",
    "gender": "w",
    "author": "Orsolya Kalász",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "Auf den gelenkigen Rücken"
        },
        "line.2": {
            "text": "junger Hunde"
        },
        "line.3": {
            "text": "jagen"
        },
        "line.4": {
            "text": "vor unseren Augen"
        },
        "line.5": {
            "text": "jene Momente"
        },
        "line.6": {
            "text": "des Glücks"
        },
        "line.7": {
            "text": "über die Parkwiese"
        },
        "line.8": {
            "text": "die wir einst"
        },
        "line.9": {
            "text": "gehabt haben werden."
        }
    }
}