{
    "title": "mars-hypothese-4365",
    "gender": "m",
    "author": "Christian Schloyer",
    "author_birth": "1976",
    "poem": {
        "line.1": {
            "text": "die verschmolzenen über"
        },
        "line.2": {
            "text": "reste eines exorbi-"
        },
        "line.3": {
            "text": "tanten sand"
        },
        "line.4": {
            "text": "burgensterbens wasser mars"
        },
        "line.5": {
            "text": "mikroben mars"
        },
        "line.6": {
            "text": "libellen + narbenmeer"
        },
        "line.7": {
            "text": "eigentlich will"
        },
        "line.8": {
            "text": "ich dich haben weil du so"
        },
        "line.9": {
            "text": "schön bist"
        }
    }
}