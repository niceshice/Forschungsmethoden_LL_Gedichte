{
    "title": "schaffenskrise-12767",
    "gender": "m",
    "author": "F. W. Bernstein",
    "author_birth": "1938",
    "poem": {
        "line.1": {
            "text": "Ach wie soll ich Schönes schreiben"
        },
        "line.2": {
            "text": "wenns nicht geht."
        },
        "line.3": {
            "text": "Wie kann ich denn Lyrik treiben"
        },
        "line.4": {
            "text": "wenns so um mich steht?"
        },
        "line.5": {
            "text": "Nicht mal etwas Geniales"
        },
        "line.6": {
            "text": "fällt mir ein -"
        },
        "line.7": {
            "text": "Ach es ist ja so egal - es"
        },
        "line.8": {
            "text": "soll nicht sein."
        },
        "line.9": {
            "text": "Was ich hab? Man kenns! Man nennts"
        },
        "line.10": {
            "text": "kreative Impotenz."
        },
        "line.11": {
            "text": "Und davon kommt diese"
        },
        "line.12": {
            "text": "sogenannte Schaffenskrise."
        },
        "line.13": {
            "text": "Hier mein Bericht."
        },
        "line.14": {
            "text": "Und ist das nicht"
        },
        "line.15": {
            "text": "ein Gedicht?"
        }
    }
}