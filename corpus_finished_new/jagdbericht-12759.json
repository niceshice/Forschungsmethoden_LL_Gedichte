{
    "title": "jagdbericht-12759",
    "gender": "m",
    "author": "F. W. Bernstein",
    "author_birth": "1938",
    "poem": {
        "line.1": {
            "text": "Im Hinterhof Kaninchenjagd"
        },
        "line.2": {
            "text": "Kaninchen hat sich freigenagt"
        },
        "line.3": {
            "text": "aus seinem Häuschen ausgebrochen"
        },
        "line.4": {
            "text": "rausgekrochen"
        },
        "line.5": {
            "text": "Flüchtling wird entdeckt"
        },
        "line.6": {
            "text": "hat sich schlecht versteckt"
        },
        "line.7": {
            "text": "es hüpft im Hinterhof HOLLA"
        },
        "line.8": {
            "text": "schon sind fünf Hasen Jäger da."
        },
        "line.9": {
            "text": "Kaninchen wird gejagt, und das"
        },
        "line.10": {
            "text": "hüpft, schlägt Haken, macht viel Spass."
        },
        "line.11": {
            "text": "Die Jäger laufen, man hört sie keuchen"
        },
        "line.12": {
            "text": "wie sie das freie Kaninchen scheuchen."
        },
        "line.13": {
            "text": "Wir wollen die Jäger jagen lassen,"
        },
        "line.14": {
            "text": "und uns mit wichtigeren Themen befassen:"
        },
        "line.15": {
            "text": "mit Genen, Terror, Zeitkritik"
        },
        "line.16": {
            "text": "Korruption und Politik"
        },
        "line.17": {
            "text": "Attentate und dann diese"
        },
        "line.18": {
            "text": "Pi pa Po Parteienkrise"
        },
        "line.19": {
            "text": "die FDP, die CSU"
        },
        "line.20": {
            "text": "die Linken geben auch keine Ruh"
        },
        "line.21": {
            "text": "Wir unterbrechen- ein neuer Bericht:"
        },
        "line.22": {
            "text": "sie haben das Kaninchen immer noch nicht"
        },
        "line.23": {
            "text": "Wir halten sie auf dem Laufenden und"
        },
        "line.24": {
            "text": "zur Beunruhigung besteht bis jetzt gar kein Grund"
        }
    }
}