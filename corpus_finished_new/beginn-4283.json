{
    "title": "beginn-4283",
    "gender": "w",
    "author": "Brigitte Struzyk",
    "author_birth": "1946",
    "poem": {
        "line.1": {
            "text": "Wieder und wieder"
        },
        "line.2": {
            "text": "am Anfang am Ende"
        },
        "line.3": {
            "text": "Dichter beflecken sich"
        },
        "line.4": {
            "text": "selten mit Ruhm"
        },
        "line.5": {
            "text": "Sucht sucht"
        },
        "line.6": {
            "text": "die andere Form"
        },
        "line.7": {
            "text": "von Gewinn"
        },
        "line.8": {
            "text": "Power Flower"
        },
        "line.9": {
            "text": "Dauer Trauer"
        },
        "line.10": {
            "text": "Kommentiert der Bauer"
        },
        "line.11": {
            "text": "Wenn er kompostiert"
        }
    }
}