{
    "title": "lumpenbrust-11528",
    "gender": "w",
    "author": "Ulrike Draesner",
    "author_birth": "1962",
    "poem": {
        "line.1": {
            "text": "brchnchsp"
        },
        "line.2": {
            "text": "brchnchspr"
        },
        "line.3": {
            "text": "brchnchsprch"
        },
        "line.4": {
            "text": "brach nicht sprüh"
        },
        "line.5": {
            "text": "bauch nicht früh"
        },
        "line.6": {
            "text": "brauch nicht sprech"
        },
        "line.7": {
            "text": "lippkarü! lippkarü!"
        },
        "line.8": {
            "text": "im schnabel dickfleischige"
        },
        "line.9": {
            "text": "zunge. schnabel aufgemacht"
        },
        "line.10": {
            "text": "schnabel zugemacht. zunge."
        },
        "line.11": {
            "text": "saßen als tulpengemälde auf"
        },
        "line.12": {
            "text": "der stange, drei millionen gulden"
        },
        "line.13": {
            "text": "wert. husch: viktorianisch kolonial"
        },
        "line.14": {
            "text": "über gußeiserner wanne zwischen"
        },
        "line.15": {
            "text": "gemalten frauengesichtern, pudern"
        },
        "line.16": {
            "text": "und nelken. husch: schaukelnde"
        },
        "line.17": {
            "text": "früchte, die farben gauguin."
        },
        "line.18": {
            "text": "der krallenabdruck eine woche"
        },
        "line.19": {
            "text": "auf meinem arm. so zweigeteilt"
        },
        "line.20": {
            "text": "diese brüter schnäbler äuger"
        },
        "line.21": {
            "text": "zu spüren: der körper"
        },
        "line.22": {
            "text": "eine ganz und gar verborgene"
        },
        "line.23": {
            "text": "dünnknochige gestalt, klein"
        },
        "line.24": {
            "text": "darüber die liegende dichte federkleid"
        },
        "line.25": {
            "text": "wie es ihre stark atmende brust"
        },
        "line.26": {
            "text": "ihre lümpchen von sprache"
        },
        "line.27": {
            "text": "umschloss."
        },
        "line.28": {
            "text": "sie aber, die füße rückend, watschelten"
        },
        "line.29": {
            "text": "wie behindert über die käfigstange"
        },
        "line.30": {
            "text": "ins abendlicht. mond und sterne"
        },
        "line.31": {
            "text": "verblassten vor ihrer brillanz"
        },
        "line.32": {
            "text": "die allein aus der verdauung"
        },
        "line.33": {
            "text": "von blut zu farbe rührt."
        },
        "line.34": {
            "text": "rühren kann."
        },
        "line.35": {
            "text": "leise sie die schnäbel"
        },
        "line.36": {
            "text": "stießen:"
        },
        "line.37": {
            "text": "angesichts des regenwaldes"
        },
        "line.38": {
            "text": "seele nicht nötig"
        },
        "line.39": {
            "text": ". too lush"
        },
        "line.40": {
            "text": "der wald. lush, falscher"
        },
        "line.41": {
            "text": "freund."
        },
        "line.42": {
            "text": "tracht sprache pracht. krallen"
        },
        "line.43": {
            "text": "druck. lampenlust in"
        },
        "line.44": {
            "text": "lupenbrust"
        },
        "line.45": {
            "text": "lumpenblüh"
        },
        "line.46": {
            "text": "lippkarü"
        }
    }
}