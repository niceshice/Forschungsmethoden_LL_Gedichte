{
    "title": "wule-waschbaers-fingerlied-1427",
    "gender": "m",
    "author": "Fredrik Vahle",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "Flinke Finger, flinke Finger,"
        },
        "line.2": {
            "text": "jede Hand hat fünf so Dinger."
        },
        "line.3": {
            "text": "Können kitzeln, können bohren"
        },
        "line.4": {
            "text": "in der Nase, in den Ohren."
        },
        "line.5": {
            "text": "Können drücken, können tupfen,"
        },
        "line.6": {
            "text": "können dich ganz sachte zupfen."
        },
        "line.7": {
            "text": "Können zeigen, können tippen,"
        },
        "line.8": {
            "text": "können kratzen, können schnippen."
        },
        "line.9": {
            "text": "Können klopfen, können kneifen,"
        },
        "line.10": {
            "text": "und man braucht sie immerzu,"
        },
        "line.11": {
            "text": "um das Leben zu begreifen."
        }
    }
}