{
    "title": "nach-guenter-eich-und-immanuel-kant-15640",
    "gender": "m",
    "author": "Wilhelm  Bartsch",
    "author_birth": "1950",
    "poem": {
        "line.1": {
            "text": "Bin ich ausgehöhlt nun von Termiten,"
        },
        "line.2": {
            "text": "Absturzschreiber nur, Black Box?"
        },
        "line.3": {
            "text": "Drohnen über mir und Satelliten,"
        },
        "line.4": {
            "text": "in mir das orakelnde Gesocks."
        },
        "line.5": {
            "text": "2017"
        }
    }
}