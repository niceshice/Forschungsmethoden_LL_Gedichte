{
    "title": "reh-sleeping-not-sleeping-5212",
    "gender": "m",
    "author": "Tom Schulz",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "in der 1002. Nacht"
        },
        "line.2": {
            "text": "wir durchpflügten gerade das Herz"
        },
        "line.3": {
            "text": "Bergwerk, wir Lippenblütler"
        },
        "line.4": {
            "text": "voll schamloser Lippen"
        },
        "line.5": {
            "text": "wir entschädigungslosen Kalifen die"
        },
        "line.6": {
            "text": "mit dem Einhorn"
        },
        "line.7": {
            "text": "as such"
        },
        "line.8": {
            "text": "Diebesgut aus jenem Samarkand"
        },
        "line.9": {
            "text": "ich treibe die zehntausend Kamele auf"
        },
        "line.10": {
            "text": "löse den Wechsel ein, die Verlobungs"
        },
        "line.11": {
            "text": "Ringe im Kanton"
        },
        "line.12": {
            "text": "Seerosensee"
        },
        "line.13": {
            "text": "(gestohlen) aus einem Gedicht das"
        },
        "line.14": {
            "text": "mit der Sprache schlief"
        },
        "line.15": {
            "text": "(die luftige Nummer"
        },
        "line.16": {
            "text": "bis die Lymphknoten die Röte"
        },
        "line.17": {
            "text": "des Abends dekantierten)"
        },
        "line.18": {
            "text": "wir leuchteten auf der Tonika"
        },
        "line.19": {
            "text": "des Steinkirschenbuketts, o Trauben"
        },
        "line.20": {
            "text": "Blut auf der Rotweinklaviatur"
        },
        "line.21": {
            "text": "o Süße der Birnen (Bienen)"
        },
        "line.22": {
            "text": "gebackener Klitsch der Fruchtschnitte"
        },
        "line.23": {
            "text": "lass uns davon schnalzen auf der Überton"
        },
        "line.24": {
            "text": "Leiter, lass die Immen gut sein"
        },
        "line.25": {
            "text": "(Summer des Sommers), die ramponierten"
        },
        "line.26": {
            "text": "Tiere der Beflüsterung"
        },
        "line.27": {
            "text": "für Teresa"
        }
    }
}