{
    "title": "keiner-weine-54",
    "gender": "m",
    "author": "Gottfried Benn",
    "author_birth": "1886",
    "poem": {
        "line.1": {
            "text": "Rosen, gottweißwoher so schön,"
        },
        "line.2": {
            "text": "in grünen Himmeln die Stadt"
        },
        "line.3": {
            "text": "abends"
        },
        "line.4": {
            "text": "in der Vergänglichkeit der Jahre!"
        },
        "line.5": {
            "text": "Mit welcher Sehnsucht gedenke ich der Zeit,"
        },
        "line.6": {
            "text": "wo mir eine Mark dreißig lebenswichtig waren,"
        },
        "line.7": {
            "text": "ja, notgedrungen, ich sie zählte,"
        },
        "line.8": {
            "text": "meine Tage ihnen anpassen mußte,"
        },
        "line.9": {
            "text": "was sage ich Tage: Wochen, mit Brot und Pflaumenmus"
        },
        "line.10": {
            "text": "aus irdenen Töpfen"
        },
        "line.11": {
            "text": "vom heimatlichen Dorf mitgenommen,"
        },
        "line.12": {
            "text": "noch von häuslicher Armut beschienen,"
        },
        "line.13": {
            "text": "wie weh war alles, wie schön und zitternd!"
        },
        "line.14": {
            "text": "Was soll der Glanz der europäischen Auguren,"
        },
        "line.15": {
            "text": "der großen Namen,"
        },
        "line.16": {
            "text": "der Pour le mrite,"
        },
        "line.17": {
            "text": "die auf sich sehn und weiter schaffen,"
        },
        "line.18": {
            "text": "ach, nur Vergehendes ist schön,"
        },
        "line.19": {
            "text": "rückblickend die Armut,"
        },
        "line.20": {
            "text": "sowie das Dumpfe, das sich nicht erkennt,"
        },
        "line.21": {
            "text": "schluchzt und stempeln geht,"
        },
        "line.22": {
            "text": "wunderbar dieser Hades,"
        },
        "line.23": {
            "text": "der das Dumpfe nimmt"
        },
        "line.24": {
            "text": "wie die Auguren"
        },
        "line.25": {
            "text": "keiner weine,"
        },
        "line.26": {
            "text": "keiner sage: ich, so allein."
        }
    }
}