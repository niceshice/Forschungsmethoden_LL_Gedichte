{
    "title": "eine-personenbeschreibung-219",
    "gender": "m",
    "author": "Kurt  Aebli",
    "author_birth": "1955",
    "poem": {
        "line.1": {
            "text": "E"
        },
        "line.2": {
            "text": "ine Personenbeschreibung vielleicht auch"
        },
        "line.3": {
            "text": "ein Selbstmordversuch oder San Francisco"
        },
        "line.4": {
            "text": "ohne Trottoirs ohne Reiseutensilien ganze"
        },
        "line.5": {
            "text": "Partien ausgewechselt im Stil französischer"
        },
        "line.6": {
            "text": "Vorurteile und die Katatonikerin in ihrem"
        },
        "line.7": {
            "text": "Simultanwinter merkwürdig trotz"
        },
        "line.8": {
            "text": "Leugnung mikroskopisch kleiner"
        },
        "line.9": {
            "text": "Illusionen"
        }
    }
}