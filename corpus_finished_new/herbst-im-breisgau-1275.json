{
    "title": "herbst-im-breisgau-1275",
    "gender": "w",
    "author": "Marie Luise Kaschnitz",
    "author_birth": "1901",
    "poem": {
        "line.1": {
            "text": "I"
        },
        "line.2": {
            "text": "Drei Schritte von meinem Vaterhaus"
        },
        "line.3": {
            "text": "Bin ich über meinen Schatten gesprungen."
        },
        "line.4": {
            "text": "Da hingen die Dächer firstab im Blau"
        },
        "line.5": {
            "text": "Die Linden wurzelten im Wolkenbett"
        },
        "line.6": {
            "text": "Die Toten flogen vom Weinberg auf"
        },
        "line.7": {
            "text": "Seltene Vögel."
        },
        "line.8": {
            "text": "Gekleidet in die graue Wolle der Waldrebe"
        },
        "line.9": {
            "text": "Steigt der Herbst von der Höhe."
        },
        "line.10": {
            "text": "Sitzt bei den Kindern am Wiesenfeuer."
        },
        "line.11": {
            "text": "Die braten die Frösche"
        },
        "line.12": {
            "text": "Die knacken die Schenkel"
        },
        "line.13": {
            "text": "Die schlagen wenn der Abend graut"
        },
        "line.14": {
            "text": "Aus dem wilden schwarzen Kartoffelkraut"
        },
        "line.15": {
            "text": "Funken wie Sterne."
        },
        "line.16": {
            "text": "Der Sog der Schwalben ist stärker als alles andre"
        },
        "line.17": {
            "text": "Er zieht aus der glitzernden Wiese die Zeitlose auf"
        },
        "line.18": {
            "text": "Und die Nebel die kommen und fliehen."
        },
        "line.19": {
            "text": "Weil die Stare so hoch im Himmel schrieen"
        },
        "line.20": {
            "text": "Verlassen die Bienen den Efeu"
        },
        "line.21": {
            "text": "Und die Nebel die kommen und fliehen."
        },
        "line.22": {
            "text": "Die Blätter der Linde lassen sich fallen"
        },
        "line.23": {
            "text": "Und die Blätter der Rosen."
        },
        "line.24": {
            "text": "Ein Zug dorfaus"
        },
        "line.25": {
            "text": "Die riesigen Sonnenblumen voraus"
        },
        "line.26": {
            "text": "Die wilden schwarzen Medusen."
        },
        "line.27": {
            "text": "Dem Fels im Walde steigt der Nebel zu."
        },
        "line.28": {
            "text": "Begräbt am Hang die Buchen und den Wein."
        },
        "line.29": {
            "text": "Wo sonst die rauhen Wurzeln sich verschlingen"
        },
        "line.30": {
            "text": "Hängt graues Tauwerk aus den Eisenringen."
        },
        "line.31": {
            "text": "Versteinte Muscheln färben sich opal"
        },
        "line.32": {
            "text": "Meerüber kommen die verlornen Segelschiffe"
        },
        "line.33": {
            "text": "Und die Kinder gehen schlafen in der Grotte."
        },
        "line.34": {
            "text": "Feine Skelette legen sich zur Ruh."
        },
        "line.35": {
            "text": "Im Hohlweg zieht die kleine Prozession"
        },
        "line.36": {
            "text": "Jesus aus Holz geschnitzt"
        },
        "line.37": {
            "text": "Auf dem Esel aus Holz geschnitzt."
        },
        "line.38": {
            "text": "Jesus mit rosenroten Wangen"
        },
        "line.39": {
            "text": "Die kleinen Räder knarren und singen"
        },
        "line.40": {
            "text": "Eine Krone für mich eine Krone für Dich"
        },
        "line.41": {
            "text": "Aus der roten Berberitze."
        },
        "line.42": {
            "text": "In den Springbrunnen fällt die Nacht"
        },
        "line.43": {
            "text": "Wie ein Stein vom himmel."
        },
        "line.44": {
            "text": "Schlägt dem Putto ins breite Gesicht,"
        },
        "line.45": {
            "text": "Reißt ihm die Locken herunter."
        },
        "line.46": {
            "text": "Auf der Rose dem schwankenden Lächeln"
        },
        "line.47": {
            "text": "Treiben die Fische tot."
        },
        "line.48": {
            "text": "Im grünen Osten steht der Fürst der Welt"
        },
        "line.49": {
            "text": "Die Blüte in der Hand."
        },
        "line.50": {
            "text": "Im roten Westen steigt mit Lilienhänden"
        },
        "line.51": {
            "text": "Das Fleisch gen Himmel."
        },
        "line.52": {
            "text": "Mein Bett das leichte Holz"
        },
        "line.53": {
            "text": "Treibt auf dem versandenden Strome."
        },
        "line.54": {
            "text": "Die Uhren schlagen. Keine Stunde gilt."
        },
        "line.55": {
            "text": "II"
        },
        "line.56": {
            "text": "Ausgestreckt"
        },
        "line.57": {
            "text": "Das Gesicht in die Mulde gepreßt,"
        },
        "line.58": {
            "text": "Die Hände rechts und links"
        },
        "line.59": {
            "text": "Im Wald verkrallt,"
        },
        "line.60": {
            "text": "Den Mund voll Ackerkrume,"
        },
        "line.61": {
            "text": "Quellwasser im Haar,"
        },
        "line.62": {
            "text": "Den Atem angehalten"
        },
        "line.63": {
            "text": "Nußlaubatem:"
        },
        "line.64": {
            "text": "Alles soll bleiben,"
        },
        "line.65": {
            "text": "Keiner gehe fort."
        },
        "line.66": {
            "text": "Denn dies ist ein Ort,"
        },
        "line.67": {
            "text": "Wo der Vogel im hohen Tambour,"
        },
        "line.68": {
            "text": "Der wundgeschlagenen,"
        },
        "line.69": {
            "text": "Seinen Ausweg findet."
        },
        "line.70": {
            "text": "Und dies ist ein Ort,"
        },
        "line.71": {
            "text": "Wo der Hund mit dem goldbraunen Fell,"
        },
        "line.72": {
            "text": "Der im Walde lärmt,"
        },
        "line.73": {
            "text": "Heimkehrt am Abend."
        },
        "line.74": {
            "text": "Wo die Liebe wandert"
        },
        "line.75": {
            "text": "Auf Schären des Untergangs"
        },
        "line.76": {
            "text": "Im Herzen der roten Sonne."
        },
        "line.77": {
            "text": "Aber nichts bleibt,"
        },
        "line.78": {
            "text": "Nur die Glieder"
        },
        "line.79": {
            "text": "Der Kette, die glatten, runden"
        },
        "line.80": {
            "text": "Milchweißen, fuchsfellbraunen"
        },
        "line.81": {
            "text": "Spielen mit meinen Fingern."
        },
        "line.82": {
            "text": "Glühender Kiesel"
        },
        "line.83": {
            "text": "Kühle Kastanie"
        },
        "line.84": {
            "text": "Ein Sommer"
        },
        "line.85": {
            "text": "Ein Winter"
        },
        "line.86": {
            "text": "Ein Sommer."
        },
        "line.87": {
            "text": "Meine Inseln blühen mir auf"
        },
        "line.88": {
            "text": "Im grauen Verputz der Mauer."
        },
        "line.89": {
            "text": "Meine Briefe schreibe ich"
        },
        "line.90": {
            "text": "Mit der leichten Forellengräte"
        },
        "line.91": {
            "text": "Über den Hügelkamm."
        },
        "line.92": {
            "text": "Abends sitz ich am Feuer,"
        },
        "line.93": {
            "text": "Bau in die Flasche"
        },
        "line.94": {
            "text": "Ein Haus, einen Brunnen, acht Linden,"
        },
        "line.95": {
            "text": "Ein Spruchband aus Schilfgras,"
        },
        "line.96": {
            "text": "Kein Wort darauf."
        },
        "line.97": {
            "text": "Denn die Schrift der Sterne wird klarer,"
        },
        "line.98": {
            "text": "Wenn die Sterne verschwinden,"
        },
        "line.99": {
            "text": "Der Leib, von den Schlangen erstickt,"
        },
        "line.100": {
            "text": "Vergißt die Schlangen,"
        },
        "line.101": {
            "text": "Die den Tod übergangen,"
        },
        "line.102": {
            "text": "Die Knöchlein"
        },
        "line.103": {
            "text": "Im Mörser tanzen und singen"
        }
    }
}