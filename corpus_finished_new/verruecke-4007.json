{
    "title": "verruecke-4007",
    "gender": "w",
    "author": "Elke Erb",
    "author_birth": "1938",
    "poem": {
        "line.1": {
            "text": "Geh doch auf die Eisscholle: klare Verhältnisse,"
        },
        "line.2": {
            "text": "geh vom Unort, fort auf die Eisscholle und dann"
        },
        "line.3": {
            "text": "die nächste nach der ersterwähnten"
        },
        "line.4": {
            "text": "(klare Verhältnisse). Verrücke"
        },
        "line.5": {
            "text": "die eigene Position, dort"
        },
        "line.6": {
            "text": "befindest du dich,"
        },
        "line.7": {
            "text": "kein Fazit."
        },
        "line.8": {
            "text": "Passate, die Eisschollen wandern so auch, das Wasser"
        },
        "line.9": {
            "text": "wallt, wellt, wogt; kommt nicht mehr auf? Musik?"
        },
        "line.10": {
            "text": "Die Idee der Idee, Mondlicht, Veilchenduft"
        },
        "line.11": {
            "text": "(eines Körbchens, am Markt früh,"
        },
        "line.12": {
            "text": "in dem noch nassen Grase"
        },
        "line.13": {
            "text": "gepflückt, unter feinen"
        },
        "line.14": {
            "text": "Federwölkchen.)"
        },
        "line.15": {
            "text": "21.11.04"
        }
    }
}