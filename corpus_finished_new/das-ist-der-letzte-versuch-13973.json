{
    "title": "das-ist-der-letzte-versuch-13973",
    "gender": "w",
    "author": "Andra Schwarz",
    "author_birth": "1982",
    "poem": {
        "line.1": {
            "text": "Das ist der letzte versuch: weit hinter uns"
        },
        "line.2": {
            "text": "aufgerauhtes land im kiefer sitzt seit tagen die"
        },
        "line.3": {
            "text": "eingefrorene zunge ich trete hinaus bis an den rand"
        },
        "line.4": {
            "text": "stürzt licht hinab wir schwanken schürfen nach"
        },
        "line.5": {
            "text": "im untergrund was hier versickert"
        },
        "line.6": {
            "text": "wer geht"
        },
        "line.7": {
            "text": ","
        },
        "line.8": {
            "text": "wem folgen wir"
        },
        "line.9": {
            "text": "zieht sich das netz zusammen"
        },
        "line.10": {
            "text": "angewachsene füße die fährten ausgespäht"
        },
        "line.11": {
            "text": "das watt der saum von dem du lebst und"
        },
        "line.12": {
            "text": "eine hand die lange danach noch zittert"
        }
    }
}