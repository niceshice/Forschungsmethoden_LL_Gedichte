{
    "title": "milchstrassentraene-bronze-gegossen-14813",
    "gender": "m",
    "author": "Paul-Henri  Campbell",
    "author_birth": "1982",
    "poem": {
        "line.1": {
            "text": "nur sterne sind noch schön"
        },
        "line.2": {
            "text": "junge spiegelungen dunkel und unruhig teleskope"
        },
        "line.3": {
            "text": "wundern sich auf offnem feld durch sentimentale"
        },
        "line.4": {
            "text": "geschärfte linsen im licht"
        },
        "line.5": {
            "text": "sehen"
        },
        "line.6": {
            "text": "galaxienstaub schleiernd weit draußen"
        },
        "line.7": {
            "text": "wie wund violet nebelweiß kupfern"
        },
        "line.8": {
            "text": "stumm über schwarze haut der nacht"
        },
        "line.9": {
            "text": "sehen"
        },
        "line.10": {
            "text": "nur geometrie erahnt sie"
        },
        "line.11": {
            "text": "vom boden herauf ragt fußt im quadrat der sockel"
        },
        "line.12": {
            "text": "er hebt oder sagt man erhebt sich trägt zeigt figur"
        },
        "line.13": {
            "text": "die drittelgegossene träne"
        },
        "line.14": {
            "text": "sieh"
        },
        "line.15": {
            "text": "legere legiert mit hohem kupfergehalt"
        },
        "line.16": {
            "text": "damit ovid recht behält knall in der spät-"
        },
        "line.17": {
            "text": "zeit urform blechkapellchen zinnsoldat"
        },
        "line.18": {
            "text": "sieh"
        },
        "line.19": {
            "text": "alle engel sind fort doch in tränen ist noch wärme"
        },
        "line.20": {
            "text": "entrückt kullert fern ihr licht erreicht uns als trauer"
        },
        "line.21": {
            "text": "in milchstraßen weint ein magnetismus der körper"
        }
    }
}