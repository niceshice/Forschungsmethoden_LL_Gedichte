{
    "title": "ich-8400",
    "gender": "w",
    "author": "Doris Runge",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "sagt diese stimme"
        },
        "line.2": {
            "text": "mein erbstück"
        },
        "line.3": {
            "text": "mein wortführer"
        },
        "line.4": {
            "text": "mein lispelnder versucher"
        },
        "line.5": {
            "text": "mein mönchischer bruder"
        },
        "line.6": {
            "text": "ich teile deine zelle"
        },
        "line.7": {
            "text": "die verrückten früchte"
        },
        "line.8": {
            "text": "deiner hirnschale"
        },
        "line.9": {
            "text": "ich bin deine hand"
        },
        "line.10": {
            "text": "dein mund"
        },
        "line.11": {
            "text": "ich bin da"
        },
        "line.12": {
            "text": "wenn der kuß fort ist"
        },
        "line.13": {
            "text": "(dein bißchen glück war immer sprachlos)"
        },
        "line.14": {
            "text": "ich zimmre dir ein leben"
        },
        "line.15": {
            "text": "beleg dich rückwärts mit daten"
        },
        "line.16": {
            "text": "beschönige verschweige"
        },
        "line.17": {
            "text": "das macht sinn"
        },
        "line.18": {
            "text": "ich war dein himmelschreiender anfang"
        },
        "line.19": {
            "text": "deine greisenhafte Verweigerung"
        },
        "line.20": {
            "text": "dein trotziges ja hier bin ich"
        },
        "line.21": {
            "text": "und ich bin"
        },
        "line.22": {
            "text": "auf dem rückzug"
        },
        "line.23": {
            "text": "langsam"
        },
        "line.24": {
            "text": "unaufhaltsam"
        }
    }
}