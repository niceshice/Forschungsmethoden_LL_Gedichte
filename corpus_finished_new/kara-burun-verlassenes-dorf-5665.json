{
    "title": "kara-burun-verlassenes-dorf-5665",
    "gender": "m",
    "author": "Manfred Peter Hein",
    "author_birth": "1931",
    "poem": {
        "line.1": {
            "text": "Gras am Felssturz"
        },
        "line.2": {
            "text": "Schwarzer Berg"
        },
        "line.3": {
            "text": "Pfad übers Kap"
        },
        "line.4": {
            "text": "Unterm Vogellaut der Mittagshitze malvenfarbenes Meer"
        },
        "line.5": {
            "text": "Und mein Vers hier"
        },
        "line.6": {
            "text": "Ich bin nicht Iskender"
        },
        "line.7": {
            "text": "Und wo ist Asien"
        },
        "line.8": {
            "text": "Mein Vers Widervers"
        },
        "line.9": {
            "text": "Winterphantom Säulenbruchstück"
        },
        "line.10": {
            "text": "Auf dem Schneepfad ins Dunkel"
        },
        "line.11": {
            "text": "Meinen Bruder Das Grautier hab ich gesehn Ohr Auge Huf"
        },
        "line.12": {
            "text": "In der Platanenhöhle"
        },
        "line.13": {
            "text": "Da war das Wasser"
        },
        "line.14": {
            "text": "Durst sagt es Durst"
        },
        "line.15": {
            "text": "Noch Liebe träumend  Grüner Vogel"
        }
    }
}