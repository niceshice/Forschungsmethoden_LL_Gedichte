{
    "title": "entgegen-zu-stehen-ix-1354",
    "gender": "w",
    "author": "Anja Utler",
    "author_birth": "1973",
    "poem": {
        "line.1": {
            "text": "aufschrecken, schließlich und finde:"
        },
        "line.2": {
            "text": "den kehlkopf entkernt ist"
        },
        "line.3": {
            "text": "gehöhlt ihn: betasten die hände,"
        },
        "line.4": {
            "text": "sie: nesteln, gefiedert, vom"
        },
        "line.5": {
            "text": "brustkorb ab flechten sich"
        },
        "line.6": {
            "text": "tiefer ins: schilf schlucken: licht,"
        },
        "line.7": {
            "text": "gurgeln, dunkel ja, dämmrig"
        },
        "line.8": {
            "text": "sie: stricken kielwärts ja kehlwärts"
        },
        "line.9": {
            "text": "sich: hohlräume, mulden aus"
        },
        "line.10": {
            "text": "halmen aus fingern aus (..)"
        },
        "line.11": {
            "text": "gleichsam: den dommeln zu  nistplatz,"
        },
        "line.12": {
            "text": "im"
        },
        "line.13": {
            "text": "singenden rohr"
        },
        "line.14": {
            "text": "heißt es  will wie"
        },
        "line.15": {
            "text": "verfangen sein  treibgut  vernäht"
        },
        "line.16": {
            "text": "werden bis in die: stimmritze raschle"
        },
        "line.17": {
            "text": "fast zittre ich höre dich wieder: sagst"
        },
        "line.18": {
            "text": "lied sagst du lied  was ist: lied"
        }
    }
}