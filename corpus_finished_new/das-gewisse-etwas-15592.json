{
    "title": "das-gewisse-etwas-15592",
    "gender": "m",
    "author": "Steffen  Mensching",
    "author_birth": "1958",
    "poem": {
        "line.1": {
            "text": "Etwas, neben einem Hund, eine Art"
        },
        "line.2": {
            "text": "Schaferhund, schwarz, etwas zottelig,"
        },
        "line.3": {
            "text": "Etwas, neben einem Hut, schlagt"
        },
        "line.4": {
            "text": "Kopfuber auf den Burgersteig, etwa"
        },
        "line.5": {
            "text": "Acht Meter vor dem Cafe, bleibt etwas"
        },
        "line.6": {
            "text": "Liegen, wider Erwarten, etwas erregt"
        },
        "line.7": {
            "text": "Zwanzig Sekunden lang Aufmerksamkeit"
        },
        "line.8": {
            "text": "Im Polizeifunk, etwas, etwa Mitte"
        },
        "line.9": {
            "text": "Vierzig, mannlich, erbricht etwas"
        },
        "line.10": {
            "text": "Blut auf die erbettelten Munzen, etwa"
        },
        "line.11": {
            "text": "Sieben Mark, die etwas nicht mehr"
        },
        "line.12": {
            "text": "In Wein verwandeln wird, etwas, zu dem"
        },
        "line.13": {
            "text": "Sich der Notarzt, etwas außer Atem,"
        },
        "line.14": {
            "text": "Den Weg bahnt, etwas, das Harry heißt,"
        },
        "line.15": {
            "text": "Kalle, Keule oder so etwa, etwas"
        },
        "line.16": {
            "text": "Mit zehn Zahnen, verwaschenen"
        },
        "line.17": {
            "text": "Augen rast mit etwa achtzig km/h"
        },
        "line.18": {
            "text": "Und Sirenen durch den Freitagabendstau,"
        },
        "line.19": {
            "text": "Etwas ohne Identitat, Wohnsitz"
        },
        "line.20": {
            "text": "Und Angehorige liegt, etwas sabbernd,"
        },
        "line.21": {
            "text": "In einem Bett, etwas, das lange"
        },
        "line.22": {
            "text": "In keinem Bett lag, hangt am Tropf,"
        },
        "line.23": {
            "text": "Etwas fur das jede Hilfe etwas"
        },
        "line.24": {
            "text": "Zu spat kommt, etwas, dem man etwa"
        },
        "line.25": {
            "text": "Ein, zwei Tage gibt, hochstens"
        },
        "line.26": {
            "text": "Eine Woche, etwas, das als Kind etwas"
        },
        "line.27": {
            "text": "Werden wollte, Lokomotivfuhrer,"
        },
        "line.28": {
            "text": "Etwas mit Rechtschreibschwache"
        },
        "line.29": {
            "text": "Und gelbgrauen Fingern, etwas"
        },
        "line.30": {
            "text": "Vernarbt, unter der Sauerstoffmaske,"
        },
        "line.31": {
            "text": "Etwas unter dem Existenzminimum,"
        },
        "line.32": {
            "text": "Etwas, das nie mit dem Flugzeug flog,"
        },
        "line.33": {
            "text": "Etwas, das der Diensthabende"
        },
        "line.34": {
            "text": "Der Station, etwas ubermudet, abschaltet,"
        },
        "line.35": {
            "text": "Wahrend der Tagesschausprecher etwas"
        },
        "line.36": {
            "text": "Regen ankundigt, etwas mit einer Nummer"
        },
        "line.37": {
            "text": "Am großen Zeh des rechten Fußes,"
        },
        "line.38": {
            "text": "Schon etwas kalt, wird rasiert"
        },
        "line.39": {
            "text": "Und gewaschen, etwas verbrennt,"
        },
        "line.40": {
            "text": "Etwas spater, zu etwas Asche, etwas"
        },
        "line.41": {
            "text": "Spurloses, auf das im Tierheim"
        },
        "line.42": {
            "text": "Etwas wartet, ein Hund, eine Art"
        },
        "line.43": {
            "text": "Schaferhund, schwarz, etwas zottelig."
        }
    }
}