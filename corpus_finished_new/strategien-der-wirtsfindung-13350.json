{
    "title": "strategien-der-wirtsfindung-13350",
    "gender": "w",
    "author": "Brigitta Falkner",
    "author_birth": "1959",
    "poem": {
        "line.1": {
            "text": ">>>"
        },
        "line.2": {
            "text": "Audio @ Video below"
        },
        "line.3": {
            "text": "Taxonomische Tabellen"
        },
        "line.4": {
            "text": "mit durch Wassertropfen"
        },
        "line.5": {
            "text": "vergrößerten Leerstellen."
        },
        "line.6": {
            "text": "Urzeitliche Vögel beim Rupfen"
        },
        "line.7": {
            "text": "des Gefieders; der Boden"
        },
        "line.8": {
            "text": "von Rissen und Rillen"
        },
        "line.9": {
            "text": "durchzogen, den Schleifspuren"
        },
        "line.10": {
            "text": "tonnenschwerer Tetrapoden;"
        },
        "line.11": {
            "text": "die Vögel  mit Krallen"
        },
        "line.12": {
            "text": "an den Flügelschwingen,"
        },
        "line.13": {
            "text": "scharf wie Säbelklingen,"
        },
        "line.14": {
            "text": "Reptilienzähnen"
        },
        "line.15": {
            "text": "in den Schnäbeln"
        },
        "line.16": {
            "text": "und langen Schwänzen"
        },
        "line.17": {
            "text": "mit beweglichen Wirbeln;"
        },
        "line.18": {
            "text": "blaugrün wie die Zedernnadeln"
        },
        "line.19": {
            "text": "glänzen die Federn"
        },
        "line.20": {
            "text": "unter den Wedeln"
        },
        "line.21": {
            "text": "der von Strudeln und Turbulenzen,"
        },
        "line.22": {
            "text": "jagenden Raptorenrudeln"
        },
        "line.23": {
            "text": "und tieffliegenden Riesenlibellen"
        },
        "line.24": {
            "text": "zersausten Wurmfarnpflanzen."
        },
        "line.25": {
            "text": "Wie durch Anaglyphenbrillen"
        },
        "line.26": {
            "text": "sieht man sie hervorschnellen"
        },
        "line.27": {
            "text": "unter den verlausten Blättern"
        },
        "line.28": {
            "text": "der Natternzungengewächse:"
        },
        "line.29": {
            "text": "die jungen Echsen"
        },
        "line.30": {
            "text": "mit ihren gezackten Rücken"
        },
        "line.31": {
            "text": "kleinen Drachen gleichend;"
        },
        "line.32": {
            "text": "Urahnen der Warane,"
        },
        "line.33": {
            "text": "die aus dem Nichts auftauchend,"
        },
        "line.34": {
            "text": "mit ihren Schwänzen"
        },
        "line.35": {
            "text": "die kugelrunden Zapfen"
        },
        "line.36": {
            "text": "der Sumpfzypressen"
        },
        "line.37": {
            "text": "lässig ins Astloch schlenzen"
        },
        "line.38": {
            "text": "bevor sie den Blicken"
        },
        "line.39": {
            "text": "der Vögel entschlüpfen,"
        },
        "line.40": {
            "text": "und die spitzen Zacken"
        },
        "line.41": {
            "text": "hinter dem Termitenhügel"
        },
        "line.42": {
            "text": "im Gestrüpp"
        },
        "line.43": {
            "text": "verschwinden,"
        },
        "line.44": {
            "text": "unter dem Bärlapp"
        },
        "line.45": {
            "text": "und schwarzwarzigen Gehölz."
        },
        "line.46": {
            "text": "Igel mit Schnäbeln"
        },
        "line.47": {
            "text": "statt Schnauzen"
        },
        "line.48": {
            "text": "kreuzen ihre Wege,"
        },
        "line.49": {
            "text": "zu fußballgroßen Kugeln"
        },
        "line.50": {
            "text": "zusammengerollt."
        },
        "line.51": {
            "text": "Amphibische Mutationen,"
        },
        "line.52": {
            "text": "krallenbewehrte Zombiefische,"
        },
        "line.53": {
            "text": "von pelzigen Parasiten befallen,"
        },
        "line.54": {
            "text": "bewohnen die Sumpfregionen."
        },
        "line.55": {
            "text": "Sumpffarnrhizome durchdringen"
        },
        "line.56": {
            "text": "die verschlungenen Tunnelsysteme"
        },
        "line.57": {
            "text": "eozänischer Maulwurfgrillen."
        },
        "line.58": {
            "text": "Springen gehört zu den Dingen,"
        },
        "line.59": {
            "text": "welche die Collembolen"
        },
        "line.60": {
            "text": "mit ihren abdominalen Anhängen verrichten."
        },
        "line.61": {
            "text": "Die fossilen Torfschichten perforierend,"
        },
        "line.62": {
            "text": "flüchten die gelenkigen Sackkiefler"
        },
        "line.63": {
            "text": "mit den zweizinkigen Sprunggabeln"
        },
        "line.64": {
            "text": "ins mesozoische Dickicht der Farnbüsche."
        },
        "line.65": {
            "text": "In der Mundtasche"
        },
        "line.66": {
            "text": "des Springschwanzes versteckt:"
        },
        "line.67": {
            "text": "das Werkzeug, womit das Urinsekt"
        },
        "line.68": {
            "text": "das Ding als Ganzes"
        },
        "line.69": {
            "text": "zu vernichten bezweckt."
        },
        "line.70": {
            "text": "Aus dem Auge eines Raptorschädels"
        },
        "line.71": {
            "text": "kriecht eine Riesenraupe,"
        },
        "line.72": {
            "text": "erklimmt den Stängel"
        },
        "line.73": {
            "text": "eines Farnwedels,"
        },
        "line.74": {
            "text": "frisst sich in Zeitlupe"
        },
        "line.75": {
            "text": "durch den Blätterdschungel,"
        },
        "line.76": {
            "text": "bis das Gefiepe"
        },
        "line.77": {
            "text": "der Desmanen verstummt,"
        },
        "line.78": {
            "text": "der Urahnen der Maulwürfe."
        },
        "line.79": {
            "text": "Vom Beutezug"
        },
        "line.80": {
            "text": "der gierigen Riesenlarve"
        },
        "line.81": {
            "text": "zeugen bis heute"
        },
        "line.82": {
            "text": "die einspurigen Schneisen"
        },
        "line.83": {
            "text": "durch das Dickicht gezogen,"
        },
        "line.84": {
            "text": "die vom Gewicht der fetten,"
        },
        "line.85": {
            "text": "mit Knochensplittern gespickten"
        },
        "line.86": {
            "text": "Schmetterlingslarve"
        },
        "line.87": {
            "text": "geknickten Schachtelhalme"
        },
        "line.88": {
            "text": "und zermalmten Urinsekten:"
        },
        "line.89": {
            "text": "dorsoventral abgeflacht"
        },
        "line.90": {
            "text": "das Subareal im Pilzgeflecht"
        },
        "line.91": {
            "text": "bewohnende Schaben."
        },
        "line.92": {
            "text": "Silberfische. Springschwänze."
        },
        "line.93": {
            "text": "Karbonische Schnabelkerfen,"
        },
        "line.94": {
            "text": "zur Gänze im Bernstein begraben:"
        },
        "line.95": {
            "text": "mit Spinnenbeinen, Insektenlarven"
        },
        "line.96": {
            "text": "und Sprunggabeln als Grabbeigaben."
        },
        "line.97": {
            "text": "Über den Kobeln der Säbelzahnhörnchen"
        },
        "line.98": {
            "text": "blinken die Sternchen"
        },
        "line.99": {
            "text": "in der wabernden Lufthülle."
        },
        "line.100": {
            "text": "Spezielle Optiken sorgen"
        },
        "line.101": {
            "text": "für flimmerfreie Sternbilder."
        },
        "line.102": {
            "text": "Den Blicken verborgen,"
        },
        "line.103": {
            "text": "im Boden der Farnwälder,"
        },
        "line.104": {
            "text": "leben Würmer, Maden und Mikroben;"
        },
        "line.105": {
            "text": "Myriaden von wirbellosen Wesen"
        },
        "line.106": {
            "text": "wuchern in den Löchern und Spalten,"
        },
        "line.107": {
            "text": "den Gebeinen und Schädeln der Ahnen."
        },
        "line.108": {
            "text": "Bizarre Mehrzeller besiedeln"
        },
        "line.109": {
            "text": "die Zwischenwelten"
        },
        "line.110": {
            "text": "der wechselfeuchten Flechten,"
        },
        "line.111": {
            "text": "Bryophyten und Bromelien."
        },
        "line.112": {
            "text": "Alienartige Achtbeiner"
        },
        "line.113": {
            "text": "die Bärtierchen."
        },
        "line.114": {
            "text": "Sie überleben im Vakuum,"
        },
        "line.115": {
            "text": "bei Frost und Dürre,"
        },
        "line.116": {
            "text": "in vereisten arktischen Tümpeln,"
        },
        "line.117": {
            "text": "im heißen Wüstensand,"
        },
        "line.118": {
            "text": "wo sie zu Tönnchen verschrumpeln"
        },
        "line.119": {
            "text": "und in Trockenstarre verharren."
        },
        "line.120": {
            "text": "Würde man nach Jahren"
        },
        "line.121": {
            "text": "die Tiere mit Wasser benetzen,"
        },
        "line.122": {
            "text": "ploppten die Stummelbeinchen"
        },
        "line.123": {
            "text": "mit den Bärentatzen"
        },
        "line.124": {
            "text": "nacheinander aus den Tönnchen,"
        },
        "line.125": {
            "text": "und sie tapsten einfach weiter,"
        },
        "line.126": {
            "text": "Millimeter für Millimeter,"
        },
        "line.127": {
            "text": "als wäre nichts gewesen."
        },
        "line.128": {
            "text": "Mit der Auferstehungsrose"
        },
        "line.129": {
            "text": "teilen sie die Fähigkeit"
        },
        "line.130": {
            "text": "zur Kryptobiose."
        },
        "line.131": {
            "text": "Wäre der Trick"
        },
        "line.132": {
            "text": "beliebig oft anwendbar,"
        },
        "line.133": {
            "text": "lebten sie ewig"
        },
        "line.134": {
            "text": "(hier herrscht Uneinigkeit"
        },
        "line.135": {
            "text": "unter der Strichlisten führenden Spezies)."
        },
        "line.136": {
            "text": "Winzige wanzenartige Wesen"
        },
        "line.137": {
            "text": "wuseln zwischen den Milzfarnen"
        },
        "line.138": {
            "text": "und Pilzhyphen; Warnlaute, ein Zischen,"
        },
        "line.139": {
            "text": "gefolgt von Grunzgeräuschen,"
        },
        "line.140": {
            "text": "vermutlich Balzrufen"
        },
        "line.141": {
            "text": "(Vorstufen eines Gesangs?),"
        },
        "line.142": {
            "text": "entschlüpfen den Stimmköpfen"
        },
        "line.143": {
            "text": "der taubengrossen missing links"
        },
        "line.144": {
            "text": "zwischen Sauriern und Vögeln."
        },
        "line.145": {
            "text": "Wie bei den Gesängen des Sperlings"
        },
        "line.146": {
            "text": "(das Ergebnis äonenlangen"
        },
        "line.147": {
            "text": "Trainings der Syrinx)"
        },
        "line.148": {
            "text": "lautet die Regel: der Lauteste siegt."
        },
        "line.149": {
            "text": "Steigt der Pegel, verwischen die Grenzen"
        },
        "line.150": {
            "text": "zwischen Grunzen und Zischen,"
        },
        "line.151": {
            "text": "den Lock- und Warnrufen der Urvögel."
        },
        "line.152": {
            "text": "Die helikale Anordnung der Schuppen"
        },
        "line.153": {
            "text": "verweist auf einen Fichtenzapfen (Picea abies)."
        },
        "line.154": {
            "text": "Die triviale Anordnung der Noppen"
        },
        "line.155": {
            "text": "verweist auf einen schlichten Schlapfen (Flip Flop)."
        },
        "line.156": {
            "text": "Laichgründe verweisen auf Tiefgang."
        },
        "line.157": {
            "text": "Das Ende verweist auf den Anfang."
        }
    }
}