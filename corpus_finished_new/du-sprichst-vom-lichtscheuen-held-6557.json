{
    "title": "du-sprichst-vom-lichtscheuen-held-6557",
    "gender": "w",
    "author": "Ilana Shmueli",
    "author_birth": "1924",
    "poem": {
        "line.1": {
            "text": "Du sprichst vom lichtscheuen Held"
        },
        "line.2": {
            "text": "des Nichtgetanhabens"
        },
        "line.3": {
            "text": "du sprichst vom dauernden Rennen"
        },
        "line.4": {
            "text": "auf ausgetretenen Pfaden"
        },
        "line.5": {
            "text": "vom Stehen am Kreuzweg"
        },
        "line.6": {
            "text": "du sprichst..."
        },
        "line.7": {
            "text": "denke:"
        },
        "line.8": {
            "text": "es geht"
        },
        "line.9": {
            "text": "um das fiebrige"
        },
        "line.10": {
            "text": "Vielleichtdoch"
        }
    }
}