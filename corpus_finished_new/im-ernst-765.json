{
    "title": "im-ernst-765",
    "gender": "m",
    "author": "Mirko Bonné",
    "author_birth": "1965",
    "poem": {
        "line.1": {
            "text": "Danke Sterne dank non-fiction danke"
        },
        "line.2": {
            "text": "wenn der Zyklop sein Auge aufschiebt"
        },
        "line.3": {
            "text": "in Sommernächten und Klassen im Ernst"
        },
        "line.4": {
            "text": "in den Sitzschalen der Sternwarte"
        },
        "line.5": {
            "text": "Unterschiede zum Kino aufgehen"
        },
        "line.6": {
            "text": "Sexy Weltraum sexy Ernsthaftigkeit"
        },
        "line.7": {
            "text": "sexy Unschuld und sexy Sonne sexy"
        },
        "line.8": {
            "text": "Sirius Hauptstern im Großen Hund im Ernst"
        },
        "line.9": {
            "text": "Sirius hat neun Lichtjahre Abstand"
        },
        "line.10": {
            "text": "und einen weißen Zwerg als Begleiter"
        },
        "line.11": {
            "text": "Wäre nicht Winter die Wege nicht weiß"
        },
        "line.12": {
            "text": "die beiden symmetrischen Wäldchen"
        },
        "line.13": {
            "text": "kein klares Verhältnis und wären im Ernst"
        },
        "line.14": {
            "text": "die Sterne an der Kuppelinnenwand"
        },
        "line.15": {
            "text": "Botschaft einer Zauberhand voll Glück"
        },
        "line.16": {
            "text": "Danke Leuchten dank Entfernung"
        },
        "line.17": {
            "text": "danke Ödnis und danke Name danke"
        },
        "line.18": {
            "text": "Stille Schnee Errungenschaft und Ernst"
        },
        "line.19": {
            "text": "und danke Venus Venus danke danke"
        },
        "line.20": {
            "text": "weißer Zwerg und Planetariumwäldchen"
        }
    }
}