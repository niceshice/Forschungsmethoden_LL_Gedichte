{
    "title": "verschnittenes-gras-du-tastest-3042",
    "gender": "w",
    "author": "Karin  Fellner",
    "author_birth": "1970",
    "poem": {
        "line.1": {
            "text": "verschnittenes gras: du tastest"
        },
        "line.2": {
            "text": "es ab in der brust, ein grünes"
        },
        "line.3": {
            "text": "gefühl, prüfst die haltbarkeit"
        },
        "line.4": {
            "text": "von wiesen, hunger nach"
        },
        "line.5": {
            "text": "gärten"
        },
        "line.6": {
            "text": "beauty beyond"
        },
        "line.7": {
            "text": "production"
        },
        "line.8": {
            "text": "aus rotem klee,"
        },
        "line.9": {
            "text": "das süße ende der halme"
        },
        "line.10": {
            "text": "im mund ein kühles"
        },
        "line.11": {
            "text": "versprechen."
        }
    }
}