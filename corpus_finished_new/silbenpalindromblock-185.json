{
    "title": "silbenpalindromblock-185",
    "gender": "m",
    "author": "Oskar Pastior",
    "author_birth": "1927",
    "poem": {
        "line.1": {
            "text": "kommenfladen lobster, kleiner libero,mußkalvin nach baku"
        },
        "line.2": {
            "text": "wiemenge,wennkurfürstendammverdieinhermacht;viel"
        },
        "line.3": {
            "text": "krone undberlinischetodesbeisalsagst du, aberkannsie"
        },
        "line.4": {
            "text": "tingeldelphi weißnehmen?flaubert aaltesich (geschickt, mutarm)dasdebakel; wieenziansieden mensakloner wie schla-"
        },
        "line.5": {
            "text": "fend, lauter"
        },
        "line.6": {
            "text": "stif"
        },
        "line.7": {
            "text": "ter, laufend schlawinerklosamendensie an-"
        },
        "line.8": {
            "text": "ziehen;wickelbadedasarmutschicktgesichte,albertfloh"
        },
        "line.9": {
            "text": "mähnenweis fiedelgeld in sie; canberra, du sagst salbei; desto"
        },
        "line.10": {
            "text": "genielieber undnekrophilmacht hereindieverdammten fürs"
        },
        "line.11": {
            "text": "kurvengemeng wie kubanachwienkalmus,oh berlinerkleister, lob den flamenco"
        }
    }
}