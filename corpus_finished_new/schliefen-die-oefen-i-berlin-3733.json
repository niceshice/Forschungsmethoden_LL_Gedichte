{
    "title": "schliefen-die-oefen-i-berlin-3733",
    "gender": "w",
    "author": "Uljana Wolf",
    "author_birth": "1979",
    "poem": {
        "line.1": {
            "text": "berlin"
        },
        "line.2": {
            "text": "als wir aufwachten mit nestern"
        },
        "line.3": {
            "text": "im haar die wir nacht nannten"
        },
        "line.4": {
            "text": "schlugen die genesenden väter"
        },
        "line.5": {
            "text": "schaufelhändig alle klappen zu"
        },
        "line.6": {
            "text": "schliefen die öfen ohne uns"
        },
        "line.7": {
            "text": "in ihr vergessen zu nehmen"
        }
    }
}