{
    "title": "scheinbar-796",
    "gender": "m",
    "author": "Michael Lentz",
    "author_birth": "1964",
    "poem": {
        "line.1": {
            "text": "Ein Barsch"
        },
        "line.2": {
            "text": "schrieb an"
        },
        "line.3": {
            "text": "Narbe sich"
        },
        "line.4": {
            "text": "ran: Schieb"
        },
        "line.5": {
            "text": "Schrein ab;"
        },
        "line.6": {
            "text": "Barschein,"
        },
        "line.7": {
            "text": "Arsch, Bein,"
        },
        "line.8": {
            "text": "Nase brich;"
        },
        "line.9": {
            "text": "nasch Bier"
        },
        "line.10": {
            "text": "nie barsch;"
        },
        "line.11": {
            "text": "schab rein"
        },
        "line.12": {
            "text": "Bein rasch,"
        },
        "line.13": {
            "text": "schar Bein"
        },
        "line.14": {
            "text": "bis Rachen"
        },
        "line.15": {
            "text": "Beinschar:"
        },
        "line.16": {
            "text": "schrieb an"
        },
        "line.17": {
            "text": "Raben sich"
        },
        "line.18": {
            "text": "ein Barsch"
        },
        "line.19": {
            "text": "sich raben"
        },
        "line.20": {
            "text": "barsch. Ein"
        },
        "line.21": {
            "text": "Scheinbar"
        },
        "line.22": {
            "text": "brach sein"
        },
        "line.23": {
            "text": "Scharbein,"
        },
        "line.24": {
            "text": "Barschein"
        },
        "line.25": {
            "text": "brach sein"
        },
        "line.26": {
            "text": "Scheinbar."
        }
    }
}