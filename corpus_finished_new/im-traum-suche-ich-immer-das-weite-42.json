{
    "title": "im-traum-suche-ich-immer-das-weite-42",
    "gender": "m",
    "author": "H. C. Artmann",
    "author_birth": "1921",
    "poem": {
        "line.1": {
            "text": "im traum suche ich immer das weite und"
        },
        "line.2": {
            "text": "erwache im bett. frage: wie weit ist mein"
        },
        "line.3": {
            "text": "bett vom traum entfernt?"
        }
    }
}