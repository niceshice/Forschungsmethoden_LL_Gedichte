{
    "title": "scham-2958",
    "gender": "w",
    "author": "Nora Gomringer",
    "author_birth": "1980",
    "poem": {
        "line.1": {
            "text": "Neben Martin"
        },
        "line.2": {
            "text": "sah ich auf den Hang"
        },
        "line.3": {
            "text": "vom Rosengarten aus"
        },
        "line.4": {
            "text": "Meine Sandalenfüße"
        },
        "line.5": {
            "text": "Warm von Urin"
        },
        "line.6": {
            "text": "Keine Rose"
        },
        "line.7": {
            "text": "wollt mich erstechen"
        }
    }
}