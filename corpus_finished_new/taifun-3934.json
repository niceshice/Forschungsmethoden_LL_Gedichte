{
    "title": "taifun-3934",
    "gender": "m",
    "author": "Dieter M. Gräf",
    "author_birth": "1960",
    "poem": {
        "line.1": {
            "text": "himmel, Rollerschwärme, zurück"
        },
        "line.2": {
            "text": "ins Vereinigungshotel, sein"
        },
        "line.3": {
            "text": "wachgerufener Name um vier ("
        },
        "line.4": {
            "text": "szu"
        },
        "line.5": {
            "text": "):"
        },
        "line.6": {
            "text": "die im Aufzug fehlende Zahl"
        },
        "line.7": {
            "text": "ist der Tod in der Koloratur"
        },
        "line.8": {
            "text": "der geliebten Frau nebenan,"
        },
        "line.9": {
            "text": "und am Morgen: die fließende"
        },
        "line.10": {
            "text": "Straße, verschwindet, Schlamm"
        },
        "line.11": {
            "text": "brühe mit treibender Tonne,"
        },
        "line.12": {
            "text": "auf der ein Junge sitzt. Ein"
        },
        "line.13": {
            "text": "äugig wir, zu den Bildschirmen"
        },
        "line.14": {
            "text": "hin, aus denen mehr und"
        },
        "line.15": {
            "text": "mehr Wasser kommt, Tod ("
        },
        "line.16": {
            "text": "szu"
        },
        "line.17": {
            "text": ")."
        },
        "line.18": {
            "text": "Am Vortag standen Bild"
        },
        "line.19": {
            "text": "schirme am Teich, Karaoke"
        },
        "line.20": {
            "text": "ecken. In herbeizitierter Landschaft"
        },
        "line.21": {
            "text": "eine neue eröffnen, zum Singen,"
        },
        "line.22": {
            "text": "während die Instrumente"
        },
        "line.23": {
            "text": "in den Boxen bleiben,"
        },
        "line.24": {
            "text": "oder man installiert einen Wasser"
        },
        "line.25": {
            "text": "fall mit glücksverheißenden Tempel"
        },
        "line.26": {
            "text": "karpfen. Schwimmen davon, nun."
        }
    }
}