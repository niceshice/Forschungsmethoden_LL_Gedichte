{
    "title": "tierleben-9-4018",
    "gender": "m",
    "author": "Felix Philipp Ingold",
    "author_birth": "1942",
    "poem": {
        "line.1": {
            "text": "9"
        },
        "line.2": {
            "text": "Jede Pracht ist"
        },
        "line.3": {
            "text": "wie Humor und Schwermut"
        },
        "line.4": {
            "text": "sauer. Auf Dauer"
        },
        "line.5": {
            "text": "angelegt und doch nie"
        },
        "line.6": {
            "text": "so modern wie"
        },
        "line.7": {
            "text": "Hund oder Huhn. Was sich"
        },
        "line.8": {
            "text": "zwischen Röcheln und"
        },
        "line.9": {
            "text": "Ga-Gackern zum Seebeben"
        },
        "line.10": {
            "text": "braut. Rauht"
        },
        "line.11": {
            "text": "der Ost so richtig auf"
        },
        "line.12": {
            "text": "kriegt er einen immer wieder"
        },
        "line.13": {
            "text": "neuen Übernamen. Der"
        },
        "line.14": {
            "text": "gilt bis die Luft"
        },
        "line.15": {
            "text": "still steht und Huhn"
        },
        "line.16": {
            "text": "und Hund lauthals den Atem"
        },
        "line.17": {
            "text": "teilen."
        },
        "line.18": {
            "text": "(\"Pracht ist"
        },
        "line.19": {
            "text": "auf Dauer angelegt wie"
        },
        "line.20": {
            "text": "immer nie ein"
        },
        "line.21": {
            "text": "Röcheln oder Gackern."
        },
        "line.22": {
            "text": "Rauht der Ost"
        },
        "line.23": {
            "text": "auch nur ein wenig auf"
        },
        "line.24": {
            "text": "kriegt er einen"
        },
        "line.25": {
            "text": "neuen Namen. Derweil"
        },
        "line.26": {
            "text": "der Zwilling"
        },
        "line.27": {
            "text": "still den Atem teilt. Nur"
        },
        "line.28": {
            "text": "aber mit wem.\")"
        }
    }
}