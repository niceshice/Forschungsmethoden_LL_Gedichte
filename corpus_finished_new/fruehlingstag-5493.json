{
    "title": "fruehlingstag-5493",
    "gender": "m",
    "author": "Hermann Hesse",
    "author_birth": "1877",
    "poem": {
        "line.1": {
            "text": "Wind im Gesträuch und Vogelpfiff"
        },
        "line.2": {
            "text": "Und hoch im höchsten süßen Blau"
        },
        "line.3": {
            "text": "Ein stilles, stolzes Wolkenschiff. . ."
        },
        "line.4": {
            "text": "Ich träume von einer blonden Frau,"
        },
        "line.5": {
            "text": "Ich träume von meiner Jugendzeit,"
        },
        "line.6": {
            "text": "Der hohe Himmel blau und weit"
        },
        "line.7": {
            "text": "Ist meiner Sehnsucht Wiege,"
        },
        "line.8": {
            "text": "Darin ich stillgesinnt"
        },
        "line.9": {
            "text": "Und selig warm"
        },
        "line.10": {
            "text": "Mit leisem Summen liege,"
        },
        "line.11": {
            "text": "So wie in seiner Mutter Arm"
        },
        "line.12": {
            "text": "Ein Kind."
        }
    }
}