    "poem": {
        "line.1": {
            "text": "Als ich dich noch siezte,"
        },
        "line.2": {
            "text": "sagte ich"
        },
        "line.3": {
            "text": "ich"
        },
        "line.4": {
            "text": "und meinte damit"
        },
        "line.5": {
            "text": "mich."
        },
        "line.6": {
            "text": "Seit gestern duze ich dich,"
        },
        "line.7": {
            "text": "wei√ü aber noch nicht,"
        },
        "line.8": {
            "text": "wie ich mich umbenennen soll."
        }
    }
}