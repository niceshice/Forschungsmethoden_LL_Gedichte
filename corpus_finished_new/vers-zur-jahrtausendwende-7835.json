{
    "title": "vers-zur-jahrtausendwende-7835",
    "gender": "m",
    "author": "Reiner Kunze",
    "author_birth": "1933",
    "poem": {
        "line.1": {
            "text": "Wir haben immer eine wahl,"
        },
        "line.2": {
            "text": "und sei's, uns denen nicht zu beugen,"
        },
        "line.3": {
            "text": "die sie uns nahmen"
        }
    }
}