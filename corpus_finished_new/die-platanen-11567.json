{
    "title": "die-platanen-11567",
    "gender": "m",
    "author": "Michael Donhauser",
    "author_birth": "1956",
    "poem": {
        "line.1": {
            "text": "Die Platanen"
        },
        "line.2": {
            "text": "wie sie sagen"
        },
        "line.3": {
            "text": "dass sie rauschen"
        },
        "line.4": {
            "text": "wenn im Wehen"
        },
        "line.5": {
            "text": "sich erheben"
        },
        "line.6": {
            "text": "ihre Bl√§tter"
        },
        "line.7": {
            "text": "wenn sie sinken"
        },
        "line.8": {
            "text": "etwas silbern"
        },
        "line.9": {
            "text": "durch die Jahre"
        },
        "line.10": {
            "text": "durch die Schatten"
        },
        "line.11": {
            "text": "auf die Wege"
        },
        "line.12": {
            "text": "und den Rasen"
        }
    }
}