{
    "title": "findungen-13935",
    "gender": "w",
    "author": "Róža Domašcyna",
    "author_birth": "1951",
    "poem": {
        "line.1": {
            "text": "im sich geneigt sein"
        },
        "line.2": {
            "text": "das in sich geneigt sein"
        },
        "line.3": {
            "text": "im mitten entzwei sein den kern"
        },
        "line.4": {
            "text": "im fallenlassen halt"
        }
    }
}