{
    "title": "hinterlassenschaft-8402",
    "gender": "w",
    "author": "Doris Runge",
    "author_birth": "1943",
    "poem": {
        "line.1": {
            "text": "gras"
        },
        "line.2": {
            "text": "wie grün den toten"
        },
        "line.3": {
            "text": "das haar wächst"
        },
        "line.4": {
            "text": "für den wind"
        },
        "line.5": {
            "text": "der stein"
        },
        "line.6": {
            "text": "über dem einschlag"
        },
        "line.7": {
            "text": "eingeflochten von moos"
        },
        "line.8": {
            "text": "das gedächtnis"
        },
        "line.9": {
            "text": "grünverhangen"
        },
        "line.10": {
            "text": "aller scharfen umrisse"
        },
        "line.11": {
            "text": "aller schatten beraubt"
        },
        "line.12": {
            "text": "aber dein handschuh"
        },
        "line.13": {
            "text": "meine hand in deiner"
        },
        "line.14": {
            "text": "neben dem stein im gras"
        },
        "line.15": {
            "text": "zehn lebendige finger"
        },
        "line.16": {
            "text": "bereit"
        },
        "line.17": {
            "text": "das gras zu schneiden"
        },
        "line.18": {
            "text": "den stein zu wälzen"
        }
    }
}