{
    "title": "balance-balance-1520",
    "gender": "w",
    "author": "Elfriede Gerstl",
    "author_birth": "1932",
    "poem": {
        "line.1": {
            "text": "dieses sitzen in der sonne von rom"
        },
        "line.2": {
            "text": "ist keine lösung"
        },
        "line.3": {
            "text": "für dieses und jenes in wien"
        },
        "line.4": {
            "text": "und in meinem kopf"
        },
        "line.5": {
            "text": "dieses sitzen auf stufen weit weg"
        },
        "line.6": {
            "text": "von wien und allem"
        },
        "line.7": {
            "text": "im kampf mit lästigen papierlüsternen ameisen"
        },
        "line.8": {
            "text": "hält mich in schwebe"
        },
        "line.9": {
            "text": "über gar nicht unwahrscheinlichen abstürzen"
        },
        "line.10": {
            "text": "wie die aua- und die alitalia-vögel"
        },
        "line.11": {
            "text": "über den so genannten wirklichen abgründen"
        },
        "line.12": {
            "text": "von denen man wegschaut"
        },
        "line.13": {
            "text": "auf kleine käfrige buchstaben"
        },
        "line.14": {
            "text": "oder sonst was"
        }
    }
}