{
    "title": "anbruch-13884",
    "gender": "w",
    "author": "Kerstin Becker",
    "author_birth": "1969",
    "poem": {
        "line.1": {
            "text": "hinterm Hof hinterm Dorf"
        },
        "line.2": {
            "text": "hinterm Feld hört die Welt"
        },
        "line.3": {
            "text": "auf im Wald bellt nichts"
        },
        "line.4": {
            "text": "als ferne Reh"
        },
        "line.5": {
            "text": "keckern die Füchse die Wanne"
        },
        "line.6": {
            "text": "aus blindem Übergangsmetall"
        },
        "line.7": {
            "text": "harrt unser"
        },
        "line.8": {
            "text": "die wir nach Alter schmutzig in sie steigen"
        },
        "line.9": {
            "text": "dampfend alle sieben Tage"
        },
        "line.10": {
            "text": "im Gottesrhythmus"
        },
        "line.11": {
            "text": "zur Dämmerung im Stall"
        },
        "line.12": {
            "text": "am Lokus ist die Liebe eingesägt"
        },
        "line.13": {
            "text": "ein Herz durch das die grünen"
        },
        "line.14": {
            "text": "Fliegen schießen menschlicher"
        },
        "line.15": {
            "text": "Gestank"
        },
        "line.16": {
            "text": "der gelbe Roggen wogt"
        },
        "line.17": {
            "text": "ohn Mutterkorn zum Horizont"
        },
        "line.18": {
            "text": "die Kriebelkrankheit"
        },
        "line.19": {
            "text": "rafft uns nicht der Hofhund jagt"
        },
        "line.20": {
            "text": "sich selbst im Kreis er stammt"
        },
        "line.21": {
            "text": "von wilden Wölfen ab wir sind"
        },
        "line.22": {
            "text": "hier eingeborn"
        }
    }
}