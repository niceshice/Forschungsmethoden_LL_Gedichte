    "gender": "m",
    "author": "Thien Tran",
    "poem": {
        "line.1": {
            "text": "der luftleere Raum war uns"
        },
        "line.2": {
            "text": "zu unpers\u00f6nlich. wir strebten auseinander"
        },
        "line.3": {
            "text": "Hanna geriet in ein Energiefeld"
        },
        "line.4": {
            "text": "au\u00dferhalb des Intendierten. so jedenfalls"
        },
        "line.5": {
            "text": "berichtete sie. als wir wieder zusammenfanden"
        },
        "line.6": {
            "text": "dass es unertr\u00e4glich sei. in einer Raum-"
        },
        "line.7": {
            "text": "Zeit- und Bewegungsextension"
        },
        "line.8": {
            "text": "zu denken"
        },
        "line.9": {
            "text": "und Hanna liebte ihren K\u00f6rper"
        },
        "line.10": {
            "text": "gut m\u00f6glich dass die Grundkonfiguration"
        },
        "line.11": {
            "text": "ihrer Neuronensoftware nicht ausreichte"
        },
        "line.12": {
            "text": "zumindest nicht. um die Progressivform"
        },
        "line.13": {
            "text": "zu integrieren. also machte sie kehrt"
        },
        "line.14": {
            "text": "noch ehe ich mich entschuldigen konnte"
        },
        "line.15": {
            "text": "und noch wollte ich mich"
        },
        "line.16": {
            "text": "entschuldigen"
        },
        "line.17": {
            "text": "zu sp\u00e4t: um uns herum bildeten sich"
        },
        "line.18": {
            "text": "durchsichtige Basalt- und Eisenmolek\u00fcle"
        },
        "line.19": {
            "text": "die beiden Grundelemente des St\u00e4dtischen"
        },
        "line.20": {
            "text": "und wir \u00fcberpr\u00fcften noch einmal die Koordinaten"
        },
        "line.21": {
            "text": "unserer Radarger\u00e4te. und stellten fest"
        },
        "line.22": {
            "text": "unter uns f\u00fchrte die Landebahn direkt"
        },
        "line.23": {
            "text": "in die Zivilisation."
        }
    }
}