{
    "title": "kiarostami-13981",
    "categories": [
        "Natur",
        "Tiere"
    ],
    "gender": "m",
    "author": "Jan Volker R\u00f6hnert",
    "poem": {
        "line.1": {
            "text": "Die Schildkr\u00f6te auf ihrem Panzer"
        },
        "line.2": {
            "text": "geht in der Luft, auf allen vieren,"
        },
        "line.3": {
            "text": "bis sie wieder im Staube steht,"
        },
        "line.4": {
            "text": "der K\u00e4fer rollt seine Kugel"
        },
        "line.5": {
            "text": "aus Schafkot, ein Sisyphos,"
        },
        "line.6": {
            "text": "durch Dornen und Dreck,"
        },
        "line.7": {
            "text": "der Film, ganz gro\u00df am Boden"
        },
        "line.8": {
            "text": "die Spur, die er hinterl\u00e4sst"
        }
    }
}