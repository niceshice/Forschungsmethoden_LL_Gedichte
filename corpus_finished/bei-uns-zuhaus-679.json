{
    "title": "bei-uns-zuhaus-679",
    "categories": [
        "Lyrik f\u00fcr Kinder",
        "Essen & Trinken",
        "Familie",
        "Natur"
    ],
    "gender": "m",
    "author": "J\u00fcrg Schubiger",
    "poem": {
        "line.1": {
            "text": "Das Licht geht an,"
        },
        "line.2": {
            "text": "der Kuchen auf,"
        },
        "line.3": {
            "text": "die Schwester aus"
        },
        "line.4": {
            "text": "bei uns zuhaus."
        },
        "line.5": {
            "text": "Die T\u00fcren gehn,"
        },
        "line.6": {
            "text": "die Freunde sehn herein"
        },
        "line.7": {
            "text": "und gr\u00fcssen."
        },
        "line.8": {
            "text": "Nur Tische, St\u00fchle,"
        },
        "line.9": {
            "text": "Betten stehn"
        },
        "line.10": {
            "text": "bei uns zuhaus"
        },
        "line.11": {
            "text": "bockstill und auf vier F\u00fcssen."
        }
    }
}