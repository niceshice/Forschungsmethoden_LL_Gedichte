{
    "title": "bei-uns-zuhaus-679",
    "categories": [
        "Lyrik für Kinder",
        "Essen & Trinken",
        "Familie",
        "Natur"
    ],
    "gender": "m",
    "author": "Jürg Schubiger",
    "poem": {
        "line.1": {
            "text": "Das Licht geht an,"
        },
        "line.2": {
            "text": "der Kuchen auf,"
        },
        "line.3": {
            "text": "die Schwester aus"
        },
        "line.4": {
            "text": "bei uns zuhaus."
        },
        "line.5": {
            "text": "Die Tren gehn,"
        },
        "line.6": {
            "text": "die Freunde sehn herein"
        },
        "line.7": {
            "text": "und grssen."
        },
        "line.8": {
            "text": "Nur Tische, Sthle,"
        },
        "line.9": {
            "text": "Betten stehn"
        },
        "line.10": {
            "text": "bei uns zuhaus"
        },
        "line.11": {
            "text": "bockstill und auf vier Fssen."
        }
    }
}