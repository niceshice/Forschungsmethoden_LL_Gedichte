{
    "title": "die-horizontbaeume-752",
    "categories": [
        "Natur",
        "Pflanzen"
    ],
    "gender": "m",
    "author": "Mirko Bonn\u00e9",
    "poem": {
        "line.1": {
            "text": "Geh"
        },
        "line.2": {
            "text": "sonderbar"
        },
        "line.3": {
            "text": "ohne etwas zu verlassen zu erreichen"
        },
        "line.4": {
            "text": "von Schwelle zu Schwelle"
        },
        "line.5": {
            "text": "ein vorgefertigter Spross w\u00e4chst"
        },
        "line.6": {
            "text": "unter dem Armbanduhrglas"
        },
        "line.7": {
            "text": "B\u00e4ume von Stunden"
        },
        "line.8": {
            "text": "durch Immergleiches marschiert"
        },
        "line.9": {
            "text": "mit anhaltendem Atem"
        },
        "line.10": {
            "text": "die Frau an der Leine gesteht"
        },
        "line.11": {
            "text": "die schief zusammengen\u00e4hten"
        },
        "line.12": {
            "text": "Kleider der Nachbarin als k\u00f6rpergewordene"
        },
        "line.13": {
            "text": "Idee ihres Wahnsinns ein"
        },
        "line.14": {
            "text": "geh"
        },
        "line.15": {
            "text": "so"
        },
        "line.16": {
            "text": "h\u00e4ngen"
        },
        "line.17": {
            "text": "die B\u00e4ume im Horizont"
        }
    }
}