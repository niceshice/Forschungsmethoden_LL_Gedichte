{
    "title": "die-horizontbaeume-752",
    "categories": [
        "Natur",
        "Pflanzen"
    ],
    "gender": "m",
    "author": "Mirko Bonn√©",
    "poem": {
        "line.1": {
            "text": "Geh"
        },
        "line.2": {
            "text": "sonderbar"
        },
        "line.3": {
            "text": "ohne etwas zu verlassen zu erreichen"
        },
        "line.4": {
            "text": "von Schwelle zu Schwelle"
        },
        "line.5": {
            "text": "ein vorgefertigter Spross wchst"
        },
        "line.6": {
            "text": "unter dem Armbanduhrglas"
        },
        "line.7": {
            "text": "Bume von Stunden"
        },
        "line.8": {
            "text": "durch Immergleiches marschiert"
        },
        "line.9": {
            "text": "mit anhaltendem Atem"
        },
        "line.10": {
            "text": "die Frau an der Leine gesteht"
        },
        "line.11": {
            "text": "die schief zusammengenhten"
        },
        "line.12": {
            "text": "Kleider der Nachbarin als krpergewordene"
        },
        "line.13": {
            "text": "Idee ihres Wahnsinns ein"
        },
        "line.14": {
            "text": "geh"
        },
        "line.15": {
            "text": "so"
        },
        "line.16": {
            "text": "hngen"
        },
        "line.17": {
            "text": "die Bume im Horizont"
        }
    }
}