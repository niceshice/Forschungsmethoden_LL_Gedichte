{
    "title": "wildern-13889",
    "categories": [
        "Natur",
        "Landschaft",
        "Pflanzen",
        "Tiere"
    ],
    "gender": "w",
    "author": "Kerstin Becker",
    "poem": {
        "line.1": {
            "text": "wir taumeln"
        },
        "line.2": {
            "text": "in Libellenwolken"
        },
        "line.3": {
            "text": "mit unstabilem K\u00f6rperkern"
        },
        "line.4": {
            "text": "durchs nabelhohe Gras"
        },
        "line.5": {
            "text": "Holzb\u00f6cke harren"
        },
        "line.6": {
            "text": "wie Engel eines warmen Tiers"
        },
        "line.7": {
            "text": "wir ziehn"
        },
        "line.8": {
            "text": "auf hornhautlosen Sohlen"
        },
        "line.9": {
            "text": "die derbe Erde dr\u00fcckt"
        },
        "line.10": {
            "text": "das glasge Wasser sticht"
        },
        "line.11": {
            "text": "in einem B\u00e4chlein helle"
        },
        "line.12": {
            "text": "am andern Ufer saften"
        },
        "line.13": {
            "text": "Beeren deren Samen Laut"
        },
        "line.14": {
            "text": "geben wenn wir kaun"
        },
        "line.15": {
            "text": "im T\u00fcmpel bei den alten"
        },
        "line.16": {
            "text": "Kalkbr\u00fcchen treiben wir"
        },
        "line.17": {
            "text": "K\u00f6pfchen in das Wasser"
        },
        "line.18": {
            "text": "in teufelsscharfem Kraut"
        }
    }
}