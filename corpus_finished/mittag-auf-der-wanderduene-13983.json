{
    "title": "mittag-auf-der-wanderduene-13983",
    "categories": [
        "Natur"
    ],
    "gender": "m",
    "author": "Jan Volker RÃ¶hnert",
    "poem": {
        "line.1": {
            "text": "Du schliet die Augen"
        },
        "line.2": {
            "text": "langsam, die Welt"
        },
        "line.3": {
            "text": "ist nichts auer Wind"
        },
        "line.4": {
            "text": "du ffnest sie wieder"
        },
        "line.5": {
            "text": "langsam, die Dinge"
        },
        "line.6": {
            "text": "hellen sich auf:"
        },
        "line.7": {
            "text": "Sand-"
        },
        "line.8": {
            "text": "krner, durch die Heere"
        },
        "line.9": {
            "text": "von Ameisen ziehen"
        },
        "line.10": {
            "text": "darunter die Wiesen"
        },
        "line.11": {
            "text": "neblig am Fluss"
        },
        "line.12": {
            "text": "der Wespenbussard dreht"
        },
        "line.13": {
            "text": "ber den Kiefern"
        },
        "line.14": {
            "text": "ab auer Sicht"
        },
        "line.15": {
            "text": "nichts wie es ist"
        },
        "line.16": {
            "text": "bleibt in zwei Augenblicken"
        },
        "line.17": {
            "text": "auer dem Wind"
        }
    }
}