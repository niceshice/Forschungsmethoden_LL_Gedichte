    "author": "Bj√∂rn  Kuhligk",
    "poem": {
        "line.1": {
            "text": "(fr Peter Wawerzinek)"
        },
        "line.2": {
            "text": "Man steht an Ksten, unterkhlt"
        },
        "line.3": {
            "text": "auf Klippen, in Tlern"
        },
        "line.4": {
            "text": "berpokert von Gipfeln"
        },
        "line.5": {
            "text": "man hat zwei Zimmerpflanzen"
        },
        "line.6": {
            "text": "die bekommen Wasser und"
        },
        "line.7": {
            "text": "dieser Kaktus, der durchhlt"
        },
        "line.8": {
            "text": "man mu nicht wedeln, die Wsche"
        },
        "line.9": {
            "text": "trocknet von selbst, die zwei Leben"
        },
        "line.10": {
            "text": "die man hatte, da pat ein drittes drauf"
        },
        "line.11": {
            "text": "man steht unter Bumen, worunter"
        },
        "line.12": {
            "text": "auch sonst, da stecken Misteln drin"
        },
        "line.13": {
            "text": "das sieht verstrend aus"
        },
        "line.14": {
            "text": "man ist geduldet, ein lebender Gast"
        },
        "line.15": {
            "text": "mit Schuhwerk am Knchel, man ist"
        },
        "line.16": {
            "text": "ein dummes Gewirr, was Grres will"
        }
    }
}