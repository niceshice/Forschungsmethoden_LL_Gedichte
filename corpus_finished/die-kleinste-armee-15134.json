{
    "title": "die-kleinste-armee-15134",
    "categories": [
        "Leben & Beziehungen"
    ],
    "gender": "m",
    "author": "Georg Le\u00df",
    "poem": {
        "line.1": {
            "text": "sie marschiert gleich zu zweit, diese kleinste Armee"
        },
        "line.2": {
            "text": "nicht die kleine, die kleinste"
        },
        "line.3": {
            "text": "auch \u00fcber die Parkpl\u00e4tze? ja"
        },
        "line.4": {
            "text": "auch \u00fcber die Parkpl\u00e4tze"
        },
        "line.5": {
            "text": "durch die B\u00fcsche? ja"
        },
        "line.6": {
            "text": "auch durch die B\u00fcsche"
        },
        "line.7": {
            "text": "durchs Schilf? ja"
        },
        "line.8": {
            "text": "selbstverst\u00e4ndlich auch durchs Schilf"
        },
        "line.9": {
            "text": "\u00fcber die Wipfel? nein"
        },
        "line.10": {
            "text": "\u00fcber die Wipfel nun gerade nicht"
        },
        "line.11": {
            "text": "marschiert und marschiert"
        },
        "line.12": {
            "text": "die kleinste Armee"
        }
    }
}