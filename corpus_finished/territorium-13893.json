{
    "title": "territorium-13893",
    "categories": [
        "Natur",
        "Sommer"
    ],
    "gender": "w",
    "author": "Kerstin Becker",
    "poem": {
        "line.1": {
            "text": "die Panzerstra\u00dfe zwischen den W\u00e4ldern"
        },
        "line.2": {
            "text": "in denen wir Sommer f\u00fcr Sommer"
        },
        "line.3": {
            "text": "mit Sprossenschmerzen"
        },
        "line.4": {
            "text": "mit dicken Grinden"
        },
        "line.5": {
            "text": "mit schwarzen Fu\u00dfsohlen"
        },
        "line.6": {
            "text": "aufgehen ist f\u00fcr die Ewigkeit"
        },
        "line.7": {
            "text": "Staubschleier grauen wir pesen"
        },
        "line.8": {
            "text": "davon eingeh\u00fcllt mit dem Ostwind aus"
        },
        "line.9": {
            "text": "gediente Dachpappefetzen fliegen"
        },
        "line.10": {
            "text": "im s\u00fc\u00dflichen Simsongestank"
        },
        "line.11": {
            "text": "Rohre f\u00fcr lange Leitungen liegen"
        },
        "line.12": {
            "text": "brach wo die wei\u00dfen Birken"
        },
        "line.13": {
            "text": "an Wegerichpisten schie\u00dfen wir"
        },
        "line.14": {
            "text": "fummeln wir suchen"
        },
        "line.15": {
            "text": "immer heftiger knutschend"
        },
        "line.16": {
            "text": "unsern Zusammenhang"
        }
    }
}