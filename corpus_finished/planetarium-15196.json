        "Pflanzen"
    ],
    "gender": "w",
    "author": "Anja Kampmann",
    "poem": {
        "line.1": {
            "text": "hren wir uns atmen in der nacht"
        },
        "line.2": {
            "text": "am morgen ein specht"
        },
        "line.3": {
            "text": "wertet die lochkarten aus"
        },
        "line.4": {
            "text": "der gewhnlichen trume"
        },
        "line.5": {
            "text": "dort sind wir nicht gewesen"
        },
        "line.6": {
            "text": "schwrze wie ein fliehendes reh"
        },
        "line.7": {
            "text": "die uns umschloss fern der huser"
        },
        "line.8": {
            "text": "wenig lsst sich sagen"
        },
        "line.9": {
            "text": "ber die verbindungslinien der sterne"
        },
        "line.10": {
            "text": "die bume stehen nass und voller tropfen"
        },
        "line.11": {
            "text": "im planetarium frherer zeit"
        },
        "line.12": {
            "text": "war es eine lochkarte"
        },
        "line.13": {
            "text": "die jemand mit nadeln durchstach"
        },
        "line.14": {
            "text": "wie um die schmerzenden stellen"
        },
        "line.15": {
            "text": "zu finden."
        }
    }
}