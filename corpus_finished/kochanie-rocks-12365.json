{
    "title": "kochanie-rocks-12365",
    "categories": [
        "Erotische Poesie",
        "Leben & Beziehungen",
        "Alkohol & Drogen"
    ],
    "gender": "m",
    "author": "Martin  Piekar",
    "poem": {
        "line.1": {
            "text": "R\u00fchre mich anstatt"
        },
        "line.2": {
            "text": "Mich zu ersch\u00fcttern: ich bin"
        },
        "line.3": {
            "text": "Kein Glasabenteuer: vermeng"
        },
        "line.4": {
            "text": "Mich nicht"
        },
        "line.5": {
            "text": "Mit Vorsicht"
        },
        "line.6": {
            "text": "Sch\u00fctze ich vor Vern\u00fcchterung:"
        },
        "line.7": {
            "text": "M\u00f6chte Sonne endlich"
        },
        "line.8": {
            "text": "Mit deinen Haaren"
        },
        "line.9": {
            "text": "Verh\u00e4ngen"
        },
        "line.10": {
            "text": "Ins Kaltdunkel"
        },
        "line.11": {
            "text": "Wo Schummerungen sich liebkosen"
        },
        "line.12": {
            "text": "In Multiperspektivit\u00e4t oder:"
        },
        "line.13": {
            "text": "Wie wir in der Trommel"
        },
        "line.14": {
            "text": "Eines Revolvers"
        },
        "line.15": {
            "text": "Stecken zielsicher auf"
        },
        "line.16": {
            "text": "Balz gehalten: gib Acht"
        },
        "line.17": {
            "text": "Und nicht weniger in deinen Drink"
        },
        "line.18": {
            "text": "Verzehrst du mich nach dir?"
        },
        "line.19": {
            "text": "Ich will keinen Longdrink mimen"
        },
        "line.20": {
            "text": "M\u00fcssen: wer ist schon"
        },
        "line.21": {
            "text": "In Bereitschaft daf\u00fcr"
        },
        "line.22": {
            "text": "Denn Zeit ist alles was wir haben"
        },
        "line.23": {
            "text": "Das uns nichts bedeutet"
        },
        "line.24": {
            "text": "Ich empfehle mich:"
        },
        "line.25": {
            "text": "Schl\u00fcrf mich kochanie"
        },
        "line.26": {
            "text": "On the Rocks"
        },
        "line.27": {
            "text": "Zutaten:"
        },
        "line.28": {
            "text": "Bei mir"
        }
    }
}