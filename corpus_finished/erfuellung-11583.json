    "poem": {
        "line.1": {
            "text": "Seit wir einander verstehen,"
        },
        "line.2": {
            "text": "bleibt nichts mehr zu sagen."
        },
        "line.3": {
            "text": "Unsere im Zeitraffer"
        },
