{
    "title": "einander-verweisende-weisen-13932",
    "categories": [
        "Leben & Beziehungen"
    ],
    "gender": "w",
    "author": "R\u00f3\u017ea Doma\u0161cyna",
    "poem": {
        "line.1": {
            "text": "der nicht ging kam nicht der nicht kam"
        },
        "line.2": {
            "text": "ging nicht so sind wir noch beieinander"
        },
        "line.3": {
            "text": "umeinander f\u00fcreinander"
        },
        "line.4": {
            "text": "keinen einklang bezeichnend"
        },
        "line.5": {
            "text": "eine einigung beiderseits: nein"
        },
        "line.6": {
            "text": "meistens ein weil derweil"
        },
        "line.7": {
            "text": "einstweilen voreilig eilig"
        },
        "line.8": {
            "text": "schneidige eide schleimige pleiten"
        },
        "line.9": {
            "text": "eier keifen schreie keile"
        },
        "line.10": {
            "text": "bleierne beine weiter treiben"
        },
        "line.11": {
            "text": "leises hei\u00dfes eisen bei beiden"
        },
        "line.12": {
            "text": "seinen ihren wunsch verleiden"
        },
        "line.13": {
            "text": "feines weinen allein"
        },
        "line.14": {
            "text": "wei\u00df keines weiter"
        }
    }
}