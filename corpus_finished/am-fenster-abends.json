{
    "title": "am-fenster-abends",
    "categories": [
        "Poetologische Gedichte",
        "Natur",
        "Landschaft",
        "Tiere"
    ],
    "gender": "w",
    "author": "Erika Burkart",
    "poem": {
        "line.1": {
            "text": "Vor nachtgr\u00fcnen Matten"
        },
        "line.2": {
            "text": "unterwegs zu W\u00f6rtern,"
        },
        "line.3": {
            "text": "Klartext zu einem"
        },
        "line.4": {
            "text": "dunklen Gedicht."
        },
        "line.5": {
            "text": "Das innere und das \u00e4ussere Land,"
        },
        "line.6": {
            "text": "der Himmel fl\u00f6sst G\u00e4rten, Getrennte"
        },
        "line.7": {
            "text": "gehen dort Hand in Hand."
        },
        "line.8": {
            "text": "In zeichenm\u00e4chtiger Schw\u00e4rze"
        },
        "line.9": {
            "text": "vom Dachrand flatternd ein Vogel,"
        },
        "line.10": {
            "text": "Gedanke, Engel in Fetzen,"
        },
        "line.11": {
            "text": "der im simmernden Grau-Licht"
        },
        "line.12": {
            "text": "verschwand."
        }
    }
}