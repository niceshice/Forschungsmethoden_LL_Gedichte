    "gender": "w",
    "author": "Christine N\u00f6stlinger",
    "poem": {
        "line.1": {
            "text": "Vater, hier bin ich!"
        },
        "line.2": {
            "text": "Das Bein ist gebrochen."
        },
        "line.3": {
            "text": "Den Gips,"
        },
        "line.4": {
            "text": "den behalt ich"
        },
        "line.5": {
            "text": "f\u00fcr sieben Wochen."
        },
        "line.6": {
            "text": "Walters Vater"
        },
        "line.7": {
            "text": "wird dich verklagen,"
        },
        "line.8": {
            "text": "denn ich habe dem Walter"
        },
        "line.9": {
            "text": "die Brille zerschlagen."
        },
        "line.10": {
            "text": "Was von den Skiern"
        },
        "line.11": {
            "text": "noch \u00fcbrig ist,"
        },
        "line.12": {
            "text": "liegt teils auf der Piste,"
        },
        "line.13": {
            "text": "teils auf dem Mist."
        },
        "line.14": {
            "text": "Wegen der Rechnung"
        },
        "line.15": {
            "text": "f\u00fcr die Fensterscheiben"
        },
        "line.16": {
            "text": "wird dir der Wirt"
        },
        "line.17": {
            "text": "pers\u00f6nlich schreiben."
        },
        "line.18": {
            "text": "Und den Fotoapparat,"
        },
        "line.19": {
            "text": "den find ich nicht mehr."
        },
        "line.20": {
            "text": "Herzlichen Gl\u00fcckwunsch"
        },
        "line.21": {
            "text": "zu meiner Wiederkehr!"
        }
    }
}