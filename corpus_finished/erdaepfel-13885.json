{
    "title": "erdaepfel-13885",
    "categories": [
        "Natur",
        "Tiere"
    ],
    "gender": "w",
    "author": "Kerstin Becker",
    "poem": {
        "line.1": {
            "text": "Wirbeltiere mit den krummen R\u00fccken"
        },
        "line.2": {
            "text": "b\u00fccken wir biegen uns fassen"
        },
        "line.3": {
            "text": "die K\u00fchle klebender Klumpen"
        },
        "line.4": {
            "text": "Trauerr\u00e4nder wir stoppeln"
        },
        "line.5": {
            "text": "Hohlgenagte"
        },
        "line.6": {
            "text": "stochern M\u00e4usen nach stolpern"
        },
        "line.7": {
            "text": "Ackerschmalz auf den Lippen"
        },
        "line.8": {
            "text": "werfen uns Runde"
        },
        "line.9": {
            "text": "wie Liebesperlen zu bei\u00dfen"
        },
        "line.10": {
            "text": "zwischen den Furchen"
        },
        "line.11": {
            "text": "ins Ungesch\u00e4lte"
        }
    }
}