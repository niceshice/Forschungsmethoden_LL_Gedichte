    "poem": {
        "line.1": {
            "text": "ich m\u00f6chte wach sein aber"
        },
        "line.2": {
            "text": "du musst schlafen"
        },
        "line.3": {
            "text": "du h\u00f6rst heut abend tango sagst das"
        },
        "line.4": {
            "text": "w\u00e4re die musik nach der dir ist und mich"
        },
        "line.5": {
            "text": "verlangt es mehr nach stille"
        },
        "line.6": {
            "text": "und als wir tanzen wollen"
        },
        "line.7": {
            "text": "trittst du mir immer wieder auf die f\u00fc\u00dfe"
        },
        "line.8": {
            "text": "du schw\u00e4rmst von duftend und"
        },
        "line.9": {
            "text": "wei\u00dfbl\u00fchendem jasmin"
        },
        "line.10": {
            "text": "ich h\u00f6r die mutter klagen \u00fcber ihn"
        },
        "line.11": {
            "text": "ich stecke mir hibiskus an"
        },
        "line.12": {
            "text": "du hasst dies blumen brechen"
        },
        "line.13": {
            "text": "wo es doch blumen gibt aus plastik und"
        },
        "line.14": {
            "text": "aus seide"
        },
        "line.15": {
            "text": "ich les ein buch und du"
        },
        "line.16": {
            "text": "schl\u00e4gst deines zu"
        },
        "line.17": {
            "text": "du sagst das rot auf deinen lippen"
        },
        "line.18": {
            "text": "steht dir nicht ich geh und wisch es fort"
        },
        "line.19": {
            "text": "ich zieh ein leichtes wei\u00df \u00fcber und seh dir an"
        },
        "line.20": {
            "text": "du denkst mir w\u00fcrde schwarz viel besser stehn"
        },
        "line.21": {
            "text": "doch du sagst nichts der tag ist lang"
        },
        "line.22": {
            "text": "beklommen vergehen die stunden"
        },
        "line.23": {
            "text": "den hibiskus tu"
        },
        "line.24": {
            "text": "bitte in den m\u00fcll sagst du"
        },
        "line.25": {
            "text": "die bl\u00fcten machen mich allergisch"
        },
        "line.26": {
            "text": "vor allem in geschlossenen r\u00e4umen"
        },
        "line.27": {
            "text": "du legst dich ins bett du brauchst deine ruh"
        },
        "line.28": {
            "text": "ich bin ganz wach und du beginnst zu tr\u00e4umen"
        }
    }
