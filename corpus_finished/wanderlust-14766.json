    "poem": {
        "line.1": {
            "text": "als du dich aus mir rausziehst ert\u00f6nt"
        },
        "line.2": {
            "text": "das wandern ist des m\u00fcllers"
        },
        "line.3": {
            "text": "lust doch ich kann gar nicht so"
        },
        "line.4": {
            "text": "lustig gucken wie du mit"
        },
        "line.5": {
            "text": "deinen sch\u00f6nen augen im moment"
        },
        "line.6": {
            "text": "dann schn\u00fcrst du schon dein"
        },
        "line.7": {
            "text": "b\u00fcndel siehst nach vorn nach hinten"
        },
        "line.8": {
            "text": "sagst ich muss jetzt ins b\u00fcro"
        },
        "line.9": {
            "text": "in wirklichkeit als du das sagst"
        },
        "line.10": {
            "text": "bist du schon fort"
        },
        "line.11": {
            "text": "du schreibst mir einen brief"
        },
        "line.12": {
            "text": "auf dem computer und adressierst ihn an"
        },
        "line.13": {
            "text": "die m\u00fcllerin was du nicht wei\u00dft"
        },
        "line.14": {
            "text": "auch m\u00fcllerinnen haben ihren stolz"
        },
        "line.15": {
            "text": "und ihren preis"
        },
        "line.16": {
            "text": "sie warten bis das mehl"
        },
        "line.17": {
            "text": "gemahlen ist und keinen augenblick"
        },
        "line.18": {
            "text": "mehr l\u00e4nger auch wenn das telefon"
        },
        "line.19": {
            "text": "verbunden ist bis ins b\u00fcro"
        },
        "line.20": {
            "text": "auch wenn nun briefe tr\u00f6pfeln regnen"
        },
        "line.21": {
            "text": "was h\u00e4lt mich hier ich gehe fort"
        },
        "line.22": {
            "text": "das w\u00e4re doch gelacht"
        },
        "line.23": {
            "text": "ich bin die m\u00fcllerin vertr\u00e4ge gibt"
        },
        "line.24": {
            "text": "es nicht die liebe liebt"
        },
        "line.25": {
            "text": "das wandern gott hat sie so gemacht"
        }
