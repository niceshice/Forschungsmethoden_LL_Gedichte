{
    "title": "auszaehlreime-1420",
    "categories": [
        "Lyrik f\u00fcr Kinder",
        "Gesellschaftskritik",
        "Leben & Beziehungen",
        "Reimgedicht"
    ],
    "gender": "w",
    "author": "Christine N\u00f6stlinger",
    "poem": {
        "line.1": {
            "text": "Einer ist reich"
        },
        "line.2": {
            "text": "und einer ist arm,"
        },
        "line.3": {
            "text": "einer erfriert"
        },
        "line.4": {
            "text": "und einer hat's warm."
        },
        "line.5": {
            "text": "Einer stiehlt"
        },
        "line.6": {
            "text": "und einer kauft,"
        },
        "line.7": {
            "text": "einer schwimmt oben"
        },
        "line.8": {
            "text": "und einer ersauft."
        },
        "line.9": {
            "text": "Einer riecht gut"
        },
        "line.10": {
            "text": "und einer stinkt,"
        },
        "line.11": {
            "text": "einer f\u00e4hrt weg"
        },
        "line.12": {
            "text": "und einer winkt."
        },
        "line.13": {
            "text": "Einer hat \u00dcberflu\u00df"
        },
        "line.14": {
            "text": "und einer hat Sorgen,"
        },
        "line.15": {
            "text": "einer kann schenken"
        },
        "line.16": {
            "text": "und einer mu\u00df borgen."
        },
        "line.17": {
            "text": "Einer hat Hunger"
        },
        "line.18": {
            "text": "und einer hat Brot."
        },
        "line.19": {
            "text": "Einer lebt noch"
        },
        "line.20": {
            "text": "und einer ist tot."
        }
    }
}