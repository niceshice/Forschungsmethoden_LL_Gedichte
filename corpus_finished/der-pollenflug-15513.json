{
    "title": "der-pollenflug-15513",
    "categories": [
        "Natur",
        "Fr\u00fchling",
        "Landschaft",
        "Pflanzen"
    ],
    "gender": "m",
    "author": "Andr\u00e9 Schinkel",
    "poem": {
        "line.1": {
            "text": "Im Fr\u00fchjahr ziehen die Wolken,"
        },
        "line.2": {
            "text": "Rot-gelb vor bl\u00fchender Lust,"
        },
        "line.3": {
            "text": "Durch die Schatten, die gr\u00fcnen"
        },
        "line.4": {
            "text": "Hyperbeln des Vorgebirges;"
        },
        "line.5": {
            "text": "Die Amseln rufen vergeblich"
        },
        "line.6": {
            "text": "Und plustern die Br\u00fcste"
        },
        "line.7": {
            "text": "Gegen die Elstern, die strebsam"
        },
        "line.8": {
            "text": "Die Nester leerr\u00e4umen; und"
        },
        "line.9": {
            "text": "Der Pirol federt scheu den schwarz-"
        },
        "line.10": {
            "text": "Gelben Stempel der Flucht"
        },
        "line.11": {
            "text": "In die ausgelieferte Landschaft,"
        },
        "line.12": {
            "text": "Wo Pollenflug herrscht;"
        },
        "line.13": {
            "text": "Welch eine Gegend, denkst du,"
        },
        "line.14": {
            "text": "Reist bedauernsvoll ab: im"
        },
        "line.15": {
            "text": "R\u00fccken die Herden der H\u00fcgel,"
        },
        "line.16": {
            "text": "Die der Tr\u00e4nke zugehn."
        }
    }
}