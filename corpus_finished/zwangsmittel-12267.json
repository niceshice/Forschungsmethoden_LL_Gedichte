    "poem": {
        "line.1": {
            "text": "Nicht nur"
        },
        "line.2": {
            "text": "der Paraguayjasmin zittert"
        },
        "line.3": {
            "text": "eine olfaktorische Distanz"
        },
        "line.4": {
            "text": "zwischen meinen Gang"
        },
        "line.5": {
            "text": "und die Bodenstndigkeit"
        },
        "line.6": {
            "text": "dieses Mittwochs;"
        },
        "line.7": {
            "text": "Nicht nur"
        },
        "line.8": {
            "text": "die Verfgbarkeit stndige von"
        },
        "line.9": {
            "text": "Piperin, Ortungssignalen und Flugblttern"
        },
        "line.10": {
            "text": "macht jedes Impuls-Mbae"
        },
        "line.11": {
            "text": "gnzlich unzuverlssig;"
        },
        "line.12": {
            "text": "Nicht nur"
        },
        "line.13": {
            "text": "dieses stockende Nachzucken"
        },
        "line.14": {
            "text": "Eintrudeln von Sehreizen"
        },
        "line.15": {
            "text": "als Information"
        },
        "line.16": {
            "text": "pulverisiert die Bewegung"
        },
        "line.17": {
            "text": "wiegender Bltter"
        },
        "line.18": {
            "text": "kurzer Videos"
        },
        "line.19": {
            "text": "und berittener Polizey;"
        },
        "line.20": {
            "text": "Wo aber die groe Herde"
        },
        "line.21": {
            "text": "wartet da bist Du auch"
        },
        "line.22": {
            "text": "kleine Hekovia via Helios Rubine"
        },
        "line.23": {
            "text": "Celis immer ein bisschen zu"
        },
        "line.24": {
            "text": "Sonnenglanz zu"
        },
        "line.25": {
            "text": "Sonnenknall zu"
        },
        "line.26": {
            "text": "Splitter und Scherbe."
        },
        "line.27": {
            "text": "Glaubst Du wirklich"
        },
        "line.28": {
            "text": "dass der Pfeffer"
        },
        "line.29": {
            "text": "alles besser machen"
        },
        "line.30": {
            "text": "wird?"
        },
        "line.31": {
            "text": "Tucanasche soll man geben reiben"
        },
        "line.32": {
            "text": "gegen das da dass da hlfe in"
        },
        "line.33": {
            "text": "die Augen wider Trbung"
        },
        "line.34": {
            "text": "und Brechreiz:"
        },
        "line.35": {
            "text": "Egogo bogo kurur!,"
        },
        "line.36": {
            "text": "Du kleine Krte"
        },
        "line.37": {
            "text": "aus dem"
        },
        "line.38": {
            "text": "Flagmagmegtempela"
        },
        "line.39": {
            "text": "tmpelst Du Dich meinem"
        },
        "line.40": {
            "text": "Bezirk entgegen"
        },
        "line.41": {
            "text": "bezirzt betimpelst maulst"
        },
        "line.42": {
            "text": "was das Zeug hlt"
        },
        "line.43": {
            "text": "und im Zaum fllt einem"
        },
        "line.44": {
            "text": "dazu mutig Streik ein,"
        },
        "line.45": {
            "text": "Und wenn Ihr nicht auch noch"
        },
        "line.46": {
            "text": "phonetisch eine Dysbalance"
        },
        "line.47": {
            "text": "in diesen Luftraum aerosolt"
        },
        "line.48": {
            "text": "in dem man Krten"
        },
        "line.49": {
            "text": "von Frschen unterscheidet"
        },
        "line.50": {
            "text": "nicht und Froschkrallen"
        },
        "line.51": {
            "text": "Sehgift sind unter Zweigen, sind"
        },
        "line.52": {
            "text": "Schlielich paraguayische Vulkane:"
        },
        "line.53": {
            "text": "Phlegmatisches Magma"
        },
        "line.54": {
            "text": "mdeine Vorbehalte, mdein"
        },
        "line.55": {
            "text": "Wunsch nach Wandern,"
        },
        "line.56": {
            "text": "mdein ausbleibendes"
        },
        "line.57": {
            "text": "Ausbruchsverhalten."
        }
    }
