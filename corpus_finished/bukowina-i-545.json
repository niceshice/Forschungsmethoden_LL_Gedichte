{
    "title": "bukowina-i-545",
    "categories": [
        "Sprache",
        "Mutter",
        "Natur",
        "Tiere"
    ],
    "gender": "w",
    "author": "Rose Ausl\u00e4nder",
    "poem": {
        "line.1": {
            "text": "Gr\u00fcne Mutter"
        },
        "line.2": {
            "text": "Bukowina"
        },
        "line.3": {
            "text": "Schmetterlinge im Haar"
        },
        "line.4": {
            "text": "Trink"
        },
        "line.5": {
            "text": "sagt die Sonne"
        },
        "line.6": {
            "text": "rote Melonenmilch"
        },
        "line.7": {
            "text": "wei\u00dfe Kukuruzmilch"
        },
        "line.8": {
            "text": "ich machte sie s\u00fc\u00df"
        },
        "line.9": {
            "text": "Violette F\u00f6hrenzapfen"
        },
        "line.10": {
            "text": "Luftfl\u00fcgel V\u00f6gel und Laub"
        },
        "line.11": {
            "text": "Der Karpatenr\u00fccken"
        },
        "line.12": {
            "text": "v\u00e4terlich"
        },
        "line.13": {
            "text": "l\u00e4dt dich ein"
        },
        "line.14": {
            "text": "dich zu tragen"
        },
        "line.15": {
            "text": "Vier Sprachen"
        },
        "line.16": {
            "text": "Viersprachenlieder"
        },
        "line.17": {
            "text": "Menschen"
        },
        "line.18": {
            "text": "die sich verstehn"
        }
    }
}