{
    "title": "horizontbetrachter-3177",
    "categories": [
        "Natur",
        "Tiere"
    ],
    "gender": "m",
    "author": "Bj\u00f6rn  Kuhligk",
    "poem": {
        "line.1": {
            "text": "Hier ist ein Wald"
        },
        "line.2": {
            "text": "da sind die B\u00e4ume"
        },
        "line.3": {
            "text": "darin sind die Ringe"
        },
        "line.4": {
            "text": "darin schl\u00e4ft die Angst"
        },
        "line.5": {
            "text": "du schl\u00e4gst zu mit der Axt"
        },
        "line.6": {
            "text": "und trinkst den Harzstein mit"
        },
        "line.7": {
            "text": "und das Lieblingstier"
        },
        "line.8": {
            "text": "das ist der Affe im Zoo"
        },
        "line.9": {
            "text": "den kannst du besuchen"
        },
        "line.10": {
            "text": "und er dich nicht"
        }
    }
}