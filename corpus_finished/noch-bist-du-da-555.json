{
    "title": "noch-bist-du-da-555",
    "categories": [
        "Leben & Beziehungen",
        "Alter",
        "Identit\u00e4t (Individuum)"
    ],
    "gender": "w",
    "author": "Rose Ausl\u00e4nder",
    "poem": {
        "line.1": {
            "text": "Wirf deine Angst"
        },
        "line.2": {
            "text": "in die Luft"
        },
        "line.3": {
            "text": "Bald"
        },
        "line.4": {
            "text": "ist deine Zeit um"
        },
        "line.5": {
            "text": "bald"
        },
        "line.6": {
            "text": "w\u00e4chst der Himmel"
        },
        "line.7": {
            "text": "unter dem Gras"
        },
        "line.8": {
            "text": "fallen deine Tr\u00e4ume"
        },
        "line.9": {
            "text": "ins Nirgends"
        },
        "line.10": {
            "text": "Noch"
        },
        "line.11": {
            "text": "duftet die Nelke"
        },
        "line.12": {
            "text": "singt die Drossel"
        },
        "line.13": {
            "text": "noch darfst du lieben"
        },
        "line.14": {
            "text": "Worte verschenken"
        },
        "line.15": {
            "text": "noch bist du da"
        },
        "line.16": {
            "text": "Sei was du bist"
        },
        "line.17": {
            "text": "Gib was du hast"
        }
    }
}