    "title": "einen-vorgeborenen-13206",
    "categories": [
        "Leben & Beziehungen",
        "Religion / Spiritualit\u00e4t",
        "Schreiben (Gedichte)",
        "Cut-up-Rhythmen"
    ],
    "gender": "m",
    "author": "Max Czollek",
    "poem": {
        "line.1": {
            "text": "I."
        },
        "line.2": {
            "text": "in die st\u00e4dte kam ich zur stunde"
        },
        "line.3": {
            "text": "der"
        },
        "line.4": {
            "text": "hochzeiten"
        },
        "line.5": {
            "text": "als da"
        },
        "line.6": {
            "text": "freude herrschte"
        },
        "line.7": {
            "text": "unter den menschen"
        },
        "line.8": {
            "text": "ich tanzte mit ihnen"
        },
        "line.9": {
            "text": "schlief unter den stummen"
        },
        "line.10": {
            "text": "ohne sprache den mund voll"
        },
        "line.11": {
            "text": "gestopft mit br\u00fccken"
        },
        "line.12": {
            "text": "die kraft meiner arme"
        },
        "line.13": {
            "text": "ging in koffern"
        },
        "line.14": {
            "text": "trug ich die angst"
        },
        "line.15": {
            "text": "II."
        },
        "line.16": {
            "text": "es ist wahr"
        },
        "line.17": {
            "text": "ich tauchte im gro\u00dfen meer"
        },
        "line.18": {
            "text": "verlor mein haar dabei"
        },
        "line.19": {
            "text": "getragen von gl\u00fcck"
        },
        "line.20": {
            "text": "als das aussetzte"
        },
        "line.21": {
            "text": "war ich unterwegs"
        },
        "line.22": {
            "text": "die hoffnung d\u00fcnn wie ein blatt"
        },
        "line.23": {
            "text": "im wald (ich rede von b\u00e4umen"
        },
        "line.24": {
            "text": "rede ich)"
        },
        "line.25": {
            "text": "und kann den weg nicht finden"
        },
        "line.26": {
            "text": "zu den h\u00e4usern aus luft"
        },
        "line.27": {
            "text": "III."
        },
        "line.28": {
            "text": "wirklich ich lebe in zeiten"
        },
        "line.29": {
            "text": "wo die ungl\u00fccklichen nicht"
        },
        "line.30": {
            "text": "mehr weinen wir einfach"
        },
        "line.31": {
            "text": "weiterschreiben"
        },
        "line.32": {
            "text": ""
        },
        "line.33": {
            "text": "\u00fcberall"
        },
        "line.34": {
            "text": "die finger am abzug wer"
        },
        "line.35": {
            "text": "kann da noch freundlich"
        },
        "line.36": {
            "text": "bleiben was hilft es wozu"
        },
        "line.37": {
            "text": "sind wir geworden am ende"
        },
        "line.38": {
            "text": "der eismeere"
        },
        "line.39": {
            "text": "wohin die stra\u00dfen f\u00fchrten"
        }
    }
}