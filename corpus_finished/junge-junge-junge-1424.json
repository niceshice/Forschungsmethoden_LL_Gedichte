    "gender": "m",
    "author": "Fredrik Vahle",
    "poem": {
        "line.1": {
            "text": "Heidi hat sich in Hannover verfranzt."
        },
        "line.2": {
            "text": "Silke ist in K\u00f6ln angepetert worden."
        },
        "line.3": {
            "text": "Bei Gabi hat es ausgerolft."
        },
        "line.4": {
            "text": "Katharina sieht so angejant aus."
        },
        "line.5": {
            "text": "Barbara macht einen v\u00f6llig \u00fcberfritzten Eindruck."
        },
        "line.6": {
            "text": "Christine ist ziemlich ausgeknutet."
        },
        "line.7": {
            "text": "Dorothea ist immer noch richtig verpault."
        },
        "line.8": {
            "text": "Nicole ist schon wieder zugeklaust."
        },
        "line.9": {
            "text": "Junge, Junge, Junge!"
        }
    }
}