{
    "title": "du-bist-eine-lilie-15524",
    "gender": "",
    "author": "André Schinkel",
    "poem": {
        "line.1": {
            "text": "Du bist eine Lilie"
        },
        "line.2": {
            "text": "Aus Lodern und Schnee –"
        },
        "line.3": {
            "text": "Ein Licht, ein graziles,"
        },
        "line.4": {
            "text": "Aus Düften gedreht."
        },
        "line.5": {
            "text": "Ich lieb den Gedanken,"
        },
        "line.6": {
            "text": "Daß du mich so liebst,"
        },
        "line.7": {
            "text": "Als gäbs keine Schranken"
        },
        "line.8": {
            "text": "Im Sehnsuchts-Betrieb."
        },
        "line.9": {
            "text": "Timalien, Tangaren"
        },
        "line.10": {
            "text": "Umschwirren dich bunt –"
        },
        "line.11": {
            "text": "Ihre Syrinx-Fanfaren"
        },
        "line.12": {
            "text": "Bereisen das Rund:"
        },
        "line.13": {
            "text": "Aus Rauch und Bromelie"
        },
        "line.14": {
            "text": "Und Feuer bei Nacht –"
        },
        "line.15": {
            "text": "Aus Glanz und Corbelie,"
        },
        "line.16": {
            "text": "Blatt und Küssen gemacht."
        },
        "line.17": {
            "text": "Gefiederte Träume –"
        },
        "line.18": {
            "text": "Bis tief in den Tag"
        },
        "line.19": {
            "text": "Durchfluten die Räume,"
        },
        "line.20": {
            "text": "In die ich dich trag."
        },
        "line.21": {
            "text": "Als gäbs keine Schranken"
        },
        "line.22": {
            "text": "Im Sehnsuchts-Betrieb:"
        },
        "line.23": {
            "text": "Lieb ich den Gedanken,"
        },
        "line.24": {
            "text": "Daß du mich so liebst."
        },
        "line.25": {
            "text": "Ich tauche den Schnabel"
        },
        "line.26": {
            "text": "Ganz tief in dich ein;"
        },
        "line.27": {
            "text": "Ich führe die Wabe"
        },
        "line.28": {
            "text": "Im Flügelkleid heim:"
        },
        "line.29": {
            "text": "Eine Glücks-Amazilie"
        },
        "line.30": {
            "text": "Im Feder-Karree –"
        },
        "line.31": {
            "text": "Du bist eine Lilie"
        },
        "line.32": {
            "text": "Aus Lodern und Schnee."
        }
    }
}