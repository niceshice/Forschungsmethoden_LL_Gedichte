    "gender": "m",
    "author": "Martin  Piekar",
    "poem": {
        "line.1": {
            "text": "III"
        },
        "line.2": {
            "text": "Ich frage einen Orkan. zgig."
        },
        "line.3": {
            "text": "Ich frage mich ein paar"
        },
        "line.4": {
            "text": "Hundert Kilometer ber deine Antwort."
        },
        "line.5": {
            "text": "Mglichkeiten im Zugzwang."
        },
        "line.6": {
            "text": "Pseudofhrten im Verstand gewittert."
        },
        "line.7": {
            "text": "Fensterrahmen projizieren Landschaften"
        },
        "line.8": {
            "text": "Als Vestibl. vorbeihastendes Terrain"
        },
        "line.9": {
            "text": "Haltlos in meinen Augen. derzeitig"
        },
        "line.10": {
            "text": "Ohne Anhaltspunkt. one-way. wenn nichts"
        },
        "line.11": {
            "text": "Bestimmt ist zu harren. Reisednis."
        },
        "line.12": {
            "text": "Fragen wie Wunden. die sich"
        },
        "line.13": {
            "text": "Nicht schlieen. und in den paar"
        },
        "line.14": {
            "text": "Hundert Kilometern werde ich"
        },
        "line.15": {
            "text": "Dich nicht fragen. weil es"
        },
        "line.16": {
            "text": "Wundgedacht ist. ausgefragt nach Hause."
        },
        "line.17": {
            "text": "Hier, dort wartet immer"
        },
        "line.18": {
            "text": "Eine Frau  nein nicht mehr Mutter"
        },
        "line.19": {
            "text": "eine Frau,"
        },
        "line.20": {
            "text": "Die meinen Kopf zu halten"
        },
        "line.21": {
            "text": "Versteht."
        }
    }
}