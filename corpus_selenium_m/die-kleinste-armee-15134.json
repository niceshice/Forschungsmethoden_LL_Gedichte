{
    "title": "die-kleinste-armee-15134",
    "categories": [
        "Leben & Beziehungen"
    ],
    "gender": "m",
    "author": "Georg Le√ü",
    "poem": {
        "line.1": {
            "text": "sie marschiert gleich zu zweit, diese kleinste Armee"
        },
        "line.2": {
            "text": "nicht die kleine, die kleinste"
        },
        "line.3": {
            "text": "auch ber die Parkpltze? ja"
        },
        "line.4": {
            "text": "auch ber die Parkpltze"
        },
        "line.5": {
            "text": "durch die Bsche? ja"
        },
        "line.6": {
            "text": "auch durch die Bsche"
        },
        "line.7": {
            "text": "durchs Schilf? ja"
        },
        "line.8": {
            "text": "selbstverstndlich auch durchs Schilf"
        },
        "line.9": {
            "text": "ber die Wipfel? nein"
        },
        "line.10": {
            "text": "ber die Wipfel nun gerade nicht"
        },
        "line.11": {
            "text": "marschiert und marschiert"
        },
        "line.12": {
            "text": "die kleinste Armee"
        }
    }
}