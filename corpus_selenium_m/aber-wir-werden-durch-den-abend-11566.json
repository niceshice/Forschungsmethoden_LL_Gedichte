{
    "title": "aber-wir-werden-durch-den-abend-11566",
    "categories": [
        "Leben & Beziehungen"
    ],
    "gender": "m",
    "author": "Michael Donhauser",
    "poem": {
        "line.1": {
            "text": "Aber wir werden"
        },
        "line.2": {
            "text": "durch den Abend"
        },
        "line.3": {
            "text": "ewige Gebrde"
        },
        "line.4": {
            "text": "leicht bewahren"
        },
        "line.5": {
            "text": "erahnte Sterne"
        },
        "line.6": {
            "text": "Denn es waren"
        },
        "line.7": {
            "text": "Tage im Licht"
        },
        "line.8": {
            "text": "und wir lagen"
        },
        "line.9": {
            "text": "gaben uns selig"
        },
        "line.10": {
            "text": "fremde Namen"
        },
        "line.11": {
            "text": "Nannten den Abend"
        },
        "line.12": {
            "text": "nannten uns leise"
        },
        "line.13": {
            "text": "werden versiegen"
        },
        "line.14": {
            "text": "sanft und bleiben"
        },
        "line.15": {
            "text": "wie berhrt"
        }
    }
}