{
    "title": "mystical-wife-11833",
    "categories": [
        "Poetologische Gedichte",
        "Schreiben (Gedichte)",
        "Leben & Beziehungen"
    ],
    "gender": "m",
    "author": "Steffen  Jacobs",
    "poem": {
        "line.1": {
            "text": "Deine Hand an meinem Vers,"
        },
        "line.2": {
            "text": "Heben, Senken, schnes Schwanken."
        },
        "line.3": {
            "text": "Jetzt erst bin ich wirklich hier"
        },
        "line.4": {
            "text": "und bin nicht nur in Gedanken."
        },
        "line.5": {
            "text": "Strophen, ganz in deiner Hand"
        },
        "line.6": {
            "text": "finden sie ins Freie, Lichte."
        },
        "line.7": {
            "text": "Erst wenn du sie nicht mehr hltst,"
        },
        "line.8": {
            "text": "sind es wieder nur Gedichte."
        },
        "line.9": {
            "text": "Und du lt sie leichthin fallen,"
        },
        "line.10": {
            "text": "ffnest deinen Mund zum Schlu,"
        },
        "line.11": {
            "text": "sagst: So wisse, Lieber, mein"
        },
        "line.12": {
            "text": "Rhythmus ist der Daktylus."
        }
    }
}