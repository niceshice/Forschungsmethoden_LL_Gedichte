    "poem": {
        "line.1": {
            "text": "1"
        },
        "line.2": {
            "text": "von drauen von hohenschnhausen"
        },
        "line.3": {
            "text": "komm ich her, wo sie zu hause sind,"
        },
        "line.4": {
            "text": "und was berichten sie vom mann"
        },
        "line.5": {
            "text": "der nachts im fenster steht,"
        },
        "line.6": {
            "text": "an heizungsrohre schlgt"
        },
        "line.7": {
            "text": "und in die trume glotzt?"
        },
        "line.8": {
            "text": "die zhne zeig ihm"
        },
        "line.9": {
            "text": "wenn du schlafen kannst"
        },
        "line.10": {
            "text": "(und auch diese fahne"
        },
        "line.11": {
            "text": "ist rot)."
        },
        "line.12": {
            "text": "2"
        },
        "line.13": {
            "text": "darauf eine mitarbeit"
        },
        "line.14": {
            "text": "und einen mitarbeiter"
        },
        "line.15": {
            "text": "mehr pro wohneinheit,"
        },
        "line.16": {
            "text": "arbeit ist ja jetzt berhaupt"
        },
        "line.17": {
            "text": "das wichtigste (ein hkchen"
        },
        "line.18": {
            "text": "an jedes ende des kreuzes)."
        },
        "line.19": {
            "text": "3"
        },
        "line.20": {
            "text": "die nacht ertchtigt dich im gasbeton."
        },
        "line.21": {
            "text": "nimm die hnde aus der tasche,"
        },
        "line.22": {
            "text": "schneid dir mal die haare,"
        },
        "line.23": {
            "text": "pickelrobert unter linden,"
        },
        "line.24": {
            "text": "plattentrottel unter plattentrotteln,"
        },
        "line.25": {
            "text": "zieh nicht leine, das ist deine sache"
        },
        "line.26": {
            "text": "hier, du deutsche sau,"
        },
        "line.27": {
            "text": "du knstler."
        }
