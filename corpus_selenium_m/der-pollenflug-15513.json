{
    "title": "der-pollenflug-15513",
    "gender": "m",
    "author": "Andr√© Schinkel",
    "poem": {
        "line.1": {
            "text": "Im Frhjahr ziehen die Wolken,"
        },
        "line.2": {
            "text": "Rot-gelb vor blhender Lust,"
        },
        "line.3": {
            "text": "Durch die Schatten, die grnen"
        },
        "line.4": {
            "text": "Hyperbeln des Vorgebirges;"
        },
        "line.5": {
            "text": "Die Amseln rufen vergeblich"
        },
        "line.6": {
            "text": "Und plustern die Brste"
        },
        "line.7": {
            "text": "Gegen die Elstern, die strebsam"
        },
        "line.8": {
            "text": "Die Nester leerrumen; und"
        },
        "line.9": {
            "text": "Der Pirol federt scheu den schwarz-"
        },
        "line.10": {
            "text": "Gelben Stempel der Flucht"
        },
        "line.11": {
            "text": "In die ausgelieferte Landschaft,"
        },
        "line.12": {
            "text": "Wo Pollenflug herrscht;"
        },
        "line.13": {
            "text": "Welch eine Gegend, denkst du,"
        },
        "line.14": {
            "text": "Reist bedauernsvoll ab: im"
        },
        "line.15": {
            "text": "Rcken die Herden der Hgel,"
        },
        "line.16": {
            "text": "Die der Trnke zugehn."
        }
    }
}