{
    "title": "expedition-10074",
    "categories": [
        "Natur",
        "Landschaft"
    ],
    "gender": "m",
    "author": "Andreas Neeser",
    "poem": {
        "line.1": {
            "text": "Am Wegrecht vorbei"
        },
        "line.2": {
            "text": "der Aufstieg durchs Land unserer Mütter."
        },
        "line.3": {
            "text": "Blank liegt die Steinader, Gras"
        },
        "line.4": {
            "text": "wächst nach innen"
        },
        "line.5": {
            "text": "wir queren die Halde"
        },
        "line.6": {
            "text": "Geschmirgel im dumpfen Geröll."
        },
        "line.7": {
            "text": "Schrittweise legt sich der Schlagstaub"
        },
        "line.8": {
            "text": "ich messe die Höhe der Wand"
        },
        "line.9": {
            "text": "wenn es Abend wird"
        },
        "line.10": {
            "text": "treibe ich den Haken ins taube Gestein."
        }
    }
}