    "poem": {
        "line.1": {
            "text": "der luftleere Raum war uns"
        },
        "line.2": {
            "text": "zu unpersnlich. wir strebten auseinander"
        },
        "line.3": {
            "text": "Hanna geriet in ein Energiefeld"
        },
        "line.4": {
            "text": "auerhalb des Intendierten. so jedenfalls"
        },
        "line.5": {
            "text": "berichtete sie. als wir wieder zusammenfanden"
        },
        "line.6": {
            "text": "dass es unertrglich sei. in einer Raum-"
        },
        "line.7": {
            "text": "Zeit- und Bewegungsextension"
        },
        "line.8": {
            "text": "zu denken"
        },
        "line.9": {
            "text": "und Hanna liebte ihren Krper"
        },
        "line.10": {
            "text": "gut mglich dass die Grundkonfiguration"
        },
        "line.11": {
            "text": "ihrer Neuronensoftware nicht ausreichte"
        },
        "line.12": {
            "text": "zumindest nicht. um die Progressivform"
        },
        "line.13": {
            "text": "zu integrieren. also machte sie kehrt"
        },
        "line.14": {
            "text": "noch ehe ich mich entschuldigen konnte"
        },
        "line.15": {
            "text": "und noch wollte ich mich"
        },
        "line.16": {
            "text": "entschuldigen"
        },
        "line.17": {
            "text": "zu spt: um uns herum bildeten sich"
        },
        "line.18": {
            "text": "durchsichtige Basalt- und Eisenmolekle"
        },
        "line.19": {
            "text": "die beiden Grundelemente des Stdtischen"
        },
        "line.20": {
            "text": "und wir berprften noch einmal die Koordinaten"
        },
        "line.21": {
            "text": "unserer Radargerte. und stellten fest"
        },
        "line.22": {
            "text": "unter uns fhrte die Landebahn direkt"
        },
        "line.23": {
            "text": "in die Zivilisation."
        }
    }
