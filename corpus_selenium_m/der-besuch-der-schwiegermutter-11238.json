    "gender": "m",
    "author": "Thomas Rosenl√∂cher",
    "poem": {
        "line.1": {
            "text": "Das hab ich bei berdbel gekauft."
        },
        "line.2": {
            "text": "Ja, Nappi ist billiger. Ehrlich gesagt."
        },
        "line.3": {
            "text": "Allerdings. Immerhin. Teils, teils."
        },
        "line.4": {
            "text": "Blo keinen Kerl mehr ins Haus."
        },
        "line.5": {
            "text": "Am Tittisee. Allein der Name."
        },
        "line.6": {
            "text": "Der ganze Bus hat gelacht."
        },
        "line.7": {
            "text": "War frher auch lustiger bei euch."
        },
        "line.8": {
            "text": "Gibts hier kein Rollwoll Kaufhaus?"
        },
        "line.9": {
            "text": "Wolln alle dasselbe, die Kerle."
        },
        "line.10": {
            "text": "Die Socken gewaschen. Das wars."
        },
        "line.11": {
            "text": "Obwohl. Na ja. Klappt vielleicht noch."
        },
        "line.12": {
            "text": "Was guckst du so, Schwiegersohn?"
        },
        "line.13": {
            "text": "Schreib mal ein Gedicht ber mich."
        },
        "line.14": {
            "text": "Nein, nicht von Goethe. Was Schnes."
        },
        "line.15": {
            "text": "Htte so gern wieder einmal gelacht."
        }
    }
}