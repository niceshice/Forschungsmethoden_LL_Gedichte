{
    "title": "der-donau-2616",
    "gender": "m",
    "author": "Werner SÃ¶llner",
    "poem": {
        "line.1": {
            "text": "Halb lag sie auf Erde"
        },
        "line.2": {
            "text": "halb lag sie auf Gras"
        },
        "line.3": {
            "text": "Und etwas sich flieend"
        },
        "line.4": {
            "text": "nach innen las"
        },
        "line.5": {
            "text": "Halb schlo er die Augen"
        },
        "line.6": {
            "text": "und halb war er ganz"
        },
        "line.7": {
            "text": "Sprechende Pappeln"
        },
        "line.8": {
            "text": "und Kieselglanz"
        },
        "line.9": {
            "text": "Halb atme ich Wasser"
        },
        "line.10": {
            "text": "und halb trink ich Stein"
        },
        "line.11": {
            "text": "Ist fast vergangen"
        },
        "line.12": {
            "text": "Bald wird es sein"
        }
    }
}